      "utterance": [
        {
          "id": "MDRW2100019288.1.1",
          "form": "우인단 조무래기들이",
          "original_form": "우인단 조무래기들이",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019288.1.2",
          "form": "자꾸 말을 해",
          "original_form": "자꾸 말을 해",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019288.1.3",
          "form": "감우퀘하는게 갑자기 감우가 범인은 쟤라고",
          "original_form": "감우퀘하는게 갑자기 감우가 범인은 쟤라고",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019288.1.4",
          "form": "길가던 우인단 조무래기를 집는거야",
          "original_form": "길가던 우인단 조무래기를 집는거야",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019288.1.5",
          "form": "근데 걘 딸기 따먹고있었대.....",
          "original_form": "근데 걘 딸기 따먹고있었대.....",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019288.1.6",
          "form": "뭔가 불쌍해졌어...",
          "original_form": "뭔가 불쌍해졌어...",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019288.1.7",
          "form": "아 ㅋㅋㅋㅋㅋㅋ 감우 퀘스트에 그런 내용이 있었나??",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ 감우 퀘스트에 그런 내용이 있었나??",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019288.1.8",
          "form": "근데 진짜 몬드랑 리월까지만 해도",
          "original_form": "근데 진짜 몬드랑 리월까지만 해도",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019288.1.9",
          "form": "악역들이 약간",
          "original_form": "악역들이 약간",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019288.1.10",
          "form": "로켓단 느낌이었거든?",
          "original_form": "로켓단 느낌이었거든?",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019288.1.11",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ감우가 선인의 감으로",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ감우가 선인의 감으로",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019288.1.12",
          "form": "범인은 사실 저 우인단 조무래기같은데요...!",
          "original_form": "범인은 사실 저 우인단 조무래기같은데요...!",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.13",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.14",
          "form": "아니였어...",
          "original_form": "아니였어...",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.15",
          "form": "선인의 감 무슨 의미가 잇엇냐구ㅋㅋㅋ.....",
          "original_form": "선인의 감 무슨 의미가 잇엇냐구ㅋㅋㅋ.....",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.16",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.17",
          "form": "근데 갑자기 이나즈마 오니까",
          "original_form": "근데 갑자기 이나즈마 오니까",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.18",
          "form": "죄다 음침해졌어.....",
          "original_form": "죄다 음침해졌어.....",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019288.1.19",
          "form": "몹이 막 원령이질 않나",
          "original_form": "몹이 막 원령이질 않나",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.20",
          "form": "길가는 무사들이 덤벼서 다 죽이고다니고있고",
          "original_form": "길가는 무사들이 덤벼서 다 죽이고다니고있고",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.21",
          "form": "온갖곳이 결계에",
          "original_form": "온갖곳이 결계에",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.22",
          "form": "ㅇㅇ 이나즈마 진짜 음침해",
          "original_form": "ㅇㅇ 이나즈마 진짜 음침해",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.23",
          "form": "이것도 의도한건가??",
          "original_form": "이것도 의도한건가??",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.24",
          "form": "컨텐츠를 그렇게 섬세하게 제작했을까?",
          "original_form": "컨텐츠를 그렇게 섬세하게 제작했을까?",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019288.1.25",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.26",
          "form": "막 몇백년간 쌓인",
          "original_form": "막 몇백년간 쌓인",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.27",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.28",
          "form": "애ㄱㄱㄱㄱ막이",
          "original_form": "애ㄱㄱㄱㄱ막이",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.29",
          "form": "앸막이!!!!",
          "original_form": "앸막이!!!!",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.30",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019288.1.31",
          "form": "엥 그러게?",
          "original_form": "엥 그러게?",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019288.1.32",
          "form": "박침",
          "original_form": "박침",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019288.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019288.1.34",
          "form": "여튼 대ㅣ앸ㅣ막이같은거나",
          "original_form": "여튼 대ㅣ앸ㅣ막이같은거나",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019288.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.36",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.37",
          "form": "ㅅㅣ키고있음도",
          "original_form": "ㅅㅣ키고있음도",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.38",
          "form": "갸박친다고 쓰고싶은데",
          "original_form": "갸박친다고 쓰고싶은데",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.39",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.40",
          "form": "걍 국가자체가 좀",
          "original_form": "걍 국가자체가 좀",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.41",
          "form": "음침해",
          "original_form": "음침해",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.42",
          "form": "이게....중국이 보는 일본.....?",
          "original_form": "이게....중국이 보는 일본.....?",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019288.1.43",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019288.1.44",
          "form": "심지어 평범한 중국인도 아니고",
          "original_form": "심지어 평범한 중국인도 아니고",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019288.1.45",
          "form": "일본 서브컬쳐에 빠질 대로 빠져있는 중국인이",
          "original_form": "일본 서브컬쳐에 빠질 대로 빠져있는 중국인이",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019288.1.46",
          "form": "만들어낸 게임인데",
          "original_form": "만들어낸 게임인데",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019288.1.47",
          "form": "내용 대체 무엇...",
          "original_form": "내용 대체 무엇...",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019288.1.48",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ일뽕라노벨오타쿠중국인이",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ일뽕라노벨오타쿠중국인이",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019288.1.49",
          "form": "만든건데",
          "original_form": "만든건데",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019288.1.50",
          "form": "이지경이야",
          "original_form": "이지경이야",
