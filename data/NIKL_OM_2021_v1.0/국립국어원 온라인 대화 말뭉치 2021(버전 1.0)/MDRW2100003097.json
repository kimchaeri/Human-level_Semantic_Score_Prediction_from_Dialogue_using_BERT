        },
        {
          "id": "MDRW2100003097.1.2",
          "form": "세월이 ㅋㅋㅋ",
          "original_form": "세월이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.3",
          "form": "무척 재밌었어",
          "original_form": "무척 재밌었어",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.4",
          "form": "name1는?",
          "original_form": "&name1&는?",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.5",
          "form": "나도 ㅋㅋ",
          "original_form": "나도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.6",
          "form": "중고등학교때는 초 인싸였어",
          "original_form": "중고등학교때는 초 인싸였어",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.7",
          "form": "우와 흥미롭다",
          "original_form": "우와 흥미롭다",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.8",
          "form": "성격이 좀 바뀌나?",
          "original_form": "성격이 좀 바뀌나?",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003097.1.9",
          "form": "그럴 수 있지 ㅋㅋ",
          "original_form": "그럴 수 있지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003097.1.10",
          "form": "학급 임원도 계속했고",
          "original_form": "학급 임원도 계속했고",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003097.1.11",
          "form": "우와 멋지다",
          "original_form": "우와 멋지다",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003097.1.12",
          "form": "반 친구들 다 고루고루 친했어",
          "original_form": "반 친구들 다 고루고루 친했어",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003097.1.13",
          "form": "재밌었겠다",
          "original_form": "재밌었겠다",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003097.1.14",
          "form": "우리학교는 특히 급식이 완전 맛있어서",
          "original_form": "우리학교는 특히 급식이 완전 맛있어서",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003097.1.15",
          "form": "부러워 급식 ㅠㅠ",
          "original_form": "부러워 급식 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003097.1.16",
          "form": "급식이 전국적으로 유명했어 ㅋㅋㅋ",
          "original_form": "급식이 전국적으로 유명했어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003097.1.17",
          "form": "급식 먹고 싶다 ㅋㅋㅋ",
          "original_form": "급식 먹고 싶다 ㅋㅋㅋ",
