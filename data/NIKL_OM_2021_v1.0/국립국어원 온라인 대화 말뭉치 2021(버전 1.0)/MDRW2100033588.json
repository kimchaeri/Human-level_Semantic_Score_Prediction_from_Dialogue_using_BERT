      "utterance": [
        {
          "id": "MDRW2100033588.1.1",
          "form": "팔도요?",
          "original_form": "팔도요?",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100033588.1.2",
          "form": "팔도비빔면이용??",
          "original_form": "팔도비빔면이용??",
          "speaker_id": "2",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100033588.1.3",
          "form": "아 그러고보니 올 여름에 비빔면을 한번도 안먹었어요!!",
          "original_form": "아 그러고보니 올 여름에 비빔면을 한번도 안먹었어요!!",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100033588.1.4",
          "form": "역대급 더위라고 해서 평상시 여름처럼 입맛없겠다 싶었는데 ㅎㅎ 생각보다 더위도 짧았고 에어컨 덕분에 ㅎㅎㅎ 비빔면을 안 사먹었네요",
          "original_form": "역대급 더위라고 해서 평상시 여름처럼 입맛없겠다 싶었는데 ㅎㅎ 생각보다 더위도 짧았고 에어컨 덕분에 ㅎㅎㅎ 비빔면을 안 사먹었네요",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100033588.1.5",
          "form": "날씨와 입맛의 ㅋ오묘한 관계ㅋㅋ",
          "original_form": "날씨와 입맛의 ㅋ오묘한 관계ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033588.1.6",
          "form": "2018년인가.. 엄청더웠잖아요 ㅎㅎ 그때 비빔면 하루에 한개씩 먹었거든요 ㅎㅎ",
          "original_form": "2018년인가.. 엄청더웠잖아요 ㅎㅎ 그때 비빔면 하루에 한개씩 먹었거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100033588.1.7",
          "form": "올해는 비빔면 장사가 좀 안됐겠는데여? ㅋㅋㅋ",
          "original_form": "올해는 비빔면 장사가 좀 안됐겠는데여? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033588.1.8",
          "form": "비빔면 종류는 어떤거 드세요?",
          "original_form": "비빔면 종류는 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033588.1.9",
          "form": "와~ 하루에 한개 ㅋㅋ진비빔 양많고 매콤하더라구요",
          "original_form": "와~ 하루에 한개 ㅋㅋ진비빔 양많고 매콤하더라구요",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100033588.1.10",
          "form": "오 진비빔은 안먹어봤는데 기회가 되면 먹어봐야겠어요 ㅋㅋㅋ",
          "original_form": "오 진비빔은 안먹어봤는데 기회가 되면 먹어봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033588.1.11",
          "form": "전 아무래도 팔도비빔면이 제일 무난한것 같더라구욤",
          "original_form": "전 아무래도 팔도비빔면이 제일 무난한것 같더라구욤",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033588.1.12",
          "form": "그리구... 도토리쫄비빔면인가?",
          "original_form": "그리구... 도토리쫄비빔면인가?",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033588.1.13",
          "form": "엇 첨들어봐요",
          "original_form": "엇 첨들어봐요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033588.1.14",
          "form": "이거 혜리가 광고했었을거예요!",
          "original_form": "이거 혜리가 광고했었을거예요!",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033588.1.15",
          "form": "이거 매콤하고 맛있었어요 ㅋㅋ",
          "original_form": "이거 매콤하고 맛있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033588.1.16",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033588.1.17",
          "form": "추천합니당ㅋㅋㅋ",
          "original_form": "추천합니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100033588.1.18",
          "form": "내년여름엔ㅋㅋㅋ 그걸먹어보겠어요",
          "original_form": "내년여름엔ㅋㅋㅋ 그걸먹어보겠어요",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100033588.1.19",
          "form": "ㅋㅋㅋㅋㅋ1년 기다려야겠네요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ1년 기다려야겠네요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033588.1.20",
          "form": "비가 그치고 다시 좀 더워졌으면 좋겠어요.",
          "original_form": "비가 그치고 다시 좀 더워졌으면 좋겠어요.",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033588.1.21",
          "form": "비빔면도 먹고 냉면도 먹게!!",
          "original_form": "비빔면도 먹고 냉면도 먹게!!",
          "speaker_id": "2",
