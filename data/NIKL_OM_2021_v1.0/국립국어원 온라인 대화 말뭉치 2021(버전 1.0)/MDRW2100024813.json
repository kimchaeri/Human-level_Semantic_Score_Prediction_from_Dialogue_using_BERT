      "utterance": [
        {
          "id": "MDRW2100024813.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100024813.1.2",
          "form": "아니요ㅜㅠ 지금 서브웨이쿠키 먹는 중이에요!",
          "original_form": "아니요ㅜㅠ 지금 서브웨이쿠키 먹는 중이에요!",
          "speaker_id": "2",
          "time": "20210831 19:03"
        },
        {
          "id": "MDRW2100024813.1.3",
          "form": "쿠기만 드셔도 괜찮으신가요?",
          "original_form": "쿠기만 드셔도 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024813.1.4",
          "form": "안괜찮아요ㅜㅠ",
          "original_form": "안괜찮아요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024813.1.5",
          "form": "9시에 저녁약속이 있어서요!!",
          "original_form": "9시에 저녁약속이 있어서요!!",
          "speaker_id": "2",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024813.1.6",
          "form": "아하! 늦은 약속이 있으시군요",
          "original_form": "아하! 늦은 약속이 있으시군요",
          "speaker_id": "1",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024813.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024813.1.8",
          "form": "저녁드셨나요???",
          "original_form": "저녁드셨나요???",
          "speaker_id": "2",
          "time": "20210831 19:05"
        },
        {
          "id": "MDRW2100024813.1.9",
          "form": "이따가 뭐 드시기로 하셨나요!?",
          "original_form": "이따가 뭐 드시기로 하셨나요!?",
          "speaker_id": "1",
          "time": "20210831 19:05"
        },
        {
          "id": "MDRW2100024813.1.10",
          "form": "네 전 먹었어요",
          "original_form": "네 전 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:05"
        },
        {
          "id": "MDRW2100024813.1.11",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024813.1.12",
          "form": "미역국이랑 있는 반찬이랑 밥 먹었어요ㅎㅎ",
          "original_form": "미역국이랑 있는 반찬이랑 밥 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024813.1.13",
          "form": "미역국 맛있겠다!!!",
          "original_form": "미역국 맛있겠다!!!",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024813.1.14",
          "form": "미역국 안먹은지 오래됐어요",
          "original_form": "미역국 안먹은지 오래됐어요",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024813.1.15",
          "form": "저도 오랜만에 먹었는데 너무 맛있더라고요!",
          "original_form": "저도 오랜만에 먹었는데 너무 맛있더라고요!",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024813.1.16",
          "form": "소고기 미역국이랑 홍합 미역국 중에 어떤 걸 더 좋아하시나요?",
          "original_form": "소고기 미역국이랑 홍합 미역국 중에 어떤 걸 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024813.1.17",
          "form": "소고기요!!!",
          "original_form": "소고기요!!!",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024813.1.18",
          "form": "무조건 소고기!!!",
          "original_form": "무조건 소고기!!!",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024813.1.19",
          "form": "소고기가 짱이죠!!",
          "original_form": "소고기가 짱이죠!!",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024813.1.20",
          "form": "저도 소고기 미역국 더 좋아해요ㅎㅎ",
          "original_form": "저도 소고기 미역국 더 좋아해요ㅎㅎ",
