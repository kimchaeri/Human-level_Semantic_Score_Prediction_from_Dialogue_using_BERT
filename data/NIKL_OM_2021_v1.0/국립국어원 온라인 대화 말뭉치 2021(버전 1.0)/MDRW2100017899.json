      "utterance": [
        {
          "id": "MDRW2100017899.1.1",
          "form": "최근 구매 물건은 무엇인가요?",
          "original_form": "최근 구매 물건은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017899.1.2",
          "form": "화장품이요",
          "original_form": "화장품이요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017899.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017899.1.4",
          "form": "어떤 화장품사셨어요?",
          "original_form": "어떤 화장품사셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017899.1.5",
          "form": "저는 고양이 용품이요",
          "original_form": "저는 고양이 용품이요",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017899.1.6",
          "form": "세럼샀구요 고양이용품이라면 장난감?",
          "original_form": "세럼샀구요 고양이용품이라면 장난감?",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017899.1.7",
          "form": "모래랑 사료요!",
          "original_form": "모래랑 사료요!",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017899.1.8",
          "form": "살 예정인 물건은 있으신가요?",
          "original_form": "살 예정인 물건은 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017899.1.9",
          "form": "저는 머그컵사려구요",
          "original_form": "저는 머그컵사려구요",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017899.1.10",
          "form": "집에서 쓸 머그컵이요?",
          "original_form": "집에서 쓸 머그컵이요?",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017899.1.11",
          "form": "사무실용이요",
          "original_form": "사무실용이요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017899.1.12",
          "form": "오래되서 바꾸려구요",
          "original_form": "오래되서 바꾸려구요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017899.1.13",
          "form": "맘에 드는건 찾으셨어요?",
          "original_form": "맘에 드는건 찾으셨어요?",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017899.1.14",
          "form": "노란색이랑 검정색중에 고민중이에요",
          "original_form": "노란색이랑 검정색중에 고민중이에요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100017899.1.15",
          "form": "같은 모양인가요?",
          "original_form": "같은 모양인가요?",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100017899.1.16",
          "form": "네 같은모양이에요 추천해주시겠어요?",
          "original_form": "네 같은모양이에요 추천해주시겠어요?",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100017899.1.17",
          "form": "저는 노란색이 좋다고 생각해요",
          "original_form": "저는 노란색이 좋다고 생각해요",
