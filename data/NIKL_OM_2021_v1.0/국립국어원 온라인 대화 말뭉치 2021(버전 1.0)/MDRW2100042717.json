      "utterance": [
        {
          "id": "MDRW2100042717.1.1",
          "form": "네네 지하철안이에요",
          "original_form": "네네 지하철안이에요",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042717.1.2",
          "form": "8시30분까지가야해서요",
          "original_form": "8시30분까지가야해서요",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042717.1.3",
          "form": "ㅜㅠ 비오는데 힘내세요",
          "original_form": "ㅜㅠ 비오는데 힘내세요",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042717.1.4",
          "form": "저도 곧 빗속으로",
          "original_form": "저도 곧 빗속으로",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042717.1.5",
          "form": "출퇴근시간 얼마나되세요?",
          "original_form": "출퇴근시간 얼마나되세요?",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042717.1.6",
          "form": "아직많이 안와요",
          "original_form": "아직많이 안와요",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042717.1.7",
          "form": "출근40분쯤요",
          "original_form": "출근40분쯤요",
          "speaker_id": "1",
          "time": "20210907 07:46"
        },
        {
          "id": "MDRW2100042717.1.8",
          "form": "머시나요??",
          "original_form": "머시나요??",
          "speaker_id": "1",
          "time": "20210907 07:46"
        },
        {
          "id": "MDRW2100042717.1.9",
          "form": "전 그래도 가까운편이라 다행이에요ㅜㅜ",
          "original_form": "전 그래도 가까운편이라 다행이에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042717.1.10",
          "form": "대중교통이용하세요?",
          "original_form": "대중교통이용하세요?",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042717.1.11",
          "form": "네네 이제 지하철 곧 내려요",
          "original_form": "네네 이제 지하철 곧 내려요",
          "speaker_id": "1",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042717.1.12",
          "form": "아하 9시까지인가보네요",
          "original_form": "아하 9시까지인가보네요",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100042717.1.13",
          "form": "네ㅋㅋ 가서 커피타고 이래저래ㅋㅋ",
          "original_form": "네ㅋㅋ 가서 커피타고 이래저래ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042717.1.14",
          "form": "열일하세요! 칼퇴해야죠",
          "original_form": "열일하세요! 칼퇴해야죠",
          "speaker_id": "2",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042717.1.15",
          "form": "비도오는데요",
          "original_form": "비도오는데요",
          "speaker_id": "2",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042717.1.16",
          "form": "넵 꼭 칼퇴하세요!!",
          "original_form": "넵 꼭 칼퇴하세요!!",
          "speaker_id": "1",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042717.1.17",
          "form": "오늘 바쁘시죠",
          "original_form": "오늘 바쁘시죠",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042717.1.18",
          "form": "회의때문에 시간 뺏겨요ㅜㅜ",
          "original_form": "회의때문에 시간 뺏겨요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042717.1.19",
          "form": "의미없는..",
          "original_form": "의미없는..",
          "speaker_id": "1",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042717.1.20",
          "form": "아...완전이해가요",
          "original_form": "아...완전이해가요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042717.1.21",
          "form": "회의는 필요한사람끼리했으면좋겠어요",
          "original_form": "회의는 필요한사람끼리했으면좋겠어요",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042717.1.22",
          "form": "아.. 진짜요.. 두시간넘게 해요",
          "original_form": "아.. 진짜요.. 두시간넘게 해요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042717.1.23",
          "form": "화장실 가고싶은데!!!",
          "original_form": "화장실 가고싶은데!!!",
          "speaker_id": "1",
          "time": "20210907 09:38"
        }
      ]
