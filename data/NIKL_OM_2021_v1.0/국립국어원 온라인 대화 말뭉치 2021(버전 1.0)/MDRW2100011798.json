      "utterance": [
        {
          "id": "MDRW2100011798.1.1",
          "form": "영화자주보시나요?",
          "original_form": "영화자주보시나요?",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011798.1.2",
          "form": "영화보는거 좋아해요!",
          "original_form": "영화보는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011798.1.3",
          "form": "마지막에 보신거는요?",
          "original_form": "마지막에 보신거는요?",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011798.1.4",
          "form": "모가디슈 봤어요!",
          "original_form": "모가디슈 봤어요!",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011798.1.5",
          "form": "엄훠 최신영화ㅠㅠ",
          "original_form": "엄훠 최신영화ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011798.1.6",
          "form": "재밌더라구요 배우들 연기도좋고",
          "original_form": "재밌더라구요 배우들 연기도좋고",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011798.1.7",
          "form": "저는 기억도 안나네요ㅠㅠ 극한직업이 마지막인가..",
          "original_form": "저는 기억도 안나네요ㅠㅠ 극한직업이 마지막인가..",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011798.1.8",
          "form": "헉 집에서도 따로 안보시나요",
          "original_form": "헉 집에서도 따로 안보시나요",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011798.1.9",
          "form": "네 육아에 찌들어 ㅋ 영화볼 시간이 ㅠㅠ  없는것 같아요",
          "original_form": "네 육아에 찌들어 ㅋ 영화볼 시간이 ㅠㅠ  없는것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011798.1.10",
          "form": "뮤지컬은 보신적있나요",
          "original_form": "뮤지컬은 보신적있나요",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011798.1.11",
