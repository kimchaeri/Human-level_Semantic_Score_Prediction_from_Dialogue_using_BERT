      "utterance": [
        {
          "id": "MDRW2100015273.1.1",
          "form": "나 이제 머리를 너무 짧게 자르지 말아야겠어",
          "original_form": "나 이제 머리를 너무 짧게 자르지 말아야겠어",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015273.1.2",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015273.1.3",
          "form": "숱이 없어ㅋㅋ",
          "original_form": "숱이 없어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015273.1.4",
          "form": "길러봐야지ㅋㅋ",
          "original_form": "길러봐야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015273.1.5",
          "form": "시러\r\n짧은 머리가 어울려",
          "original_form": "시러\r\n짧은 머리가 어울려",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015273.1.6",
          "form": "기르면더 없어보여",
          "original_form": "기르면더 없어보여",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015273.1.7",
          "form": "장발말고~~~ ㅋㅋ",
          "original_form": "장발말고~~~ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015273.1.8",
          "form": "파마하게?",
          "original_form": "파마하게?",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015273.1.9",
          "form": "한번은??? ㅋㅋ",
          "original_form": "한번은??? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015273.1.10",
          "form": "파마약이 두피에 안좋아",
          "original_form": "파마약이 두피에 안좋아",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015273.1.11",
          "form": "글치~ 요샌 드라이기도 온풍 안씀",
          "original_form": "글치~ 요샌 드라이기도 온풍 안씀",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015273.1.12",
          "form": "염색약은 더더욱  안좋음",
          "original_form": "염색약은 더더욱  안좋음",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.13",
          "form": "나도 머리카락 너무 빠져서 고민이야ㅠㅠ",
          "original_form": "나도 머리카락 너무 빠져서 고민이야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.14",
          "form": "살면서한번정도는..",
          "original_form": "살면서한번정도는..",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.15",
          "form": "파마도 안하고 염색도 안하는데  왜인지",
          "original_form": "파마도 안하고 염색도 안하는데  왜인지",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.16",
          "form": "자기도 탈모예방효과있는 샴푸써",
          "original_form": "자기도 탈모예방효과있는 샴푸써",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.17",
          "form": "근데 자긴 머리가 곱슬인데 파마하나?",
          "original_form": "근데 자긴 머리가 곱슬인데 파마하나?",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015273.1.18",
          "form": "너무 건조한듯",
          "original_form": "너무 건조한듯",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.19",
          "form": "나 탈모는 아니지 않아?",
          "original_form": "나 탈모는 아니지 않아?",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.20",
          "form": "좀 길러보고ㅋㅋ",
          "original_form": "좀 길러보고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.21",
          "form": "빠지면 탈모지ㅋ",
          "original_form": "빠지면 탈모지ㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.22",
          "form": "난 빠지는 만큼 나는데?",
          "original_form": "난 빠지는 만큼 나는데?",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.23",
          "form": "잔머리 넘 많앙",
          "original_form": "잔머리 넘 많앙",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.24",
          "form": "그게 언제까지나 그럴것같아???? ㅋㅋ",
          "original_form": "그게 언제까지나 그럴것같아???? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.25",
          "form": "미용실가도 머리숱 너무 많아서 숱쳐야한데ㅋ",
          "original_form": "미용실가도 머리숱 너무 많아서 숱쳐야한데ㅋ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015273.1.26",
          "form": "허긴 예전보단 숱이 적어지긴했엉",
          "original_form": "허긴 예전보단 숱이 적어지긴했엉",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015273.1.27",
          "form": "있을때 잘해ㅋㅋ",
          "original_form": "있을때 잘해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        }
      ]
    }
  ]
