      "utterance": [
        {
          "id": "MDRW2100016028.1.1",
          "form": "우째 등원 클리어 했어요?? 출근 준비중??",
          "original_form": "우째 등원 클리어 했어요?? 출근 준비중??",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016028.1.2",
          "form": "출근하고 있지",
          "original_form": "출근하고 있지",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.3",
          "form": "마지막 신호대기",
          "original_form": "마지막 신호대기",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.4",
          "form": "9시까지 출근인데",
          "original_form": "9시까지 출근인데",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.5",
          "form": "매일 겨우겨우 출근함",
          "original_form": "매일 겨우겨우 출근함",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.6",
          "form": "숙제 잘 챙겨서 보냈나??",
          "original_form": "숙제 잘 챙겨서 보냈나??",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ얍얍~빠짐없이 보냈슈^^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ얍얍~빠짐없이 보냈슈^^",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016028.1.8",
          "form": "진짜name2언니 아슬아슬하네 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜&name2&언니 아슬아슬하네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016028.1.9",
          "form": "주차하면 딱 9시네 ㅋㅋㅋㅋ",
          "original_form": "주차하면 딱 9시네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016028.1.10",
          "form": "어 맨날 9시 아슬아슬하게",
          "original_form": "어 맨날 9시 아슬아슬하게",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016028.1.11",
          "form": "근데 애들보내고 하믄 빠듯하긴 해여~",
          "original_form": "근데 애들보내고 하믄 빠듯하긴 해여~",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016028.1.12",
          "form": "차도 밀린다 아녀요.??",
          "original_form": "차도 밀린다 아녀요.??",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016028.1.13",
          "form": "언넝 도장찍고오ㅏ여 ㅋㅋㅋ",
          "original_form": "언넝 도장찍고오ㅏ여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016028.1.14",
          "form": "옷 갈아입는 중",
          "original_form": "옷 갈아입는 중",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016028.1.15",
          "form": "이마이크로 하고 있나",
          "original_form": "이마이크로 하고 있나",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016028.1.16",
          "form": "오늘은 가뿐하게 혼자 출근하겠네",
          "original_form": "오늘은 가뿐하게 혼자 출근하겠네",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016028.1.17",
          "form": "아뇨 ㅋㅋㅋㅋ손으로 한타한타 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋ손으로 한타한타 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016028.1.18",
          "form": "정성 들이지 마라",
          "original_form": "정성 들이지 마라",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.19",
          "form": "쉬는 날 알입니다",
          "original_form": "쉬는 날 알입니다",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.20",
          "form": "아 오늘 목요일이네",
          "original_form": "아 오늘 목요일이네",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.21",
          "form": "완전 부럽노",
          "original_form": "완전 부럽노",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.22",
          "form": "오늘 뿌 염하러 갑니다",
          "original_form": "오늘 뿌 염하러 갑니다",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.23",
          "form": "뭐 할 거니",
          "original_form": "뭐 할 거니",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.24",
          "form": "이거 엄청 편하네요",
          "original_form": "이거 엄청 편하네요",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.26",
          "form": "둘째 통장도 만들어갑니다",
          "original_form": "둘째 통장도 만들어갑니다",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.27",
          "form": "장난 아니지",
          "original_form": "장난 아니지",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016028.1.28",
          "form": "너무 신이 납니다",
          "original_form": "너무 신이 납니다",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.29",
          "form": "오 오늘 할 일 많네",
          "original_form": "오 오늘 할 일 많네",
          "speaker_id": "2",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.30",
          "form": "진짜 갈 걸 그랬네요",
          "original_form": "진짜 갈 걸 그랬네요",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.31",
          "form": "집안 일 빼고 다 합니다",
          "original_form": "집안 일 빼고 다 합니다",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.32",
          "form": "그래 오타 나도 내가 대충 알아들을게",
          "original_form": "그래 오타 나도 내가 대충 알아들을게",
          "speaker_id": "2",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.33",
          "form": "설거지가 산더미 청소기 도 돌려야 해요",
          "original_form": "설거지가 산더미 청소기 도 돌려야 해요",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.34",
          "form": "띄어쓰기도 잘하네요",
          "original_form": "띄어쓰기도 잘하네요",
          "speaker_id": "1",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.35",
          "form": "설거지는 정말 싫다",
          "original_form": "설거지는 정말 싫다",
          "speaker_id": "2",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100016028.1.36",
          "form": "전 요즘 다 싫어요 청소 이동 청소기도 싫어요",
          "original_form": "전 요즘 다 싫어요 청소 이동 청소기도 싫어요",
          "speaker_id": "1",
          "time": "20210826 09:00"
        }
      ]
