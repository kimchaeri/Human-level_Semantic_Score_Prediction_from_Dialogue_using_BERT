      "utterance": [
        {
          "id": "MDRW2100027819.1.1",
          "form": "아 달달한거 좋아하시네요",
          "original_form": "아 달달한거 좋아하시네요",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027819.1.2",
          "form": "맞아요! 근데요즘 설탕줄여서",
          "original_form": "맞아요! 근데요즘 설탕줄여서",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027819.1.3",
          "form": "자일리톨 먹어용",
          "original_form": "자일리톨 먹어용",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027819.1.4",
          "form": "껌이요?",
          "original_form": "껌이요?",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027819.1.5",
          "form": "사탕으로 있어요!",
          "original_form": "사탕으로 있어요!",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027819.1.6",
          "form": "어디서나온 제품인가요?",
          "original_form": "어디서나온 제품인가요?",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027819.1.7",
          "form": "인터넷에서 파는데 비싸서요즘은 만들어요ㅋㅋ",
          "original_form": "인터넷에서 파는데 비싸서요즘은 만들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027819.1.8",
          "form": "만드는게 쉽나요?",
          "original_form": "만드는게 쉽나요?",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027819.1.9",
          "form": "네네 금방녹아서 요령생기면 쉬워요",
          "original_form": "네네 금방녹아서 요령생기면 쉬워요",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027819.1.10",
          "form": "와\r\n맛있나요? 웬지 제스타일일거같아요",
          "original_form": "와\r\n맛있나요? 웬지 제스타일일거같아요",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027819.1.11",
          "form": "자일리톨껌이 시원한것처럼 사탕도 시원달달해서 좋아요!",
          "original_form": "자일리톨껌이 시원한것처럼 사탕도 시원달달해서 좋아요!",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027819.1.12",
          "form": "name2님도 좋아하실거같아용ㅎㅎ",
          "original_form": "&name2&님도 좋아하실거같아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027819.1.13",
          "form": "네 그런맛좋아해요",
          "original_form": "네 그런맛좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027819.1.14",
          "form": "저도요! 언젠가 그걸로 가게도 차리고싶어요ㅋㅋㅋ",
          "original_form": "저도요! 언젠가 그걸로 가게도 차리고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027819.1.15",
          "form": "오 좋을거같아요 저 사먹을거같네요",
          "original_form": "오 좋을거같아요 저 사먹을거같네요",
          "speaker_id": "2",
