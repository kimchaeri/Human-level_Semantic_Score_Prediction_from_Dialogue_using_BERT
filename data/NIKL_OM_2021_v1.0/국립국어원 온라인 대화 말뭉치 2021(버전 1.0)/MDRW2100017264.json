      "utterance": [
        {
          "id": "MDRW2100017264.1.1",
          "form": "아 나 피부과가서 관리받고싶어",
          "original_form": "아 나 피부과가서 관리받고싶어",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017264.1.2",
          "form": "ㅋㅋㅋㅋㅋ피부 난리났어",
          "original_form": "ㅋㅋㅋㅋㅋ피부 난리났어",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017264.1.3",
          "form": "피부과에서 보통 관리 뭐받아?",
          "original_form": "피부과에서 보통 관리 뭐받아?",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017264.1.4",
          "form": "효과있나",
          "original_form": "효과있나",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017264.1.5",
          "form": "근데 일단 우리안과는 그거있어",
          "original_form": "근데 일단 우리안과는 그거있어",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.6",
          "form": "안구건조증 레이저치료ㅋㅋㅋㅋ",
          "original_form": "안구건조증 레이저치료ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.7",
          "form": "근데 1주씩 4번 2주씩 2번 이렇게가 기본인데",
          "original_form": "근데 1주씩 4번 2주씩 2번 이렇게가 기본인데",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.8",
          "form": "효과있나봐",
          "original_form": "효과있나봐",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.9",
          "form": "나는 일단 피부전체적으로 싹 관리받고싶음..",
          "original_form": "나는 일단 피부전체적으로 싹 관리받고싶음..",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.10",
          "form": "와씨 나 안구건조증 심해서 고민인데",
          "original_form": "와씨 나 안구건조증 심해서 고민인데",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.11",
          "form": "치료 얼마여?",
          "original_form": "치료 얼마여?",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017264.1.12",
          "form": "지금 이벤트가라 10만원이고 실비청구 가능",
          "original_form": "지금 이벤트가라 10만원이고 실비청구 가능",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017264.1.13",
          "form": "근데 그전에 안구건조증 검사 받아야됑",
          "original_form": "근데 그전에 안구건조증 검사 받아야됑",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017264.1.14",
          "form": "안구건조증검사 15만원인데 비급여코드들어가ㅋㅋ10만원정도",
          "original_form": "안구건조증검사 15만원인데 비급여코드들어가ㅋㅋ10만원정도",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017264.1.15",
          "form": "춘천 안과에서 여기밖에없대..ㅎ",
          "original_form": "춘천 안과에서 여기밖에없대..ㅎ",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017264.1.16",
          "form": "아니 무슨 치료비용도 아니고 검사하는게 15만원?",
          "original_form": "아니 무슨 치료비용도 아니고 검사하는게 15만원?",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017264.1.17",
          "form": "하...돈없는 사람 눈 빡빡하게 살아야지",
          "original_form": "하...돈없는 사람 눈 빡빡하게 살아야지",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017264.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017264.1.19",
          "form": "이렇게 돈벌 이유가 추가되었다",
          "original_form": "이렇게 돈벌 이유가 추가되었다",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017264.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017264.1.21",
          "form": "그치 눈 검사하는거 겁나비싸",
          "original_form": "그치 눈 검사하는거 겁나비싸",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017264.1.22",
          "form": "근데 여기 검사기계 강대뺨쳐",
          "original_form": "근데 여기 검사기계 강대뺨쳐",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017264.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017264.1.24",
          "form": "개인병원치고 많음",
          "original_form": "개인병원치고 많음",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017264.1.25",
          "form": "역시 병원은 기계빨이야",
          "original_form": "역시 병원은 기계빨이야",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017264.1.26",
          "form": "기계가 확실히 최신형 두는곳이 잘나가더라",
          "original_form": "기계가 확실히 최신형 두는곳이 잘나가더라",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017264.1.27",
          "form": "피부과도 그렇고",
          "original_form": "피부과도 그렇고",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017264.1.28",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017264.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그걸로 마케팅하는듯ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그걸로 마케팅하는듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017264.1.30",
          "form": "내친구 친구는 무슨 시술도 받더라",
          "original_form": "내친구 친구는 무슨 시술도 받더라",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017264.1.31",
          "form": "무섭지도 않나봐",
          "original_form": "무섭지도 않나봐",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017264.1.32",
          "form": "어우 난 시술은 무서움...",
          "original_form": "어우 난 시술은 무서움...",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017264.1.33",
          "form": "교정도 사실 덧니가 앞니니까 스트레스라 치료차원에서 하는거지",
          "original_form": "교정도 사실 덧니가 앞니니까 스트레스라 치료차원에서 하는거지",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017264.1.34",
          "form": "아니었으면 하지도않음..",
          "original_form": "아니었으면 하지도않음..",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017264.1.35",
          "form": "치아는 교정이니까 할수있지",
          "original_form": "치아는 교정이니까 할수있지",
          "speaker_id": "1",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017264.1.36",
          "form": "근데 시술? 허허",
          "original_form": "근데 시술? 허허",
          "speaker_id": "1",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017264.1.37",
          "form": "걍 생긴대로 사는겨~~~",
          "original_form": "걍 생긴대로 사는겨~~~",
          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017264.1.38",
          "form": "허허 나도 시술은...",
          "original_form": "허허 나도 시술은...",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017264.1.39",
          "form": "얼굴 막 손대고 이러는거 무서움ㅠ",
