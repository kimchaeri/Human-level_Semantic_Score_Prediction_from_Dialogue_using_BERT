      "utterance": [
        {
          "id": "MDRW2100018300.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018300.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018300.1.3",
          "form": "보시는 프로그램있으신가요?",
          "original_form": "보시는 프로그램있으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018300.1.4",
          "form": "최근 프로그램은 아닌데",
          "original_form": "최근 프로그램은 아닌데",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018300.1.5",
          "form": "티빙으로 신서유기 힐링캠프 보고있어요",
          "original_form": "티빙으로 신서유기 힐링캠프 보고있어요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018300.1.6",
          "form": "주로 보시는 프로그램이나 유튜브 채널 있으신가요",
          "original_form": "주로 보시는 프로그램이나 유튜브 채널 있으신가요",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018300.1.7",
          "form": "저도 신서유기 재밌게 봤어요ㅋㅋ",
          "original_form": "저도 신서유기 재밌게 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018300.1.8",
          "form": "저는 요즘 심야괴담회나 대탈출 보고있어요",
          "original_form": "저는 요즘 심야괴담회나 대탈출 보고있어요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018300.1.9",
          "form": "심야괴담회는 무서운 이야기라 못볼거같은데",
          "original_form": "심야괴담회는 무서운 이야기라 못볼거같은데",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018300.1.10",
          "form": "대탈출은 저도 뜨문뜨문 봤어요",
          "original_form": "대탈출은 저도 뜨문뜨문 봤어요",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018300.1.11",
          "form": "대탈출 재밌지 않나용 ㅋㅋ",
          "original_form": "대탈출 재밌지 않나용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018300.1.12",
          "form": "저도 심야괴담회는 혼자 보는건 무서워서 가족이랑 같이 봐요!",
          "original_form": "저도 심야괴담회는 혼자 보는건 무서워서 가족이랑 같이 봐요!",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018300.1.13",
          "form": "요즘 대탈출은 스토리때문에",
          "original_form": "요즘 대탈출은 스토리때문에",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018300.1.14",
          "form": "이해가 잘안가서",
          "original_form": "이해가 잘안가서",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018300.1.15",
          "form": "보는맛은 좀 떨어진거같아요",
          "original_form": "보는맛은 좀 떨어진거같아요",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018300.1.16",
          "form": "대탈출1때 많이 봤거든요",
          "original_form": "대탈출1때 많이 봤거든요",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018300.1.17",
          "form": "1보다는 확실히 스토리가 많아졌더라구요",
          "original_form": "1보다는 확실히 스토리가 많아졌더라구요",
          "speaker_id": "2",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100018300.1.18",
          "form": "그래도 소재가 마음에 들어서 계속 보고있어요",
          "original_form": "그래도 소재가 마음에 들어서 계속 보고있어요",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018300.1.19",
          "form": "좀비편은 항상 재밌더라구여",
          "original_form": "좀비편은 항상 재밌더라구여",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018300.1.20",
          "form": "나머지는 몰입하기에 너무 오글거려서 ㅋㅋ",
          "original_form": "나머지는 몰입하기에 너무 오글거려서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018300.1.21",
          "form": "맞아요 좀비편 진짜 재밌어요",
          "original_form": "맞아요 좀비편 진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018300.1.22",
          "form": "대탈출은 공포편이 제일 재밌더라구요",
          "original_form": "대탈출은 공포편이 제일 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018300.1.23",
          "form": "처음 좀비편에서",
          "original_form": "처음 좀비편에서",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018300.1.24",
          "form": "엄청 웃었던거 같아요",
          "original_form": "엄청 웃었던거 같아요",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018300.1.25",
          "form": "최근 좀비감옥편은 갑자기 끝나서",
          "original_form": "최근 좀비감옥편은 갑자기 끝나서",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018300.1.26",
          "form": "당황스러웠어요 ㅋㅋ",
          "original_form": "당황스러웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018300.1.27",
          "form": "오오 맞아요 .. 갑자기 뛰어가다가 끝나더라구요",
          "original_form": "오오 맞아요 .. 갑자기 뛰어가다가 끝나더라구요",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018300.1.28",
          "form": "대탈출은 감옥탈출은 ..좀 안되나봐요 예전에도 실패했었잖아요..",
          "original_form": "대탈출은 감옥탈출은 ..좀 안되나봐요 예전에도 실패했었잖아요..",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018300.1.29",
          "form": "ㅋㅋㅋㅋ 항상 어이없이 끝나서",
          "original_form": "ㅋㅋㅋㅋ 항상 어이없이 끝나서",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018300.1.30",
          "form": "개인적으로 몰카였으면 했는데",
          "original_form": "개인적으로 몰카였으면 했는데",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018300.1.31",
          "form": "옛날 무한도전 박명수가",
          "original_form": "옛날 무한도전 박명수가",
          "speaker_id": "1",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018300.1.32",
          "form": "사다리 없애고 끝난 느낌이 오마주되서 아쉽더라고여",
          "original_form": "사다리 없애고 끝난 느낌이 오마주되서 아쉽더라고여",
          "speaker_id": "1",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018300.1.33",
          "form": "ㅋㅋㅋㅋ 그쵸 신서유기도 그렇고 대탈출도 그렇고 진짜 끝 하면 끝나더라구요...",
          "original_form": "ㅋㅋㅋㅋ 그쵸 신서유기도 그렇고 대탈출도 그렇고 진짜 끝 하면 끝나더라구요...",
          "speaker_id": "2",
          "time": "20210827 00:24"
        }
      ]
