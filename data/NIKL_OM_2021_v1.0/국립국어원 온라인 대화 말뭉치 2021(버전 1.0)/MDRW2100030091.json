      "utterance": [
        {
          "id": "MDRW2100030091.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030091.1.2",
          "form": "저는 무교에요!",
          "original_form": "저는 무교에요!",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100030091.1.3",
          "form": "name1님은 종교있으신가요?",
          "original_form": "&name1&님은 종교있으신가요?",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030091.1.4",
          "form": "아뇨! 저도 무교!",
          "original_form": "아뇨! 저도 무교!",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100030091.1.5",
          "form": "교회 한번도 안가보셧어요?",
          "original_form": "교회 한번도 안가보셧어요?",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100030091.1.6",
          "form": "어렸을때 친구 따라서 한 번 가봤어요ㅋㅋㅋ",
          "original_form": "어렸을때 친구 따라서 한 번 가봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030091.1.7",
          "form": "저도ㅋㅋ",
          "original_form": "저도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030091.1.8",
          "form": "통성기도.. 목격 ㅋㅋ",
          "original_form": "통성기도.. 목격 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030091.1.9",
          "form": "지금까지 이 주제 나왔을때 종교가 있으신 분을 아직 한 분도 못봤어요ㅋㅋㅋㅋ",
          "original_form": "지금까지 이 주제 나왔을때 종교가 있으신 분을 아직 한 분도 못봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030091.1.10",
          "form": "통성기도는 어떤걸 하는건가요?",
          "original_form": "통성기도는 어떤걸 하는건가요?",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030091.1.11",
          "form": "거짓말하시는건가.. ㅋㅋ",
          "original_form": "거짓말하시는건가.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100030091.1.12",
          "form": "막울면서 반성해요....",
          "original_form": "막울면서 반성해요....",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100030091.1.13",
          "form": "소리지르면서..",
          "original_form": "소리지르면서..",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030091.1.14",
          "form": "아니 애초에 죄를 짓지말던가...",
          "original_form": "아니 애초에 죄를 짓지말던가...",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030091.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ아이고ㅋㅋㅋㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아이고ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100030091.1.16",
          "form": "초장부터 너무 쎈걸 보고 오셨네요",
          "original_form": "초장부터 너무 쎈걸 보고 오셨네요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100030091.1.17",
          "form": "저는 그래도 친구따라간 교회에서 종교문제는 좀 신중하게 생각해보란 얘기 들어서",
          "original_form": "저는 그래도 친구따라간 교회에서 종교문제는 좀 신중하게 생각해보란 얘기 들어서",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030091.1.18",
          "form": "ㅋㅋ 초딩때 봤어요ㅋㅋ 꽁짜간식 먹으러 갔다가",
          "original_form": "ㅋㅋ 초딩때 봤어요ㅋㅋ 꽁짜간식 먹으러 갔다가",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030091.1.19",
          "form": "지금 생각해보면 양심있으신 분들이었던 것 같아요",
          "original_form": "지금 생각해보면 양심있으신 분들이었던 것 같아요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030091.1.20",
          "form": "진정한 종교인들이셨네요",
          "original_form": "진정한 종교인들이셨네요",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100030091.1.21",
          "form": "전 연극보는데 교회안믿는다고 가족들 다 지옥불에 빠진 내용ㅋㅋ",
          "original_form": "전 연극보는데 교회안믿는다고 가족들 다 지옥불에 빠진 내용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100030091.1.22",
          "form": "아이고 왜이렇게 극단적이셨지ㅋㅋㄱㅋㅋㅋ",
          "original_form": "아이고 왜이렇게 극단적이셨지ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030091.1.23",
          "form": "좀 영업방향을 잘못 잡으신 것 같네요",
          "original_form": "좀 영업방향을 잘못 잡으신 것 같네요",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100030091.1.24",
          "form": "옛날 교회라서... 요즘은 안그러겠죠ㅜ",
          "original_form": "옛날 교회라서... 요즘은 안그러겠죠ㅜ",
