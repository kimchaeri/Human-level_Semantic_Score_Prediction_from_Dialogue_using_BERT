      "utterance": [
        {
          "id": "MDRW2100042966.1.1",
          "form": "국내여행은",
          "original_form": "국내여행은",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042966.1.2",
          "form": "어떠세요",
          "original_form": "어떠세요",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042966.1.3",
          "form": "앗 국내여행은 많이 안다녀봤어요",
          "original_form": "앗 국내여행은 많이 안다녀봤어요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042966.1.4",
          "form": "약간 여행을 면세점 가는 맛으로 가서",
          "original_form": "약간 여행을 면세점 가는 맛으로 가서",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042966.1.5",
          "form": "그래도 여수가 참 좋더라고요",
          "original_form": "그래도 여수가 참 좋더라고요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042966.1.6",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042966.1.7",
          "form": "ㅋㅋㅋㄱ면세점즐겁긴해요",
          "original_form": "ㅋㅋㅋㄱ면세점즐겁긴해요",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.8",
          "form": "여수갔던기억은",
          "original_form": "여수갔던기억은",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.9",
          "form": "엄청먹던기억밖에 없네요",
          "original_form": "엄청먹던기억밖에 없네요",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.10",
          "form": "회먹고 기념이라고",
          "original_form": "회먹고 기념이라고",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.11",
          "form": "여수밤바다듣고 왔어요",
          "original_form": "여수밤바다듣고 왔어요",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 식도락으로 다니시는군욬ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 식도락으로 다니시는군욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042966.1.13",
          "form": "아니 어떤 연관이(?)",
          "original_form": "아니 어떤 연관이(?)",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.14",
          "form": "ㅋㅋㅋㅋㅋㄱㄱㄱ그게젤 좋더라구요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱㄱ그게젤 좋더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.16",
          "form": "여수니깐, , ,",
          "original_form": "여수니깐, , ,",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.17",
          "form": "리얼 국내는...식도락..",
          "original_form": "리얼 국내는...식도락..",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.18",
          "form": "ㅋㅋㄱ그쵸?",
          "original_form": "ㅋㅋㄱ그쵸?",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.19",
          "form": "대전은 가보셨나요",
          "original_form": "대전은 가보셨나요",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.20",
          "form": "어 네",
          "original_form": "어 네",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.21",
          "form": "성심당",
          "original_form": "성심당",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.22",
          "form": "ㅋㅋㅋㄱㄱㅋㄱㄱㄱㅋㄱㄱㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㅋㅋㄱㄱㅋㄱㄱㄱㅋㄱㄱㄱㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.23",
          "form": "근데 이번에 그 대전 3대치킨 티비 나오는데",
          "original_form": "근데 이번에 그 대전 3대치킨 티비 나오는데",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.24",
          "form": "너무 먹고싶더라규요",
          "original_form": "너무 먹고싶더라규요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ대전은 성심당",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ대전은 성심당",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042966.1.26",
          "form": "성심당이 하는 파스타집..",
          "original_form": "성심당이 하는 파스타집..",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.27",
          "form": "성심당이 하는..어쩌구..",
          "original_form": "성심당이 하는..어쩌구..",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.28",
          "form": "3대치킨??은첨들어봣어요",
          "original_form": "3대치킨??은첨들어봣어요",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.29",
          "form": "성심당유니버스",
          "original_form": "성심당유니버스",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.30",
          "form": "우동맛집인데 성심당이하는",
          "original_form": "우동맛집인데 성심당이하는",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.31",
          "form": "그니가요",
          "original_form": "그니가요",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.32",
          "form": "대전 3대치킨 있다던데",
          "original_form": "대전 3대치킨 있다던데",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.33",
          "form": "모르셨구나(?)",
          "original_form": "모르셨구나(?)",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042966.1.34",
          "form": "사실 저도 어제 밤에 알았어요",
          "original_form": "사실 저도 어제 밤에 알았어요",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042966.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042966.1.36",
          "form": "대전에 기념품은",
          "original_form": "대전에 기념품은",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042966.1.37",
          "form": "성심당 빵.",
          "original_form": "성심당 빵.",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042966.1.38",
          "form": "ㅋㅋㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
