      "utterance": [
        {
          "id": "MDRW2100016913.1.1",
          "form": "아뇨 걔가 거기서 알바했어가지고",
          "original_form": "아뇨 걔가 거기서 알바했어가지고",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016913.1.2",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.3",
          "form": "친한 애들끼리 7명이서 갔는데",
          "original_form": "친한 애들끼리 7명이서 갔는데",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.4",
          "form": "가격 얼마 나왔게요",
          "original_form": "가격 얼마 나왔게요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.5",
          "form": "7명이고",
          "original_form": "7명이고",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.6",
          "form": "똠얌꿍 1.6",
          "original_form": "똠얌꿍 1.6",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.7",
          "form": "20만원 예상해봅니다",
          "original_form": "20만원 예상해봅니다",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.8",
          "form": "7년전이에요",
          "original_form": "7년전이에요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.9",
          "form": "분짜도 시켰을거고",
          "original_form": "분짜도 시켰을거고",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016913.1.10",
          "form": "분짜는없었어여ㅋ",
          "original_form": "분짜는없었어여ㅋ",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.11",
          "form": "7년전이면 몇년도죠?",
          "original_form": "7년전이면 몇년도죠?",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.12",
          "form": "14년?",
          "original_form": "14년?",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.13",
          "form": "ㅇㅇ..?",
          "original_form": "ㅇㅇ..?",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.14",
          "form": "2014년인가..",
          "original_form": "2014년인가..",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.15",
          "form": "제가 고2때네요",
          "original_form": "제가 고2때네요",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016913.1.17",
          "form": "고3인가??",
          "original_form": "고3인가??",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.18",
          "form": "네 어려서 좋겠어요",
          "original_form": "네 어려서 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.20",
          "form": "으르신!",
          "original_form": "으르신!",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.21",
          "form": "고2세요 님",
          "original_form": "고2세요 님",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.22",
          "form": "전 그땐 제 용돈으로 밥을 못사먹어서 모르겠습니다!",
          "original_form": "전 그땐 제 용돈으로 밥을 못사먹어서 모르겠습니다!",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016913.1.25",
          "form": "쨌든20만원까지는 안나오고",
          "original_form": "쨌든20만원까지는 안나오고",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.26",
          "form": "15만원정도 나와서",
          "original_form": "15만원정도 나와서",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.27",
          "form": "1인당 2만얼마씩 더치페이했져",
          "original_form": "1인당 2만얼마씩 더치페이했져",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.28",
          "form": "워...",
          "original_form": "워...",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.29",
          "form": "넘나 비쌌...",
          "original_form": "넘나 비쌌...",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.30",
          "form": "그당시 두당 2만원이면",
          "original_form": "그당시 두당 2만원이면",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.31",
          "form": "지금 5만원쯤 될텐데",
          "original_form": "지금 5만원쯤 될텐데",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.32",
          "form": "체감이",
          "original_form": "체감이",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016913.1.33",
          "form": "그때 김밥 1500원이었자나요",
          "original_form": "그때 김밥 1500원이었자나요",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016913.1.34",
          "form": "대학근처에 볶음밥 하나가 5천원이었는데",
          "original_form": "대학근처에 볶음밥 하나가 5천원이었는데",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016913.1.35",
          "form": "담배가 2500원일땐데",
          "original_form": "담배가 2500원일땐데",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016913.1.36",
          "form": "ㅋㅋㅋㅋㅋ하여간..",
          "original_form": "ㅋㅋㅋㅋㅋ하여간..",
          "speaker_id": "2",
          "time": "20210826 16:17"
        }
      ]
