      "utterance": [
        {
          "id": "MDRW2100011325.1.1",
          "form": "여기 동네 미용실에서 파마했는데",
          "original_form": "여기 동네 미용실에서 파마했는데",
          "speaker_id": "1",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100011325.1.2",
          "form": "너무 빨리 풀리고 머리가 이상한거야ㅜㅜ",
          "original_form": "너무 빨리 풀리고 머리가 이상한거야ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011325.1.3",
          "form": "어디서 했길래??",
          "original_form": "어디서 했길래??",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011325.1.4",
          "form": "요즘 미용사 다 자격증만 따서 하는 사람들 많아가;;",
          "original_form": "요즘 미용사 다 자격증만 따서 하는 사람들 많아가;;",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011325.1.5",
          "form": "울 사촌언니도 내 20살쯤부터 미용실에서 일했는데..아직도 뭔가 좀 어색함 ㅋㅋㅋㅋㅋ",
          "original_form": "울 사촌언니도 내 20살쯤부터 미용실에서 일했는데..아직도 뭔가 좀 어색함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100011325.1.6",
          "form": "아 진짜 머리를 더 자르고 다시하고싶은데ㅋㅋ",
          "original_form": "아 진짜 머리를 더 자르고 다시하고싶은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011325.1.7",
          "form": "그냥 다니지뭐ㅋㅋ",
          "original_form": "그냥 다니지뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011325.1.8",
          "form": "ㅋㅋㅋㅋ다시 해준다 하더나??",
          "original_form": "ㅋㅋㅋㅋ다시 해준다 하더나??",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.9",
          "form": "근데. 다시해도..첨 실패한집은..실패하더라..",
          "original_form": "근데. 다시해도..첨 실패한집은..실패하더라..",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.10",
          "form": "아니 한지좀 되서ㅋㅋ",
          "original_form": "아니 한지좀 되서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.11",
          "form": "예뻐지기 어려움 ㅋㅋㅋ",
          "original_form": "예뻐지기 어려움 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.12",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011325.1.14",
          "form": "머리카락이",
          "original_form": "머리카락이",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.15",
          "form": "손상 장난아니라서",
          "original_form": "손상 장난아니라서",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.16",
          "form": "그냥 놔뒀지뭐ㅠ",
          "original_form": "그냥 놔뒀지뭐ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.17",
          "form": "그럼 잘라야 안되나??",
          "original_form": "그럼 잘라야 안되나??",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.18",
          "form": "더 타고 올라가잖아ㅠ",
          "original_form": "더 타고 올라가잖아ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.19",
          "form": "손상된거는 암만 영양줘도 안되더라ㅠ",
          "original_form": "손상된거는 암만 영양줘도 안되더라ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011325.1.20",
          "form": "응 잘라야지 ㅜㅋㅋㅋ",
          "original_form": "응 잘라야지 ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.21",
          "form": "미용실 다른곳 예약해서",
          "original_form": "미용실 다른곳 예약해서",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.22",
          "form": "머리 많이 길렀는데 아깝구만~",
          "original_form": "머리 많이 길렀는데 아깝구만~",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.23",
          "form": "상담해보려구",
          "original_form": "상담해보려구",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.24",
          "form": "확 잘라야지뭐ㅜ",
          "original_form": "확 잘라야지뭐ㅜ",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.25",
          "form": "근데 머리 엄청 빨리 긴다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 머리 엄청 빨리 긴다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011325.1.26",
          "form": "난 손톱만 긴다ㅠ",
          "original_form": "난 손톱만 긴다ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011325.1.27",
          "form": "응 나는 진짜 빨리기는것같다ㅋㅋ",
          "original_form": "응 나는 진짜 빨리기는것같다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011325.1.28",
          "form": "손톱ㅋㅋㅋ영양이 많나ㅋㅋㅋ",
          "original_form": "손톱ㅋㅋㅋ영양이 많나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011325.1.29",
          "form": "그런가봐; 울 애들도 그렇다..손톱 정말 빨리길고ㅠ",
          "original_form": "그런가봐; 울 애들도 그렇다..손톱 정말 빨리길고ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011325.1.30",
          "form": "이런거만 닮노..",
          "original_form": "이런거만 닮노..",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011325.1.31",
          "form": "맞다 울애들도 손톱은 빨리길더라ㅋㅋ",
          "original_form": "맞다 울애들도 손톱은 빨리길더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011325.1.32",
