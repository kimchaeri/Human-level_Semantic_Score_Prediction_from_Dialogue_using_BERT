      "utterance": [
        {
          "id": "MDRW2100029126.1.1",
          "form": "역시나 토핑질문입니다!! 피자는 토핑이 많잖아요ㅎㅎ",
          "original_form": "역시나 토핑질문입니다!! 피자는 토핑이 많잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029126.1.2",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029126.1.3",
          "form": "어떤 토핑을 가장 좋아하시나요!!!",
          "original_form": "어떤 토핑을 가장 좋아하시나요!!!",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029126.1.4",
          "form": "저는 포테이토 원픽입니당",
          "original_form": "저는 포테이토 원픽입니당",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029126.1.5",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029126.1.6",
          "form": "크.. 담백함의 포테이토를 선택하셨군요",
          "original_form": "크.. 담백함의 포테이토를 선택하셨군요",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029126.1.7",
          "form": "개인적으로 포테이토에 핫소스를 못참는편인데",
          "original_form": "개인적으로 포테이토에 핫소스를 못참는편인데",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029126.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029126.1.9",
          "form": "먹잘알이시군요",
          "original_form": "먹잘알이시군요",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029126.1.10",
          "form": "아직 부족함이 많습니다ㅎㅎ",
          "original_form": "아직 부족함이 많습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029126.1.11",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029126.1.12",
          "form": "포테이토에 핫소스는못참지",
          "original_form": "포테이토에 핫소스는못참지",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029126.1.13",
          "form": "저도좋아해요",
          "original_form": "저도좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029126.1.14",
          "form": "혹시 치즈크러스트 도우는 어떤가요",
          "original_form": "혹시 치즈크러스트 도우는 어떤가요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029126.1.15",
          "form": "요고까지 맞으면 더 좋을텐데ㅎㅎ",
          "original_form": "요고까지 맞으면 더 좋을텐데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029126.1.16",
          "form": "그거 필수아닌가요",
          "original_form": "그거 필수아닌가요",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029126.1.17",
          "form": "저는 치즈광인이랍니다ㅋㅋ",
          "original_form": "저는 치즈광인이랍니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029126.1.18",
          "form": "크으...",
          "original_form": "크으...",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029126.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029126.1.20",
          "form": "name2은",
          "original_form": "&name2&은",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029126.1.21",
          "form": "뭐좋아하시나용",
          "original_form": "뭐좋아하시나용",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029126.1.22",
          "form": "하긴 피자 좋아하는 사람에게 치즈좋아하냐는 이야기는 이상하네요ㅎㅎ",
          "original_form": "하긴 피자 좋아하는 사람에게 치즈좋아하냐는 이야기는 이상하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029126.1.23",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.24",
          "form": "아 음식에는 관대한편이라 어지간하면 좋아라합니다ㅋㅋ",
          "original_form": "아 음식에는 관대한편이라 어지간하면 좋아라합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.25",
          "form": "치즈크러스트말고",
          "original_form": "치즈크러스트말고",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.26",
          "form": "고구마링을좋아하시는분들은",
          "original_form": "고구마링을좋아하시는분들은",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.27",
          "form": "꽤있더라구요",
          "original_form": "꽤있더라구요",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.28",
          "form": "저도고구마링 싫진않지만",
          "original_form": "저도고구마링 싫진않지만",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.29",
          "form": "고구마무스링도 나쁘진 않지만",
          "original_form": "고구마무스링도 나쁘진 않지만",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.30",
          "form": "그래도치즈파입니다ㅎㅎ",
          "original_form": "그래도치즈파입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029126.1.31",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
