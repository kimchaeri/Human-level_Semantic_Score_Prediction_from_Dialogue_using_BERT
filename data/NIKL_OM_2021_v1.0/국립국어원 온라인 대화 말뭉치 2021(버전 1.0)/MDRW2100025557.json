      "utterance": [
        {
          "id": "MDRW2100025557.1.1",
          "form": "안녕하세요 여름휴가 다녀오셨나요",
          "original_form": "안녕하세요 여름휴가 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025557.1.2",
          "form": "아직 가진 못했는데 제주도 가보고 싶어요",
          "original_form": "아직 가진 못했는데 제주도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100025557.1.3",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100025557.1.4",
          "form": "저는 부산에 가보려다가 4단계라는 소리듣고 접었어요ㅜ",
          "original_form": "저는 부산에 가보려다가 4단계라는 소리듣고 접었어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025557.1.5",
          "form": "에고 이놈의 코로나",
          "original_form": "에고 이놈의 코로나",
          "speaker_id": "1",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025557.1.6",
          "form": "오 저는 부산 살아요",
          "original_form": "오 저는 부산 살아요",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025557.1.7",
          "form": "부산 어디 가고 싶으셨나요",
          "original_form": "부산 어디 가고 싶으셨나요",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025557.1.8",
          "form": "저는 사실 이미 바닷가옆에 살고있어서요 ㅋㅋ",
          "original_form": "저는 사실 이미 바닷가옆에 살고있어서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025557.1.9",
          "form": "백화점들가고 싶엇어요",
          "original_form": "백화점들가고 싶엇어요",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025557.1.10",
          "form": "맛집투어랑",
          "original_form": "맛집투어랑",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025557.1.11",
          "form": "아그리고 달맞이고개랑",
          "original_form": "아그리고 달맞이고개랑",
          "speaker_id": "1",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025557.1.12",
          "form": "와우 ㅋㅋ 부산 바다가 목적은 아니셨군요",
          "original_form": "와우 ㅋㅋ 부산 바다가 목적은 아니셨군요",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100025557.1.13",
          "form": "요즘 4단계지만 건물안에는 사람이 많더라구요",
          "original_form": "요즘 4단계지만 건물안에는 사람이 많더라구요",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025557.1.14",
          "form": "달맞이고개 대구탕 잘하는 집이 있다고해서요 ㅋㅋ",
          "original_form": "달맞이고개 대구탕 잘하는 집이 있다고해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025557.1.15",
          "form": "부산 진짜 맛집많아요ㅜㅜ",
          "original_form": "부산 진짜 맛집많아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025557.1.16",
          "form": "오 시원하니 맛있겠어요",
          "original_form": "오 시원하니 맛있겠어요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025557.1.17",
          "form": "저는 맛집 잘 몰라서 ㅠㅠ",
          "original_form": "저는 맛집 잘 몰라서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025557.1.18",
          "form": "저는 먹는거에 관심이 많아서 ㅋㅋ",
          "original_form": "저는 먹는거에 관심이 많아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025557.1.19",
          "form": "뭔가 여행을가면동선이 다 from맛집 to맛집 ㅋㅋㅋ",
          "original_form": "뭔가 여행을가면동선이 다 from맛집 to맛집 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025557.1.20",
          "form": "캬 같이 다니면 좋겠네요 ㅋㅋㅋ 먹는게 남는거죠",
          "original_form": "캬 같이 다니면 좋겠네요 ㅋㅋㅋ 먹는게 남는거죠",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025557.1.21",
          "form": "제주도 연돈도 가보고싶어요",
          "original_form": "제주도 연돈도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025557.1.22",
          "form": "아 제주도 연돈 ㅋㅋ오늘 재밌는거봤어요",
          "original_form": "아 제주도 연돈 ㅋㅋ오늘 재밌는거봤어요",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.23",
          "form": "요새는 주문 인터넷으로받는가본데",
          "original_form": "요새는 주문 인터넷으로받는가본데",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.24",
          "form": "오 뭐 보셨어요",
          "original_form": "오 뭐 보셨어요",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.25",
          "form": "중고나라에서 팔더라고요",
          "original_form": "중고나라에서 팔더라고요",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.26",
          "form": "4만원에",
          "original_form": "4만원에",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.27",
          "form": "그러게요 그래도 치열하다던데",
          "original_form": "그러게요 그래도 치열하다던데",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025557.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025557.1.29",
          "form": "콘서트 티켓팔듯이",
          "original_form": "콘서트 티켓팔듯이",
          "speaker_id": "1",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025557.1.30",
          "form": "무섭네요",
          "original_form": "무섭네요",
          "speaker_id": "2",
