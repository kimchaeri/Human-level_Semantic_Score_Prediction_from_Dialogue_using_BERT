      "utterance": [
        {
          "id": "MDRW2100003793.1.1",
          "form": "좀 춥네",
          "original_form": "좀 춥네",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003793.1.2",
          "form": "그러게 비가 와서 그런가",
          "original_form": "그러게 비가 와서 그런가",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.3",
          "form": "바람도 많이 불고",
          "original_form": "바람도 많이 불고",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.4",
          "form": "옹 그래도 더운것 보단 낫다",
          "original_form": "옹 그래도 더운것 보단 낫다",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.5",
          "form": "그렇지 너무 더웠다 여름에",
          "original_form": "그렇지 너무 더웠다 여름에",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.6",
          "form": "옹 난 여름이 넘싫어",
          "original_form": "옹 난 여름이 넘싫어",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.7",
          "form": "나도 근데 추운 것도 싫다",
          "original_form": "나도 근데 추운 것도 싫다",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.8",
          "form": "여름에 더우면 넘 짜증남 ㅠ",
          "original_form": "여름에 더우면 넘 짜증남 ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.9",
          "form": "가을이 딱이지",
          "original_form": "가을이 딱이지",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.10",
          "form": "맞당",
          "original_form": "맞당",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003793.1.11",
          "form": "가을이 좀 오래가면 좋겠군",
          "original_form": "가을이 좀 오래가면 좋겠군",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003793.1.12",
          "form": "그럼좋겠다~~ 시원한 가을이 넘 좋다",
          "original_form": "그럼좋겠다~~ 시원한 가을이 넘 좋다",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003793.1.13",
          "form": "비 오면 이제 완전한 가을이네",
          "original_form": "비 오면 이제 완전한 가을이네",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003793.1.14",
          "form": "글겡 ㅋ 장마동안 계속 시원할듯",
          "original_form": "글겡 ㅋ 장마동안 계속 시원할듯",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003793.1.15",
          "form": "오늘은 날씨가 선선하넴",
          "original_form": "오늘은 날씨가 선선하넴",
