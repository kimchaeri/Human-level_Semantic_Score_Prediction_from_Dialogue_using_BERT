      "utterance": [
        {
          "id": "MDRW2100014388.1.1",
          "form": "외모는 불편하지만 않을 수준이면 뭐...",
          "original_form": "외모는 불편하지만 않을 수준이면 뭐...",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014388.1.2",
          "form": "남자는 키 172 이상만 되면 불편없고",
          "original_form": "남자는 키 172 이상만 되면 불편없고",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014388.1.3",
          "form": "여자도 그냥 150 이상이면 되구",
          "original_form": "여자도 그냥 150 이상이면 되구",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014388.1.4",
          "form": "살찐사람들은 독하게 맘먹고 빼면 되고",
          "original_form": "살찐사람들은 독하게 맘먹고 빼면 되고",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014388.1.5",
          "form": "ㅋㅋㅋ저 어제 체해서 난리났었네요...",
          "original_form": "ㅋㅋㅋ저 어제 체해서 난리났었네요...",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100014388.1.6",
          "form": "쨌든...요즘 유튜브 뭐봐요?",
          "original_form": "쨌든...요즘 유튜브 뭐봐요?",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100014388.1.7",
          "form": "아이구... 체했을땐 억지로 토하면 한방에 끝나는데",
          "original_form": "아이구... 체했을땐 억지로 토하면 한방에 끝나는데",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100014388.1.8",
          "form": "유튜브 잘 안보긴 하는데",
          "original_form": "유튜브 잘 안보긴 하는데",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100014388.1.9",
          "form": "김성환유튜브 동천백산 유튜브 보고있어용",
          "original_form": "김성환유튜브 동천백산 유튜브 보고있어용",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100014388.1.10",
          "form": "동천백산..?",
          "original_form": "동천백산..?",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014388.1.11",
          "form": "아 신천지같은거 아닙니다",
          "original_form": "아 신천지같은거 아닙니다",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100014388.1.12",
          "form": "주짓수ㅋㅋㅋㅋ",
          "original_form": "주짓수ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100014388.1.13",
          "form": "ㅋㅋㅋ아아ㅋㅋㅋ저는 무슨 등산 그런거인줄ㅋㅋ",
          "original_form": "ㅋㅋㅋ아아ㅋㅋㅋ저는 무슨 등산 그런거인줄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100014388.1.14",
          "form": "김성환아저씨 유튜브도 하시나봐요",
          "original_form": "김성환아저씨 유튜브도 하시나봐요",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100014388.1.15",
          "form": "오 누군지 알아요??",
          "original_form": "오 누군지 알아요??",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100014388.1.16",
          "form": "배우 김성환 아저씨...?",
          "original_form": "배우 김성환 아저씨...?",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100014388.1.17",
          "form": "아닌가....",
          "original_form": "아닌가....",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100014388.1.18",
          "form": "아닙니ㄷ...",
          "original_form": "아닙니ㄷ...",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100014388.1.19",
          "form": "헬스하시는분인데 나름 유명하셔요",
          "original_form": "헬스하시는분인데 나름 유명하셔요",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100014388.1.20",
          "form": "누님은 무슨유튜브봐여",
          "original_form": "누님은 무슨유튜브봐여",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100014388.1.21",
          "form": "아.....ㅋㅋㅋㅋㅋㅋ배우 김성환아저씨인줄....ㅜ",
          "original_form": "아.....ㅋㅋㅋㅋㅋㅋ배우 김성환아저씨인줄....ㅜ",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100014388.1.22",
          "form": "땅끄부부...참피디...뷰티유튜브여러가지...?",
          "original_form": "땅끄부부...참피디...뷰티유튜브여러가지...?",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100014388.1.23",
          "form": "확실히 관심사가 다르네요",
          "original_form": "확실히 관심사가 다르네요",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100014388.1.24",
          "form": "저도 전부 첨들어봄ㅋㅋㅋㅋ",
          "original_form": "저도 전부 첨들어봄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100014388.1.25",
          "form": "참피디는 들어봤다",
          "original_form": "참피디는 들어봤다",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100014388.1.26",
          "form": "참피디 간장게장편 보면 간장게장이 엄청 먹고싶어져여",
          "original_form": "참피디 간장게장편 보면 간장게장이 엄청 먹고싶어져여",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014388.1.27",
          "form": "으어어이ㅡㄱ니ㅂㅅㄱㄷㅂㆍ니디",
          "original_form": "으어어이ㅡㄱ니ㅂㅅㄱㄷㅂㆍ니디",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100014388.1.28",
          "form": "간장게장 진짜 먹고싶다",
          "original_form": "간장게장 진짜 먹고싶다",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100014388.1.29",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100014388.1.30",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100014388.1.31",
          "form": "간장게장 앙념게장 진짜 좋아하는데",
          "original_form": "간장게장 앙념게장 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100014388.1.32",
          "form": "참피디 유튜브도 구독해요ㅋㅋㅋㅋ맛있는 거 많이 나옴",
          "original_form": "참피디 유튜브도 구독해요ㅋㅋㅋㅋ맛있는 거 많이 나옴",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100014388.1.33",
          "form": "저 그것도 봤어요",
          "original_form": "저 그것도 봤어요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100014388.1.34",
          "form": "쯔양 유튜브",
          "original_form": "쯔양 유튜브",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100014388.1.35",
          "form": "라면먹는거",
          "original_form": "라면먹는거",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100014388.1.36",
          "form": "라면 20개씩 먹는거보면 도전의식 생겨서",
          "original_form": "라면 20개씩 먹는거보면 도전의식 생겨서",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100014388.1.37",
          "form": "5개 끓여보려다가",
          "original_form": "5개 끓여보려다가",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100014388.1.38",
          "form": "머리맞았어요",
          "original_form": "머리맞았어요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100014388.1.39",
          "form": "ㅋㅋㅋㅋㅋ쯔양은 도대체 위가 어떻게 되어있길래 그 양이 다 들어갈까요...",
          "original_form": "ㅋㅋㅋㅋㅋ쯔양은 도대체 위가 어떻게 되어있길래 그 양이 다 들어갈까요...",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100014388.1.40",
          "form": "일반인은 함부로 따라하면 안돼요ㅋㅋ",
          "original_form": "일반인은 함부로 따라하면 안돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100014388.1.41",
          "form": "와 그런거보면",
          "original_form": "와 그런거보면",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014388.1.42",
          "form": "심장이 두근거리지 않아요?",
          "original_form": "심장이 두근거리지 않아요?",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014388.1.43",
          "form": "나도 뭔가 할 수 있을것같은",
          "original_form": "나도 뭔가 할 수 있을것같은",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014388.1.44",
          "form": "아뇨...?",
          "original_form": "아뇨...?",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100014388.1.45",
          "form": "저는 그냥 저사람은 인간이 아니다 하는데...",
          "original_form": "저는 그냥 저사람은 인간이 아니다 하는데...",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100014388.1.46",
          "form": "그러니까 마른거죠",
          "original_form": "그러니까 마른거죠",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100014388.1.47",
          "form": "반성하세요",
          "original_form": "반성하세요",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100014388.1.48",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100014388.1.49",
          "form": "ㅋㅋㅋㅋ저 다이어트중인데요...ㅜ?",
          "original_form": "ㅋㅋㅋㅋ저 다이어트중인데요...ㅜ?",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100014388.1.50",
          "form": "땅끄부부 유튜브보면서 운동 중...",
          "original_form": "땅끄부부 유튜브보면서 운동 중...",
          "speaker_id": "2",
          "time": "20210826 12:28"
        }
      ]
