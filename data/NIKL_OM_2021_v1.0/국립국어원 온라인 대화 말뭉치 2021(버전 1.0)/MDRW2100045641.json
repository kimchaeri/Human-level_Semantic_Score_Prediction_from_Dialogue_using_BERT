      "utterance": [
        {
          "id": "MDRW2100045641.1.1",
          "form": "정답 알려드릴까요?",
          "original_form": "정답 알려드릴까요?",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045641.1.2",
          "form": "마트에서 흔하게 파는 잎채소인가요?",
          "original_form": "마트에서 흔하게 파는 잎채소인가요?",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045641.1.3",
          "form": "아악 힌트만요!",
          "original_form": "아악 힌트만요!",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045641.1.4",
          "form": "힌트만 좀만 주세요",
          "original_form": "힌트만 좀만 주세요",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045641.1.5",
          "form": "마트에서 보기 어려워요",
          "original_form": "마트에서 보기 어려워요",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045641.1.6",
          "form": "작고 동글동글해요",
          "original_form": "작고 동글동글해요",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045641.1.7",
          "form": "와 보기 어렵다니 난이도 헬모드",
          "original_form": "와 보기 어렵다니 난이도 헬모드",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045641.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045641.1.9",
          "form": "힌트 하나만 더요!",
          "original_form": "힌트 하나만 더요!",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045641.1.10",
          "form": "같은 이름을 가진게 있는데 이건 앞에 수식어가 붙어요!",
          "original_form": "같은 이름을 가진게 있는데 이건 앞에 수식어가 붙어요!",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045641.1.11",
          "form": "어떤 수식어예요?",
          "original_form": "어떤 수식어예요?",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045641.1.12",
          "form": "작다는 수식어요!",
          "original_form": "작다는 수식어요!",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045641.1.13",
          "form": "정답이 몇 글자인가요!",
          "original_form": "정답이 몇 글자인가요!",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045641.1.14",
          "form": "이러다 다 알려달라 하겠어요",
          "original_form": "이러다 다 알려달라 하겠어요",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045641.1.15",
          "form": "5글자요!",
          "original_form": "5글자요!",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045641.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ전혀 힌트가 되지 않는 기분이예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ전혀 힌트가 되지 않는 기분이예요",
          "speaker_id": "1",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045641.1.17",
          "form": "그런가요?ㅋㅋㅋㅋ",
          "original_form": "그런가요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045641.1.18",
          "form": "괜히 이걸로 했나봐욬ㅋㅋㅋㅋ",
          "original_form": "괜히 이걸로 했나봐욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045641.1.19",
          "form": "초성이라도..! 알려주세요!",
          "original_form": "초성이라도..! 알려주세요!",
          "speaker_id": "1",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045641.1.20",
          "form": "ㅁㄴㅇㅂㅊ",
          "original_form": "ㅁㄴㅇㅂㅊ",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045641.1.21",
          "form": "이렇게 5글자입니다!",
          "original_form": "이렇게 5글자입니다!",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045641.1.22",
          "form": "미니양배추????!",
          "original_form": "미니양배추????!",
          "speaker_id": "1",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045641.1.23",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.24",
          "form": "정답이요!!",
          "original_form": "정답이요!!",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.25",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.26",
          "form": "이럴수가 양배추였다니",
          "original_form": "이럴수가 양배추였다니",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.27",
          "form": "근데 구워먹기도 하는군요 처움 알았엉ㅅ",
          "original_form": "근데 구워먹기도 하는군요 처움 알았엉ㅅ",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.28",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.29",
          "form": "네 방울 양배추라고도 하는데",
          "original_form": "네 방울 양배추라고도 하는데",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045641.1.30",
          "form": "작아서 구워먹기도하는데 삶거나 볶아서 먹는경우가 더 많은 것 같더라구요",
          "original_form": "작아서 구워먹기도하는데 삶거나 볶아서 먹는경우가 더 많은 것 같더라구요",
          "speaker_id": "2",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045641.1.31",
          "form": "방울양배추 ㅋㅋㅋ이름 넘 귀여워요",
          "original_form": "방울양배추 ㅋㅋㅋ이름 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045641.1.32",
          "form": "확실히 마트에서는 못본 것 같아요",
          "original_form": "확실히 마트에서는 못본 것 같아요",
          "speaker_id": "1",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045641.1.33",
          "form": "진짜 희귀",
          "original_form": "진짜 희귀",
          "speaker_id": "1",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045641.1.34",
          "form": "저는 본적이 있었거든욬ㅋㅋ",
          "original_form": "저는 본적이 있었거든욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045641.1.35",
          "form": "별로 먹고 싶진 않아서 안 먹었지만옼ㅋㅋㅋ",
          "original_form": "별로 먹고 싶진 않아서 안 먹었지만옼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045641.1.36",
          "form": "희귀템을 목격하셨네요 ㅋㅋㅋㅋㅋㅋ멋져요",
          "original_form": "희귀템을 목격하셨네요 ㅋㅋㅋㅋㅋㅋ멋져요",
