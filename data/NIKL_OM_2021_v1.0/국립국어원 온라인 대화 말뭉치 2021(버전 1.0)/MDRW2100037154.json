      "utterance": [
        {
          "id": "MDRW2100037154.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037154.1.2",
          "form": "name1님!! 오랜만이에요!!!",
          "original_form": "&name1&님!! 오랜만이에요!!!",
          "speaker_id": "2",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037154.1.3",
          "form": "저는 지금 아몬드 아그작아그작 먹는중이에여",
          "original_form": "저는 지금 아몬드 아그작아그작 먹는중이에여",
          "speaker_id": "2",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037154.1.4",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037154.1.5",
          "form": "밥이 먹고 싶지만 ㅠ 조금이라도 더 치고,,",
          "original_form": "밥이 먹고 싶지만 ㅠ 조금이라도 더 치고,,",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037154.1.6",
          "form": "악ㅋㅋㅋㅋㅋㅋㅋ 좀만 더 힘내세요!!!",
          "original_form": "악ㅋㅋㅋㅋㅋㅋㅋ 좀만 더 힘내세요!!!",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037154.1.7",
          "form": "저녁메뉴 정하셨나요???",
          "original_form": "저녁메뉴 정하셨나요???",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037154.1.8",
          "form": "아니오 그냥 집에가고 싶어요",
          "original_form": "아니오 그냥 집에가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037154.1.9",
          "form": "그래서 name2님의 저녁메뉴자랑이 듣고 싶습니다 ㅋㅋ",
          "original_form": "그래서 &name2&님의 저녁메뉴자랑이 듣고 싶습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037154.1.10",
          "form": "전계획이지금업서요",
          "original_form": "전계획이지금업서요",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037154.1.11",
          "form": "저도 지금 주전부리들 먹고있는중이고 ㅋㅋㅋㅋ아직 저녁생각 안하구 있었어요!! ㅋㅋㅋ",
          "original_form": "저도 지금 주전부리들 먹고있는중이고 ㅋㅋㅋㅋ아직 저녁생각 안하구 있었어요!! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037154.1.12",
          "form": "망고말랭이도 먹고있어욥ㅋㅋㅋㅋㅋ",
          "original_form": "망고말랭이도 먹고있어욥ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037154.1.13",
          "form": "다네요 달아 ㅋㅋㅋㅋ",
          "original_form": "다네요 달아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037154.1.14",
          "form": "고구마 말랭이도 땡기네요 ㅋㅋㅋㅋ없어서 못먹어요",
          "original_form": "고구마 말랭이도 땡기네요 ㅋㅋㅋㅋ없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037154.1.15",
          "form": "저도 말랭이류를 좋아해요 턱이 아플때까지 먹어서",
          "original_form": "저도 말랭이류를 좋아해요 턱이 아플때까지 먹어서",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100037154.1.16",
          "form": "좀 적게 꺼내야해요 ㅋㅋ",
          "original_form": "좀 적게 꺼내야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100037154.1.17",
          "form": "왠지 말랭이는 건강식 느낌이라 ㅋㅋㅋㅋ막 먹게되네요 ㅋㅋㅋㅋ엄청 단데 ㅋㅋㅋㅋㅋ",
          "original_form": "왠지 말랭이는 건강식 느낌이라 ㅋㅋㅋㅋ막 먹게되네요 ㅋㅋㅋㅋ엄청 단데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037154.1.18",
          "form": "말리다 보니 그렇죠",
          "original_form": "말리다 보니 그렇죠",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037154.1.19",
          "form": "그래서 방심하다보면 말랭이가 고구마 3개치고 이럴때가 좀 있어요 ㅋ",
          "original_form": "그래서 방심하다보면 말랭이가 고구마 3개치고 이럴때가 좀 있어요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037154.1.20",
          "form": "와 말라서 큰게 더 쪼그라든거란 생각을 못해봤어요 ㄷㄷㄷ",
          "original_form": "와 말라서 큰게 더 쪼그라든거란 생각을 못해봤어요 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037154.1.21",
          "form": "제가 지금 망고 한3개는 먹은거군요...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 지금 망고 한3개는 먹은거군요...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037154.1.22",
          "form": "망고 1개일수도 있죠!",
          "original_form": "망고 1개일수도 있죠!",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037154.1.23",
          "form": "아니에요.. 한봉지 다먹었어요.. 아몬드랑 번갈아서.. ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니에요.. 한봉지 다먹었어요.. 아몬드랑 번갈아서.. ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037154.1.24",
          "form": "단거 먹었더니 매콤한거 땡기네요 ㅋㅋㅋㅋ",
          "original_form": "단거 먹었더니 매콤한거 땡기네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037154.1.25",
          "form": "이제 매콤한 밥을 드셔야겠어요",
          "original_form": "이제 매콤한 밥을 드셔야겠어요",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037154.1.26",
          "form": "매콤한 과자를 먹으면 다시 단 과자가 땡길 뿐이니가요",
          "original_form": "매콤한 과자를 먹으면 다시 단 과자가 땡길 뿐이니가요",
          "speaker_id": "1",
          "time": "20210901 18:36"
        }
      ]
