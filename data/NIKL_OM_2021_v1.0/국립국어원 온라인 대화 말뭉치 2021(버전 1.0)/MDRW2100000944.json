      "utterance": [
        {
          "id": "MDRW2100000944.1.1",
          "form": "나 요즘 유튜브에서 계속 노래만 듣거든",
          "original_form": "나 요즘 유튜브에서 계속 노래만 듣거든",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000944.1.2",
          "form": "유튜브 뮤직써서?",
          "original_form": "유튜브 뮤직써서?",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.3",
          "form": "아니 유튜브뮤직 말고 그냥 유튜브로 사람들이 모아놓은걸로 듣는다",
          "original_form": "아니 유튜브뮤직 말고 그냥 유튜브로 사람들이 모아놓은걸로 듣는다",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.4",
          "form": "가끔 가사도 나오는 걸로 듣는다",
          "original_form": "가끔 가사도 나오는 걸로 듣는다",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.5",
          "form": "아 모아둔거 내취향으로 잘 찾으면",
          "original_form": "아 모아둔거 내취향으로 잘 찾으면",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.6",
          "form": "지금은 드라마 오에스티 모음 듣고있다",
          "original_form": "지금은 드라마 오에스티 모음 듣고있다",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.7",
          "form": "노래건지는거 많아",
          "original_form": "노래건지는거 많아",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.8",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.9",
          "form": "옛날 드라마?",
          "original_form": "옛날 드라마?",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000944.1.10",
          "form": "네방구석카세트테이프가 되고싶어",
          "original_form": "네방구석카세트테이프가 되고싶어",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.11",
          "form": "이 채널 좋다",
          "original_form": "이 채널 좋다",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.12",
          "form": "2010년대 오에스티모음같네",
          "original_form": "2010년대 오에스티모음같네",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.13",
          "form": "나한테 하는말인줄 알았네",
          "original_form": "나한테 하는말인줄 알았네",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.14",
          "form": "깜짝이야!",
          "original_form": "깜짝이야!",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.15",
          "form": "네 방구석 테이프는 되고싶지 않아!",
          "original_form": "네 방구석 테이프는 되고싶지 않아!",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.16",
          "form": "나중에 들어볼께",
          "original_form": "나중에 들어볼께",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.17",
          "form": "추천고마우ㅜ",
          "original_form": "추천고마우ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.18",
          "form": "유튜브 채널 이름이야",
          "original_form": "유튜브 채널 이름이야",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.19",
          "form": "송은이같이 말하네",
          "original_form": "송은이같이 말하네",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.20",
          "form": "응. 명곡들이 많아.",
          "original_form": "응. 명곡들이 많아.",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.21",
          "form": "좋은 뜻이지?",
          "original_form": "좋은 뜻이지?",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000944.1.22",
          "form": "당연하지!",
          "original_form": "당연하지!",
          "speaker_id": "2",
          "time": "20210819 20:49"
        }
      ]
    }
