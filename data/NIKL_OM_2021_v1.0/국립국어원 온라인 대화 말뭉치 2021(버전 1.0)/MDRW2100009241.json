      "utterance": [
        {
          "id": "MDRW2100009241.1.1",
          "form": "상황에 보고 녹차라떼 만들어줄게",
          "original_form": "상황에 보고 녹차라떼 만들어줄게",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009241.1.2",
          "form": "꼭 만들어줘",
          "original_form": "꼭 만들어줘",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009241.1.3",
          "form": "너는 뭐 마실래?",
          "original_form": "너는 뭐 마실래?",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009241.1.4",
          "form": "안 마실래",
          "original_form": "안 마실래",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009241.1.5",
          "form": "아메리카노 밖에 안마시니?",
          "original_form": "아메리카노 밖에 안마시니?",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009241.1.6",
          "form": "응 커피만 마셔",
          "original_form": "응 커피만 마셔",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009241.1.7",
          "form": "커피는 무슨 맛으로 먹는지 모르겠다",
          "original_form": "커피는 무슨 맛으로 먹는지 모르겠다",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009241.1.8",
          "form": "너무 써",
          "original_form": "너무 써",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009241.1.9",
          "form": "커피 맛있는데 뭘 마실거야?",
          "original_form": "커피 맛있는데 뭘 마실거야?",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009241.1.10",
          "form": "나는 민트초코",
          "original_form": "나는 민트초코",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009241.1.11",
          "form": "민트초코 마신다고?",
          "original_form": "민트초코 마신다고?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009241.1.12",
          "form": "민트초코가 제일 좋은데 녹차라떼 밤라떼밖에 없잖아",
          "original_form": "민트초코가 제일 좋은데 녹차라떼 밤라떼밖에 없잖아",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009241.1.13",
          "form": "민트초코 얼마야?",
          "original_form": "민트초코 얼마야?",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009241.1.14",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009241.1.15",
          "form": "사다주게?",
          "original_form": "사다주게?",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009241.1.16",
          "form": "아니 그냥 하는 말이야",
          "original_form": "아니 그냥 하는 말이야",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009241.1.17",
          "form": "ㅋㅋㅋㅌㅋ",
          "original_form": "ㅋㅋㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009241.1.18",
          "form": "오늘 내가 치킨사줬잖아",
          "original_form": "오늘 내가 치킨사줬잖아",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009241.1.19",
          "form": "사줘",
          "original_form": "사줘",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009241.1.20",
          "form": "치킨 또 산다고?",
          "original_form": "치킨 또 산다고?",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009241.1.21",
          "form": "아까 치킨 내가 돈냈음",
          "original_form": "아까 치킨 내가 돈냈음",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009241.1.22",
          "form": "몰랐어?",
          "original_form": "몰랐어?",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009241.1.23",
          "form": "처음에는 몰랐어",
          "original_form": "처음에는 몰랐어",
          "speaker_id": "2",
          "time": "20210822 22:31"
        }
      ]
