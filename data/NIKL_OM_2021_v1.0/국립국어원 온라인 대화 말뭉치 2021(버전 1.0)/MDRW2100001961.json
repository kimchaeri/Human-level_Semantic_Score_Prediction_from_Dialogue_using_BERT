      "utterance": [
        {
          "id": "MDRW2100001961.1.1",
          "form": "요즘 집에서만 밥먹으니 설거지가 진짜 힘든 것 같아",
          "original_form": "요즘 집에서만 밥먹으니 설거지가 진짜 힘든 것 같아",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001961.1.2",
          "form": "그냥 둬~ 내가 시간날 때 할게",
          "original_form": "그냥 둬~ 내가 시간날 때 할게",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001961.1.3",
          "form": "신랑도 쉬어야 하니깐 ㅠ",
          "original_form": "신랑도 쉬어야 하니깐 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001961.1.4",
          "form": "놔둬 ㅎㅎ",
          "original_form": "놔둬 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001961.1.5",
          "form": "포장을 해와도 집 그릇은 계속 쓰니까",
          "original_form": "포장을 해와도 집 그릇은 계속 쓰니까",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001961.1.6",
          "form": "쌓이고 또 쌓이고",
          "original_form": "쌓이고 또 쌓이고",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001961.1.7",
          "form": "너무 스트레스 받지 말고 할 수 있는 만큼만 해 ㅠ",
          "original_form": "너무 스트레스 받지 말고 할 수 있는 만큼만 해 ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001961.1.8",
          "form": "요즘 집에만 있으니 더 스트레스 받는듯 ㅠ",
          "original_form": "요즘 집에만 있으니 더 스트레스 받는듯 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001961.1.9",
          "form": "식기세척기라도 하나 사",
          "original_form": "식기세척기라도 하나 사",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001961.1.10",
          "form": "놓을 데도 없어 ㅎㅎ",
          "original_form": "놓을 데도 없어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001961.1.11",
          "form": "6인용 사면 되지",
          "original_form": "6인용 사면 되지",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001961.1.12",
          "form": "있으면 좀 나을라나 안써봐서 ㅠ",
          "original_form": "있으면 좀 나을라나 안써봐서 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001961.1.13",
          "form": "좀 낫지 않을까~ 만족도가 높다고 하던데",
          "original_form": "좀 낫지 않을까~ 만족도가 높다고 하던데",
