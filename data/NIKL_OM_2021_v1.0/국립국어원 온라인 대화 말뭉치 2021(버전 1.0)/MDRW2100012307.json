      "utterance": [
        {
          "id": "MDRW2100012307.1.1",
          "form": "안녕하세요 아르바이트 해본적 있으세요?",
          "original_form": "안녕하세요 아르바이트 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012307.1.2",
          "form": "네 해봫어요",
          "original_form": "네 해봫어요",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012307.1.3",
          "form": "어떤 아르바이트 였나요?",
          "original_form": "어떤 아르바이트 였나요?",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012307.1.4",
          "form": "카페 알바였어요",
          "original_form": "카페 알바였어요",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012307.1.5",
          "form": "카페알바는 메뉴가 많아서 외우는게 힘들다고 들었어요 어떠셨어요?",
          "original_form": "카페알바는 메뉴가 많아서 외우는게 힘들다고 들었어요 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012307.1.6",
          "form": "맞아요 근데 다른 카페보단 없는 편이어서 다행이었아요",
          "original_form": "맞아요 근데 다른 카페보단 없는 편이어서 다행이었아요",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012307.1.7",
          "form": "어려운 건 사장님이 해줬었고요",
          "original_form": "어려운 건 사장님이 해줬었고요",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012307.1.8",
          "form": "카페알바하다 생긴 에피소드 있나요?",
          "original_form": "카페알바하다 생긴 에피소드 있나요?",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012307.1.9",
          "form": "마감 시간인데 주문해가지고 20분 늦게 집에 간 적 있어요",
          "original_form": "마감 시간인데 주문해가지고 20분 늦게 집에 간 적 있어요",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012307.1.10",
          "form": "앗....마감시간 얼마안남은데 괜찮겠냐는 말은 나가시라는말인데...",
          "original_form": "앗....마감시간 얼마안남은데 괜찮겠냐는 말은 나가시라는말인데...",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012307.1.11",
          "form": "못알아들으신분이 있었군요",
          "original_form": "못알아들으신분이 있었군요",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012307.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012307.1.13",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012307.1.14",
          "form": "알바하신 적 있으세요?",
          "original_form": "알바하신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012307.1.15",
          "form": "저는 서빙알바 해봤어요",
          "original_form": "저는 서빙알바 해봤어요",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012307.1.16",
          "form": "치킨집 알바였는데 술있는곳 알바는 하는게 아니구나 했어요",
          "original_form": "치킨집 알바였는데 술있는곳 알바는 하는게 아니구나 했어요",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012307.1.17",
          "form": "어떤 게 가장 힘들었나요?",
          "original_form": "어떤 게 가장 힘들었나요?",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012307.1.18",
          "form": "술취한 손님들이 알아듣기 힘들 말을 하는것...",
          "original_form": "술취한 손님들이 알아듣기 힘들 말을 하는것...",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012307.1.19",
          "form": "같은말 반복하는거 고집부리는거 그런게 힘들었어요",
          "original_form": "같은말 반복하는거 고집부리는거 그런게 힘들었어요",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012307.1.20",
          "form": "동네 아저씨들이 자주 오던곳이라 그런가 취객이 꽤있엇어요",
          "original_form": "동네 아저씨들이 자주 오던곳이라 그런가 취객이 꽤있엇어요",
          "speaker_id": "1",
          "time": "20210824 21:46"
        }
      ]
