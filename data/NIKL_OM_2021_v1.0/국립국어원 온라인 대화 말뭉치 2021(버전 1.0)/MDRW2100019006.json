      },
      "utterance": [
        {
          "id": "MDRW2100019006.1.1",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.2",
          "form": "우리 올해는 에버랜드도",
          "original_form": "우리 올해는 에버랜드도",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.3",
          "form": "가야하는데",
          "original_form": "가야하는데",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.4",
          "form": "10월10일이면",
          "original_form": "10월10일이면",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.5",
          "form": "못가려나",
          "original_form": "못가려나",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.6",
          "form": "좀 어정쩡 하긴 하네요",
          "original_form": "좀 어정쩡 하긴 하네요",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.7",
          "form": "그때 상황이 어떤지",
          "original_form": "그때 상황이 어떤지",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.8",
          "form": "코로나가..",
          "original_form": "코로나가..",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.9",
          "form": "그걸 봐야겠는데요",
          "original_form": "그걸 봐야겠는데요",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.10",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.11",
          "form": "에버랜드는",
          "original_form": "에버랜드는",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.12",
          "form": "근데 백신 2차 맞고",
          "original_form": "근데 백신 2차 맞고",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.13",
          "form": "갈수있는거 맞냐고",
          "original_form": "갈수있는거 맞냐고",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.14",
          "form": "2주 까지 지나면",
          "original_form": "2주 까지 지나면",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100019006.1.15",
          "form": "10일은 훌쩍 넘을거같긴해요",
          "original_form": "10일은 훌쩍 넘을거같긴해요",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.16",
          "form": "나 거의 반포기했다",
          "original_form": "나 거의 반포기했다",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.17",
          "form": "에버랜드..",
          "original_form": "에버랜드..",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.18",
          "form": "나도..",
          "original_form": "나도..",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.19",
          "form": "ㅇ0버랜드야",
          "original_form": "ㅇ0버랜드야",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.20",
          "form": "그냥 내가 에버랜드 사고싶음",
          "original_form": "그냥 내가 에버랜드 사고싶음",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.21",
          "form": "님 못 오면 혼자라도 가야겠다..",
          "original_form": "님 못 오면 혼자라도 가야겠다..",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100019006.1.22",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.23",
          "form": "심연의상상...",
          "original_form": "심연의상상...",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.24",
          "form": "꼭 같이 갈 필요없지",
          "original_form": "꼭 같이 갈 필요없지",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.25",
          "form": "에버랜드 우리가 가는날 그냥",
          "original_form": "에버랜드 우리가 가는날 그냥",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.26",
          "form": "아무도 없엇으면 젛겟음",
          "original_form": "아무도 없엇으면 젛겟음",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.27",
          "form": "사람들 다 쫓아내",
          "original_form": "사람들 다 쫓아내",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019006.1.28",
          "form": "그래 맞다",
          "original_form": "그래 맞다",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019006.1.29",
          "form": "저번에 본 동해바다도",
          "original_form": "저번에 본 동해바다도",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019006.1.30",
          "form": "가서 기차 타보고싶음",
          "original_form": "가서 기차 타보고싶음",
          "speaker_id": "2",
          "time": "20210827 14:41"
        }
      ]
    }
  ]
