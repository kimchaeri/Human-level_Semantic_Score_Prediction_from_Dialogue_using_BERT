      "utterance": [
        {
          "id": "MDRW2100032818.1.1",
          "form": "어떤음식인지 맞춰져?",
          "original_form": "어떤음식인지 맞춰져?",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100032818.1.2",
          "form": "아니 더 물어봐야 알겠어",
          "original_form": "아니 더 물어봐야 알겠어",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100032818.1.3",
          "form": "멕시코에도 비슷한 음식이 있다구?",
          "original_form": "멕시코에도 비슷한 음식이 있다구?",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100032818.1.4",
          "form": "응 있었던것같아",
          "original_form": "응 있었던것같아",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.5",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.6",
          "form": "퓨전으로 먹었어",
          "original_form": "퓨전으로 먹었어",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.8",
          "form": "궁금한걸",
          "original_form": "궁금한걸",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.9",
          "form": "쌀이 들어가?",
          "original_form": "쌀이 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.10",
          "form": "안들어가",
          "original_form": "안들어가",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.11",
          "form": "그래??? 빵도 쌀도 면도 아니라니",
          "original_form": "그래??? 빵도 쌀도 면도 아니라니",
          "speaker_id": "1",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.12",
          "form": "내가 이러니까 잘못 말을 해줬나 싶을정도로",
          "original_form": "내가 이러니까 잘못 말을 해줬나 싶을정도로",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.13",
          "form": "쉬운음식이야",
          "original_form": "쉬운음식이야",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032818.1.14",
          "form": "이런 정답 말하지 말아봐 내가 맞춰볼래",
          "original_form": "이런 정답 말하지 말아봐 내가 맞춰볼래",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032818.1.15",
          "form": "밀가루 들어가?",
          "original_form": "밀가루 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032818.1.16",
          "form": "밀가루 말고 다른가루가 들어갈것같아",
          "original_form": "밀가루 말고 다른가루가 들어갈것같아",
          "speaker_id": "2",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032818.1.17",
          "form": "뭐지 그러면 전분이 들어가?",
          "original_form": "뭐지 그러면 전분이 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032818.1.18",
          "form": "전분이 들어가는 곳도 있겠다",
          "original_form": "전분이 들어가는 곳도 있겠다",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032818.1.19",
          "form": "대부분 들어갈지도",
          "original_form": "대부분 들어갈지도",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032818.1.20",
          "form": "고기가 들어가?",
          "original_form": "고기가 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032818.1.21",
          "form": "응 고기가 들어가",
          "original_form": "응 고기가 들어가",
          "speaker_id": "2",
          "time": "20210906 00:33"
        }
      ]
