      "utterance": [
        {
          "id": "MDRW2100006981.1.1",
          "form": "네ㅋㅋ냉파스타군여ㅋㅋ",
          "original_form": "네ㅋㅋ냉파스타군여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006981.1.2",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100006981.1.3",
          "form": "이번 방학에는 파스타에 빠져서 열심히 해먹었던거같아요",
          "original_form": "이번 방학에는 파스타에 빠져서 열심히 해먹었던거같아요",
          "speaker_id": "1",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100006981.1.4",
          "form": "파스타 많이 좋아하시나봐요ㅋㅋ",
          "original_form": "파스타 많이 좋아하시나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100006981.1.5",
          "form": "저는 투움바파스타 좋아해요! 매운크림빠네파스타도요!",
          "original_form": "저는 투움바파스타 좋아해요! 매운크림빠네파스타도요!",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100006981.1.6",
          "form": "와 투움바랑 빠네 맛있죠",
          "original_form": "와 투움바랑 빠네 맛있죠",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100006981.1.7",
          "form": "저는 집에서 먹기엔 알리오올리오랑 토마토파스타가 편해서 자주 먹어요 ㅎㅎ",
          "original_form": "저는 집에서 먹기엔 알리오올리오랑 토마토파스타가 편해서 자주 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100006981.1.8",
          "form": "그쵸 집에서 먹게되면 토마토파스타가 제일 기본적인거 같아요ㅎ",
          "original_form": "그쵸 집에서 먹게되면 토마토파스타가 제일 기본적인거 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100006981.1.9",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100006981.1.10",
          "form": "예전에 투움바가 너무 먹고싶어서 신라면투움바 레시피보고 만들어 먹은적이 있었는데 그것도 꽤 괜찮았어요ㅋㅋ",
          "original_form": "예전에 투움바가 너무 먹고싶어서 신라면투움바 레시피보고 만들어 먹은적이 있었는데 그것도 꽤 괜찮았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100006981.1.11",
          "form": "파스타면이 없을 때...!!",
          "original_form": "파스타면이 없을 때...!!",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100006981.1.12",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100006981.1.13",
          "form": "요즘 로제신라면?이라구 비슷하게 레시피 유행하더라고요",
          "original_form": "요즘 로제신라면?이라구 비슷하게 레시피 유행하더라고요",
          "speaker_id": "1",
          "time": "20210823 15:35"
        },
        {
          "id": "MDRW2100006981.1.14",
          "form": "아ㅋㅋㅋ잘만 만들면 꽤 맛있어요ㅋㅋ저는 실패작들이 몇 번 있어서 초반에는 좀 그랬지만요",
          "original_form": "아ㅋㅋㅋ잘만 만들면 꽤 맛있어요ㅋㅋ저는 실패작들이 몇 번 있어서 초반에는 좀 그랬지만요",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100006981.1.15",
          "form": "담에 한번 만들어봐야겠어요 ㅋㅋ",
          "original_form": "담에 한번 만들어봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100006981.1.16",
          "form": "네ㅋㅋ한 번 만들어보세용",
          "original_form": "네ㅋㅋ한 번 만들어보세용",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100006981.1.17",
          "form": "저도 다른 음식 많이 해먹으면서 요리실력을 좀 늘려야겠어요ㅋㅋ",
          "original_form": "저도 다른 음식 많이 해먹으면서 요리실력을 좀 늘려야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100006981.1.18",
          "form": "요즘 벌써 고구마가 나오더라구요!",
          "original_form": "요즘 벌써 고구마가 나오더라구요!",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100006981.1.19",
          "form": "그래서 맨날 허니버터고구마 해먹고있어요 ㅋㅋ",
          "original_form": "그래서 맨날 허니버터고구마 해먹고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100006981.1.20",
          "form": "우와? 허니버터고구마요?",
          "original_form": "우와? 허니버터고구마요?",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100006981.1.21",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100006981.1.22",
          "form": "군고구마 반갈라서 꿀이랑 버터 위에 치즈뿌리고 십분정도만 오븐에 구우면 완성이에요 !!",
          "original_form": "군고구마 반갈라서 꿀이랑 버터 위에 치즈뿌리고 십분정도만 오븐에 구우면 완성이에요 !!",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100006981.1.23",
          "form": "우와....! 적어놔야지...!!!",
          "original_form": "우와....! 적어놔야지...!!!",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100006981.1.24",
          "form": "ㅎㅎ 재료만 있으면 진짜 간단한데 완전 외식맛이에요",
          "original_form": "ㅎㅎ 재료만 있으면 진짜 간단한데 완전 외식맛이에요",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
