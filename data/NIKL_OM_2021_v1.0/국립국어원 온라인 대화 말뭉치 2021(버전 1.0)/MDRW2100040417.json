      "utterance": [
        {
          "id": "MDRW2100040417.1.1",
          "form": "허리굽히면 땅에 손끝 겨우 닿는것 같아요",
          "original_form": "허리굽히면 땅에 손끝 겨우 닿는것 같아요",
          "speaker_id": "2",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100040417.1.2",
          "form": "헉 그래도 유연하시네요!",
          "original_form": "헉 그래도 유연하시네요!",
          "speaker_id": "1",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040417.1.3",
          "form": "전 겨우 무릎 넘깁니다",
          "original_form": "전 겨우 무릎 넘깁니다",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100040417.1.4",
          "form": "90도도 허리 아파요ㅜ",
          "original_form": "90도도 허리 아파요ㅜ",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100040417.1.5",
          "form": "스트레칭을 자주 해야할 것 같아요",
          "original_form": "스트레칭을 자주 해야할 것 같아요",
          "speaker_id": "2",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100040417.1.6",
          "form": "평소 스트레칭을 하시나요?",
          "original_form": "평소 스트레칭을 하시나요?",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100040417.1.7",
          "form": "일하다가 일어나서 한번씩 움직이는 편이에요",
          "original_form": "일하다가 일어나서 한번씩 움직이는 편이에요",
          "speaker_id": "2",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100040417.1.8",
          "form": "평소 습관이 중요하네요",
          "original_form": "평소 습관이 중요하네요",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100040417.1.9",
          "form": "평생 목각이라고 몸을 안 움직이니 더 굳는듯",
          "original_form": "평생 목각이라고 몸을 안 움직이니 더 굳는듯",
          "speaker_id": "1",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100040417.1.10",
          "form": "ㅎㅎ맞아요 저도 어깨랑 허리에힘이 많이 들어가있다더라고요",
          "original_form": "ㅎㅎ맞아요 저도 어깨랑 허리에힘이 많이 들어가있다더라고요",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100040417.1.11",
          "form": "홈트도 하시나요",
          "original_form": "홈트도 하시나요",
          "speaker_id": "1",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100040417.1.12",
          "form": "아니요 집에서는 아무것도...ㅎㅎ",
          "original_form": "아니요 집에서는 아무것도...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100040417.1.13",
          "form": "집에 오면 완전 퍼지게 되죠ㅎ",
          "original_form": "집에 오면 완전 퍼지게 되죠ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100040417.1.14",
          "form": "네 ㅎㅎ 얼굴 씻기도 바쁘고 아이 시중드느라홈트가 뭔가요 ㅎㅎ",
          "original_form": "네 ㅎㅎ 얼굴 씻기도 바쁘고 아이 시중드느라홈트가 뭔가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100040417.1.15",
          "form": "아이고 아이 시중ㄷㄷ정말 쉴 틈이 없네요ㅜ",
          "original_form": "아이고 아이 시중ㄷㄷ정말 쉴 틈이 없네요ㅜ",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100040417.1.16",
          "form": "네 그래서 일주일에 두번 가는 필라테스 운동시간이 소중해요",
