      "utterance": [
        {
          "id": "MDRW2100019981.1.1",
          "form": "주제 바꿨습니다ㅎㅎ",
          "original_form": "주제 바꿨습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019981.1.2",
          "form": "요즘 구매하신 것 있으세요?",
          "original_form": "요즘 구매하신 것 있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019981.1.3",
          "form": "최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "original_form": "최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019981.1.4",
          "form": "에그팬이 뭐에요??",
          "original_form": "에그팬이 뭐에요??",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019981.1.5",
          "form": "계란후라이만 할수있도록 작게만든팬이에요~",
          "original_form": "계란후라이만 할수있도록 작게만든팬이에요~",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019981.1.6",
          "form": "아침에 꼭 계란하나구워먹어서 되게유용한거같아요",
          "original_form": "아침에 꼭 계란하나구워먹어서 되게유용한거같아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019981.1.7",
          "form": "되게 좋을 것 같네요",
          "original_form": "되게 좋을 것 같네요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019981.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019981.1.9",
          "form": "흠 전 최근에 당근에서 비니를 샀아요",
          "original_form": "흠 전 최근에 당근에서 비니를 샀아요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019981.1.10",
          "form": "곧 겨울이라(?)",
          "original_form": "곧 겨울이라(?)",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019981.1.11",
          "form": "오!! 미리미리 준비하는거좋죠~",
          "original_form": "오!! 미리미리 준비하는거좋죠~",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019981.1.12",
          "form": "저도 며칠전에 가을아우터샀어요 ㅋㅋ",
          "original_form": "저도 며칠전에 가을아우터샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019981.1.13",
          "form": "뭐샀어요?",
          "original_form": "뭐샀어요?",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019981.1.14",
          "form": "저 숏트렌치자켓이요 ㅎㅎ",
          "original_form": "저 숏트렌치자켓이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019981.1.15",
          "form": "오 예쁠거 같아요 ㅎㅎ",
          "original_form": "오 예쁠거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019981.1.16",
          "form": "전 블레이저를 샀답니다",
          "original_form": "전 블레이저를 샀답니다",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019981.1.17",
          "form": "오우 블레이저~ 어떤브랜드 사셨나요??",
          "original_form": "오우 블레이저~ 어떤브랜드 사셨나요??",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019981.1.18",
          "form": "무신사스탠다드요~ㅋㅋㅋㅋ",
          "original_form": "무신사스탠다드요~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019981.1.19",
          "form": "가성비 좋고 이뻐요",
          "original_form": "가성비 좋고 이뻐요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019981.1.20",
          "form": "맞아요 무신사가 되게 가성비 괜찮더라구요!!",
          "original_form": "맞아요 무신사가 되게 가성비 괜찮더라구요!!",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019981.1.21",
          "form": "무탠다드 슬랙슨가? 그거도좋더라구요",
          "original_form": "무탠다드 슬랙슨가? 그거도좋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019981.1.22",
          "form": "맞아요 그거도 있어요ㅋㅋ",
          "original_form": "맞아요 그거도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019981.1.23",
          "form": "앜ㅋㅋㅌㅌㅋㅋ 역시!!",
          "original_form": "앜ㅋㅋㅌㅌㅋㅋ 역시!!",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019981.1.24",
          "form": "기본템 사기 딱이더라구요ㅎㅎ",
          "original_form": "기본템 사기 딱이더라구요ㅎㅎ",
