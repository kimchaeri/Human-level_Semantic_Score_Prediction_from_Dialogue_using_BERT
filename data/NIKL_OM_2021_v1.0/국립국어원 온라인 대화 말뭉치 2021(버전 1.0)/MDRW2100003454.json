      "utterance": [
        {
          "id": "MDRW2100003454.1.1",
          "form": "저번에 치료받은 무릎은 좀 괜찮아?",
          "original_form": "저번에 치료받은 무릎은 좀 괜찮아?",
          "speaker_id": "2",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100003454.1.2",
          "form": "ㅇㅇ 많이 좋아졌어. 그 때는 아예 펴지도 못하겠더니 지금은 아주 좋아.",
          "original_form": "ㅇㅇ 많이 좋아졌어. 그 때는 아예 펴지도 못하겠더니 지금은 아주 좋아.",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003454.1.3",
          "form": "다행이네~ 계단 같은거 오르내릴 때 조심해",
          "original_form": "다행이네~ 계단 같은거 오르내릴 때 조심해",
          "speaker_id": "2",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003454.1.4",
          "form": "그것보다 무리하게 스트레칭 하지 않으려고",
          "original_form": "그것보다 무리하게 스트레칭 하지 않으려고",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003454.1.5",
          "form": "스트레칭하다 다쳤었나?",
          "original_form": "스트레칭하다 다쳤었나?",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003454.1.6",
          "form": "ㅇㅇ 몸이 너무 뻣뻣한 것 같아서 유연성 기르려다ㅠㅠ",
          "original_form": "ㅇㅇ 몸이 너무 뻣뻣한 것 같아서 유연성 기르려다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003454.1.7",
          "form": "이런이런",
          "original_form": "이런이런",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003454.1.8",
          "form": "한 번 그러고 나니 무서워서 못하겠어",
          "original_form": "한 번 그러고 나니 무서워서 못하겠어",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003454.1.9",
          "form": "운동할 때도 넘 무리하면 안돼",
          "original_form": "운동할 때도 넘 무리하면 안돼",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003454.1.10",
          "form": "아예 걷기도 힘들 정도였던지라 다신 그러기 싫어 ㅎ",
          "original_form": "아예 걷기도 힘들 정도였던지라 다신 그러기 싫어 ㅎ",
          "speaker_id": "1",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003454.1.11",
          "form": "지금부터 잘 관리해야 나이들어서도 문제없지",
          "original_form": "지금부터 잘 관리해야 나이들어서도 문제없지",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003454.1.12",
          "form": "무릎 아프고나서는 스쿼트도 안해",
          "original_form": "무릎 아프고나서는 스쿼트도 안해",
          "speaker_id": "1",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003454.1.13",
          "form": "그냥 가벼운 산책만 해",
          "original_form": "그냥 가벼운 산책만 해",
          "speaker_id": "1",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003454.1.14",
          "form": "당분간은 그 정도만 해~무리 말고",
          "original_form": "당분간은 그 정도만 해~무리 말고",
          "speaker_id": "2",
          "time": "20210820 23:20"
        }
      ]
