      "utterance": [
        {
          "id": "MDRW2100015208.1.1",
          "form": "저녁 드셨나요??",
          "original_form": "저녁 드셨나요??",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015208.1.2",
          "form": "네네 name1님도 식사하셨어요?",
          "original_form": "네네 &name1&님도 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015208.1.3",
          "form": "네 저는 들깨슈제비 먹었어요!",
          "original_form": "네 저는 들깨슈제비 먹었어요!",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015208.1.4",
          "form": "뭐드셨나요??",
          "original_form": "뭐드셨나요??",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015208.1.5",
          "form": "우와 들깨수제비 넘 맛있겠에요",
          "original_form": "우와 들깨수제비 넘 맛있겠에요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015208.1.6",
          "form": "저는 그냥 있는 반찬꺼내서 먹었어요ㅜㅜ",
          "original_form": "저는 그냥 있는 반찬꺼내서 먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015208.1.7",
          "form": "반찬 직접 만드세요??",
          "original_form": "반찬 직접 만드세요??",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015208.1.8",
          "form": "간단한거는 해보기도 하는데 귀찮아서 보통은 배달시켜요",
          "original_form": "간단한거는 해보기도 하는데 귀찮아서 보통은 배달시켜요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015208.1.9",
          "form": "배달 일주일에 몇번정도 시켜드세요??",
          "original_form": "배달 일주일에 몇번정도 시켜드세요??",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015208.1.10",
          "form": "대중없긴한데 주 1-2회정도?",
          "original_form": "대중없긴한데 주 1-2회정도?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015208.1.11",
          "form": "name1님은 배달많이 하는 편이세요?",
          "original_form": "&name1&님은 배달많이 하는 편이세요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015208.1.12",
          "form": "저는 시기가 있는 것 같아요",
          "original_form": "저는 시기가 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015208.1.13",
          "form": "한창 밀키트같은거 사먹고 요리하다가",
          "original_form": "한창 밀키트같은거 사먹고 요리하다가",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015208.1.14",
          "form": "배달만 시켜먹다가 왔다갔다",
          "original_form": "배달만 시켜먹다가 왔다갔다",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015208.1.15",
          "form": "그쵸그쵸 저도 그래요",
          "original_form": "그쵸그쵸 저도 그래요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015208.1.16",
          "form": "밀키트도 해보고, 반찬만 구입도 해보고, 배달음식도 먹고, 대충 만들고 무한루프",
          "original_form": "밀키트도 해보고, 반찬만 구입도 해보고, 배달음식도 먹고, 대충 만들고 무한루프",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015208.1.17",
          "form": "요새는 밀키트 맛난게 많아서 좋아요",
          "original_form": "요새는 밀키트 맛난게 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015208.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015208.1.19",
          "form": "내일은 뭐드실건가요??",
          "original_form": "내일은 뭐드실건가요??",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015208.1.20",
          "form": "갑자기 먹고싶은게 넘 많이 떠오르네요ㅋㅋㅋ",
          "original_form": "갑자기 먹고싶은게 넘 많이 떠오르네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015208.1.21",
          "form": "다 말씀해보세요 ㅋㅋㅋ",
          "original_form": "다 말씀해보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015208.1.22",
          "form": "ㅋㅋㅋㅋㅋ다 치기에는 손가락이 너무 불쌍해서ㅋㅋㅋㄲㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ다 치기에는 손가락이 너무 불쌍해서ㅋㅋㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015208.1.23",
          "form": "자제하고 머리속에서 대결을 붙였습니다 낙지볶음을 먹고 후식으로 설빙을 먹어야겠어요",
          "original_form": "자제하고 머리속에서 대결을 붙였습니다 낙지볶음을 먹고 후식으로 설빙을 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015208.1.24",
          "form": "디저트까지?  대박",
          "original_form": "디저트까지?  대박",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015208.1.25",
          "form": "ㅋㅋㅋ인절미설빙은 사랑이에요",
          "original_form": "ㅋㅋㅋ인절미설빙은 사랑이에요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015208.1.26",
          "form": "밥 안먹고 빙수랑 케이크만 먹으면서 살고 싶으네요",
          "original_form": "밥 안먹고 빙수랑 케이크만 먹으면서 살고 싶으네요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015208.1.27",
          "form": "케이크는 어떤 케이크 좋아하세요??",
          "original_form": "케이크는 어떤 케이크 좋아하세요??",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015208.1.28",
          "form": "저는 생크림케이크요",
          "original_form": "저는 생크림케이크요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015208.1.29",
          "form": "생크림케이크 하니까",
          "original_form": "생크림케이크 하니까",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015208.1.30",
          "form": "투썸 스초생이 생각나네요",
          "original_form": "투썸 스초생이 생각나네요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015208.1.31",
          "form": "밤에 이럼 안되는데ㅜㅜㅜㅠㅠ",
          "original_form": "밤에 이럼 안되는데ㅜㅜㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015208.1.32",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015208.1.33",
          "form": "투썸이 스벅보다 케이크류가 훨 맛난거같아요",
          "original_form": "투썸이 스벅보다 케이크류가 훨 맛난거같아요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015208.1.34",
          "form": "맞아요! 저도 갑자기 배고파지는 것 같아요",
          "original_form": "맞아요! 저도 갑자기 배고파지는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        }
      ]
