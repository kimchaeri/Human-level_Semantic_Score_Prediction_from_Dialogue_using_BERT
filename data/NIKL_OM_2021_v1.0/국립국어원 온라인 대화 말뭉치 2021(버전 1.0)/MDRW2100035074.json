        {
          "id": "MDRW2100035074.1.1",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210829 01:59"
        },
        {
          "id": "MDRW2100035074.1.2",
          "form": "안녕하시요",
          "original_form": "안녕하시요",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100035074.1.3",
          "form": "당신은 일본에 가고싶나요?",
          "original_form": "당신은 일본에 가고싶나요?",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100035074.1.4",
          "form": "가게되면 가겟죠",
          "original_form": "가게되면 가겟죠",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100035074.1.5",
          "form": "전 원래 콘서트땜에 가고싶었는데",
          "original_form": "전 원래 콘서트땜에 가고싶었는데",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100035074.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100035074.1.7",
          "form": "가수들이 자꾸 은퇴하네요ㅋㅋ",
          "original_form": "가수들이 자꾸 은퇴하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100035074.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n우껴여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n우껴여",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100035074.1.9",
          "form": "점점 사라지면 어쩌죠?",
          "original_form": "점점 사라지면 어쩌죠?",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100035074.1.10",
          "form": "두 가수 아닌가여",
          "original_form": "두 가수 아닌가여",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100035074.1.11",
          "form": "그래도 그둘 정말 가고 싶었어요",
          "original_form": "그래도 그둘 정말 가고 싶었어요",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100035074.1.12",
          "form": "은퇴라고하길래 한번 찍어봣는데 ㅎ",
          "original_form": "은퇴라고하길래 한번 찍어봣는데 ㅎ",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100035074.1.13",
          "form": "이러다 아무도 안남을듯요",
          "original_form": "이러다 아무도 안남을듯요",
          "speaker_id": "2",
          "time": "20210829 02:03"
