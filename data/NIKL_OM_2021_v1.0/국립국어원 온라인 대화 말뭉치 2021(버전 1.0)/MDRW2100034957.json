      "utterance": [
        {
          "id": "MDRW2100034957.1.1",
          "form": "휴 저는 카트끌고 터덜터덜 가는데",
          "original_form": "휴 저는 카트끌고 터덜터덜 가는데",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.2",
          "form": "코스트코 푸드코드 가보셨어요?",
          "original_form": "코스트코 푸드코드 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.3",
          "form": "엇 저두 걸어서 가요ㅋㅋㅋㅋ",
          "original_form": "엇 저두 걸어서 가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.4",
          "form": "저 최근에 가봤는데",
          "original_form": "저 최근에 가봤는데",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.5",
          "form": "앉아서 먹는 테이블 다 치우고",
          "original_form": "앉아서 먹는 테이블 다 치우고",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.6",
          "form": "양파 그런것도 다 치우고",
          "original_form": "양파 그런것도 다 치우고",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.7",
          "form": "무조건 포장만 돼요",
          "original_form": "무조건 포장만 돼요",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.8",
          "form": "그런거",
          "original_form": "그런거",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034957.1.9",
          "form": "없어졓을거같은데",
          "original_form": "없어졓을거같은데",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034957.1.10",
          "form": "팔긴파나봐요 아직;  ㅇ",
          "original_form": "팔긴파나봐요 아직;  ㅇ",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034957.1.11",
          "form": "최근에 여기서 피자 샀는데",
          "original_form": "최근에 여기서 피자 샀는데",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034957.1.12",
          "form": "미국식 피자라 그런지 엄청 짰어요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "미국식 피자라 그런지 엄청 짰어요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034957.1.13",
          "form": "코스트코 영양제 종류는 가끔 싸더라구요",
          "original_form": "코스트코 영양제 종류는 가끔 싸더라구요",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034957.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.15",
          "form": "맛있지만 미국식이라니",
          "original_form": "맛있지만 미국식이라니",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.16",
          "form": "짜고 단거먹는 동네아닌가요",
          "original_form": "짜고 단거먹는 동네아닌가요",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.17",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.18",
          "form": "저희동네에",
          "original_form": "저희동네에",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.19",
          "form": "코스트코 요플레나",
          "original_form": "코스트코 요플레나",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034957.1.20",
          "form": "홈플러스가 없어서 아쉬워요ㅠ",
          "original_form": "홈플러스가 없어서 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.21",
          "form": "요플레 맛있나요?",
          "original_form": "요플레 맛있나요?",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.22",
          "form": "비비고 삼각김밥 맛있어요",
          "original_form": "비비고 삼각김밥 맛있어요",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.23",
          "form": "그릭요거트요 싸고 양많아요",
          "original_form": "그릭요거트요 싸고 양많아요",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.24",
          "form": "아아 그거",
          "original_form": "아아 그거",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.25",
          "form": "요즘 되게 맛있다고",
          "original_form": "요즘 되게 맛있다고",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.26",
          "form": "유명하더라구요",
          "original_form": "유명하더라구요",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.28",
          "form": "그릭요거트 맛있구나 코스트코... 메모...",
          "original_form": "그릭요거트 맛있구나 코스트코... 메모...",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100034957.1.29",
          "form": "역시 많이 사보셨군요",
          "original_form": "역시 많이 사보셨군요",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034957.1.30",
          "form": "신세계 스타필드 가보셨어요?",
          "original_form": "신세계 스타필드 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034957.1.31",
          "form": "복합 쇼핑몰인데",
          "original_form": "복합 쇼핑몰인데",
          "speaker_id": "1",
          "time": "20210828 22:55"
        }
      ]
    }
