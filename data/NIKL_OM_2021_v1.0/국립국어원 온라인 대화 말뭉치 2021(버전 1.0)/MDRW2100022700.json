      "utterance": [
        {
          "id": "MDRW2100022700.1.1",
          "form": "요번애 그 군대이야기?",
          "original_form": "요번애 그 군대이야기?",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.2",
          "form": "ㅎㅎㅎ 군대이야기요?",
          "original_form": "ㅎㅎㅎ 군대이야기요?",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.3",
          "form": "그것두 재밌대서 보려구요",
          "original_form": "그것두 재밌대서 보려구요",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.4",
          "form": "그건 뭔지 잘모르겠어요 강철부대?",
          "original_form": "그건 뭔지 잘모르겠어요 강철부대?",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.5",
          "form": "ㅋㅋㅋㅋㅋ네 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.6",
          "form": "아뇨아뇨 드라마에요",
          "original_form": "아뇨아뇨 드라마에요",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022700.1.7",
          "form": "그 정해인나오구",
          "original_form": "그 정해인나오구",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.8",
          "form": "아 ㅎㅎ 하얀거탑 이런건가",
          "original_form": "아 ㅎㅎ 하얀거탑 이런건가",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.9",
          "form": "헉 정해인이 나와요?",
          "original_form": "헉 정해인이 나와요?",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.10",
          "form": "그럼꼭 찾아볼게요",
          "original_form": "그럼꼭 찾아볼게요",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.11",
          "form": "하얀거탑은 의사..",
          "original_form": "하얀거탑은 의사..",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 죄송",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 죄송",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.14",
          "form": "저 그 설국열차도 재밌게 봤는데 보셨어요?",
          "original_form": "저 그 설국열차도 재밌게 봤는데 보셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.15",
          "form": "네네 재밋대요 그 관심병사 관리하는건가",
          "original_form": "네네 재밋대요 그 관심병사 관리하는건가",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.16",
          "form": "뭐그런거같덩데요",
          "original_form": "뭐그런거같덩데요",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.17",
          "form": "되게 소재가 흥미롭네요",
          "original_form": "되게 소재가 흥미롭네요",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.18",
          "form": "앜ㅋㅋㅋ아니요 ㅠㅠ 그거 좀 징그럽개서 안봣어욬ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ아니요 ㅠㅠ 그거 좀 징그럽개서 안봣어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.19",
          "form": "엥 ㅠㅠ 징그러웠나",
          "original_form": "엥 ㅠㅠ 징그러웠나",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.20",
          "form": "아 그 연양갱이엇나",
          "original_form": "아 그 연양갱이엇나",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.21",
          "form": "전 영화로 보고 재밌게 봐서 드라마로도 봤어요!",
          "original_form": "전 영화로 보고 재밌게 봐서 드라마로도 봤어요!",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022700.1.22",
          "form": "아 그거 바퀴벌레로 만들었다고 ㅋㅋㅋㅋ",
          "original_form": "아 그거 바퀴벌레로 만들었다고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.23",
          "form": "아 드라마도잇어요?",
          "original_form": "아 드라마도잇어요?",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.24",
          "form": "오 한번봐봐야겟어요",
          "original_form": "오 한번봐봐야겟어요",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.25",
          "form": "네네 미국에서 만들었어요 설국열차 드라마",
          "original_form": "네네 미국에서 만들었어요 설국열차 드라마",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.26",
          "form": "이제 곧 시즌3 나와요!",
          "original_form": "이제 곧 시즌3 나와요!",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.27",
          "form": "저는 영화는 많이안봐서요!",
          "original_form": "저는 영화는 많이안봐서요!",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.28",
          "form": "오 한번봐랴겟어요",
          "original_form": "오 한번봐랴겟어요",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.29",
          "form": "근데 미국 배우의 연기감수성과 한국이랑 좀 다른거 같아요",
          "original_form": "근데 미국 배우의 연기감수성과 한국이랑 좀 다른거 같아요",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.30",
          "form": "한국 네티즌은 연기논란 있더라구요",
          "original_form": "한국 네티즌은 연기논란 있더라구요",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 공감",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 공감",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.32",
          "form": "아 긇군요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 긇군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.33",
          "form": "강철부대는 보셧어요?",
          "original_form": "강철부대는 보셧어요?",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022700.1.34",
          "form": "강철부대요?",
          "original_form": "강철부대요?",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022700.1.35",
          "form": "아니요 아직 본 적 없어요...",
          "original_form": "아니요 아직 본 적 없어요...",
          "speaker_id": "1",
