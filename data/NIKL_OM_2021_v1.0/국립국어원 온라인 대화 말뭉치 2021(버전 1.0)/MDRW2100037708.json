      "utterance": [
        {
          "id": "MDRW2100037708.1.1",
          "form": "아, 좋죠",
          "original_form": "아, 좋죠",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037708.1.2",
          "form": "전 숙주 듬뿍해서 먹어요",
          "original_form": "전 숙주 듬뿍해서 먹어요",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037708.1.3",
          "form": "저도요!! 숙주많이주세여~ ㅋㅋㅋ해요 ㅋㅋ",
          "original_form": "저도요!! 숙주많이주세여~ ㅋㅋㅋ해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037708.1.4",
          "form": "사이공본가 아세요?",
          "original_form": "사이공본가 아세요?",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037708.1.5",
          "form": "혹시 한 3900원? 쌀국수  파는데  말하시는건가요?",
          "original_form": "혹시 한 3900원? 쌀국수  파는데  말하시는건가요?",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037708.1.6",
          "form": "이제 많이올랐어요 ㅎㅎㅎ 5500원이든가..",
          "original_form": "이제 많이올랐어요 ㅎㅎㅎ 5500원이든가..",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037708.1.7",
          "form": "그래도 저렴하쥬",
          "original_form": "그래도 저렴하쥬",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037708.1.8",
          "form": "저눈 여기쌀국수가 느끼하지않고 담백해서 맛있도라규요",
          "original_form": "저눈 여기쌀국수가 느끼하지않고 담백해서 맛있도라규요",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100037708.1.9",
          "form": "네, 그리고 빨리 나와서 급할때 부담없이 후루룩 먹기 좋아요",
          "original_form": "네, 그리고 빨리 나와서 급할때 부담없이 후루룩 먹기 좋아요",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037708.1.10",
          "form": "여기 아시는군요!",
          "original_form": "여기 아시는군요!",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037708.1.11",
          "form": "며칠전에 얼큰쌀국수 처음먹었는데 와 진짜 얼크..은! 해요",
          "original_form": "며칠전에 얼큰쌀국수 처음먹었는데 와 진짜 얼크..은! 해요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037708.1.12",
          "form": "오, 그건 안먹어봤는데",
          "original_form": "오, 그건 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037708.1.13",
          "form": "다음에 기회되면 먹어봐야겠어요",
          "original_form": "다음에 기회되면 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037708.1.14",
          "form": "저도 늘 쌀국수먹다가 배달시키려니 기본배달비가 쎄서 ㅡㅡ;",
          "original_form": "저도 늘 쌀국수먹다가 배달시키려니 기본배달비가 쎄서 ㅡㅡ;",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037708.1.15",
          "form": "하나더 시켰는데 거의 짬뽕이에요 ㅎㅎ",
          "original_form": "하나더 시켰는데 거의 짬뽕이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037708.1.16",
          "form": "면만 쌀면인..",
          "original_form": "면만 쌀면인..",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037708.1.17",
          "form": "허... 그럼 그냥 짬뽕이랑 느낌이 다른가요?",
          "original_form": "허... 그럼 그냥 짬뽕이랑 느낌이 다른가요?",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037708.1.18",
          "form": "아니요 완전 짬뽕인데 해물은 없구 밀가루 면아닌 쌀면이요!!",
          "original_form": "아니요 완전 짬뽕인데 해물은 없구 밀가루 면아닌 쌀면이요!!",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037708.1.19",
          "form": "짬뽕보단 더 많이 들어가긴하더라구요!!",
          "original_form": "짬뽕보단 더 많이 들어가긴하더라구요!!",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037708.1.20",
          "form": "호, 궁금해지네요,",
          "original_form": "호, 궁금해지네요,",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037708.1.21",
          "form": "담에 백신 맞고나면 가봐야지~",
          "original_form": "담에 백신 맞고나면 가봐야지~",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037708.1.22",
          "form": "ㅋㅋㅋㅋ백신맞구 몸보신 많이 한다더라구요!! 언제세요?",
          "original_form": "ㅋㅋㅋㅋ백신맞구 몸보신 많이 한다더라구요!! 언제세요?",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037708.1.23",
          "form": "맞기전에 고기드시구 가세용!!",
          "original_form": "맞기전에 고기드시구 가세용!!",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100037708.1.24",
          "form": "2차가 10월 1일이에요",
          "original_form": "2차가 10월 1일이에요",
          "speaker_id": "2",
          "time": "20210901 18:12"
        },
        {
          "id": "MDRW2100037708.1.25",
          "form": "저 벌써부터 뭐먹고 가지? 고민하다구요ㅋㅋㅋ",
          "original_form": "저 벌써부터 뭐먹고 가지? 고민하다구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:12"
        },
        {
          "id": "MDRW2100037708.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 추석부터 빌드업 쌓아야죠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 추석부터 빌드업 쌓아야죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037708.1.27",
          "form": "명절음식부터 시작한다...!",
          "original_form": "명절음식부터 시작한다...!",
          "speaker_id": "1",
          "time": "20210901 18:24"
