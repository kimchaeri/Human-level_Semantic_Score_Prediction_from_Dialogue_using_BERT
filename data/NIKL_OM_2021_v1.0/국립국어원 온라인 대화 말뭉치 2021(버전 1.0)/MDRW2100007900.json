      "utterance": [
        {
          "id": "MDRW2100007900.1.1",
          "form": "현대 문학 작품 중에 재밌게 본 책 있으세요?",
          "original_form": "현대 문학 작품 중에 재밌게 본 책 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007900.1.2",
          "form": "김초엽 작가님의 우리가 빛의 속도로 갈 수 없다면 재미있게 봤어요",
          "original_form": "김초엽 작가님의 우리가 빛의 속도로 갈 수 없다면 재미있게 봤어요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007900.1.3",
          "form": "단편집인데 스토리가 다 재밌어요",
          "original_form": "단편집인데 스토리가 다 재밌어요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007900.1.4",
          "form": "오 재밌을 거 같아요",
          "original_form": "오 재밌을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007900.1.5",
          "form": "최근에는 어떤 책 읽으셨나요?",
          "original_form": "최근에는 어떤 책 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007900.1.6",
          "form": "최근에는 시작의 기술이라고 약간 자기계발서같은 책인데",
          "original_form": "최근에는 시작의 기술이라고 약간 자기계발서같은 책인데",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007900.1.7",
          "form": "내용이 새로워서 좋았어요",
          "original_form": "내용이 새로워서 좋았어요",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007900.1.8",
          "form": "유명한 책이더라고요",
          "original_form": "유명한 책이더라고요",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007900.1.9",
          "form": "책 정말 좋아하시나봐요",
          "original_form": "책 정말 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007900.1.10",
          "form": "저도 이번 방학 때 조금 읽었는데",
          "original_form": "저도 이번 방학 때 조금 읽었는데",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007900.1.11",
          "form": "name2 님은 한달에 책 몇 권 읽는 편이세요?",
          "original_form": "&name2& 님은 한달에 책 몇 권 읽는 편이세요?",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007900.1.12",
          "form": "저도 많이 읽는 편은 아니라서 많이해도 한 3권?정도에요",
          "original_form": "저도 많이 읽는 편은 아니라서 많이해도 한 3권?정도에요",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007900.1.13",
          "form": "학기중에는 더 힘들것같기도 하구요 ㅎ",
          "original_form": "학기중에는 더 힘들것같기도 하구요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007900.1.14",
          "form": "저도 방학 때 몰아서 읽는 편이에요",
          "original_form": "저도 방학 때 몰아서 읽는 편이에요",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007900.1.15",
          "form": "소설도 좋아하시나요?",
          "original_form": "소설도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007900.1.16",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007900.1.17",
          "form": "어떤 장르 즐겨보시나요?",
          "original_form": "어떤 장르 즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007900.1.18",
          "form": "저는 장르상관없이 재밌으면 봐요 ㅎ",
          "original_form": "저는 장르상관없이 재밌으면 봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007900.1.19",
          "form": "보통은 인터넷이나 유튜브에서 추천하는 거 보고 있어요",
          "original_form": "보통은 인터넷이나 유튜브에서 추천하는 거 보고 있어요",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007900.1.20",
          "form": "책 추천해주는 채널이 있나요?",
          "original_form": "책 추천해주는 채널이 있나요?",
          "speaker_id": "1",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007900.1.21",
          "form": "채널이라기보단 영상들이 뜨더라구요",
          "original_form": "채널이라기보단 영상들이 뜨더라구요",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007900.1.22",
          "form": "북튜버들도 보긴 해요",
          "original_form": "북튜버들도 보긴 해요",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007900.1.23",
          "form": "북튜버 추천해주실 수 있나요?",
          "original_form": "북튜버 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007900.1.24",
          "form": "겨울서점이나 다이애나의 책장 추천해요!",
          "original_form": "겨울서점이나 다이애나의 책장 추천해요!",
          "speaker_id": "2",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007900.1.25",
          "form": "오 감사합니다",
          "original_form": "오 감사합니다",
