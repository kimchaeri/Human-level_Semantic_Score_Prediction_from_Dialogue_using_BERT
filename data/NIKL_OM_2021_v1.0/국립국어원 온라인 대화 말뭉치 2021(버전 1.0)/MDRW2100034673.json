      "utterance": [
        {
          "id": "MDRW2100034673.1.1",
          "form": "원에선 우예 시간 딱딱 맞춰서 자나 몰러~",
          "original_form": "원에선 우예 시간 딱딱 맞춰서 자나 몰러~",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.2",
          "form": "오뚜기 피자 ㅋㅋㅋㅋㅋ",
          "original_form": "오뚜기 피자 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.3",
          "form": "ㅋㅋㅋ 그러게 원에서는",
          "original_form": "ㅋㅋㅋ 그러게 원에서는",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.4",
          "form": "잘잔대",
          "original_form": "잘잔대",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.5",
          "form": "우리둘째도",
          "original_form": "우리둘째도",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.6",
          "form": "밥도 잘먹고 ㅋㅋㅋㅋ",
          "original_form": "밥도 잘먹고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.7",
          "form": "ㅋㅋ집에서는 안자ㅡㅡㅋㅋ",
          "original_form": "ㅋㅋ집에서는 안자ㅡㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.8",
          "form": "그러이ㅋ",
          "original_form": "그러이ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.9",
          "form": "난 거짓말하나 싶다 ㅋㅋㅋㅋㅋ",
          "original_form": "난 거짓말하나 싶다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.10",
          "form": "참 아이러니하다",
          "original_form": "참 아이러니하다",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034673.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ둘다 못믿음",
          "original_form": "ㅋㅋㅋㅋㅋㅋ둘다 못믿음",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.12",
          "form": "집에선 밥도 앉아서 먹지를 않는데 ㅋㅋㅋㅋ",
          "original_form": "집에선 밥도 앉아서 먹지를 않는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.13",
          "form": "너무다르니ㅋㅋ",
          "original_form": "너무다르니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.14",
          "form": "그라이 울집도",
          "original_form": "그라이 울집도",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.15",
          "form": "진짜..언제까지 이래해야되는고ㅋ",
          "original_form": "진짜..언제까지 이래해야되는고ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ끝이.있겠제??ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ끝이.있겠제??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.17",
          "form": "그렇겠징...ㅋㄱㅋㅋ",
          "original_form": "그렇겠징...ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.18",
          "form": "좀더크면 낫지않겠나ㅋ",
          "original_form": "좀더크면 낫지않겠나ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.19",
          "form": "난 아까 넘 배고파서 혼자.밥비벼먹고~",
          "original_form": "난 아까 넘 배고파서 혼자.밥비벼먹고~",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034673.1.20",
          "form": "지금도 안자고",
          "original_form": "지금도 안자고",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034673.1.21",
          "form": "ㅋㅋㅋㅋㅋ이제.낮잠 영 안자는거 아닌가 싶다;;",
          "original_form": "ㅋㅋㅋㅋㅋ이제.낮잠 영 안자는거 아닌가 싶다;;",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034673.1.22",
          "form": "먹는색종이  먹고있다",
          "original_form": "먹는색종이  먹고있다",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034673.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ역시 잘 먹는다 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ역시 잘 먹는다 ㅋㅋㅋㅋ",
