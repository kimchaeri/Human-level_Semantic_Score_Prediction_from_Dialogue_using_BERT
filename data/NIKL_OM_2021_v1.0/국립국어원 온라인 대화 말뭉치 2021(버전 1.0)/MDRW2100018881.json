      "utterance": [
        {
          "id": "MDRW2100018881.1.1",
          "form": "안녕하세요~ 식사하셨어요?",
          "original_form": "안녕하세요~ 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018881.1.2",
          "form": "안녕하세요~ 전 아직이요!",
          "original_form": "안녕하세요~ 전 아직이요!",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018881.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018881.1.4",
          "form": "네 좀 전에 먹었어요!",
          "original_form": "네 좀 전에 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018881.1.5",
          "form": "뭐 드실 예정이세요?",
          "original_form": "뭐 드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018881.1.6",
          "form": "저는 복숭아 먹으려고요 ㅎㅎ",
          "original_form": "저는 복숭아 먹으려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018881.1.7",
          "form": "복숭아만 드시게요? 점심은 원래 간단하게 드시나요?",
          "original_form": "복숭아만 드시게요? 점심은 원래 간단하게 드시나요?",
          "speaker_id": "1",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018881.1.8",
          "form": "아뇨 원래 밥먹는데",
          "original_form": "아뇨 원래 밥먹는데",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018881.1.9",
          "form": "오늘 재택이라... 재택때는 귀찮아서 잘 안챙겨먹어요",
          "original_form": "오늘 재택이라... 재택때는 귀찮아서 잘 안챙겨먹어요",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018881.1.10",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018881.1.11",
          "form": "저는 닭볶음탕이랑 밥이랑 같이 먹었어요!",
          "original_form": "저는 닭볶음탕이랑 밥이랑 같이 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018881.1.12",
          "form": "닭요리 좋아하시나요?",
          "original_form": "닭요리 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018881.1.13",
          "form": "아 완전 좋아해요 고기짱",
          "original_form": "아 완전 좋아해요 고기짱",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018881.1.14",
          "form": "치킨도 좋아하시나요?",
          "original_form": "치킨도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018881.1.15",
          "form": "당그니죠",
          "original_form": "당그니죠",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018881.1.16",
          "form": "치킨 드시면 어디서 시켜드세요?",
          "original_form": "치킨 드시면 어디서 시켜드세요?",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018881.1.17",
          "form": "저는 푸라닭 좋아해요!",
          "original_form": "저는 푸라닭 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018881.1.18",
          "form": "블랙알리오 진짜 맛있게 먹었어요!!",
          "original_form": "블랙알리오 진짜 맛있게 먹었어요!!",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018881.1.19",
          "form": "푸라닭... 비싼 만큼 맛있더라고요",
          "original_form": "푸라닭... 비싼 만큼 맛있더라고요",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018881.1.20",
          "form": "맞아요 ... 비싸구.. 맛있어요",
          "original_form": "맞아요 ... 비싸구.. 맛있어요",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018881.1.21",
          "form": "저는 청양마요 젤 좋아해요",
          "original_form": "저는 청양마요 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018881.1.22",
          "form": "그것도 맛있죠!",
          "original_form": "그것도 맛있죠!",
          "speaker_id": "1",
          "time": "20210827 14:12"
        }
      ]
