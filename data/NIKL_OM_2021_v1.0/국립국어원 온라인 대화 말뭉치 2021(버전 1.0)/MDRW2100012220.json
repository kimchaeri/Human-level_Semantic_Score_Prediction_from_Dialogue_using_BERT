      "utterance": [
        {
          "id": "MDRW2100012220.1.1",
          "form": "도서 어디서 주로 빌리시나요",
          "original_form": "도서 어디서 주로 빌리시나요",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012220.1.2",
          "form": "집근처 도서관에서 주로 빌려요",
          "original_form": "집근처 도서관에서 주로 빌려요",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012220.1.3",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012220.1.4",
          "form": "혹시 책 중에 좋아하는 작가 있으시나요",
          "original_form": "혹시 책 중에 좋아하는 작가 있으시나요",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012220.1.5",
          "form": "딱히 없고 고로고로 읽어요",
          "original_form": "딱히 없고 고로고로 읽어요",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012220.1.6",
          "form": "저는 이영도 작가 책 좋아해요",
          "original_form": "저는 이영도 작가 책 좋아해요",
          "speaker_id": "2",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012220.1.7",
          "form": "추천해주실만한 책 있으신가요?",
          "original_form": "추천해주실만한 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012220.1.8",
          "form": "1984라는 책도 괜찮아요",
          "original_form": "1984라는 책도 괜찮아요",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012220.1.9",
          "form": "그책은 읽은기억이 있는데",
          "original_form": "그책은 읽은기억이 있는데",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012220.1.10",
          "form": "읽으셨군요. 그러면 무협지 읽어보세요 기분전환으로",
          "original_form": "읽으셨군요. 그러면 무협지 읽어보세요 기분전환으로",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012220.1.11",
          "form": "추천감사해요",
          "original_form": "추천감사해요",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012220.1.12",
          "form": "아니요 읽어보면 좀 색다른 것을 느낄듯 작가중 설봉 추천드려요",
          "original_form": "아니요 읽어보면 좀 색다른 것을 느낄듯 작가중 설봉 추천드려요",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012220.1.13",
          "form": "설봉 추천 감사해요",
          "original_form": "설봉 추천 감사해요",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012220.1.14",
          "form": "읽어보시고 마음에 드시면 다른 책 읽어도 될듯해요",
          "original_form": "읽어보시고 마음에 드시면 다른 책 읽어도 될듯해요",
          "speaker_id": "2",
          "time": "20210824 20:41"
        }
      ]
