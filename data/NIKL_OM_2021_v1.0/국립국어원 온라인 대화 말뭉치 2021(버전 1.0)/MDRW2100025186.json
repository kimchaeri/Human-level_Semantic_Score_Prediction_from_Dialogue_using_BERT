      "utterance": [
        {
          "id": "MDRW2100025186.1.1",
          "form": "최근에 산게있니?",
          "original_form": "최근에 산게있니?",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025186.1.2",
          "form": "최근에 편지지 샀어!",
          "original_form": "최근에 편지지 샀어!",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025186.1.3",
          "form": "나한테써주려고?",
          "original_form": "나한테써주려고?",
          "speaker_id": "1",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025186.1.4",
          "form": "헛소리한다",
          "original_form": "헛소리한다",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025186.1.5",
          "form": "친구가 군대가있어서 편지써주려고",
          "original_form": "친구가 군대가있어서 편지써주려고",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025186.1.6",
          "form": "우정이 깊구나",
          "original_form": "우정이 깊구나",
          "speaker_id": "1",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025186.1.7",
          "form": "사고싶은건있니?",
          "original_form": "사고싶은건있니?",
          "speaker_id": "1",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100025186.1.8",
          "form": "게임기 조이스틱이 고장나서 새로 바꾸고 싶어",
          "original_form": "게임기 조이스틱이 고장나서 새로 바꾸고 싶어",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100025186.1.9",
          "form": "캐릭터가 자꾸 혼자 달려나가더라",
          "original_form": "캐릭터가 자꾸 혼자 달려나가더라",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100025186.1.10",
          "form": "너는? 최근에 산 물건 있니?",
          "original_form": "너는? 최근에 산 물건 있니?",
          "speaker_id": "2",
          "time": "20210901 00:21"
        },
        {
          "id": "MDRW2100025186.1.11",
          "form": "나는 중고거래로",
          "original_form": "나는 중고거래로",
          "speaker_id": "1",
          "time": "20210901 00:22"
        },
        {
          "id": "MDRW2100025186.1.12",
          "form": "갖고싶었던 옷을 샀어",
          "original_form": "갖고싶었던 옷을 샀어",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100025186.1.13",
          "form": "작년제품이라 구하기가 어렵더라",
          "original_form": "작년제품이라 구하기가 어렵더라",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100025186.1.14",
          "form": "당근에서 샀니?",
          "original_form": "당근에서 샀니?",
          "speaker_id": "2",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100025186.1.15",
          "form": "아니 나는 번개장터 자주 이용해",
          "original_form": "아니 나는 번개장터 자주 이용해",
          "speaker_id": "1",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100025186.1.16",
          "form": "넌 당근 좋아하니?",
          "original_form": "넌 당근 좋아하니?",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100025186.1.17",
          "form": "나는 중고거래 사기당할까봐 못하겠던데",
          "original_form": "나는 중고거래 사기당할까봐 못하겠던데",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100025186.1.18",
          "form": "그나마 직거래가 안심돼서",
          "original_form": "그나마 직거래가 안심돼서",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100025186.1.19",
          "form": "난 그래도 아직 중고거래에서 사기는 안당해봤어",
          "original_form": "난 그래도 아직 중고거래에서 사기는 안당해봤어",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100025186.1.20",
          "form": "너도 한번이용해봐",
          "original_form": "너도 한번이용해봐",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100025186.1.21",
          "form": "그래 한번 꼭 도전해볼게",
          "original_form": "그래 한번 꼭 도전해볼게",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100025186.1.22",
          "form": "난 또 옷을 사고싶어",
          "original_form": "난 또 옷을 사고싶어",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100025186.1.23",
          "form": "옷사면 한번만나자",
          "original_form": "옷사면 한번만나자",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100025186.1.24",
          "form": "그래 좋아",
          "original_form": "그래 좋아",
          "speaker_id": "2",
          "time": "20210901 00:27"
        }
      ]
    }
