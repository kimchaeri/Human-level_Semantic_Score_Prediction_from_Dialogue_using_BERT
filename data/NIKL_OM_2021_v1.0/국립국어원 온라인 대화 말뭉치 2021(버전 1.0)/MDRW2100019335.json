      "utterance": [
        {
          "id": "MDRW2100019335.1.1",
          "form": "몬지모르는데",
          "original_form": "몬지모르는데",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019335.1.2",
          "form": "보긴햇는데어찌하는겨",
          "original_form": "보긴햇는데어찌하는겨",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019335.1.3",
          "form": "지마켓은 있나요?",
          "original_form": "지마켓은 있나요?",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019335.1.4",
          "form": "ㅋㅋㅋ나도 아직 안해봤는데 뭔가 명령어 그거 하면 즉당 쿠폰을 주나봐",
          "original_form": "ㅋㅋㅋ나도 아직 안해봤는데 뭔가 명령어 그거 하면 즉당 쿠폰을 주나봐",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019335.1.5",
          "form": "지마켓 스마일클럽 회원임당",
          "original_form": "지마켓 스마일클럽 회원임당",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019335.1.6",
          "form": "오 ㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019335.1.7",
          "form": "ㅋㅋㅋㅋ스캐 한번도 안준다 나쁜 지마켓",
          "original_form": "ㅋㅋㅋㅋ스캐 한번도 안준다 나쁜 지마켓",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019335.1.8",
          "form": "난 2000처넌받았는데",
          "original_form": "난 2000처넌받았는데",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019335.1.9",
          "form": "두루마리 휴지 사야하는데 어디서 사야 제일 쌀까요?",
          "original_form": "두루마리 휴지 사야하는데 어디서 사야 제일 쌀까요?",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019335.1.10",
          "form": "오대박",
          "original_form": "오대박",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019335.1.11",
          "form": "몰라유왜유?",
          "original_form": "몰라유왜유?",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019335.1.12",
          "form": "ㅋㅋ엄마가 다 떨어졌다고 사라해서유..",
          "original_form": "ㅋㅋ엄마가 다 떨어졌다고 사라해서유..",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019335.1.13",
          "form": "근데 오대박 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭐야 ㅋㅋㅋ",
          "original_form": "근데 오대박 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭐야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019335.1.14",
          "form": "난.집에휴지많은데",
          "original_form": "난.집에휴지많은데",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019335.1.15",
          "form": "전에 신세계포인트 털때 30롤3개묶음짜리인가 사서..",
          "original_form": "전에 신세계포인트 털때 30롤3개묶음짜리인가 사서..",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019335.1.16",
          "form": "ㅋㅋ맘큐 출석하는거 예전에 열심히 모았는데 요즘은 출첵 맨날 구멍나서 적립금도 3천원있더라",
          "original_form": "ㅋㅋ맘큐 출석하는거 예전에 열심히 모았는데 요즘은 출첵 맨날 구멍나서 적립금도 3천원있더라",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019335.1.17",
          "form": "맘큐는 안하는디 ㅋㅋㅋ",
          "original_form": "맘큐는 안하는디 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019335.1.18",
          "form": "ㅋㅋㅋ 별로 메리트 없어진듯해 가격도 비싸고",
          "original_form": "ㅋㅋㅋ 별로 메리트 없어진듯해 가격도 비싸고",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019335.1.19",
          "form": "그렇구만 ㅜㅠㅋㅋㅋ",
          "original_form": "그렇구만 ㅜㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019335.1.20",
          "form": "휴지도 너무싸면",
          "original_form": "휴지도 너무싸면",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019335.1.21",
          "form": "별로야 ㅋㅋㅋ엄마가 전에 넘 얇앗다면서",
          "original_form": "별로야 ㅋㅋㅋ엄마가 전에 넘 얇앗다면서",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019335.1.22",
          "form": "아그래? ㅋㅋㅋㅋㅋ이제 롤도좀보자나 ㅋㅋㅋ",
          "original_form": "아그래? ㅋㅋㅋㅋㅋ이제 롤도좀보자나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019335.1.23",
          "form": "맞아 도톰한거 좋아해 ㅋㅋㅋ",
          "original_form": "맞아 도톰한거 좋아해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019335.1.24",
          "form": "3겹 이런거!",
          "original_form": "3겹 이런거!",
          "speaker_id": "1",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019335.1.25",
          "form": "마자유 ㅋㅋㅋ",
          "original_form": "마자유 ㅋㅋㅋ",
          "speaker_id": "2",
