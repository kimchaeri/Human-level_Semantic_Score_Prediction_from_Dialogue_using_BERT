      "utterance": [
        {
          "id": "MDRW2100002733.1.1",
          "form": "훌 어떻게?",
          "original_form": "훌 어떻게?",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.2",
          "form": "열심히 룰렛 돌려서 샀지 면티셔츠 2장 ㅎㅎ",
          "original_form": "열심히 룰렛 돌려서 샀지 면티셔츠 2장 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.3",
          "form": "품질도 좋고 부드럽다고 하니 기대해보겠엉 ㅎㅎ",
          "original_form": "품질도 좋고 부드럽다고 하니 기대해보겠엉 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.4",
          "form": "무슨 색인데?",
          "original_form": "무슨 색인데?",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.5",
          "form": "흰색이랑 회색",
          "original_form": "흰색이랑 회색",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.6",
          "form": "아들꺼 산거야?",
          "original_form": "아들꺼 산거야?",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.7",
          "form": "ㅇㅇ 작은 아들",
          "original_form": "ㅇㅇ 작은 아들",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002733.1.8",
          "form": "얇은 티셔츠라 작은 아들이 잘 입을 듯",
          "original_form": "얇은 티셔츠라 작은 아들이 잘 입을 듯",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002733.1.9",
          "form": "글치 한겨울에도 절대 두꺼운 건 안입는 애니",
          "original_form": "글치 한겨울에도 절대 두꺼운 건 안입는 애니",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002733.1.10",
          "form": "ㅇㅇ 그래서 샀엉 ㅎㅎ",
          "original_form": "ㅇㅇ 그래서 샀엉 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002733.1.11",
          "form": "아주 득템했네",
          "original_form": "아주 득템했네",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002733.1.12",
          "form": "오늘 유난히 좋은 상품이 많이 나오네",
          "original_form": "오늘 유난히 좋은 상품이 많이 나오네",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002733.1.13",
          "form": "색시 아주 흐뭇하겠어 ㅋㅋ",
          "original_form": "색시 아주 흐뭇하겠어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002733.1.14",
          "form": "너무 기분 좋음 ㅎㅎ",
          "original_form": "너무 기분 좋음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002733.1.15",
          "form": "티셔츠 두 벌이라 집에서 입긴 충분하겠네",
          "original_form": "티셔츠 두 벌이라 집에서 입긴 충분하겠네",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002733.1.16",
          "form": "글치 ㅎㅎ 집에 다른 것도 있으니",
          "original_form": "글치 ㅎㅎ 집에 다른 것도 있으니",
