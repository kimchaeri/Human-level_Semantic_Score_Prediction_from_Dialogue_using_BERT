      "utterance": [
        {
          "id": "MDRW2100018646.1.1",
          "form": "중고거래로 구매하신 물품은 있나요?",
          "original_form": "중고거래로 구매하신 물품은 있나요?",
          "speaker_id": "1",
          "time": "20210827 09:50"
        },
        {
          "id": "MDRW2100018646.1.2",
          "form": "저는 프리퀀시 사봤어요!",
          "original_form": "저는 프리퀀시 사봤어요!",
          "speaker_id": "2",
          "time": "20210827 09:51"
        },
        {
          "id": "MDRW2100018646.1.3",
          "form": "스타벅스 프리퀀시요?",
          "original_form": "스타벅스 프리퀀시요?",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018646.1.4",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018646.1.5",
          "form": "쏠쏠 하셨겠다",
          "original_form": "쏠쏠 하셨겠다",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018646.1.6",
          "form": "저는 전기밭솥 사봤어요",
          "original_form": "저는 전기밭솥 사봤어요",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018646.1.7",
          "form": "밥솥...",
          "original_form": "밥솥...",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018646.1.8",
          "form": "오 어떠셨어요?",
          "original_form": "오 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018646.1.9",
          "form": "자취를 하게 되어서 밥솥이 없었거든요",
          "original_form": "자취를 하게 되어서 밥솥이 없었거든요",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018646.1.10",
          "form": "만원에 사게되었고 가성비 만족해요",
          "original_form": "만원에 사게되었고 가성비 만족해요",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018646.1.11",
          "form": "기프트콘은 팔아보셨어요?",
          "original_form": "기프트콘은 팔아보셨어요?",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018646.1.12",
          "form": "저 기프티콘도 자주 사봤어요 그 기프티스타라고 아세요?",
          "original_form": "저 기프티콘도 자주 사봤어요 그 기프티스타라고 아세요?",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018646.1.13",
          "form": "와 만원이면 진짜 저렴하네요",
          "original_form": "와 만원이면 진짜 저렴하네요",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018646.1.14",
          "form": "헉 저도 그거써요",
          "original_form": "헉 저도 그거써요",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018646.1.15",
          "form": "가격 흥정안해도 되어서 편해요",
          "original_form": "가격 흥정안해도 되어서 편해요",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018646.1.16",
          "form": "맞아요 저렴하구",
          "original_form": "맞아요 저렴하구",
          "speaker_id": "2",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018646.1.17",
          "form": "전 근데 일단 사놓고 까먹어서 오히려 날린적두있어요 멍청하죠 -_-;",
          "original_form": "전 근데 일단 사놓고 까먹어서 오히려 날린적두있어요 멍청하죠 -_-;",
          "speaker_id": "2",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018646.1.18",
          "form": "기프트콘이 그러기가 너무 쉬운거 같아요ㅠㅠ",
          "original_form": "기프트콘이 그러기가 너무 쉬운거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018646.1.19",
          "form": "연장이 되는것도 있고 아닌것도있고...",
          "original_form": "연장이 되는것도 있고 아닌것도있고...",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018646.1.20",
          "form": "카카오깊콘은 그냥 제가 연장하면되는데",
          "original_form": "카카오깊콘은 그냥 제가 연장하면되는데",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018646.1.21",
          "form": "중고로 산거는 그게 안돼서 불편하더라구요",
          "original_form": "중고로 산거는 그게 안돼서 불편하더라구요",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018646.1.22",
          "form": "그죠ㅠㅠ",
          "original_form": "그죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018646.1.23",
          "form": "저는 주로 파는편인데",
          "original_form": "저는 주로 파는편인데",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018646.1.24",
          "form": "받았지만 안쓰는 카페 기프트콘 팔다보니 어느새 금액이 쏠쏠해요",
          "original_form": "받았지만 안쓰는 카페 기프트콘 팔다보니 어느새 금액이 쏠쏠해요",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018646.1.25",
          "form": "저 한번 팔아보랴다가 실패했어요 주말껴서 신청하는 바람에 너무 촉박해서 ㅋㅋㅋ",
          "original_form": "저 한번 팔아보랴다가 실패했어요 주말껴서 신청하는 바람에 너무 촉박해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:02"
        }
      ]
