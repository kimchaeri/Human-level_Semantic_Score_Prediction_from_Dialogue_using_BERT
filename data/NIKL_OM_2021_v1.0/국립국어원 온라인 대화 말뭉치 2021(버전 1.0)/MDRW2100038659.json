        {
          "id": "MDRW2100038659.1.1",
          "form": "name2님은 사계절 중에 언제가 제일 좋으세요",
          "original_form": "&name2&님은 사계절 중에 언제가 제일 좋으세요",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038659.1.2",
          "form": "가을용",
          "original_form": "가을용",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038659.1.3",
          "form": "쾌적!",
          "original_form": "쾌적!",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038659.1.4",
          "form": "파란하늘 ㅋㅋ",
          "original_form": "파란하늘 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038659.1.5",
          "form": "아 가을하늘은 진짜 청량해서 너무 좋아요",
          "original_form": "아 가을하늘은 진짜 청량해서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100038659.1.6",
          "form": "코스모스도 피고 ㅋㅋ",
          "original_form": "코스모스도 피고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100038659.1.7",
          "form": "옹 이쁘죵",
          "original_form": "옹 이쁘죵",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100038659.1.8",
          "form": "name1님은용",
          "original_form": "&name1&님은용",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100038659.1.9",
          "form": "저는 여름파예요 ㅋㅋ",
          "original_form": "저는 여름파예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038659.1.10",
          "form": "아하 왜용 ?",
          "original_form": "아하 왜용 ?",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038659.1.11",
          "form": "여름에 느낄 수 있는 분위기가 좋아요 ㅋㅋㅋ 약간 가슴이 뛰는? ㅋㅋㅋ",
          "original_form": "여름에 느낄 수 있는 분위기가 좋아요 ㅋㅋㅋ 약간 가슴이 뛰는? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038659.1.12",
          "form": "아항... 잇죠잇죠 ㅋㅋㅋ",
          "original_form": "아항... 잇죠잇죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038659.1.13",
          "form": "더윈 잘 안타세요?",
          "original_form": "더윈 잘 안타세요?",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038659.1.14",
          "form": "추위보다는 덜 타는데 올해는 진짜 덥더라고요 ㅋㅋㅋ",
          "original_form": "추위보다는 덜 타는데 올해는 진짜 덥더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038659.1.15",
          "form": "저도..올해유난히 더웟어요 ㅠ",
          "original_form": "저도..올해유난히 더웟어요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038659.1.16",
          "form": "그쵸 ㅋㅋㅋ 근데 최근엔 또 갑자기 선선해져서 신기해요 ㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ 근데 최근엔 또 갑자기 선선해져서 신기해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100038659.1.17",
          "form": "마자여ㅋㅋㅋ 살만합니다",
          "original_form": "마자여ㅋㅋㅋ 살만합니다",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038659.1.18",
          "form": "에어컨 안 돌려도 괜찮더라고요 ㅎㅎ",
          "original_form": "에어컨 안 돌려도 괜찮더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038659.1.19",
          "form": "사시는 곳에 비 많이 왔나요?",
          "original_form": "사시는 곳에 비 많이 왔나요?",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038659.1.20",
          "form": "많이는아니더라도 계속요",
          "original_form": "많이는아니더라도 계속요",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100038659.1.21",
          "form": "습해요 ㅠ",
          "original_form": "습해요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100038659.1.22",
          "form": "아 그쵸 습도가 문제예요 참..",
          "original_form": "아 그쵸 습도가 문제예요 참..",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100038659.1.23",
