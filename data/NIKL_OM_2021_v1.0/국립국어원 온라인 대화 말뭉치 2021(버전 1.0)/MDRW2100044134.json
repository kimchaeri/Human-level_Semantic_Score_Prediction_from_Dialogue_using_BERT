      "utterance": [
        {
          "id": "MDRW2100044134.1.1",
          "form": "드라마 보시나요??",
          "original_form": "드라마 보시나요??",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044134.1.2",
          "form": "아뇨 저 예능만 봐요",
          "original_form": "아뇨 저 예능만 봐요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044134.1.3",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044134.1.4",
          "form": "저도 예능만 보다 경찰수업이랑 하이클래스.봐요",
          "original_form": "저도 예능만 보다 경찰수업이랑 하이클래스.봐요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044134.1.5",
          "form": "하이클래스 재밌나요?",
          "original_form": "하이클래스 재밌나요?",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044134.1.6",
          "form": "스카이캐슬 보셨나요?",
          "original_form": "스카이캐슬 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044134.1.7",
          "form": "저 얘기만 들었어요",
          "original_form": "저 얘기만 들었어요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044134.1.8",
          "form": "비슷한가요?",
          "original_form": "비슷한가요?",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044134.1.9",
          "form": "그 윗단계예요. 공부 할필요가 뭐 있어~ 이런 수준",
          "original_form": "그 윗단계예요. 공부 할필요가 뭐 있어~ 이런 수준",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044134.1.10",
          "form": "와 ㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.11",
          "form": "예고편 보고 비슷할 줄 알았는데",
          "original_form": "예고편 보고 비슷할 줄 알았는데",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.12",
          "form": "어떤 결에선 비슷하긴 하네요...",
          "original_form": "어떤 결에선 비슷하긴 하네요...",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.13",
          "form": "그럼 덜 자극적인가요¿",
          "original_form": "그럼 덜 자극적인가요¿",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.14",
          "form": "힘들게 서울의대 들어가는건 중산층이나 하는거래요 ㅋㅋ",
          "original_form": "힘들게 서울의대 들어가는건 중산층이나 하는거래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.15",
          "form": "자극적이진 않아요",
          "original_form": "자극적이진 않아요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.16",
          "form": "대놓고 저격하네요 ㅋㅋㅋㅋㅋ",
          "original_form": "대놓고 저격하네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044134.1.17",
          "form": "그럼 주 내용이 어떤건가요?",
          "original_form": "그럼 주 내용이 어떤건가요?",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044134.1.18",
          "form": "일반학교 부적응 아이가 제주 국제학교 입학해서 지금 좀 괴롭힘을 당하고 있어요",
          "original_form": "일반학교 부적응 아이가 제주 국제학교 입학해서 지금 좀 괴롭힘을 당하고 있어요",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044134.1.19",
          "form": "이 아이 엄마는 변호사인데 제가 앞부분을 못봐서..",
          "original_form": "이 아이 엄마는 변호사인데 제가 앞부분을 못봐서..",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044134.1.20",
          "form": "아 상류 학교 따돌림?인가요?",
          "original_form": "아 상류 학교 따돌림?인가요?",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044134.1.21",
          "form": "그런가봐요 . 그들만의 리그여야 하는데 이상한게 굴러 들어왔다고 표현하던데요",
          "original_form": "그런가봐요 . 그들만의 리그여야 하는데 이상한게 굴러 들어왔다고 표현하던데요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        }
      ]
