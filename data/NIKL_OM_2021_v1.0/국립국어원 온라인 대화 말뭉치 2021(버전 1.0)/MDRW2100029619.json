      "utterance": [
        {
          "id": "MDRW2100029619.1.1",
          "form": "개 키우고 싶어요",
          "original_form": "개 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029619.1.2",
          "form": "알러지 있어서 안돼",
          "original_form": "알러지 있어서 안돼",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029619.1.3",
          "form": "근데 말티즈가 너무 귀여워요 ㅠㅠ",
          "original_form": "근데 말티즈가 너무 귀여워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029619.1.4",
          "form": "저번에 친구 집 가서 개 귀엽다며",
          "original_form": "저번에 친구 집 가서 개 귀엽다며",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029619.1.5",
          "form": "사진 보여줬지",
          "original_form": "사진 보여줬지",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029619.1.6",
          "form": "name3예요 ㅋㅋㅋ",
          "original_form": "&name3&예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029619.1.7",
          "form": "이름처럼 하얀 개고 애교도 많은",
          "original_form": "이름처럼 하얀 개고 애교도 많은",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029619.1.8",
          "form": "사람 애 키우는 것도 힘든데",
          "original_form": "사람 애 키우는 것도 힘든데",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029619.1.9",
          "form": "어떻게 말 안 통하는 애를 키워",
          "original_form": "어떻게 말 안 통하는 애를 키워",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029619.1.10",
          "form": "ㅠㅠ 애 키우시고 싶다면서요",
          "original_form": "ㅠㅠ 애 키우시고 싶다면서요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029619.1.11",
          "form": "개 귀엽긴 한데",
          "original_form": "개 귀엽긴 한데",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029619.1.12",
          "form": "털 많이 날리고",
          "original_form": "털 많이 날리고",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029619.1.13",
          "form": "돈도 많이 들어",
          "original_form": "돈도 많이 들어",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029619.1.14",
          "form": "사료값이랑 병원비 ㅠㅠ",
          "original_form": "사료값이랑 병원비 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029619.1.15",
          "form": "저 아는 언니도 동물 키우는데",
          "original_form": "저 아는 언니도 동물 키우는데",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.16",
          "form": "패럿이거든요",
          "original_form": "패럿이거든요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.17",
          "form": "패럿이 뭐냐",
          "original_form": "패럿이 뭐냐",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.18",
          "form": "족제비과예요 ㅋㅋㅋ",
          "original_form": "족제비과예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.19",
          "form": "걔 키운다고",
          "original_form": "걔 키운다고",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.20",
          "form": "적금들고있더라고요",
          "original_form": "적금들고있더라고요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029619.1.21",
          "form": "사람 키우는 거랑 똑같네",
          "original_form": "사람 키우는 거랑 똑같네",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029619.1.22",
          "form": "2500정도 모았다고...",
          "original_form": "2500정도 모았다고...",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029619.1.23",
          "form": "근데 귀여워요",
          "original_form": "근데 귀여워요",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029619.1.24",
          "form": "암튼 코 분홍색이라 되게 귀여워요 ㅋㅋㅋ",
          "original_form": "암튼 코 분홍색이라 되게 귀여워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029619.1.25",
          "form": "이름도 제비예요",
          "original_form": "이름도 제비예요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029619.1.26",
          "form": "족제비의 제비 말고 수제비의 제비",
          "original_form": "족제비의 제비 말고 수제비의 제비",
