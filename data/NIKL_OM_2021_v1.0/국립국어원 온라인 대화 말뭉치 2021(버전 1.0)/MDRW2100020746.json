      "utterance": [
        {
          "id": "MDRW2100020746.1.1",
          "form": "요새 보는 드라마 있으세요?",
          "original_form": "요새 보는 드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100020746.1.2",
          "form": "저는 악마판사 클립으로 조금봤네요",
          "original_form": "저는 악마판사 클립으로 조금봤네요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100020746.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100020746.1.4",
          "form": "아 지성 나오는 건가요?",
          "original_form": "아 지성 나오는 건가요?",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100020746.1.5",
          "form": "전 최근에 넷플에 DP 봤어요",
          "original_form": "전 최근에 넷플에 DP 봤어요",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100020746.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100020746.1.7",
          "form": "dp재밌나요?",
          "original_form": "dp재밌나요?",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100020746.1.8",
          "form": "네 저는 재밌게 봤어요",
          "original_form": "네 저는 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100020746.1.9",
          "form": "군대내 가혹행위 보니 마음아프더라구요",
          "original_form": "군대내 가혹행위 보니 마음아프더라구요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100020746.1.10",
          "form": "구교환 배우 연기도 좋앗어요!",
          "original_form": "구교환 배우 연기도 좋앗어요!",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100020746.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100020746.1.12",
          "form": "다른 사람에게 추천할만 한가요?",
          "original_form": "다른 사람에게 추천할만 한가요?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100020746.1.13",
          "form": "네 전 추천할만 한거같아요",
          "original_form": "네 전 추천할만 한거같아요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100020746.1.14",
          "form": "근데 남자친구도 같이 봣는데",
          "original_form": "근데 남자친구도 같이 봣는데",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100020746.1.15",
          "form": "요샌 그렇게까지 가혹행위 없다고 하긴하더라구요",
          "original_form": "요샌 그렇게까지 가혹행위 없다고 하긴하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100020746.1.16",
          "form": "드라마적 허용이 많이 들어간거군요?",
          "original_form": "드라마적 허용이 많이 들어간거군요?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100020746.1.17",
          "form": "그런데 예전 윤일병사건도 그렇고 실제 그런 가혹행위하는 사람들이 있었으니..모티브로 한거같아요 ㅜ",
          "original_form": "그런데 예전 윤일병사건도 그렇고 실제 그런 가혹행위하는 사람들이 있었으니..모티브로 한거같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100020746.1.18",
          "form": "그렇게 남을 악랄하게 괴롭히는 사람들 보면...정말 너무한거같아요",
          "original_form": "그렇게 남을 악랄하게 괴롭히는 사람들 보면...정말 너무한거같아요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100020746.1.19",
          "form": "그쵸 드라마는 대부분 어떤 사건을 모티브로 하는게 많으니까요",
          "original_form": "그쵸 드라마는 대부분 어떤 사건을 모티브로 하는게 많으니까요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100020746.1.20",
          "form": "그러니까요 왜 남을 괴롭히는지 모르겠어요",
          "original_form": "그러니까요 왜 남을 괴롭히는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100020746.1.21",
          "form": "그러면서 희열을 느끼는건지...",
          "original_form": "그러면서 희열을 느끼는건지...",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100020746.1.22",
          "form": "그래서 그런 관련 영상이나 기사보면 제대로 끝까지 못보겠더라구요",
          "original_form": "그래서 그런 관련 영상이나 기사보면 제대로 끝까지 못보겠더라구요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100020746.1.23",
          "form": "맞아요 사실 드라마에서 너무 자극적으로 보이게 영상을 만드는 것도 조금 문제인 것 같아요",
          "original_form": "맞아요 사실 드라마에서 너무 자극적으로 보이게 영상을 만드는 것도 조금 문제인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100020746.1.24",
          "form": "맞아요..그게 모방범죄로 악용될수도 있으니까요",
          "original_form": "맞아요..그게 모방범죄로 악용될수도 있으니까요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100020746.1.25",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210907 22:28"
        }
      ]
