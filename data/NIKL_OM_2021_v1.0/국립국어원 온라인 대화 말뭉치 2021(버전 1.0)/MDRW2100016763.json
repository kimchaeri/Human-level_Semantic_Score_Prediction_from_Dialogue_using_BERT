      "utterance": [
        {
          "id": "MDRW2100016763.1.1",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016763.1.2",
          "form": "먹는낙으로살아요",
          "original_form": "먹는낙으로살아요",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016763.1.3",
          "form": "어떤거 잘드시나요",
          "original_form": "어떤거 잘드시나요",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016763.1.4",
          "form": "없어서못먹지 다 잘먹어요",
          "original_form": "없어서못먹지 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016763.1.5",
          "form": "저는 돈까스 좋아해요",
          "original_form": "저는 돈까스 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016763.1.6",
          "form": "저는 굳이 꼽으면 면!!",
          "original_form": "저는 굳이 꼽으면 면!!",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016763.1.7",
          "form": "오 마제소바 드셔보셨나요",
          "original_form": "오 마제소바 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016763.1.8",
          "form": "첨들어봣어요\n그냥 메밀소바는 엄청 좋아해요",
          "original_form": "첨들어봣어요\n그냥 메밀소바는 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016763.1.9",
          "form": "메밀소바도 맛있죻ㅎ",
          "original_form": "메밀소바도 맛있죻ㅎ",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016763.1.10",
          "form": "저녁 메뉴는 뭔가요?",
          "original_form": "저녁 메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016763.1.11",
          "form": "김치찌개입니다ㅎㅎ",
          "original_form": "김치찌개입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016763.1.12",
          "form": "김치찌개 계란후라이 국룰 아닌가요",
          "original_form": "김치찌개 계란후라이 국룰 아닌가요",
