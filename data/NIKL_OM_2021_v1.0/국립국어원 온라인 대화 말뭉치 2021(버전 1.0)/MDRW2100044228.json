      "utterance": [
        {
          "id": "MDRW2100044228.1.1",
          "form": "코울슬로는 어케만들어요",
          "original_form": "코울슬로는 어케만들어요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044228.1.2",
          "form": "양배추 양파 파프리카까지 다지고",
          "original_form": "양배추 양파 파프리카까지 다지고",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044228.1.3",
          "form": "저는 햄이랑 캔옥수수도 첨가해요",
          "original_form": "저는 햄이랑 캔옥수수도 첨가해요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044228.1.4",
          "form": "거기에 코울슬로 드레싱",
          "original_form": "거기에 코울슬로 드레싱",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044228.1.5",
          "form": "우와 드레싱은 어디서요?",
          "original_form": "우와 드레싱은 어디서요?",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044228.1.6",
          "form": "드레싱 만들어서 썼는데",
          "original_form": "드레싱 만들어서 썼는데",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044228.1.7",
          "form": "최근에 보니 마트에서 드레싱 팔더라고요",
          "original_form": "최근에 보니 마트에서 드레싱 팔더라고요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044228.1.8",
          "form": "거기에 레몬즙이랑 머스터드 좀더 넣고",
          "original_form": "거기에 레몬즙이랑 머스터드 좀더 넣고",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044228.1.9",
          "form": "만들어요",
          "original_form": "만들어요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044228.1.10",
          "form": "와... 마싯겟다",
          "original_form": "와... 마싯겟다",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044228.1.11",
          "form": "멋져용",
          "original_form": "멋져용",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044228.1.12",
          "form": "네네 맛있어요",
          "original_form": "네네 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044228.1.13",
          "form": "name1님은 뭐 드셨나요",
          "original_form": "&name1&님은 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044228.1.14",
          "form": "제육이요",
          "original_form": "제육이요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044228.1.15",
          "form": "잡에서 가져왓어용",
          "original_form": "잡에서 가져왓어용",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044228.1.16",
          "form": "홈멩드 제육이네요",
          "original_form": "홈멩드 제육이네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044228.1.17",
          "form": "홈메이드",
          "original_form": "홈메이드",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044228.1.18",
          "form": "ㅋㅋㅋㅋ 네 가끔 가져와요",
          "original_form": "ㅋㅋㅋㅋ 네 가끔 가져와요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044228.1.19",
          "form": "간식 아이스크림",
          "original_form": "간식 아이스크림",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044228.1.20",
          "form": "오 아이스크림 좋죠",
          "original_form": "오 아이스크림 좋죠",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044228.1.21",
          "form": "배라 조아하심 ?",
          "original_form": "배라 조아하심 ?",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044228.1.22",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044228.1.23",
          "form": "어떤맛 이용?",
          "original_form": "어떤맛 이용?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044228.1.24",
          "form": "바람과함께사라지다",
          "original_form": "바람과함께사라지다",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044228.1.25",
          "form": "제일 좋아해요",
          "original_form": "제일 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044228.1.26",
          "form": "헐 저도 그거조아함",
          "original_form": "헐 저도 그거조아함",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044228.1.27",
          "form": "거기 치케 맛잇죠",
          "original_form": "거기 치케 맛잇죠",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044228.1.28",
          "form": "배라 바함사 너무 부드러워용",
          "original_form": "배라 바함사 너무 부드러워용",
          "speaker_id": "1",
          "time": "20210907 22:16"
        }
      ]
    }
