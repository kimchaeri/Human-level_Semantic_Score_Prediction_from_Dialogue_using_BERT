      "utterance": [
        {
          "id": "MDRW2100013191.1.1",
          "form": "안녕하세요! 음악 듣는 거 좋아하시나요?",
          "original_form": "안녕하세요! 음악 듣는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013191.1.2",
          "form": "네 가끔 꽂히는 노래있으면 들어요 자주듣는편은 아니지만",
          "original_form": "네 가끔 꽂히는 노래있으면 들어요 자주듣는편은 아니지만",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013191.1.3",
          "form": "name1은여",
          "original_form": "&name1&은여",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013191.1.4",
          "form": "저도 비슷해요!",
          "original_form": "저도 비슷해요!",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013191.1.5",
          "form": "요즘엔 노래 다섯 개만 무한 반복으로 돌려 듣고있어요ㅎㅎ",
          "original_form": "요즘엔 노래 다섯 개만 무한 반복으로 돌려 듣고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013191.1.6",
          "form": "오 어떤노래 들으시나요",
          "original_form": "오 어떤노래 들으시나요",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013191.1.7",
          "form": "싱스트리트 영화보셨나요?",
          "original_form": "싱스트리트 영화보셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013191.1.8",
          "form": "아뇨 음악영화인거져?",
          "original_form": "아뇨 음악영화인거져?",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013191.1.9",
          "form": "그 영화ost두개랑 트로이시반 노래 세 개",
          "original_form": "그 영화ost두개랑 트로이시반 노래 세 개",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013191.1.10",
          "form": "이렇게듣고있어요",
          "original_form": "이렇게듣고있어요",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013191.1.11",
          "form": "넵넵 음악영화요!",
          "original_form": "넵넵 음악영화요!",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013191.1.12",
          "form": "오 좋다고 들었는데 한번 들어봐야겠네요",
          "original_form": "오 좋다고 들었는데 한번 들어봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013191.1.13",
          "form": "좋아하는 뮤지션은 누구에요?",
          "original_form": "좋아하는 뮤지션은 누구에요?",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013191.1.14",
          "form": "엄청 팬인 가수는 없는데 두아리파랑 찰리푸스랑 악뮤 좋아해요 ㅎㅎ",
          "original_form": "엄청 팬인 가수는 없는데 두아리파랑 찰리푸스랑 악뮤 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013191.1.15",
          "form": "name2은 좋아하시는 가수 있나요?",
          "original_form": "&name2&은 좋아하시는 가수 있나요?",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013191.1.16",
          "form": "저도 엄청 빠지고그런건아니구요 악뮤랑 자우림좋아해요ㅎㅎ",
          "original_form": "저도 엄청 빠지고그런건아니구요 악뮤랑 자우림좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013191.1.17",
          "form": "자우림도 좋죠!",
          "original_form": "자우림도 좋죠!",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013191.1.18",
          "form": "네 콘서트에서 라이브듣고 눈물좔좔흘리고왔었어요ㅠ",
          "original_form": "네 콘서트에서 라이브듣고 눈물좔좔흘리고왔었어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013191.1.19",
          "form": "자우림 콘서트 ㅠㅠㅠㅠ 저도 한 번 가보고 싶어요 ㅠㅠㅠㅠㅠ",
          "original_form": "자우림 콘서트 ㅠㅠㅠㅠ 저도 한 번 가보고 싶어요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013191.1.20",
          "form": "진짜 너무좋았어요ㅜㅜ라이브첫소절듣는데 소름이쫘악..",
          "original_form": "진짜 너무좋았어요ㅜㅜ라이브첫소절듣는데 소름이쫘악..",
          "speaker_id": "2",
