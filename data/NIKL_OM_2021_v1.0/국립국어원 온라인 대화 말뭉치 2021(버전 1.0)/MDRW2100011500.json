        {
          "id": "MDRW2100011500.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011500.1.2",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011500.1.3",
          "form": "게임 뭐하세요?",
          "original_form": "게임 뭐하세요?",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011500.1.4",
          "form": "저 배틀그라운드요!",
          "original_form": "저 배틀그라운드요!",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011500.1.5",
          "form": "그거 총게임인가요?",
          "original_form": "그거 총게임인가요?",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100011500.1.6",
          "form": "네 총게임이에요!",
          "original_form": "네 총게임이에요!",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100011500.1.7",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100011500.1.8",
          "form": "네 엄청 재밌어요 ㅎㅎ",
          "original_form": "네 엄청 재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100011500.1.9",
          "form": "다른게임도 하나요?",
          "original_form": "다른게임도 하나요?",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011500.1.10",
          "form": "롤이랑 오버워치 간혹하고",
          "original_form": "롤이랑 오버워치 간혹하고",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100011500.1.11",
          "form": "이터널 리턴도 간혹하긴해요!",
          "original_form": "이터널 리턴도 간혹하긴해요!",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100011500.1.12",
          "form": "이터널리턴은 처음듣네요",
          "original_form": "이터널리턴은 처음듣네요",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100011500.1.13",
          "form": "오딘은요?",
          "original_form": "오딘은요?",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100011500.1.14",
          "form": "오딘은 안하네용",
          "original_form": "오딘은 안하네용",
          "speaker_id": "2",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100011500.1.15",
          "form": "게임은 저도잘안하네요",
