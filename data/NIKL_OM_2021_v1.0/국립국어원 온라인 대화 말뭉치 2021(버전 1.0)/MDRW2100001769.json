      "utterance": [
        {
          "id": "MDRW2100001769.1.1",
          "form": "name2 안녕?",
          "original_form": "&name2& 안녕?",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100001769.1.2",
          "form": "오늘은 날씨가 안덥고 좋았지?",
          "original_form": "오늘은 날씨가 안덥고 좋았지?",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100001769.1.3",
          "form": "글쎄",
          "original_form": "글쎄",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001769.1.4",
          "form": "가을이 온줄 알았는데",
          "original_form": "가을이 온줄 알았는데",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001769.1.5",
          "form": "아직도 덥던데",
          "original_form": "아직도 덥던데",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001769.1.6",
          "form": "긴팔 입을 날씨는 아닌것 같아",
          "original_form": "긴팔 입을 날씨는 아닌것 같아",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001769.1.7",
          "form": "맞아 긴팔입으니까 덥기는 하더라",
          "original_form": "맞아 긴팔입으니까 덥기는 하더라",
          "speaker_id": "1",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001769.1.8",
          "form": "내일은 반팔입고가야겠어",
          "original_form": "내일은 반팔입고가야겠어",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100001769.1.9",
          "form": "그래도 요새 미세먼지가 없어서 그런지 구름이 엄청 예쁘더라",
          "original_form": "그래도 요새 미세먼지가 없어서 그런지 구름이 엄청 예쁘더라",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100001769.1.10",
          "form": "맞아! 하늘보면 요즘 힐링되는거같아",
          "original_form": "맞아! 하늘보면 요즘 힐링되는거같아",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100001769.1.11",
          "form": "내일도 날씨가 좋겠지?",
          "original_form": "내일도 날씨가 좋겠지?",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100001769.1.12",
          "form": "글쎄 요 며칠 비가 엄청와서.. 비올수도 있겠다 ㅠㅠ",
          "original_form": "글쎄 요 며칠 비가 엄청와서.. 비올수도 있겠다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100001769.1.13",
          "form": "아...비는 싫은데ㅜㅜ 우산을 들고가야할까?",
          "original_form": "아...비는 싫은데ㅜㅜ 우산을 들고가야할까?",
          "speaker_id": "1",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100001769.1.14",
          "form": "우산 하나 사무실에 놓고 다니면 좋지",
          "original_form": "우산 하나 사무실에 놓고 다니면 좋지",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100001769.1.15",
          "form": "언제 소나기 내릴지 몰라서",
          "original_form": "언제 소나기 내릴지 몰라서",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100001769.1.16",
          "form": "그러게 사무실에 예비용으로 놓고다녀야겠어",
          "original_form": "그러게 사무실에 예비용으로 놓고다녀야겠어",
          "speaker_id": "1",
          "time": "20210819 19:52"
        },
        {
          "id": "MDRW2100001769.1.17",
          "form": "날씨가 정말 동남아 날씨같아졌어",
          "original_form": "날씨가 정말 동남아 날씨같아졌어",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100001769.1.18",
          "form": "맞아 엄청 덥고 습한 거 같아ㅜㅜ",
          "original_form": "맞아 엄청 덥고 습한 거 같아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100001769.1.19",
          "form": "이러다 가을, 봄은 없어질까봐 무섭다",
          "original_form": "이러다 가을, 봄은 없어질까봐 무섭다",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100001769.1.20",
          "form": "안돼ㅜㅜ 나는 가을이 가장 좋은데ㅜㅜ",
          "original_form": "안돼ㅜㅜ 나는 가을이 가장 좋은데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100001769.1.21",
          "form": "너는 어떤 계절을 가장 좋아해?",
          "original_form": "너는 어떤 계절을 가장 좋아해?",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100001769.1.22",
          "form": "나는 봄이랑 가을을 제일 좋아해",
          "original_form": "나는 봄이랑 가을을 제일 좋아해",
          "speaker_id": "2",
          "time": "20210819 19:59"
        }
      ]
