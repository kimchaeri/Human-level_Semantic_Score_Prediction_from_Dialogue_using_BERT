        {
          "id": "MDRW2100045635.1.3",
          "form": "나 요즘 넷플릭스 열심히 봐",
          "original_form": "나 요즘 넷플릭스 열심히 봐",
          "speaker_id": "2",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045635.1.4",
          "form": "어떤거 봐?",
          "original_form": "어떤거 봐?",
          "speaker_id": "1",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045635.1.5",
          "form": "나 드라마 로스쿨",
          "original_form": "나 드라마 로스쿨",
          "speaker_id": "2",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045635.1.6",
          "form": "아 김명민 나오는거?",
          "original_form": "아 김명민 나오는거?",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045635.1.7",
          "form": "응 여기 김범나와",
          "original_form": "응 여기 김범나와",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045635.1.8",
          "form": "아 이거 류혜영 나오지",
          "original_form": "아 이거 류혜영 나오지",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045635.1.9",
          "form": "맞아 류혜영 몇년만의 드라마야",
          "original_form": "맞아 류혜영 몇년만의 드라마야",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045635.1.10",
          "form": "왜이렇게 작품 텀이 길어",
          "original_form": "왜이렇게 작품 텀이 길어",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045635.1.11",
          "form": "근데 여기서 쌍둥이 역할이야",
          "original_form": "근데 여기서 쌍둥이 역할이야",
          "speaker_id": "2",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045635.1.12",
          "form": "오 재미있겠다",
          "original_form": "오 재미있겠다",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045635.1.13",
          "form": "그리고 김명민 머리스탈 다양해",
          "original_form": "그리고 김명민 머리스탈 다양해",
