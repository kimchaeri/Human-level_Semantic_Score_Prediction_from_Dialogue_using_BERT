      "utterance": [
        {
          "id": "MDRW2100004287.1.1",
          "form": "와 학교 생활 궁금하다",
          "original_form": "와 학교 생활 궁금하다",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004287.1.2",
          "form": "요즘 학교 다녀?",
          "original_form": "요즘 학교 다녀?",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004287.1.3",
          "form": "지금은 방학기간이지",
          "original_form": "지금은 방학기간이지",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004287.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004287.1.5",
          "form": "나도ㅋㅋㅋㅋㅋ 학교 다닐 땐 어땠어?",
          "original_form": "나도ㅋㅋㅋㅋㅋ 학교 다닐 땐 어땠어?",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004287.1.6",
          "form": "약간 인싸일 거 같아",
          "original_form": "약간 인싸일 거 같아",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004287.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ비대면이라서 동아리 아니면 사람 만날 일이 없었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ비대면이라서 동아리 아니면 사람 만날 일이 없었어",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004287.1.8",
          "form": "너는 학교 다닐 때 어땠어?",
          "original_form": "너는 학교 다닐 때 어땠어?",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004287.1.9",
          "form": "아 코로나 발생하고 입학했구나ㅠㅠ",
          "original_form": "아 코로나 발생하고 입학했구나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004287.1.10",
          "form": "나는 코로나 터지기 전에는 학교생활 많이 했어",
          "original_form": "나는 코로나 터지기 전에는 학교생활 많이 했어",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004287.1.11",
          "form": "학생회 들어가고 동아리하고",
          "original_form": "학생회 들어가고 동아리하고",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004287.1.12",
          "form": "오오 멋지다",
          "original_form": "오오 멋지다",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100004287.1.13",
          "form": "학생회 하면서 힘들진 않았어?",
          "original_form": "학생회 하면서 힘들진 않았어?",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100004287.1.14",
          "form": "응 별로 일이 힘들진 않았어ㅋㅋㅋ 나름 재밌었어",
          "original_form": "응 별로 일이 힘들진 않았어ㅋㅋㅋ 나름 재밌었어",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100004287.1.15",
          "form": "사람이 힘들지",
          "original_form": "사람이 힘들지",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100004287.1.16",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100004287.1.17",
          "form": "동아리는 어떤 거 했어?",
          "original_form": "동아리는 어떤 거 했어?",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100004287.1.18",
          "form": "나는 밴드 동아리 했는데",
          "original_form": "나는 밴드 동아리 했는데",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100004287.1.19",
          "form": "나는 야구 동아리 했어",
          "original_form": "나는 야구 동아리 했어",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100004287.1.20",
          "form": "밴드 동아리 멋있다 악기 다루는 게 있어?",
          "original_form": "밴드 동아리 멋있다 악기 다루는 게 있어?",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100004287.1.21",
          "form": "야구동아리 대박ㅋㅋ",
          "original_form": "야구동아리 대박ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100004287.1.22",
          "form": "보컬이랑 기타 해",
          "original_form": "보컬이랑 기타 해",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100004287.1.23",
          "form": "와아아아아 진짜 멋있다...",
          "original_form": "와아아아아 진짜 멋있다...",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100004287.1.24",
          "form": "공연 한 적 있어?",
          "original_form": "공연 한 적 있어?",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100004287.1.25",
          "form": "응응ㅋㅋㅋ 코로나 때문에 사람들 다 들어오진 못했는데",
          "original_form": "응응ㅋㅋㅋ 코로나 때문에 사람들 다 들어오진 못했는데",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100004287.1.26",
          "form": "공연은 했어ㅋㅋㅋ",
          "original_form": "공연은 했어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100004287.1.27",
          "form": "그건 아쉽다ㅠㅜㅠ 어떤 노래 했어?",
          "original_form": "그건 아쉽다ㅠㅜㅠ 어떤 노래 했어?",
          "speaker_id": "2",
          "time": "20210822 18:39"
        },
        {
          "id": "MDRW2100004287.1.28",
          "form": "하고 싶은 노래는 따로 있었는데",
          "original_form": "하고 싶은 노래는 따로 있었는데",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100004287.1.29",
          "form": "볼빨건사춘기 워커홀릭 불렀었어ㅋㅋㅋ",
          "original_form": "볼빨건사춘기 워커홀릭 불렀었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100004287.1.30",
          "form": "아 너무너무너무 멋있어 그 노래 좋아해",
          "original_form": "아 너무너무너무 멋있어 그 노래 좋아해",
          "speaker_id": "2",
          "time": "20210822 18:42"
        },
        {
          "id": "MDRW2100004287.1.31",
          "form": "원래 어떤 노래 하고 싶었는데?",
          "original_form": "원래 어떤 노래 하고 싶었는데?",
          "speaker_id": "2",
          "time": "20210822 18:42"
        },
        {
          "id": "MDRW2100004287.1.32",
          "form": "ex 잘부탁드립니다 이거 하고 싶었었어",
          "original_form": "ex 잘부탁드립니다 이거 하고 싶었었어",
          "speaker_id": "1",
          "time": "20210822 18:42"
        },
        {
          "id": "MDRW2100004287.1.33",
          "form": "밴드 공연이랑 잘맞는 선곡이다",
          "original_form": "밴드 공연이랑 잘맞는 선곡이다",
