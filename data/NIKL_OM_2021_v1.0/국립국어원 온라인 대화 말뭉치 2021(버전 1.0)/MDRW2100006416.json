      "utterance": [
        {
          "id": "MDRW2100006416.1.1",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006416.1.2",
          "form": "오늘은 실내자전거만 한시간 탔어요",
          "original_form": "오늘은 실내자전거만 한시간 탔어요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006416.1.3",
          "form": "운동하세요??",
          "original_form": "운동하세요??",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006416.1.4",
          "form": "네 헬스요...1시간이면 많이 타셨네요",
          "original_form": "네 헬스요...1시간이면 많이 타셨네요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006416.1.5",
          "form": "맨날 드라마 보면서 타니까 시간이 금방가여",
          "original_form": "맨날 드라마 보면서 타니까 시간이 금방가여",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006416.1.6",
          "form": "좋네요 일석이조로...",
          "original_form": "좋네요 일석이조로...",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006416.1.7",
          "form": "저도 근력을 키워야할텐데...",
          "original_form": "저도 근력을 키워야할텐데...",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006416.1.8",
          "form": "일단 지방부터 빼고있네요ㅠㅠㅎ",
          "original_form": "일단 지방부터 빼고있네요ㅠㅠㅎ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006416.1.9",
          "form": "그렇죠 근력과 병행하면 더 좋다고 하죠",
          "original_form": "그렇죠 근력과 병행하면 더 좋다고 하죠",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006416.1.10",
          "form": "낼은 공복 유산소를 해야겠어요",
          "original_form": "낼은 공복 유산소를 해야겠어요",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006416.1.11",
          "form": "근력운동 어떤거 하시나요?",
          "original_form": "근력운동 어떤거 하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006416.1.12",
          "form": "저는 헬스장 기구를.사용합니다",
          "original_form": "저는 헬스장 기구를.사용합니다",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006416.1.13",
          "form": "스쿼트같은건 안하시구영?",
          "original_form": "스쿼트같은건 안하시구영?",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006416.1.14",
          "form": "네 맨몸운동보단 기구운동을 해요",
          "original_form": "네 맨몸운동보단 기구운동을 해요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006416.1.15",
          "form": "그렇군요 헬스장을 다녀야 근력 키우기는 좋겠네요",
          "original_form": "그렇군요 헬스장을 다녀야 근력 키우기는 좋겠네요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006416.1.16",
