      "utterance": [
        {
          "id": "MDRW2100023112.1.1",
          "form": "찰토마토는 뭐여",
          "original_form": "찰토마토는 뭐여",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023112.1.2",
          "form": "찰토마토라고 과즙 적고 딴딴한 토마토가 있는데",
          "original_form": "찰토마토라고 과즙 적고 딴딴한 토마토가 있는데",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023112.1.3",
          "form": "옥수수같은 이름이네",
          "original_form": "옥수수같은 이름이네",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023112.1.4",
          "form": "퍽퍽하고 과일 먹는 느낌이 전혀 안나는 이상한 토마토 있어",
          "original_form": "퍽퍽하고 과일 먹는 느낌이 전혀 안나는 이상한 토마토 있어",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023112.1.5",
          "form": "옥수수도 찰옥수수가 딱딱하잖아",
          "original_form": "옥수수도 찰옥수수가 딱딱하잖아",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023112.1.6",
          "form": "토마토토 찰토마토가 딱딱해",
          "original_form": "토마토토 찰토마토가 딱딱해",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023112.1.7",
          "form": "너 옥수수 좋아하냐",
          "original_form": "너 옥수수 좋아하냐",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023112.1.8",
          "form": "응 옥수수 좋아해",
          "original_form": "응 옥수수 좋아해",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023112.1.9",
          "form": "근데 어린옥수수가 좋아",
          "original_form": "근데 어린옥수수가 좋아",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023112.1.10",
          "form": "어린옥수수는 먹기 힘들지않냐",
          "original_form": "어린옥수수는 먹기 힘들지않냐",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023112.1.11",
          "form": "맞아 깨물면 다 뭉개지는데 대신 달고 쫀득쫀득해",
          "original_form": "맞아 깨물면 다 뭉개지는데 대신 달고 쫀득쫀득해",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023112.1.12",
          "form": "넌 큰옥수수가 더 좋아?",
          "original_form": "넌 큰옥수수가 더 좋아?",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023112.1.13",
          "form": "응 초당옥수수 먹어봤는데",
          "original_form": "응 초당옥수수 먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.14",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.15",
          "form": "초당 옥수수 이름은 많이 들어봤는데 안 먹어봤어",
          "original_form": "초당 옥수수 이름은 많이 들어봤는데 안 먹어봤어",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.16",
          "form": "특징이 있니?",
          "original_form": "특징이 있니?",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.17",
          "form": "안삶아도 먹을수있어 과일처럼",
          "original_form": "안삶아도 먹을수있어 과일처럼",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.18",
          "form": "신기하지?",
          "original_form": "신기하지?",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.19",
          "form": "우와 처음 들어봐",
          "original_form": "우와 처음 들어봐",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.20",
          "form": "아삭아삭해?",
          "original_form": "아삭아삭해?",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023112.1.21",
          "form": "응 아삭아삭해",
          "original_form": "응 아삭아삭해",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023112.1.22",
          "form": "식감이 신기하다",
          "original_form": "식감이 신기하다",
          "speaker_id": "2",
