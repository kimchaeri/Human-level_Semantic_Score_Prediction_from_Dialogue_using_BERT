      "utterance": [
        {
          "id": "MDRW2100036519.1.1",
          "form": "마요르광장 아까도 광장이었는데",
          "original_form": "마요르광장 아까도 광장이었는데",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036519.1.2",
          "form": "그러게 마요르광장은 중세까지 시장으로 사용하다가",
          "original_form": "그러게 마요르광장은 중세까지 시장으로 사용하다가",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036519.1.3",
          "form": "지금은",
          "original_form": "지금은",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036519.1.4",
          "form": "지금은 쥬요행사가 열리는 광장으로 열린데",
          "original_form": "지금은 쥬요행사가 열리는 광장으로 열린데",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036519.1.5",
          "form": "그래 엄청 중요한 광장이네",
          "original_form": "그래 엄청 중요한 광장이네",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036519.1.6",
          "form": "엉 근데 3번으 화재가 나서 재건축",
          "original_form": "엉 근데 3번으 화재가 나서 재건축",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036519.1.7",
          "form": "헐 3번이나 뭐꼬",
          "original_form": "헐 3번이나 뭐꼬",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036519.1.8",
          "form": "그러게 3번이나 화재가 난건 문제가 있네",
          "original_form": "그러게 3번이나 화재가 난건 문제가 있네",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036519.1.9",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036519.1.10",
          "form": "4층 건물이 직사각형처럼 광장을 둘러싸고 있다",
          "original_form": "4층 건물이 직사각형처럼 광장을 둘러싸고 있다",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036519.1.11",
          "form": "침략대비에 좋겠다",
          "original_form": "침략대비에 좋겠다",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036519.1.12",
          "form": "적의 침투에 대비해서 둘러쌓고 잇나봐",
          "original_form": "적의 침투에 대비해서 둘러쌓고 잇나봐",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036519.1.13",
          "form": "그러게 요새네",
          "original_form": "그러게 요새네",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036519.1.14",
          "form": "이 광장으로 통하는 문이 9개라네",
          "original_form": "이 광장으로 통하는 문이 9개라네",
