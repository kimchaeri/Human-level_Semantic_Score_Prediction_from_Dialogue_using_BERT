      "utterance": [
        {
          "id": "MDRW2100033713.1.1",
          "form": "계절 뭐 좋아하시나용?",
          "original_form": "계절 뭐 좋아하시나용?",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033713.1.2",
          "form": "전 가을이 좋아요!",
          "original_form": "전 가을이 좋아요!",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033713.1.3",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033713.1.4",
          "form": "어 저두요 !!!",
          "original_form": "어 저두요 !!!",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033713.1.5",
          "form": "가을 그 서늘한 거요 ㅠㅠㅠ",
          "original_form": "가을 그 서늘한 거요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033713.1.6",
          "form": "맞아요ㅋㅋㅋ 가을에는 옷을 따듯하게 입어도 좋고 좀 춥게 입어도 좋아서 좋아요",
          "original_form": "맞아요ㅋㅋㅋ 가을에는 옷을 따듯하게 입어도 좋고 좀 춥게 입어도 좋아서 좋아요",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033713.1.7",
          "form": "근데 너무 짧아서 슬퍼요",
          "original_form": "근데 너무 짧아서 슬퍼요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033713.1.8",
          "form": "이번에도 짧겠죠 흑",
          "original_form": "이번에도 짧겠죠 흑",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033713.1.9",
          "form": "ㅋㅋㅋ 그러게요...ㅠㅠ 그리고 모기가 가을에 갑자기 많아져서 좀 싫어졌어요...",
          "original_form": "ㅋㅋㅋ 그러게요...ㅠㅠ 그리고 모기가 가을에 갑자기 많아져서 좀 싫어졌어요...",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033713.1.10",
          "form": "맞아요 저 여태 모기 안물렸는데",
          "original_form": "맞아요 저 여태 모기 안물렸는데",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033713.1.11",
          "form": "오늘 물렸답니다 허허",
          "original_form": "오늘 물렸답니다 허허",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033713.1.12",
          "form": "저는 오늘 모기를 2마리나 잡았어요",
          "original_form": "저는 오늘 모기를 2마리나 잡았어요",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033713.1.13",
          "form": "6~8월에 날씨가 너무 더우니 이런 일도 일어나나 싶네요",
          "original_form": "6~8월에 날씨가 너무 더우니 이런 일도 일어나나 싶네요",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033713.1.14",
          "form": "날이 풀려서 모기가 돌아다닌대요 ㅠㅠㅠㅠ",
          "original_form": "날이 풀려서 모기가 돌아다닌대요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033713.1.15",
          "form": "저희 지역은 내일 비가 내린다고 하네요! name2 님 동네는요?",
          "original_form": "저희 지역은 내일 비가 내린다고 하네요! &name2& 님 동네는요?",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033713.1.16",
          "form": "저희도요 안믿겨요.....",
          "original_form": "저희도요 안믿겨요.....",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033713.1.17",
          "form": "앗... 언제까지 내린다고 하던가요",
          "original_form": "앗... 언제까지 내린다고 하던가요",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033713.1.18",
          "form": "오후는 하루종일이요",
          "original_form": "오후는 하루종일이요",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033713.1.19",
          "form": "심각하네요",
          "original_form": "심각하네요",
          "speaker_id": "2",
