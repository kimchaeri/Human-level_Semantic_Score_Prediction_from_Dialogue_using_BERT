      "utterance": [
        {
          "id": "MDRW2100018562.1.1",
          "form": "콜라보레이션 한 제품",
          "original_form": "콜라보레이션 한 제품",
          "speaker_id": "2",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018562.1.2",
          "form": "드셔본 거 있나요",
          "original_form": "드셔본 거 있나요",
          "speaker_id": "2",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018562.1.3",
          "form": "저 그거",
          "original_form": "저 그거",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018562.1.4",
          "form": "아이셔소주인가",
          "original_form": "아이셔소주인가",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018562.1.5",
          "form": "어 그거 어때요?",
          "original_form": "어 그거 어때요?",
          "speaker_id": "2",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018562.1.6",
          "form": "맛있었어요ㅋㅋ 소주인데 맛있는 음료같은느낌",
          "original_form": "맛있었어요ㅋㅋ 소주인데 맛있는 음료같은느낌",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018562.1.7",
          "form": "모르고먹으면 훅 가겠더라구요ㅋㅋ",
          "original_form": "모르고먹으면 훅 가겠더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018562.1.8",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018562.1.9",
          "form": "ㅋㅋ 전 안먹어봤는데",
          "original_form": "ㅋㅋ 전 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210827 09:44"
        },
        {
          "id": "MDRW2100018562.1.10",
          "form": "그거 궁금해요",
          "original_form": "그거 궁금해요",
          "speaker_id": "2",
          "time": "20210827 09:44"
        },
        {
          "id": "MDRW2100018562.1.11",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210827 09:44"
        },
        {
          "id": "MDRW2100018562.1.12",
          "form": "아이셔소주요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이셔소주요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100018562.1.13",
          "form": "그리고 홍차소주 아시나요",
          "original_form": "그리고 홍차소주 아시나요",
          "speaker_id": "2",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100018562.1.14",
          "form": "아하 홍차소주요?",
          "original_form": "아하 홍차소주요?",
          "speaker_id": "1",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100018562.1.15",
          "form": "그런게 있어요? 첨들어봐요",
          "original_form": "그런게 있어요? 첨들어봐요",
          "speaker_id": "1",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018562.1.16",
          "form": "네!! 콜라보레이션은 아닌데..",
          "original_form": "네!! 콜라보레이션은 아닌데..",
          "speaker_id": "2",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018562.1.17",
          "form": "키가 나와서 발명 한 거 같은..",
          "original_form": "키가 나와서 발명 한 거 같은..",
          "speaker_id": "2",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018562.1.18",
          "form": "오.. 신기하다 만들어 보셨나요",
          "original_form": "오.. 신기하다 만들어 보셨나요",
          "speaker_id": "1",
          "time": "20210827 09:46"
        },
        {
          "id": "MDRW2100018562.1.19",
          "form": "아니여..ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니여..ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018562.1.20",
          "form": "만들어보진 안ㄹ았어요",
          "original_form": "만들어보진 안ㄹ았어요",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100018562.1.21",
          "form": "콜라보제품 사보신건 있으신가요",
          "original_form": "콜라보제품 사보신건 있으신가요",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018562.1.22",
          "form": "없고",
          "original_form": "없고",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018562.1.23",
          "form": "진로에서 사은품으로 받은 적은 있어요ㅋㅋ",
          "original_form": "진로에서 사은품으로 받은 적은 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018562.1.24",
          "form": "오 진로면",
          "original_form": "오 진로면",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018562.1.25",
          "form": "파란두꺼비?",
