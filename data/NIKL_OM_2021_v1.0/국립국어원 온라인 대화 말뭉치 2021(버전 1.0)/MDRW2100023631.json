      "utterance": [
        {
          "id": "MDRW2100023631.1.1",
          "form": "가족 이야기~~!",
          "original_form": "가족 이야기~~!",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023631.1.2",
          "form": "주말에 식구들이랑 무엇을 하셨나요~~~",
          "original_form": "주말에 식구들이랑 무엇을 하셨나요~~~",
          "speaker_id": "1",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023631.1.3",
          "form": "그냥 밥해먹고, 티비보고, 얘기하고",
          "original_form": "그냥 밥해먹고, 티비보고, 얘기하고",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023631.1.4",
          "form": "집에서 있었어요",
          "original_form": "집에서 있었어요",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023631.1.5",
          "form": "오오 정말 좋은 주말을 보내셨네요~~~",
          "original_form": "오오 정말 좋은 주말을 보내셨네요~~~",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023631.1.6",
          "form": "생각해보니 저도 식구들이랑 그렇게 지냈네요,,",
          "original_form": "생각해보니 저도 식구들이랑 그렇게 지냈네요,,",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023631.1.7",
          "form": "다 그렇죠. 특히 요즘 시국엔",
          "original_form": "다 그렇죠. 특히 요즘 시국엔",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023631.1.8",
          "form": "게다가 저흰 환자도 있고",
          "original_form": "게다가 저흰 환자도 있고",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023631.1.9",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023631.1.10",
          "form": "저희 엄마도 암환자라 그런지 이따금씩 골골... 합니다...ㅠ",
          "original_form": "저희 엄마도 암환자라 그런지 이따금씩 골골... 합니다...ㅠ",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023631.1.11",
          "form": "마음을 놓을 수가 없어요~!",
          "original_form": "마음을 놓을 수가 없어요~!",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023631.1.12",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023631.1.13",
          "form": "우리는 건강관리 잘합시다",
          "original_form": "우리는 건강관리 잘합시다",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023631.1.14",
          "form": "솔직히 제 입으로 하긴 그렇지만 우린 나름 효녀지만",
          "original_form": "솔직히 제 입으로 하긴 그렇지만 우린 나름 효녀지만",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023631.1.15",
          "form": "우리가 나중에 우리만한 효자를 낳아 기를 가능성이 높진 않으니까요...",
          "original_form": "우리가 나중에 우리만한 효자를 낳아 기를 가능성이 높진 않으니까요...",
          "speaker_id": "2",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023631.1.16",
          "form": "맞아요~!~!!",
          "original_form": "맞아요~!~!!",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023631.1.17",
          "form": "공감 백만개입니다~!",
          "original_form": "공감 백만개입니다~!",
          "speaker_id": "1",
          "time": "20210830 14:40"
        },
        {
          "id": "MDRW2100023631.1.18",
          "form": "저희 집도 딸들만 효도해요",
          "original_form": "저희 집도 딸들만 효도해요",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023631.1.19",
          "form": "ㅎㅎ 우리집도요....ㅂㄷㅂㄷ",
          "original_form": "ㅎㅎ 우리집도요....ㅂㄷㅂㄷ",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100023631.1.20",
          "form": "걔는 걔대로 이런소리 억울하게 느끼겠지만..",
          "original_form": "걔는 걔대로 이런소리 억울하게 느끼겠지만..",
          "speaker_id": "2",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100023631.1.21",
          "form": "그러니깐 말이에요~!~!~!!",
          "original_form": "그러니깐 말이에요~!~!~!!",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023631.1.22",
          "form": "울 오빤 맨날 아프다면서 돈 타령만~~~!!",
          "original_form": "울 오빤 맨날 아프다면서 돈 타령만~~~!!",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023631.1.23",
          "form": "가사노동도 주로 우리한테 맡겨지고...",
          "original_form": "가사노동도 주로 우리한테 맡겨지고...",
          "speaker_id": "2",
