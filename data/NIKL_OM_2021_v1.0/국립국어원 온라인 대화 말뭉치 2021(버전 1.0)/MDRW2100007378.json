      "utterance": [
        {
          "id": "MDRW2100007378.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007378.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007378.1.3",
          "form": "오늘 비도 오는데 하루 잘 보내셨나요?",
          "original_form": "오늘 비도 오는데 하루 잘 보내셨나요?",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007378.1.4",
          "form": "네 ㅎㅎ 집안에 있었어서 되게 좋은 하루였어요",
          "original_form": "네 ㅎㅎ 집안에 있었어서 되게 좋은 하루였어요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007378.1.5",
          "form": "방정리도 하고",
          "original_form": "방정리도 하고",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007378.1.6",
          "form": "싹 치우느라 3시간 걸렸어요 ㅋㅋㅋ",
          "original_form": "싹 치우느라 3시간 걸렸어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007378.1.7",
          "form": "오 대단하세요! 저도 방정리 해야하는데 귀찮아서 미루고 있거든요ㅋㅋㅋ",
          "original_form": "오 대단하세요! 저도 방정리 해야하는데 귀찮아서 미루고 있거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007378.1.8",
          "form": "아 맞아요 ㅋㅋㅋㅋ 계속 미루고 미루다",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ 계속 미루고 미루다",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007378.1.9",
          "form": "오늘 인터넷 쇼핑을 햇는데",
          "original_form": "오늘 인터넷 쇼핑을 햇는데",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007378.1.10",
          "form": "와 도저히 둘자리가 없는거예요",
          "original_form": "와 도저히 둘자리가 없는거예요",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007378.1.11",
          "form": "진짜 안되겠다 싶어서 싹 치웠어요 ㅋㅋㅠㅠ",
          "original_form": "진짜 안되겠다 싶어서 싹 치웠어요 ㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007378.1.12",
          "form": "ㅋㅋㅋㅋㅋ알죠알죠 저도 안 되겠다 싶을 때 몰아서 치워요",
          "original_form": "ㅋㅋㅋㅋㅋ알죠알죠 저도 안 되겠다 싶을 때 몰아서 치워요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007378.1.13",
          "form": "그래도 오늘 청소하셨으니 당분간 방이 깨끗하시겠어요",
          "original_form": "그래도 오늘 청소하셨으니 당분간 방이 깨끗하시겠어요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007378.1.14",
          "form": "맞아요 ㅋㅋㅋㅋ 이제 진짜 깔끔하게 지내야지..",
          "original_form": "맞아요 ㅋㅋㅋㅋ 이제 진짜 깔끔하게 지내야지..",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007378.1.15",
          "form": "방청소는 한번하면 늘",
          "original_form": "방청소는 한번하면 늘",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007378.1.16",
          "form": "놔두면 언젠간 쓰겠지? 하던것들이",
          "original_form": "놔두면 언젠간 쓰겠지? 하던것들이",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007378.1.17",
          "form": "저기 구석에서 우르르,,",
          "original_form": "저기 구석에서 우르르,,",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007378.1.18",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007378.1.19",
          "form": "집 깨끗하게 관리하는 게 참 어렵네요",
          "original_form": "집 깨끗하게 관리하는 게 참 어렵네요",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007378.1.20",
          "form": "설거지도 번거롭고 날씨가 이래서 빨래 널기도 힘들고요ㅠ",
          "original_form": "설거지도 번거롭고 날씨가 이래서 빨래 널기도 힘들고요ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007378.1.21",
          "form": "그쵸그쵸 ㅠㅠ",
          "original_form": "그쵸그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007378.1.22",
          "form": "저희는 건조기가 있어서",
          "original_form": "저희는 건조기가 있어서",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007378.1.23",
          "form": "빨래는 그나마 수월한 편인데",
          "original_form": "빨래는 그나마 수월한 편인데",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007378.1.24",
          "form": "설거지는 정말ㅋㅋ",
          "original_form": "설거지는 정말ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007378.1.25",
          "form": "아까 했는데 눈깜빡하면 또 쌓여있고..",
          "original_form": "아까 했는데 눈깜빡하면 또 쌓여있고..",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007378.1.26",
          "form": "그러니까요.. 한 끼 먹을 때마다 한가득이에요 아주",
          "original_form": "그러니까요.. 한 끼 먹을 때마다 한가득이에요 아주",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007378.1.27",
          "form": "요리라도 하면 또 한가득이고..",
          "original_form": "요리라도 하면 또 한가득이고..",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007378.1.28",
          "form": "그니까요ㅋㅋㅋㅠㅠ",
          "original_form": "그니까요ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007378.1.29",
          "form": "요리 자주 하는 편이세요?",
          "original_form": "요리 자주 하는 편이세요?",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007378.1.30",
          "form": "설거지 나오는 거 귀찮아서 한번에 몰아서 하고 얼릴 수 있는 건 얼려서 보관해둬요!",
          "original_form": "설거지 나오는 거 귀찮아서 한번에 몰아서 하고 얼릴 수 있는 건 얼려서 보관해둬요!",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007378.1.31",
          "form": "자취생이라ㅋㅋ안 얼리면 감당이 안 되더라고요",
          "original_form": "자취생이라ㅋㅋ안 얼리면 감당이 안 되더라고요",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007378.1.32",
          "form": "오오 자취하시는구나",
          "original_form": "오오 자취하시는구나",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007378.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007378.1.34",
          "form": "자유롭고 좋아요",
          "original_form": "자유롭고 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007378.1.35",
          "form": "집안일을 다 제가 해야 해서 번거롭긴 하지만요ㅋㅋㅋㅋ",
          "original_form": "집안일을 다 제가 해야 해서 번거롭긴 하지만요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007378.1.36",
          "form": "오옹 저도 자취 로망 있는데",
          "original_form": "오옹 저도 자취 로망 있는데",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007378.1.37",
          "form": "얼마전에 화장실에서",
          "original_form": "얼마전에 화장실에서",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007378.1.38",
          "form": "알수없는 벌레를 본 이후로",
          "original_form": "알수없는 벌레를 본 이후로",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007378.1.39",
          "form": "와 자취하면 이걸 다 내가 잡고 살아야되겠지",
          "original_form": "와 자취하면 이걸 다 내가 잡고 살아야되겠지",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007378.1.40",
          "form": "이런 생각이 들면서",
          "original_form": "이런 생각이 들면서",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007378.1.41",
          "form": "살짝.. 아주 살짝 로망이 날아갔어요ㅋㅋㅋㅋ",
          "original_form": "살짝.. 아주 살짝 로망이 날아갔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007378.1.42",
          "form": "ㅋㄱㅋㄱㄱㅋㄱㄱ아무래도..그렇죠..",
          "original_form": "ㅋㄱㅋㄱㄱㅋㄱㄱ아무래도..그렇죠..",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007378.1.43",
          "form": "저도 얼마전에...바 모 벌레를 마주했어요..",
          "original_form": "저도 얼마전에...바 모 벌레를 마주했어요..",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007378.1.44",
          "form": "정말 기절하고 싶었는데 정신 붙잡고 소리지르면서 어떻게든 잡았답니다...",
          "original_form": "정말 기절하고 싶었는데 정신 붙잡고 소리지르면서 어떻게든 잡았답니다...",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007378.1.45",
          "form": "자취의 현실..",
          "original_form": "자취의 현실..",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007378.1.46",
          "form": "아악...",
          "original_form": "아악...",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007378.1.47",
          "form": "마주치고 싶지 않은 현실이네요ㅋㅋㅋㅠㅠ",
          "original_form": "마주치고 싶지 않은 현실이네요ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:04"
        }
      ]
