      "utterance": [
        {
          "id": "MDRW2100009861.1.1",
          "form": "name1님은 아직 아이는 없죠?",
          "original_form": "&name1&님은 아직 아이는 없죠?",
          "speaker_id": "2",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009861.1.2",
          "form": "자녀계획은 있나요?",
          "original_form": "자녀계획은 있나요?",
          "speaker_id": "2",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009861.1.3",
          "form": "아직 와이프랑 상의 중이에요",
          "original_form": "아직 와이프랑 상의 중이에요",
          "speaker_id": "1",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009861.1.4",
          "form": "와이프가 구직활동 중이라서",
          "original_form": "와이프가 구직활동 중이라서",
          "speaker_id": "1",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009861.1.5",
          "form": "아하 그렇군용:)",
          "original_form": "아하 그렇군용:)",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009861.1.6",
          "form": "네 어떡해야할지 잘 모르겠어요",
          "original_form": "네 어떡해야할지 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009861.1.7",
          "form": "와이프가 언제 직장 구할지도 모르겠고",
          "original_form": "와이프가 언제 직장 구할지도 모르겠고",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009861.1.8",
          "form": "가족계획은 다른 사람들이\r\n뭐라하든 두 분 생각과 마음가짐이\r\n가장 중요한거라 너무 조급하게\r\n생각마시고 천천히 고민해 보세요",
          "original_form": "가족계획은 다른 사람들이\r\n뭐라하든 두 분 생각과 마음가짐이\r\n가장 중요한거라 너무 조급하게\r\n생각마시고 천천히 고민해 보세요",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009861.1.9",
          "form": "네네 그래야죠",
          "original_form": "네네 그래야죠",
          "speaker_id": "1",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009861.1.10",
          "form": "우선 급한게 와이프 구직이니",
          "original_form": "우선 급한게 와이프 구직이니",
          "speaker_id": "1",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009861.1.11",
          "form": "거기에 우선순위 둬야죠",
          "original_form": "거기에 우선순위 둬야죠",
          "speaker_id": "1",
          "time": "20210823 12:15"
        },
        {
          "id": "MDRW2100009861.1.12",
          "form": "자녀있으세요?",
          "original_form": "자녀있으세요?",
          "speaker_id": "1",
          "time": "20210823 12:16"
        },
        {
          "id": "MDRW2100009861.1.13",
          "form": "네:) 저는 두 아이의 엄마에요",
          "original_form": "네:) 저는 두 아이의 엄마에요",
          "speaker_id": "2",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009861.1.14",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009861.1.15",
          "form": "힘들지 않으세요 육아?",
          "original_form": "힘들지 않으세요 육아?",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009861.1.16",
          "form": "육아... 정말 힘들어요ㅠ",
          "original_form": "육아... 정말 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009861.1.17",
          "form": "제가 회사다닐땐\r\n육아휴직이 꿈이었는데\r\n막상 육아하니 출근이\r\n꿈이 되어버렸어요ㅋㅋ",
          "original_form": "제가 회사다닐땐\r\n육아휴직이 꿈이었는데\r\n막상 육아하니 출근이\r\n꿈이 되어버렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009861.1.18",
          "form": "아... 그렇죠ㅎㅎ 항상 지나고나야 결핍되어야 고마움을 알게되죠",
          "original_form": "아... 그렇죠ㅎㅎ 항상 지나고나야 결핍되어야 고마움을 알게되죠",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009861.1.19",
          "form": "그래도 육아랑 직장이랑 병행은 정말 어려울것 같아요",
          "original_form": "그래도 육아랑 직장이랑 병행은 정말 어려울것 같아요",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009861.1.20",
          "form": "그쵸ㅠㅠ 워킹맘은 진짜 존경합니다",
          "original_form": "그쵸ㅠㅠ 워킹맘은 진짜 존경합니다",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009861.1.21",
          "form": "저도 첫째 육휴 끝나고 복직하고\r\n6개월정도 다녔는데 아이가\r\n어리다보니 갑자기 열나고 아플때도 있고",
          "original_form": "저도 첫째 육휴 끝나고 복직하고\r\n6개월정도 다녔는데 아이가\r\n어리다보니 갑자기 열나고 아플때도 있고",
          "speaker_id": "2",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009861.1.22",
          "form": "그럴때마다 같이 직장을 다님에도\r\n불구하고 회사에 아쉬운 소리를\r\n하게되는건 언제나 엄마인\r\n제 몫이 되더라구요",
          "original_form": "그럴때마다 같이 직장을 다님에도\r\n불구하고 회사에 아쉬운 소리를\r\n하게되는건 언제나 엄마인\r\n제 몫이 되더라구요",
          "speaker_id": "2",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009861.1.23",
          "form": "힘드셨겠네요",
          "original_form": "힘드셨겠네요",
