      "utterance": [
        {
          "id": "MDRW2100030263.1.1",
          "form": "안녕 점심 먹었니",
          "original_form": "안녕 점심 먹었니",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100030263.1.2",
          "form": "응 안녕 점심 족발이랑",
          "original_form": "응 안녕 점심 족발이랑",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030263.1.3",
          "form": "다이어트용 곤약볶음밥 먹었어",
          "original_form": "다이어트용 곤약볶음밥 먹었어",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100030263.1.4",
          "form": "족발 맛있겠다 무슨 사이즈 먹었니",
          "original_form": "족발 맛있겠다 무슨 사이즈 먹었니",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100030263.1.5",
          "form": "불족발이라 다 잘려있었어",
          "original_form": "불족발이라 다 잘려있었어",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100030263.1.6",
          "form": "오 매운거 먹었어?",
          "original_form": "오 매운거 먹었어?",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100030263.1.7",
          "form": "일부러 동생이 안매운걸로 했더라고",
          "original_form": "일부러 동생이 안매운걸로 했더라고",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100030263.1.8",
          "form": "너는 뭐먹었니",
          "original_form": "너는 뭐먹었니",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100030263.1.9",
          "form": "나도 돼지먹었어ㅋㅋㅋ 뒷다리살 불고기용 사서 구워먹었는데 얇고 담백했어",
          "original_form": "나도 돼지먹었어ㅋㅋㅋ 뒷다리살 불고기용 사서 구워먹었는데 얇고 담백했어",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030263.1.10",
          "form": "곤약볶음밥은 어땠니",
          "original_form": "곤약볶음밥은 어땠니",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030263.1.11",
          "form": "뒷다리살은 싸서 좋아",
          "original_form": "뒷다리살은 싸서 좋아",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030263.1.12",
          "form": "맞이 저렴해",
          "original_form": "맞이 저렴해",
          "speaker_id": "1",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030263.1.13",
          "form": "곤약볶음밥 요즘 생각보다 잘나와",
          "original_form": "곤약볶음밥 요즘 생각보다 잘나와",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030263.1.14",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030263.1.15",
          "form": "식감이 어때? 꼬들하니",
          "original_form": "식감이 어때? 꼬들하니",
          "speaker_id": "1",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030263.1.16",
          "form": "의외로 기술이 발달해서 밥이랑 비슷해",
          "original_form": "의외로 기술이 발달해서 밥이랑 비슷해",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100030263.1.17",
          "form": "헉 곤약이 쌀모양이야?",
          "original_form": "헉 곤약이 쌀모양이야?",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100030263.1.18",
          "form": "응 쌀모양으로 식감도비슷해",
          "original_form": "응 쌀모양으로 식감도비슷해",
          "speaker_id": "2",
          "time": "20210904 15:42"
        }
      ]
    }
