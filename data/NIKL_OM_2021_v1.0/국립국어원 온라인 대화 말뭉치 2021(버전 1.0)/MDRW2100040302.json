      "utterance": [
        {
          "id": "MDRW2100040302.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040302.1.2",
          "form": "아가씨들이 강아지 예쁘게키우시더라구요",
          "original_form": "아가씨들이 강아지 예쁘게키우시더라구요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040302.1.3",
          "form": "그런가요 ㅋㅋㅋ",
          "original_form": "그런가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.4",
          "form": "제 전부 제 심장이에요",
          "original_form": "제 전부 제 심장이에요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.5",
          "form": "너무너무 예뻐하시는군요",
          "original_form": "너무너무 예뻐하시는군요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.6",
          "form": "건강하게 잘키우세요",
          "original_form": "건강하게 잘키우세요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.8",
          "form": "하루하루 소중하달까",
          "original_form": "하루하루 소중하달까",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040302.1.9",
          "form": "집에가면 엄청반가워하겠네요",
          "original_form": "집에가면 엄청반가워하겠네요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040302.1.10",
          "form": "네네 얼른 가고싶어요",
          "original_form": "네네 얼른 가고싶어요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.11",
          "form": "홈카메라 방에 설치해놨는데",
          "original_form": "홈카메라 방에 설치해놨는데",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.12",
          "form": "이쁠것같아요.얘기만들어도",
          "original_form": "이쁠것같아요.얘기만들어도",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.13",
          "form": "거실 갔는지 안보이네요",
          "original_form": "거실 갔는지 안보이네요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.14",
          "form": "종은뭐예요?",
          "original_form": "종은뭐예요?",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.15",
          "form": "카메라설치해놓으셨군요",
          "original_form": "카메라설치해놓으셨군요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.16",
          "form": "종은 치와와에요",
          "original_form": "종은 치와와에요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040302.1.17",
          "form": "치와와귀엽죠",
          "original_form": "치와와귀엽죠",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040302.1.18",
          "form": "털은 잘안날리겠네요",
          "original_form": "털은 잘안날리겠네요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040302.1.19",
          "form": "털 엄청 날려요 ㅋㅋ장모 거든요",
          "original_form": "털 엄청 날려요 ㅋㅋ장모 거든요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040302.1.20",
          "form": "아..치와와장모예요?",
          "original_form": "아..치와와장모예요?",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040302.1.21",
          "form": "네네맞아요",
          "original_form": "네네맞아요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040302.1.22",
          "form": "털거의없는줄알았네요..매끈해서",
          "original_form": "털거의없는줄알았네요..매끈해서",
          "speaker_id": "1",
          "time": "20210904 15:56"
        }
      ]
