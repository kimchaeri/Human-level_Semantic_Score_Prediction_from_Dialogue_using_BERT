      "utterance": [
        {
          "id": "MDRW2100000718.1.1",
          "form": "요즘 건강 챙기려고 하고계신 거 있나요?",
          "original_form": "요즘 건강 챙기려고 하고계신 거 있나요?",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100000718.1.2",
          "form": "요즘 해가 좀 지면 자전거 타려고 해요",
          "original_form": "요즘 해가 좀 지면 자전거 타려고 해요",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100000718.1.3",
          "form": "조깅도 하고 싶은데 아직은 너무 덥더라구요",
          "original_form": "조깅도 하고 싶은데 아직은 너무 덥더라구요",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100000718.1.4",
          "form": "오 전 자전거 잘 못 타요",
          "original_form": "오 전 자전거 잘 못 타요",
          "speaker_id": "1",
          "time": "20210820 09:01"
        },
        {
          "id": "MDRW2100000718.1.5",
          "form": "직선은 잘 가는데 방향을 꺾는게 너무 힘들어요ㅠㅠㅋㅋㅋ",
          "original_form": "직선은 잘 가는데 방향을 꺾는게 너무 힘들어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:01"
        },
        {
          "id": "MDRW2100000718.1.6",
          "form": "네ㅎ 연습을 자꾸 해야해요~",
          "original_form": "네ㅎ 연습을 자꾸 해야해요~",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100000718.1.7",
          "form": "코로나때문에 헬스도 못하구 야외운동 찾게되요",
          "original_form": "코로나때문에 헬스도 못하구 야외운동 찾게되요",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100000718.1.8",
          "form": "맞아요ㅠㅠ 단지안에 헬스장이 있는데",
          "original_form": "맞아요ㅠㅠ 단지안에 헬스장이 있는데",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100000718.1.9",
          "form": "2.5단계부터 닫아서.. 안 연지 진짜 오래됐어요",
          "original_form": "2.5단계부터 닫아서.. 안 연지 진짜 오래됐어요",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100000718.1.10",
          "form": "아고 네 맞아요~",
          "original_form": "아고 네 맞아요~",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100000718.1.11",
          "form": "헬스장 가고 싶은데 도대체 언제 갈런지ㅎㅎ",
          "original_form": "헬스장 가고 싶은데 도대체 언제 갈런지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100000718.1.12",
          "form": "네 코로나가 무섭긴 무섭네요~",
          "original_form": "네 코로나가 무섭긴 무섭네요~",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100000718.1.13",
          "form": "밖에 걷거나 조깅이라도 해야죠ㅠ",
          "original_form": "밖에 걷거나 조깅이라도 해야죠ㅠ",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100000718.1.14",
          "form": "그러려고요ㅋㅋㅋ걷는거 좋아하세요?",
          "original_form": "그러려고요ㅋㅋㅋ걷는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100000718.1.15",
          "form": "네~ 걷기모임을 햇엇는데 둘레길 코스를\r\n팀으로 다녓는데 정말 서서히 몸이 회복되더라구요~",
          "original_form": "네~ 걷기모임을 햇엇는데 둘레길 코스를\r\n팀으로 다녓는데 정말 서서히 몸이 회복되더라구요~",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100000718.1.16",
          "form": "우와 좋았을 거 같아요",
          "original_form": "우와 좋았을 거 같아요",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100000718.1.17",
          "form": "몇시간 정도 걷는거예요?",
          "original_form": "몇시간 정도 걷는거예요?",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100000718.1.18",
          "form": "걷기모임 보통 하루 날잡아서 5시간은 걸은거\r\n같아요 쉬기도하고 식사도 해요~",
          "original_form": "걷기모임 보통 하루 날잡아서 5시간은 걸은거\r\n같아요 쉬기도하고 식사도 해요~",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100000718.1.19",
          "form": "우와 5시간이면 진짜 기네요",
          "original_form": "우와 5시간이면 진짜 기네요",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100000718.1.20",
          "form": "좋을 거 같아요ㅎㅎ식사도 하고",
          "original_form": "좋을 거 같아요ㅎㅎ식사도 하고",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100000718.1.21",
          "form": "네 정신과 육체가 다 건강해지는거 같아요~",
          "original_form": "네 정신과 육체가 다 건강해지는거 같아요~",
