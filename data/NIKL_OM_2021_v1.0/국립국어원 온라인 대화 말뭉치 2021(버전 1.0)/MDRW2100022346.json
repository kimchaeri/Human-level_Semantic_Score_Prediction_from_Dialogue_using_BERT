      "utterance": [
        {
          "id": "MDRW2100022346.1.1",
          "form": "튀김? 어때써요",
          "original_form": "튀김? 어때써요",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.2",
          "form": "콩튀김이요?",
          "original_form": "콩튀김이요?",
          "speaker_id": "2",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.3",
          "form": "네 콩튀김이요",
          "original_form": "네 콩튀김이요",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.4",
          "form": "되게 바삭하고 달짝지근하고 과자 같았어요",
          "original_form": "되게 바삭하고 달짝지근하고 과자 같았어요",
          "speaker_id": "2",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.5",
          "form": "그래서 그거 먹어보려고 여기저기 찾아봤는데 파는데가 없더라구요..",
          "original_form": "그래서 그거 먹어보려고 여기저기 찾아봤는데 파는데가 없더라구요..",
          "speaker_id": "2",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.6",
          "form": "음 고기보단 과자같구나",
          "original_form": "음 고기보단 과자같구나",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022346.1.7",
          "form": "사실 이름도 콩튀김이 맞는 지 모르겠어요",
          "original_form": "사실 이름도 콩튀김이 맞는 지 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.8",
          "form": "아무래도 학교식단이다보니",
          "original_form": "아무래도 학교식단이다보니",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.9",
          "form": "그거 먹으러 다시 입학해야되나 싶어요 ㅋㅋㅋ",
          "original_form": "그거 먹으러 다시 입학해야되나 싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.10",
          "form": "학교맛을 외부에서 느끼기는 쉽지않더라구요ㅋㅋ",
          "original_form": "학교맛을 외부에서 느끼기는 쉽지않더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.11",
          "form": "그정도에요?",
          "original_form": "그정도에요?",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.12",
          "form": "ㅠㅠ비슷한 메뉴 파는 곳도 못봤어요",
          "original_form": "ㅠㅠ비슷한 메뉴 파는 곳도 못봤어요",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.13",
          "form": "학교에서 개발한 메뉴인가봐요",
          "original_form": "학교에서 개발한 메뉴인가봐요",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.14",
          "form": "학교에 한번 물어보심이..",
          "original_form": "학교에 한번 물어보심이..",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.15",
          "form": "네ㅜㅜ 지금 와서 생각해보니까",
          "original_form": "네ㅜㅜ 지금 와서 생각해보니까",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022346.1.16",
          "form": "우리학교 되게 트렌디하고 선진적이였네요",
          "original_form": "우리학교 되게 트렌디하고 선진적이였네요",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100022346.1.17",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "1",
          "time": "20210829 12:54"
        }
      ]
