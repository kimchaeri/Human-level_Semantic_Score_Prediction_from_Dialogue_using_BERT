      "utterance": [
        {
          "id": "MDRW2100011583.1.1",
          "form": "요리 자주 하시나요",
          "original_form": "요리 자주 하시나요",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011583.1.2",
          "form": "가끔해요 요리잘하세요?",
          "original_form": "가끔해요 요리잘하세요?",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011583.1.3",
          "form": "잘은 모르겠고 매일해요",
          "original_form": "잘은 모르겠고 매일해요",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011583.1.4",
          "form": "자취하시는거에요? 결혼하셔서 매일하시는거에요?",
          "original_form": "자취하시는거에요? 결혼하셔서 매일하시는거에요?",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011583.1.5",
          "form": "결혼은 안했고 주로 제가 요리를 해요",
          "original_form": "결혼은 안했고 주로 제가 요리를 해요",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011583.1.6",
          "form": "네 그러시군요",
          "original_form": "네 그러시군요",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011583.1.7",
          "form": "주로 어떤요리하세요?",
          "original_form": "주로 어떤요리하세요?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011583.1.8",
          "form": "손안가는거요 라면이랑요 냉동볶음밥에 치즈 올리고 전자렌지돌리는거요 ㅋㅋ",
          "original_form": "손안가는거요 라면이랑요 냉동볶음밥에 치즈 올리고 전자렌지돌리는거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011583.1.9",
          "form": "요즘 밀키트도 잘나오더라고요",
          "original_form": "요즘 밀키트도 잘나오더라고요",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011583.1.10",
          "form": "아 그죠 밀키트좋은거같아요",
          "original_form": "아 그죠 밀키트좋은거같아요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011583.1.11",
          "form": "손질되어있어서 편해요",
          "original_form": "손질되어있어서 편해요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011583.1.12",
          "form": "네 재료비도 싸게들고요",
          "original_form": "네 재료비도 싸게들고요",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011583.1.13",
          "form": "버섯 대파 양파 감자 이런거 다 사면 비싸잖아요",
          "original_form": "버섯 대파 양파 감자 이런거 다 사면 비싸잖아요",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011583.1.14",
          "form": "한두개쓰는데 여러개 파니까..",
          "original_form": "한두개쓰는데 여러개 파니까..",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011583.1.15",
          "form": "맞아요 돈아낄수있죠",
          "original_form": "맞아요 돈아낄수있죠",
          "speaker_id": "1",
          "time": "20210824 14:28"
        }
