          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004279.1.13",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004279.1.14",
          "form": "버즈2는 그냥 .. 구냥 노티드..",
          "original_form": "버즈2는 그냥 .. 구냥 노티드..",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004279.1.15",
          "form": "그게 어딥니까...!!",
          "original_form": "그게 어딥니까...!!",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004279.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
