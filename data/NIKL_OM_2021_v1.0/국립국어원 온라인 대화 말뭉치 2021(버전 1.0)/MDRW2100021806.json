        },
        {
          "id": "MDRW2100021806.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021806.1.3",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100021806.1.4",
          "form": "놀이공원 좋아해서 오사카에 유니버셜 가봤어요",
          "original_form": "놀이공원 좋아해서 오사카에 유니버셜 가봤어요",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100021806.1.5",
          "form": "헉 거기 가려다 못갔는데",
          "original_form": "헉 거기 가려다 못갔는데",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100021806.1.6",
          "form": "되게 좋았어요 나중에 꼭 가세요",
          "original_form": "되게 좋았어요 나중에 꼭 가세요",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021806.1.7",
          "form": "코로나가 어서끝나야ㅠ",
          "original_form": "코로나가 어서끝나야ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021806.1.8",
          "form": "ㅠㅠㅠ 맞아요 국내 여행도 어렵고ㅠㅠ",
          "original_form": "ㅠㅠㅠ 맞아요 국내 여행도 어렵고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100021806.1.9",
          "form": "국내는 부산 가보고싶어요",
          "original_form": "국내는 부산 가보고싶어요",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100021806.1.10",
          "form": "부산 좋죠~~ 제주도좋고",
          "original_form": "부산 좋죠~~ 제주도좋고",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100021806.1.11",
          "form": "부산 너무 어릴 때 가봐서 바다만 생각나요",
          "original_form": "부산 너무 어릴 때 가봐서 바다만 생각나요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021806.1.12",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021806.1.13",
          "form": "부산 해동용궁사 좋았어요",
          "original_form": "부산 해동용궁사 좋았어요",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100021806.1.14",
          "form": "아 거기 알아요 오버워치 맵으로도 나왔다면서요 ㅋㅋ",
