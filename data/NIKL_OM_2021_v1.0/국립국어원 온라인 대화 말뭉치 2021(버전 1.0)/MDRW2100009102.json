      "utterance": [
        {
          "id": "MDRW2100009102.1.1",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009102.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009102.1.3",
          "form": "백숙해먹었습니나",
          "original_form": "백숙해먹었습니나",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009102.1.4",
          "form": "우와!!! 맛있는거 드셨네요",
          "original_form": "우와!!! 맛있는거 드셨네요",
          "speaker_id": "1",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009102.1.5",
          "form": "뭐드셨나요??",
          "original_form": "뭐드셨나요??",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009102.1.6",
          "form": "저는 집에서 삼겹살 구워먹었습니다!",
          "original_form": "저는 집에서 삼겹살 구워먹었습니다!",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009102.1.7",
          "form": "우왓ㅋㅋㅋㅋ",
          "original_form": "우왓ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009102.1.8",
          "form": "더 부러운데요????",
          "original_form": "더 부러운데요????",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009102.1.9",
          "form": "볶음밥까지 야무지게 볶아먹었어요",
          "original_form": "볶음밥까지 야무지게 볶아먹었어요",
          "speaker_id": "1",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009102.1.10",
          "form": "백숙 집에서 해먹기 은근 귀찮던데ㅠㅠ",
          "original_form": "백숙 집에서 해먹기 은근 귀찮던데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009102.1.11",
          "form": "그래서 밥솥으로 해요",
          "original_form": "그래서 밥솥으로 해요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009102.1.12",
          "form": "쌀이랑 닭이랑 파랑 마늘 넣고 돌려버려요",
          "original_form": "쌀이랑 닭이랑 파랑 마늘 넣고 돌려버려요",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009102.1.13",
          "form": "전기밥솥이요??",
          "original_form": "전기밥솥이요??",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009102.1.14",
          "form": "밥솥에 냄새 안배나요?",
          "original_form": "밥솥에 냄새 안배나요?",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009102.1.15",
          "form": "잘 씻어봅니다ㅋㅋㅋ",
          "original_form": "잘 씻어봅니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009102.1.16",
          "form": "괜찮은것 같아요",
          "original_form": "괜찮은것 같아요",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009102.1.17",
          "form": "오 저도 다음에 밥솥으로 해봐야겠어요",
          "original_form": "오 저도 다음에 밥솥으로 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009102.1.18",
          "form": "훨씬 간편하겠네요",
          "original_form": "훨씬 간편하겠네요",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009102.1.19",
          "form": "진짜 간단해요",
          "original_form": "진짜 간단해요",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009102.1.20",
          "form": "설거지도 편하고요",
          "original_form": "설거지도 편하고요",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009102.1.21",
          "form": "닭 먹고 죽도 해드셨나요? 저는 그 닭죽이 참 맛있더라구요!",
          "original_form": "닭 먹고 죽도 해드셨나요? 저는 그 닭죽이 참 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009102.1.22",
          "form": "원래 솥에다가 하면 그렇게 하는데요!",
          "original_form": "원래 솥에다가 하면 그렇게 하는데요!",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009102.1.23",
          "form": "밭솥에다가 하면 그냥 불린 쌀을 넣고 같이 돌려요!",
          "original_form": "밭솥에다가 하면 그냥 불린 쌀을 넣고 같이 돌려요!",
          "speaker_id": "2",
          "time": "20210822 21:23"
        }
      ]
    }
