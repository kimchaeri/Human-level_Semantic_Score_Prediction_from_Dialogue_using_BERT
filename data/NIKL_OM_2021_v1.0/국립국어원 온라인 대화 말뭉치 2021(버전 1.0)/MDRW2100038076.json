      "utterance": [
        {
          "id": "MDRW2100038076.1.1",
          "form": "그럼 쇼핑이야기나 하시죠",
          "original_form": "그럼 쇼핑이야기나 하시죠",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038076.1.2",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038076.1.3",
          "form": "최근에 사신 물건 있으신가요?",
          "original_form": "최근에 사신 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038076.1.4",
          "form": "많죠",
          "original_form": "많죠",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038076.1.5",
          "form": "제주 오면서 당근에서 다 무나하고",
          "original_form": "제주 오면서 당근에서 다 무나하고",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038076.1.6",
          "form": "이것저것 다 다시 사고있어요",
          "original_form": "이것저것 다 다시 사고있어요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038076.1.7",
          "form": "제주에서 아예 사시는건가요?",
          "original_form": "제주에서 아예 사시는건가요?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038076.1.8",
          "form": "내년 3년까지요",
          "original_form": "내년 3년까지요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038076.1.9",
          "form": "금방 세탁기 청소하려고",
          "original_form": "금방 세탁기 청소하려고",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038076.1.10",
          "form": "세탁조클이어샀어요",
          "original_form": "세탁조클이어샀어요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038076.1.11",
          "form": "그러시군요 아무래도 제주는 물가가 비싸지 않나요?",
          "original_form": "그러시군요 아무래도 제주는 물가가 비싸지 않나요?",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038076.1.12",
          "form": "네 물건이 문제가 아니라",
          "original_form": "네 물건이 문제가 아니라",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038076.1.13",
          "form": "배송료때문에 ㅠㅠ",
          "original_form": "배송료때문에 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038076.1.14",
          "form": "그쵸 물건사면 다 제주 및 산간지역 걸리니까요",
          "original_form": "그쵸 물건사면 다 제주 및 산간지역 걸리니까요",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038076.1.15",
          "form": "마트가보면 해물이 싸고 과일채소가 좀 비싸요",
          "original_form": "마트가보면 해물이 싸고 과일채소가 좀 비싸요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038076.1.16",
          "form": "오 해물 좋아하시면 좋겠네요 아무래도 지역은 해물이 좀 비싸니까요",
          "original_form": "오 해물 좋아하시면 좋겠네요 아무래도 지역은 해물이 좀 비싸니까요",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038076.1.17",
          "form": "그래서 걍 다이소에 많이가요",
          "original_form": "그래서 걍 다이소에 많이가요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038076.1.18",
          "form": "원래는 잘 안가는데",
          "original_form": "원래는 잘 안가는데",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038076.1.19",
          "form": "다이소는 일반지역과 가격이 비슷한가요?",
          "original_form": "다이소는 일반지역과 가격이 비슷한가요?",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038076.1.20",
          "form": "네.다행히",
          "original_form": "네.다행히",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038076.1.21",
          "form": "제주 핫플이예요",
          "original_form": "제주 핫플이예요",
