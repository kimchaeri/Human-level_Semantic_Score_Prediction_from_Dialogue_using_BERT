      "utterance": [
        {
          "id": "MDRW2100045203.1.1",
          "form": "여행 자주 다니시나요??",
          "original_form": "여행 자주 다니시나요??",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045203.1.2",
          "form": "음 저는 사실 집순이에요 ㅋㅋㅋ",
          "original_form": "음 저는 사실 집순이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045203.1.3",
          "form": "집이 좋아요 그치만 가끔 여행은 가죠",
          "original_form": "집이 좋아요 그치만 가끔 여행은 가죠",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045203.1.4",
          "form": "아무래도 여행은 바다?~라구 생각합니다",
          "original_form": "아무래도 여행은 바다?~라구 생각합니다",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045203.1.5",
          "form": "바다 좋죠ㅠㅠ",
          "original_form": "바다 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045203.1.6",
          "form": "아직 봄에 바다보러 가본적은 없는데 바다는 언제가도 좋은 것같아요",
          "original_form": "아직 봄에 바다보러 가본적은 없는데 바다는 언제가도 좋은 것같아요",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045203.1.7",
          "form": "봄에요? 저도 봄에는 안가본 아 가봤당",
          "original_form": "봄에요? 저도 봄에는 안가본 아 가봤당",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045203.1.8",
          "form": "ㅋㅋㅋㅋㅋ갑자기 생각났어요 엠티때 갔었네요",
          "original_form": "ㅋㅋㅋㅋㅋ갑자기 생각났어요 엠티때 갔었네요",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045203.1.9",
          "form": "오! 엠티로 바다라니 부럽네요!!",
          "original_form": "오! 엠티로 바다라니 부럽네요!!",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045203.1.10",
          "form": "엠티는 어떠셨어요??",
          "original_form": "엠티는 어떠셨어요??",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045203.1.11",
          "form": "ㅋㅋㅋ흑역사죠 뭐..",
          "original_form": "ㅋㅋㅋ흑역사죠 뭐..",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045203.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ좋았는데 지금 생각하면 흑역사만 생각나옄ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋았는데 지금 생각하면 흑역사만 생각나옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045203.1.13",
          "form": "ㅋㅋㅋㅋㅋ 그럴수 있죠!",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴수 있죠!",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045203.1.14",
          "form": "저는 흑역사고 자시고 할 그런 엠티가 아니었어섴ㅋㅋ큐ㅠㅠ",
          "original_form": "저는 흑역사고 자시고 할 그런 엠티가 아니었어섴ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045203.1.15",
          "form": "수학여행같은 엠티였거든욬ㅋㅋㅋ",
          "original_form": "수학여행같은 엠티였거든욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045203.1.16",
          "form": "아아 저는 대학생땤ㅋㅋㅋ 1학년 엠티때 바다 갔었어요",
          "original_form": "아아 저는 대학생땤ㅋㅋㅋ 1학년 엠티때 바다 갔었어요",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045203.1.17",
          "form": "바다가 참 이뻤죠",
          "original_form": "바다가 참 이뻤죠",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045203.1.18",
          "form": "저는 어딘지 모를 허브농장인가? 거기갔다기 교수님께서 여기는 뭐고 저기는 뭐고 뭐가 어떻고....",
          "original_form": "저는 어딘지 모를 허브농장인가? 거기갔다기 교수님께서 여기는 뭐고 저기는 뭐고 뭐가 어떻고....",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045203.1.19",
          "form": "정말 학창시절 수학여행처럼 공부목적으로 간 엠티 같은 거였어욬ㅋㅋㅋㅋㅋ",
          "original_form": "정말 학창시절 수학여행처럼 공부목적으로 간 엠티 같은 거였어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045203.1.20",
          "form": "잉ㅋㅋㅋ 허브농장이요? 되게 건전하네욬ㅋㅋㅋ",
          "original_form": "잉ㅋㅋㅋ 허브농장이요? 되게 건전하네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045203.1.21",
          "form": "교수님이 일정 짜셨나봐요ㅠㅠ",
          "original_form": "교수님이 일정 짜셨나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045203.1.22",
          "form": "그런가봐욬ㅋㅋㅋㅋ 제가 생각한 엠티는 그런게 아니었는데 말이에욬ㅋㅋㅋ",
          "original_form": "그런가봐욬ㅋㅋㅋㅋ 제가 생각한 엠티는 그런게 아니었는데 말이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045203.1.23",
          "form": "ㅋㅋㅋㅋ잊지 못할 추억이네여 정말",
          "original_form": "ㅋㅋㅋㅋ잊지 못할 추억이네여 정말",
          "speaker_id": "1",
          "time": "20210908 01:35"
        }
      ]
    }
