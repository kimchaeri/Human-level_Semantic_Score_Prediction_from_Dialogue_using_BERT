      "utterance": [
        {
          "id": "MDRW2100037990.1.1",
          "form": "근데 너 언제 잘거니??",
          "original_form": "근데 너 언제 잘거니??",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037990.1.2",
          "form": "조금이따가 자려고 ㅋㅋ",
          "original_form": "조금이따가 자려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037990.1.3",
          "form": "언니 자나?",
          "original_form": "언니 자나?",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037990.1.4",
          "form": "아니 나 이제 씻고 오려고",
          "original_form": "아니 나 이제 씻고 오려고",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037990.1.5",
          "form": "아직씻지도 못했다",
          "original_form": "아직씻지도 못했다",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037990.1.6",
          "form": "헉 그래 언니 씻고 와서",
          "original_form": "헉 그래 언니 씻고 와서",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037990.1.7",
          "form": "어여 자거랍",
          "original_form": "어여 자거랍",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037990.1.8",
          "form": "그래 니도 오늘 수고했다",
          "original_form": "그래 니도 오늘 수고했다",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037990.1.9",
          "form": "신랑 오면 둘째 맡기고 어여 쉬어",
          "original_form": "신랑 오면 둘째 맡기고 어여 쉬어",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037990.1.10",
          "form": "응응 언니도 어여 자 ㅋㅋ잘자",
          "original_form": "응응 언니도 어여 자 ㅋㅋ잘자",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037990.1.11",
          "form": "옹 빠잉",
          "original_form": "옹 빠잉",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037990.1.12",
          "form": "일어나라",
          "original_form": "일어나라",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100037990.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 아침이 밝았구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아침이 밝았구나",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100037990.1.14",
          "form": "얍얍 벌써일어나서 애들보내고왔지 히히",
          "original_form": "얍얍 벌써일어나서 애들보내고왔지 히히",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100037990.1.15",
          "form": "언니는 출근했나ㅋ",
          "original_form": "언니는 출근했나ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100037990.1.16",
          "form": "오늘은 좀 쉬어라",
          "original_form": "오늘은 좀 쉬어라",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.17",
          "form": "난 출근했지",
          "original_form": "난 출근했지",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.18",
          "form": "어제 신랑이 둘째 재웠나??",
          "original_form": "어제 신랑이 둘째 재웠나??",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ응응 둘째재움",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ응응 둘째재움",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.20",
          "form": "아 진짜 나가야되겠다. 어디든",
          "original_form": "아 진짜 나가야되겠다. 어디든",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.21",
          "form": "ㅋㅋㅋㅋㅋ 결국 신랑에게 돌아갔군",
          "original_form": "ㅋㅋㅋㅋㅋ 결국 신랑에게 돌아갔군",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.22",
          "form": "안되겠다 매일 재우기힘드니",
          "original_form": "안되겠다 매일 재우기힘드니",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.23",
          "form": "둘째 데리고??",
          "original_form": "둘째 데리고??",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.24",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100037990.1.25",
          "form": "하원하면",
          "original_form": "하원하면",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100037990.1.26",
          "form": "그래 놀이터에서 1 시간이라도 굴리고 와",
          "original_form": "그래 놀이터에서 1 시간이라도 굴리고 와",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100037990.1.27",
          "form": "1시간가지고 안될것같아.",
          "original_form": "1시간가지고 안될것같아.",
          "speaker_id": "1",
          "time": "20210902 09:33"
        }
      ]
    }
