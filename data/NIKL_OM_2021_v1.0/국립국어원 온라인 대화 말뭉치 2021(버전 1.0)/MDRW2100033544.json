      "utterance": [
        {
          "id": "MDRW2100033544.1.1",
          "form": "지금 점심으로 라면먹는데요",
          "original_form": "지금 점심으로 라면먹는데요",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100033544.1.2",
          "form": "안성탕면요",
          "original_form": "안성탕면요",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100033544.1.3",
          "form": "어떤라면 좋아하세요?",
          "original_form": "어떤라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100033544.1.4",
          "form": "오 저랑 같으세요",
          "original_form": "오 저랑 같으세요",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033544.1.5",
          "form": "저희집도 안성탕면이요",
          "original_form": "저희집도 안성탕면이요",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033544.1.6",
          "form": "아 구수하고 맛나요",
          "original_form": "아 구수하고 맛나요",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033544.1.7",
          "form": "애들도 안매워서 잘먹죠?",
          "original_form": "애들도 안매워서 잘먹죠?",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100033544.1.8",
          "form": "라면엔 계란 넣으세요?",
          "original_form": "라면엔 계란 넣으세요?",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.9",
          "form": "전 오늘은 넣어서 먹었어요 ㅎㅎ",
          "original_form": "전 오늘은 넣어서 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.10",
          "form": "애들은 짜파게티",
          "original_form": "애들은 짜파게티",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.11",
          "form": "저는 넣는거 좋아해요",
          "original_form": "저는 넣는거 좋아해요",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.12",
          "form": "다른분들은 신라면 좋아하시는거같던데",
          "original_form": "다른분들은 신라면 좋아하시는거같던데",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.13",
          "form": "그렇죠 좀있음 안성탕면 먹는다 할거예요",
          "original_form": "그렇죠 좀있음 안성탕면 먹는다 할거예요",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.14",
          "form": "제입맛엔 안성이요 ㅎ",
          "original_form": "제입맛엔 안성이요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.15",
          "form": "전 안좋아해요 ㅋㅋ",
          "original_form": "전 안좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.16",
          "form": "라면은 안성이죠",
          "original_form": "라면은 안성이죠",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100033544.1.17",
          "form": "계란을 넣은 라면 레시피 선호합니다 ㅎㅎ",
          "original_form": "계란을 넣은 라면 레시피 선호합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.18",
          "form": "신랑이 라면 잘끓여서 주말에 오면 한번씩 끓여달라해요",
          "original_form": "신랑이 라면 잘끓여서 주말에 오면 한번씩 끓여달라해요",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.19",
          "form": "신랑은 파도 넣더라구요",
          "original_form": "신랑은 파도 넣더라구요",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.20",
          "form": "오 전 꼬들파입니다",
          "original_form": "오 전 꼬들파입니다",
          "speaker_id": "2",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.21",
          "form": "저도 꼬들꼬들요",
          "original_form": "저도 꼬들꼬들요",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.22",
          "form": "맞아요 파 콩나물 고추가루 살짝해도 맛나요 ㅎㅎ",
          "original_form": "맞아요 파 콩나물 고추가루 살짝해도 맛나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.23",
          "form": "퍼진거 노노 입니다",
          "original_form": "퍼진거 노노 입니다",
          "speaker_id": "2",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100033544.1.24",
          "form": "근데 저는 제가 끓이면 꼬들이 안되더라구요 ㅋ",
          "original_form": "근데 저는 제가 끓이면 꼬들이 안되더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.25",
          "form": "저희 아들은 맨날 라면을 먹으려해요",
          "original_form": "저희 아들은 맨날 라면을 먹으려해요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.26",
          "form": "지난주엔 떡넣어달랬더니 거절당했어요 ㅋㅋ",
          "original_form": "지난주엔 떡넣어달랬더니 거절당했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.27",
          "form": "전 살짝 덜익었을때 불끄면 차리면 딱 꼬들해요 ㅎㅎ",
          "original_form": "전 살짝 덜익었을때 불끄면 차리면 딱 꼬들해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.28",
          "form": "헉 거절을 너무해요",
          "original_form": "헉 거절을 너무해요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.29",
          "form": "저도 떡라면 좋아해요",
          "original_form": "저도 떡라면 좋아해요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033544.1.30",
          "form": "휴게소에서 떡라면 먹어요 ㅋㅋ",
          "original_form": "휴게소에서 떡라면 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.31",
          "form": "덜익었을때...불끄기",
          "original_form": "덜익었을때...불끄기",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.32",
          "form": "기억해야겠어요 ㅎ",
          "original_form": "기억해야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.33",
          "form": "네 살짝 그럴때요 ㅎㅎ",
          "original_form": "네 살짝 그럴때요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.34",
          "form": "휴게소에서 떡라면 비오는날이면 더좋아요 ㅎ",
          "original_form": "휴게소에서 떡라면 비오는날이면 더좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.35",
          "form": "네 넘좋죠 요럴때 사먹음 맛나죠",
          "original_form": "네 넘좋죠 요럴때 사먹음 맛나죠",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100033544.1.36",
          "form": "축축함도 잊을수 있죠 ㅎㅎ",
          "original_form": "축축함도 잊을수 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100033544.1.37",
          "form": "아쉬운건 휴게소는 신라면 이예요 ㅜㅜ",
          "original_form": "아쉬운건 휴게소는 신라면 이예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100033544.1.38",
          "form": "다 계약을 그렇게 했나봐요",
          "original_form": "다 계약을 그렇게 했나봐요",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100033544.1.39",
          "form": "아 신라면이군요",
          "original_form": "아 신라면이군요",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
