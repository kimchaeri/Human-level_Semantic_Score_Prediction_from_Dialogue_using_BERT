      "utterance": [
        {
          "id": "MDRW2100042168.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042168.1.2",
          "form": "여행좋아해요!!",
          "original_form": "여행좋아해요!!",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042168.1.3",
          "form": "코로나전엔 정말 많이다녔는데ㅜㅜ",
          "original_form": "코로나전엔 정말 많이다녔는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042168.1.4",
          "form": "해외도 많이 가셨죠?",
          "original_form": "해외도 많이 가셨죠?",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042168.1.5",
          "form": "여러군데가본건아닌데 몇번 가긴했어요!",
          "original_form": "여러군데가본건아닌데 몇번 가긴했어요!",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042168.1.6",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042168.1.7",
          "form": "호주 브리즈번 좋았어요!",
          "original_form": "호주 브리즈번 좋았어요!",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042168.1.8",
          "form": "미세먼지걱정없는게 진짜 좋더라구요ㅋㅋ",
          "original_form": "미세먼지걱정없는게 진짜 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042168.1.9",
          "form": "아 호주 가보고싶어요! 며칠다녀오셨어요?",
          "original_form": "아 호주 가보고싶어요! 며칠다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042168.1.10",
          "form": "일주일정도있었어요!",
          "original_form": "일주일정도있었어요!",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042168.1.11",
          "form": "우리나라랑 계절이 반대라 신기하더라구요ㅋㅋ",
          "original_form": "우리나라랑 계절이 반대라 신기하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042168.1.12",
          "form": "자유요?",
          "original_form": "자유요?",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042168.1.13",
          "form": "아 친구가있어서",
          "original_form": "아 친구가있어서",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042168.1.14",
          "form": "여름의 성탄절",
          "original_form": "여름의 성탄절",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042168.1.15",
          "form": "자유로다녔어요!",
          "original_form": "자유로다녔어요!",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042168.1.16",
          "form": "맞아요그쵸ㅋㅋ",
          "original_form": "맞아요그쵸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042168.1.17",
          "form": "유월에가서 거긴 겨울이었어요!",
          "original_form": "유월에가서 거긴 겨울이었어요!",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042168.1.18",
          "form": "친구있음 너무 좋아요!",
          "original_form": "친구있음 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210906 20:25"
        }
