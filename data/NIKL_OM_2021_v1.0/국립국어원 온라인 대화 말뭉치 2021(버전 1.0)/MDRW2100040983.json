      "utterance": [
        {
          "id": "MDRW2100040983.1.1",
          "form": "유투브 즐겨보세요?",
          "original_form": "유투브 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040983.1.2",
          "form": "네 유튜브 거의 매일보죠!\r\n자주 보시나요?",
          "original_form": "네 유튜브 거의 매일보죠!\r\n자주 보시나요?",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040983.1.3",
          "form": "저는 주로 샤워할때나 지루할때봐용 매일보진않구용 !",
          "original_form": "저는 주로 샤워할때나 지루할때봐용 매일보진않구용 !",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100040983.1.4",
          "form": "오 샤워할때는 어떤거보세요?",
          "original_form": "오 샤워할때는 어떤거보세요?",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100040983.1.5",
          "form": "보통 예능프로용 @",
          "original_form": "보통 예능프로용 @",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100040983.1.6",
          "form": "아하! 샤워하면서 보면 안심심하겠네요",
          "original_form": "아하! 샤워하면서 보면 안심심하겠네요",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040983.1.7",
          "form": "네네 추천해주실 드라마있나요?",
          "original_form": "네네 추천해주실 드라마있나요?",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100040983.1.8",
          "form": "저는 최근에 마인이랑 너는나의봄 재밌게 봤어요!",
          "original_form": "저는 최근에 마인이랑 너는나의봄 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100040983.1.9",
          "form": "헉 저랑 취향이 같으시네요",
          "original_form": "헉 저랑 취향이 같으시네요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100040983.1.10",
          "form": "저두 둘다 재밌게 봤거든요 ㅋㅋ",
          "original_form": "저두 둘다 재밌게 봤거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100040983.1.11",
          "form": "주변에 너는나의봄 보시는분은 없던뎅",
          "original_form": "주변에 너는나의봄 보시는분은 없던뎅",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100040983.1.12",
          "form": "오 진짜요? 둘다보셨군요!\r\n마인은 처음에 그냥 그런 드라마인줄 알았는데",
          "original_form": "오 진짜요? 둘다보셨군요!\r\n마인은 처음에 그냥 그런 드라마인줄 알았는데",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.13",
          "form": "여성중심서사가 너무 좋더라구요ㅠ",
          "original_form": "여성중심서사가 너무 좋더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.14",
          "form": "맞아용 ㅠㅠ",
          "original_form": "맞아용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.15",
          "form": "마인 갈수록 집중잘되고",
          "original_form": "마인 갈수록 집중잘되고",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.16",
          "form": "넘 잼나게봣어여",
          "original_form": "넘 잼나게봣어여",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.17",
          "form": "인생드라마 있으신가용?",
          "original_form": "인생드라마 있으신가용?",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.18",
          "form": "마자요 진짜 너무재밌더라구요",
          "original_form": "마자요 진짜 너무재밌더라구요",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.19",
          "form": "지금은 거의 마인인거같아요ㅋㅋ",
          "original_form": "지금은 거의 마인인거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100040983.1.20",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100040983.1.21",
          "form": "흠 저는 오나의귀신님이여 ㅠㅋㅋ",
          "original_form": "흠 저는 오나의귀신님이여 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100040983.1.22",
          "form": "박보영 팬이거든용",
          "original_form": "박보영 팬이거든용",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100040983.1.23",
          "form": "아 맞아요? 오나귀도 재밌었죠ㅠ",
          "original_form": "아 맞아요? 오나귀도 재밌었죠ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100040983.1.24",
          "form": "박보영 팬이시군요!",
          "original_form": "박보영 팬이시군요!",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100040983.1.25",
          "form": "너무 귀여웠죠 박보영 ㅠㅠ",
          "original_form": "너무 귀여웠죠 박보영 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100040983.1.26",
          "form": "마자요 진짜 연기 너무 잘하시더라구요",
          "original_form": "마자요 진짜 연기 너무 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100040983.1.27",
          "form": "것도 그런데 워낙",
          "original_form": "것도 그런데 워낙",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100040983.1.28",
          "form": "로맨스류를 좋아해요!",
          "original_form": "로맨스류를 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100040983.1.29",
          "form": "님은영 ?",
          "original_form": "님은영 ?",
          "speaker_id": "1",
          "time": "20210905 22:46"
        }
      ]
    }
