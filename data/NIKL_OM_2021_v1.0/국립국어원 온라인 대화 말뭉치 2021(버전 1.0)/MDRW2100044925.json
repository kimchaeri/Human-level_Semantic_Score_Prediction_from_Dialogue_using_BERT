      "utterance": [
        {
          "id": "MDRW2100044925.1.1",
          "form": "유니버셜도 가보셨나요?",
          "original_form": "유니버셜도 가보셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044925.1.2",
          "form": "네. 진심 존잼",
          "original_form": "네. 진심 존잼",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100044925.1.3",
          "form": "저 거기 해리포터랑 스파이더맨이 젤 좋았어요",
          "original_form": "저 거기 해리포터랑 스파이더맨이 젤 좋았어요",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100044925.1.4",
          "form": "와, 다들 똑같구나",
          "original_form": "와, 다들 똑같구나",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044925.1.5",
          "form": "저도 그거  2개가 진짜 재밌었어요",
          "original_form": "저도 그거  2개가 진짜 재밌었어요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044925.1.6",
          "form": "ㅋㅋㅋㅋㅋ그런가봐욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그런가봐욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044925.1.7",
          "form": "저 해리포터 타다가 중간에 멈춰서ㅋㅋㅋㅋㅋ",
          "original_form": "저 해리포터 타다가 중간에 멈춰서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100044925.1.8",
          "form": "내리니까 바로 다시 태워주더라고요",
          "original_form": "내리니까 바로 다시 태워주더라고요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100044925.1.9",
          "form": "어머 그랬어요? 그게 고장도 나는군요",
          "original_form": "어머 그랬어요? 그게 고장도 나는군요",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100044925.1.10",
          "form": "별일 없어서 다행이에요",
          "original_form": "별일 없어서 다행이에요",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100044925.1.11",
          "form": "그러니까요 매달려서 심심해하기만 해서 다행이지요ㅋㅋㅋ",
          "original_form": "그러니까요 매달려서 심심해하기만 해서 다행이지요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100044925.1.12",
          "form": "그럼 거기서 버터맥주도 드셨어요?",
          "original_form": "그럼 거기서 버터맥주도 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100044925.1.13",
          "form": "아니오 맛 없대서ㅋㅋㅋㅋ안 먹어썽ㅅ",
          "original_form": "아니오 맛 없대서ㅋㅋㅋㅋ안 먹어썽ㅅ",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100044925.1.14",
          "form": "저도 맛없다는 소리 들었지만 궁금해서 먹어봤습니다 ㅎㅎ",
          "original_form": "저도 맛없다는 소리 들었지만 궁금해서 먹어봤습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100044925.1.15",
          "form": "겁나 엄청 어마무시하게 달아요",
          "original_form": "겁나 엄청 어마무시하게 달아요",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100044925.1.16",
          "form": "단맛강한 밀크티파우더를 엄청 찐하게 타면 비슷할듯요",
          "original_form": "단맛강한 밀크티파우더를 엄청 찐하게 타면 비슷할듯요",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100044925.1.17",
          "form": "어우... 버터맥주컵으로 드셨나요?",
          "original_form": "어우... 버터맥주컵으로 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100044925.1.18",
          "form": "네... ㅎㅎ",
          "original_form": "네... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100044925.1.19",
          "form": "한 세모금먹고 버렸어요",
          "original_form": "한 세모금먹고 버렸어요",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100044925.1.20",
          "form": "컵은 집에 잘 있습니다ㅋㅋ",
          "original_form": "컵은 집에 잘 있습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100044925.1.21",
          "form": "ㅋㅋㅋㅋㅋ기념품 사신 거네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ기념품 사신 거네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100044925.1.22",
          "form": "예,  그렇죠ㅋㅋㅋㅋㅋ",
          "original_form": "예,  그렇죠ㅋㅋㅋㅋㅋ",
