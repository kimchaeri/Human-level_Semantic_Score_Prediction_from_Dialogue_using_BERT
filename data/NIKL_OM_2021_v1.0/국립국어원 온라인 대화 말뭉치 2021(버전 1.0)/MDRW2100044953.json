        },
        {
          "id": "MDRW2100044953.1.2",
          "form": "6화!!",
          "original_form": "6화!!",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.3",
          "form": "한참 봐야하네ㅋ",
          "original_form": "한참 봐야하네ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.4",
          "form": "나 회사 권력뭐이런거는 스킵스킵하는중",
          "original_form": "나 회사 권력뭐이런거는 스킵스킵하는중",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.5",
          "form": "결과 알고봐?",
          "original_form": "결과 알고봐?",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.6",
          "form": "ㅇㅇ보다가 재미없음 하차하려구",
          "original_form": "ㅇㅇ보다가 재미없음 하차하려구",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.7",
          "form": "세상은 넓고 볼건많다",
          "original_form": "세상은 넓고 볼건많다",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044953.1.8",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044953.1.9",
          "form": "난 보다갸 하차가 잘안돼더라",
          "original_form": "난 보다갸 하차가 잘안돼더라",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044953.1.10",
          "form": "꾸엮꾸역 보게됨",
          "original_form": "꾸엮꾸역 보게됨",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044953.1.11",
          "form": "일전에도 그런 드라마가 있었는데",
          "original_form": "일전에도 그런 드라마가 있었는데",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044953.1.12",
          "form": "음 뭔지 기억날듯 잊었너",
          "original_form": "음 뭔지 기억날듯 잊었너",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.13",
          "form": "애들외모는좋았는데",
          "original_form": "애들외모는좋았는데",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.14",
          "form": "이거 한번 안보면 안보게됨 ㅋㅋ",
          "original_form": "이거 한번 안보면 안보게됨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.15",
          "form": "궁금해도 지루할때 끊어야함",
          "original_form": "궁금해도 지루할때 끊어야함",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.16",
          "form": "그러고나면안보고싶어져",
          "original_form": "그러고나면안보고싶어져",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.17",
          "form": "그게 진짜 난 잘안돼더라",
          "original_form": "그게 진짜 난 잘안돼더라",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044953.1.18",
          "form": "저번에도 완결까지 다본듯",
          "original_form": "저번에도 완결까지 다본듯",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044953.1.19",
          "form": "뭐 봤었어?",
          "original_form": "뭐 봤었어?",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044953.1.20",
          "form": "배우들 미모로 본듯ㅋ",
          "original_form": "배우들 미모로 본듯ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044953.1.21",
          "form": "아 안다 달의연인",
          "original_form": "아 안다 달의연인",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044953.1.22",
          "form": "달연 그래도 재밌지않았나? 그거 진짜 세계적으로 인기많더라 ㅋㅋ",
          "original_form": "달연 그래도 재밌지않았나? 그거 진짜 세계적으로 인기많더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044953.1.23",
          "form": "한국에선ㅇ방영당시 연출로욕 대박먹음",
          "original_form": "한국에선ㅇ방영당시 연출로욕 대박먹음",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044953.1.24",
          "form": "아니 난 진짜 별로였어\n배우들이 너무너무 아깝더라",
          "original_form": "아니 난 진짜 별로였어\n배우들이 너무너무 아깝더라",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044953.1.25",
          "form": "진짜 혹평 줄꺼야",
          "original_form": "진짜 혹평 줄꺼야",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044953.1.26",
          "form": "맞나 ㅋㅋ 난 안보길 잘했네",
          "original_form": "맞나 ㅋㅋ 난 안보길 잘했네",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044953.1.27",
          "form": "내 배우였음 속상해서 화났을듯ㅋ",
          "original_form": "내 배우였음 속상해서 화났을듯ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044953.1.28",
          "form": "엉 한국꺼 보지마",
          "original_form": "엉 한국꺼 보지마",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044953.1.29",
          "form": "아이유나오는 호텔데루나 너 좋아할거같다",
          "original_form": "아이유나오는 호텔데루나 너 좋아할거같다",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044953.1.30",
          "form": "나도 들어서 안보다 우연히 접하게돼서 봤는데 비추임",
          "original_form": "나도 들어서 안보다 우연히 접하게돼서 봤는데 비추임",
          "speaker_id": "1",
          "time": "20210908 00:27"
        }
      ]
    }
