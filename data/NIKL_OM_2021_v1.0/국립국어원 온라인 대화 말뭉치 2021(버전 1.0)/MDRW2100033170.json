      "utterance": [
        {
          "id": "MDRW2100033170.1.1",
          "form": "피자헛 엄청 오랜만에 들어 봐요",
          "original_form": "피자헛 엄청 오랜만에 들어 봐요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033170.1.2",
          "form": "저도 오랜만에 시켜 먹어 봤어요",
          "original_form": "저도 오랜만에 시켜 먹어 봤어요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033170.1.3",
          "form": "어디서 주문하셨어요",
          "original_form": "어디서 주문하셨어요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.4",
          "form": "배민에서 주문했는데 오늘 오천 원 할인해 주더라고요",
          "original_form": "배민에서 주문했는데 오늘 오천 원 할인해 주더라고요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.5",
          "form": "많이 할인해 주네요 그럼 얼마예요",
          "original_form": "많이 할인해 주네요 그럼 얼마예요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.6",
          "form": "2만 원에 얇은 피자 두 판 배달되었어요",
          "original_form": "2만 원에 얇은 피자 두 판 배달되었어요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.7",
          "form": "몇 명이서 드신 거예요",
          "original_form": "몇 명이서 드신 거예요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.8",
          "form": "세 명이서 먹으니까 딱 맞던데요",
          "original_form": "세 명이서 먹으니까 딱 맞던데요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.9",
          "form": "피자헛이 맛이 괜찮은가요",
          "original_form": "피자헛이 맛이 괜찮은가요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.10",
          "form": "고르곤졸라 피자랑 치킨 피자를 시켰어요",
          "original_form": "고르곤졸라 피자랑 치킨 피자를 시켰어요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.11",
          "form": "익숙한 맛있겠네요",
          "original_form": "익숙한 맛있겠네요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033170.1.12",
          "form": "튀김 피자에 마늘 슬라이스가 듬뿍 들어 있어서 느끼함을 잡아주었어요",
          "original_form": "튀김 피자에 마늘 슬라이스가 듬뿍 들어 있어서 느끼함을 잡아주었어요",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100033170.1.13",
          "form": "저도 한번 시켜 먹고 싶네요",
          "original_form": "저도 한번 시켜 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210906 17:56"
        }
