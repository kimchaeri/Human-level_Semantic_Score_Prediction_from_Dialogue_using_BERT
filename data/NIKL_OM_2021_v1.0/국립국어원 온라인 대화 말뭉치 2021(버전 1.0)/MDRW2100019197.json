        },
        {
          "id": "MDRW2100019197.1.9",
          "form": "편한듯하면서도 힘든ㅠ",
          "original_form": "편한듯하면서도 힘든ㅠ",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019197.1.10",
