      "utterance": [
        {
          "id": "MDRW2100043036.1.1",
          "form": "그게 잘 포장해서 창고에 보관했더니",
          "original_form": "그게 잘 포장해서 창고에 보관했더니",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043036.1.2",
          "form": "고장은 안나서 팔 수 있었어요",
          "original_form": "고장은 안나서 팔 수 있었어요",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043036.1.3",
          "form": "은근히 그런 예전 가전을 찾는 사람들이 있더라구요",
          "original_form": "은근히 그런 예전 가전을 찾는 사람들이 있더라구요",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043036.1.4",
          "form": "아 그런 꿀팁이!!",
          "original_form": "아 그런 꿀팁이!!",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043036.1.5",
          "form": "그래서 요즘엔 박스 잘안버리더라고요",
          "original_form": "그래서 요즘엔 박스 잘안버리더라고요",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043036.1.6",
          "form": "네네 저희도 그래요",
          "original_form": "네네 저희도 그래요",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043036.1.7",
          "form": "박스까지 팔면 돈을 더 받더라구요",
          "original_form": "박스까지 팔면 돈을 더 받더라구요",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043036.1.8",
          "form": "유념해야겠네요!",
          "original_form": "유념해야겠네요!",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043036.1.9",
          "form": "전 언박싱 후 바로ㅜㅠ",
          "original_form": "전 언박싱 후 바로ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043036.1.10",
          "form": "예전엔 그랬는데 중고거래를 알고 난 뒤로는 안버려요",
          "original_form": "예전엔 그랬는데 중고거래를 알고 난 뒤로는 안버려요",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043036.1.11",
          "form": "장은 자주 보시나요?",
          "original_form": "장은 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043036.1.12",
          "form": "나중에 중고로 팔 생각에",
          "original_form": "나중에 중고로 팔 생각에",
          "speaker_id": "1",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043036.1.13",
          "form": "장은 일주일에 한번정도?",
          "original_form": "장은 일주일에 한번정도?",
          "speaker_id": "1",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043036.1.14",
          "form": "시장이나 마트 가요",
          "original_form": "시장이나 마트 가요",
          "speaker_id": "1",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043036.1.15",
          "form": "아 전 엄마가 메인음식 사시면 사이드 재료 주문해요",
          "original_form": "아 전 엄마가 메인음식 사시면 사이드 재료 주문해요",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100043036.1.16",
          "form": "인터넷으로ㅋㅋ",
          "original_form": "인터넷으로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100043036.1.17",
          "form": "인터넷을 잘 사용하는군요",
          "original_form": "인터넷을 잘 사용하는군요",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043036.1.18",
          "form": "전 아직도 어색해서 오프라인으로 장 봅니다",
          "original_form": "전 아직도 어색해서 오프라인으로 장 봅니다",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043036.1.19",
          "form": "근데 확실히 보고사는게 최고예요",
          "original_form": "근데 확실히 보고사는게 최고예요",
          "speaker_id": "2",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043036.1.20",
          "form": "특히 과일이요",
          "original_form": "특히 과일이요",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043036.1.21",
          "form": "그쵸 전 직접 보는게 아직까진 마음이 편해요",
          "original_form": "그쵸 전 직접 보는게 아직까진 마음이 편해요",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043036.1.22",
          "form": "과일 잘못 사면 쓰레기 돈주고 산거니까ㅜ",
          "original_form": "과일 잘못 사면 쓰레기 돈주고 산거니까ㅜ",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043036.1.23",
          "form": "그러게요.. 박스로 사면 또 밑에까지 확인이 어려우니까 그것도 힘들구요",
          "original_form": "그러게요.. 박스로 사면 또 밑에까지 확인이 어려우니까 그것도 힘들구요",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043036.1.24",
          "form": "낱개로 사는게 좋긴 한데 박스가 싸서 항상 고민이랍니다",
          "original_form": "낱개로 사는게 좋긴 한데 박스가 싸서 항상 고민이랍니다",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043036.1.25",
          "form": "저흰 5인가족이라 그냥 박스로 사서 먹어요ㅋㅋ",
          "original_form": "저흰 5인가족이라 그냥 박스로 사서 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:38"
        }
      ]
    }
