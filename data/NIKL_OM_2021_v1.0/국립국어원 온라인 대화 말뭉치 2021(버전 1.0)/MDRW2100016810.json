      "utterance": [
        {
          "id": "MDRW2100016810.1.1",
          "form": "name2님은 반려동물 있으신가요?",
          "original_form": "&name2&님은 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016810.1.2",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016810.1.3",
          "form": "키우고 싶거나 좋아하는 동물 있으신가요?",
          "original_form": "키우고 싶거나 좋아하는 동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016810.1.4",
          "form": "동물을 막 좋아하진않아서요",
          "original_form": "동물을 막 좋아하진않아서요",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016810.1.5",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016810.1.6",
          "form": "넹 전 동물 좋아해요",
          "original_form": "넹 전 동물 좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100016810.1.7",
          "form": "그럼 고양이 카페같은 곳은 잘 안가보셨겠네요",
          "original_form": "그럼 고양이 카페같은 곳은 잘 안가보셨겠네요",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100016810.1.8",
          "form": "네네 동물키우고싶으시겠어요",
          "original_form": "네네 동물키우고싶으시겠어요",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100016810.1.9",
          "form": "어릴땐 무조건 키우고 싶었는데",
          "original_form": "어릴땐 무조건 키우고 싶었는데",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100016810.1.10",
          "form": "크니까 키우는게 쉬운 일이 아닌걸 알고",
          "original_form": "크니까 키우는게 쉬운 일이 아닌걸 알고",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100016810.1.11",
          "form": "그냥 귀여워만 하고 있어요...ㅋㅋㅋㅋ",
          "original_form": "그냥 귀여워만 하고 있어요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100016810.1.12",
          "form": "ㅋㅋㅋㅋ길가다 고양이보거나 하면 넘 좋겠어여",
          "original_form": "ㅋㅋㅋㅋ길가다 고양이보거나 하면 넘 좋겠어여",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100016810.1.13",
          "form": "헋 그 순간 30분은 순삭이죠",
          "original_form": "헋 그 순간 30분은 순삭이죠",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100016810.1.14",
          "form": "ㅋㅋㅋㅋ고양이말고는 없어요? 좋아하는동물",
          "original_form": "ㅋㅋㅋㅋ고양이말고는 없어요? 좋아하는동물",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016810.1.15",
          "form": "강아지도 정말 좋아해요",
          "original_form": "강아지도 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016810.1.16",
          "form": "도마뱀도 귀여우면 좋아해요 동물 거의다 좋아해서요ㅎㅎ",
          "original_form": "도마뱀도 귀여우면 좋아해요 동물 거의다 좋아해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100016810.1.17",
          "form": "오 신기해용 ㅎㅎ",
          "original_form": "오 신기해용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100016810.1.18",
          "form": "요즘은 앵무새가 흥미롭게 보여요",
          "original_form": "요즘은 앵무새가 흥미롭게 보여요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016810.1.19",
          "form": "유튜브 알고리즘으로 앵무새 키우는 유튜버를 봤는데 재밌더라구요",
          "original_form": "유튜브 알고리즘으로 앵무새 키우는 유튜버를 봤는데 재밌더라구요",
