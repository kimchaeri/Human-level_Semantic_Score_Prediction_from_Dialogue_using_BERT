      "utterance": [
        {
          "id": "MDRW2100041227.1.1",
          "form": "최근에 뭐 사신거 있으세요?",
          "original_form": "최근에 뭐 사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041227.1.2",
          "form": "저는 컴퓨터요!",
          "original_form": "저는 컴퓨터요!",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041227.1.3",
          "form": "오 큰 지출 하셨네요 !!",
          "original_form": "오 큰 지출 하셨네요 !!",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041227.1.4",
          "form": "컴퓨터 비싸죠 ? ㅠㅠ ㅋㅋ 요즘에",
          "original_form": "컴퓨터 비싸죠 ? ㅠㅠ ㅋㅋ 요즘에",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041227.1.5",
          "form": "모니터 한대에 15만원, 본체 50만원이라 크게 비싸진 않았어요",
          "original_form": "모니터 한대에 15만원, 본체 50만원이라 크게 비싸진 않았어요",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041227.1.6",
          "form": "모니터 두대 사서 듀얼 했어요ㅎㅎ",
          "original_form": "모니터 두대 사서 듀얼 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041227.1.7",
          "form": "저렴하게 구입하셨네요 ㅋㅋ",
          "original_form": "저렴하게 구입하셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041227.1.8",
          "form": "와우 ㅋㅋㅋ 듀얼 좋죠 좋죠",
          "original_form": "와우 ㅋㅋㅋ 듀얼 좋죠 좋죠",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041227.1.9",
          "form": "저는 요즘 지출 없다가 ㅋㅋ 오늘 자동차 와이퍼랑 필터 샀어요 ㅋ",
          "original_form": "저는 요즘 지출 없다가 ㅋㅋ 오늘 자동차 와이퍼랑 필터 샀어요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041227.1.10",
          "form": "미루고싶었지만 ㅋㅋ 사야되는것들이라 ㅋㅋ",
          "original_form": "미루고싶었지만 ㅋㅋ 사야되는것들이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041227.1.11",
          "form": "오, 차 운전하세요?",
          "original_form": "오, 차 운전하세요?",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041227.1.12",
          "form": "필수품이죠!",
          "original_form": "필수품이죠!",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041227.1.13",
          "form": "네네 ㅋㅋㅋ 장롱면허로 있다가 ㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ 장롱면허로 있다가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041227.1.14",
          "form": "운전한지 얼마 안됏어요",
          "original_form": "운전한지 얼마 안됏어요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041227.1.15",
          "form": "오, 그러셨군요",
          "original_form": "오, 그러셨군요",
          "speaker_id": "2",
          "time": "20210906 06:43"
        },
        {
          "id": "MDRW2100041227.1.16",
          "form": "중고거래는 해본 적 있으세요?",
          "original_form": "중고거래는 해본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210906 06:43"
        },
        {
          "id": "MDRW2100041227.1.17",
          "form": "네 ㅋㅋ 거래많이해요.",
          "original_form": "네 ㅋㅋ 거래많이해요.",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041227.1.18",
          "form": "최근에는 한복했어요",
          "original_form": "최근에는 한복했어요",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041227.1.19",
          "form": "중고거래하신적있나요?",
          "original_form": "중고거래하신적있나요?",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041227.1.20",
          "form": "헉, 한복을 중고거래하셨어요?? 신기하다",
          "original_form": "헉, 한복을 중고거래하셨어요?? 신기하다",
          "speaker_id": "2",
          "time": "20210906 09:04"
        },
        {
          "id": "MDRW2100041227.1.21",
          "form": "저는 먹다 질린 과자 팔고 책장 사왔어요ㅎㅎ",
          "original_form": "저는 먹다 질린 과자 팔고 책장 사왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:04"
        },
        {
          "id": "MDRW2100041227.1.22",
          "form": "네 아이한복은  거래잘되요.ㅋㅋ 팔았어요ㅋㅋ",
          "original_form": "네 아이한복은  거래잘되요.ㅋㅋ 팔았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:04"
        },
        {
          "id": "MDRW2100041227.1.23",
          "form": "ㅋㅋ질린과자ㅋ",
          "original_form": "ㅋㅋ질린과자ㅋ",
          "speaker_id": "1",
          "time": "20210906 09:05"
        },
        {
          "id": "MDRW2100041227.1.24",
          "form": "추석이 다가오니",
          "original_form": "추석이 다가오니",
          "speaker_id": "1",
          "time": "20210906 09:05"
        },
        {
          "id": "MDRW2100041227.1.25",
          "form": "한복이 빨리팔리더라구요ㅋ",
          "original_form": "한복이 빨리팔리더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210906 09:05"
        },
        {
          "id": "MDRW2100041227.1.26",
          "form": "아, 아이 한복이군요, 성장기라 사고팔면 좋겠어요",
          "original_form": "아, 아이 한복이군요, 성장기라 사고팔면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100041227.1.27",
          "form": "네네ㅋㅋ몇번못입고 팔아요ㅋ",
          "original_form": "네네ㅋㅋ몇번못입고 팔아요ㅋ",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100041227.1.28",
          "form": "저도 어릴때 한복은 몇번 안 입은것 같네요ㅋㅋㅋ",
          "original_form": "저도 어릴때 한복은 몇번 안 입은것 같네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100041227.1.29",
          "form": "장난감도 중고거래하시나요??",
          "original_form": "장난감도 중고거래하시나요??",
          "speaker_id": "2",
          "time": "20210906 09:07"
        },
        {
          "id": "MDRW2100041227.1.30",
          "form": "네 장난감도해요ㅋ",
          "original_form": "네 장난감도해요ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041227.1.31",
          "form": "오늘도 뭘팔아야될지 봤답니다ㅋ",
          "original_form": "오늘도 뭘팔아야될지 봤답니다ㅋ",
