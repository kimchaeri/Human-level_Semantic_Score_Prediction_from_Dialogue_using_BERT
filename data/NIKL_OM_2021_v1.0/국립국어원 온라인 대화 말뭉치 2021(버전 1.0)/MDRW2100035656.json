      "utterance": [
        {
          "id": "MDRW2100035656.1.1",
          "form": "주로 어떤걸 이용해서 보세요?",
          "original_form": "주로 어떤걸 이용해서 보세요?",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035656.1.2",
          "form": "저 티빙이요! 네이버멤버십 혜택으로 보고 있어요",
          "original_form": "저 티빙이요! 네이버멤버십 혜택으로 보고 있어요",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035656.1.3",
          "form": "오! 저도 쓰고있어요!",
          "original_form": "오! 저도 쓰고있어요!",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035656.1.4",
          "form": "예능보기 좋더라구요~",
          "original_form": "예능보기 좋더라구요~",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035656.1.5",
          "form": "맞아요 티빙으로 볼 수 있는 예능이 많더라구요 영화도 많이 서비스하던데 네이버멤버십은 업그레이드해야 이용할 수 있더라구요 ㅜ",
          "original_form": "맞아요 티빙으로 볼 수 있는 예능이 많더라구요 영화도 많이 서비스하던데 네이버멤버십은 업그레이드해야 이용할 수 있더라구요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035656.1.6",
          "form": "엇.. 그건 아쉽네요ㅠ",
          "original_form": "엇.. 그건 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035656.1.7",
          "form": "예능 보시는건 있으세요?",
          "original_form": "예능 보시는건 있으세요?",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035656.1.8",
          "form": "심심할 때 놀라운 토요일 보는 것 같아요 ㅋㅋ",
          "original_form": "심심할 때 놀라운 토요일 보는 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035656.1.9",
          "form": "아! 그거 재밌죠ㅋㅋㅋ",
          "original_form": "아! 그거 재밌죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035656.1.10",
          "form": "문세윤 진짜 웃겨요ㅋㅋㅋ",
          "original_form": "문세윤 진짜 웃겨요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035656.1.11",
          "form": "맞아요 ㅋㄱㅋㄱ 놀토멤버들이 서로 짝짜꿍도 잘맞고",
          "original_form": "맞아요 ㅋㄱㅋㄱ 놀토멤버들이 서로 짝짜꿍도 잘맞고",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035656.1.12",
          "form": "저는 본게임보다 간식게임이 더 재밌더라구요",
          "original_form": "저는 본게임보다 간식게임이 더 재밌더라구요",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035656.1.13",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035656.1.14",
          "form": "그것만 찾아보기도 했었어요ㅋㅋㅋ",
          "original_form": "그것만 찾아보기도 했었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035656.1.15",
          "form": "ㅋㅋㄱㄱㅋㄱ 그리고 뭔가 먹을거랑 같이 나와서 그런가 좋더라구요 먹방 좋아하거든요",
          "original_form": "ㅋㅋㄱㄱㅋㄱ 그리고 뭔가 먹을거랑 같이 나와서 그런가 좋더라구요 먹방 좋아하거든요",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035656.1.16",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035656.1.17",
          "form": "진짜 행복하게 먹어서 더 그런거같아요ㅋㅋ",
          "original_form": "진짜 행복하게 먹어서 더 그런거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035656.1.18",
          "form": "ㅊㅣ열함이 짜릿해요ㅋㅋ",
          "original_form": "ㅊㅣ열함이 짜릿해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035656.1.19",
          "form": "맞아요!! ㅋㄱㄱㄱㅋㅋ",
          "original_form": "맞아요!! ㅋㄱㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035656.1.20",
          "form": "간식게임도 종류가 다양해서 재밌는 것 같아야",
          "original_form": "간식게임도 종류가 다양해서 재밌는 것 같아야",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100035656.1.21",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100035656.1.22",
