      "utterance": [
        {
          "id": "MDRW2100044272.1.1",
          "form": "달다구리자주먹어요",
          "original_form": "달다구리자주먹어요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044272.1.2",
          "form": "요거트같은거요?",
          "original_form": "요거트같은거요?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044272.1.3",
          "form": "요거트도그렇고 에이드류나 과일주스여 ㅎㅎ",
          "original_form": "요거트도그렇고 에이드류나 과일주스여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044272.1.4",
          "form": "name2님은 카페가면 뭐드세요",
          "original_form": "&name2&님은 카페가면 뭐드세요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044272.1.5",
          "form": "전 주로 친구랑 같이가는데 수다떨어요",
          "original_form": "전 주로 친구랑 같이가는데 수다떨어요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044272.1.6",
          "form": "아 먹는거ㅋㅋ",
          "original_form": "아 먹는거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044272.1.7",
          "form": "요거트나 카페인없는 프라푸치노요",
          "original_form": "요거트나 카페인없는 프라푸치노요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044272.1.8",
          "form": "ㅋㅋㅋㅋ저랑비슷하시네여",
          "original_form": "ㅋㅋㅋㅋ저랑비슷하시네여",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044272.1.9",
          "form": "카페인 안드시나봐여 저도 잘안받아서 안먹는데",
          "original_form": "카페인 안드시나봐여 저도 잘안받아서 안먹는데",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044272.1.10",
          "form": "네 이상하게 안먹을수록 더 예민해지네요",
          "original_form": "네 이상하게 안먹을수록 더 예민해지네요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044272.1.11",
          "form": "가끔은 콜라도 힘들어요",
          "original_form": "가끔은 콜라도 힘들어요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044272.1.12",
          "form": "그쳐 저도 카페인점점 더안받는것같아요",
          "original_form": "그쳐 저도 카페인점점 더안받는것같아요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044272.1.13",
          "form": "콜라랑 홍차 초코ㅜ안녕..",
          "original_form": "콜라랑 홍차 초코ㅜ안녕..",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044272.1.14",
          "form": "앗 초코도 힘드시나요?",
          "original_form": "앗 초코도 힘드시나요?",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044272.1.15",
          "form": "네 컨디션 영안좋으면 초코도안받더라구여",
          "original_form": "네 컨디션 영안좋으면 초코도안받더라구여",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044272.1.16",
          "form": "그럼 초코 아이스크림이나 케익도 안되시는거예요?",
          "original_form": "그럼 초코 아이스크림이나 케익도 안되시는거예요?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044272.1.17",
          "form": "초코도 진한 카카오들어간거요 팜유이런건 괜찮더라구여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "초코도 진한 카카오들어간거요 팜유이런건 괜찮더라구여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044272.1.18",
          "form": "아 카카오가 문제군요",
          "original_form": "아 카카오가 문제군요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044272.1.19",
          "form": "네 카카오에도 카페인이함유되어있으니까 그런거같아여",
          "original_form": "네 카카오에도 카페인이함유되어있으니까 그런거같아여",
