        },
        {
          "id": "MDRW2100042517.1.2",
          "form": "당근마켓 써?",
          "original_form": "당근마켓 써?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042517.1.3",
          "form": "자주 쓰진 않는데 가끔",
          "original_form": "자주 쓰진 않는데 가끔",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042517.1.4",
          "form": "필요한거 잇음 검색은 해봐",
          "original_form": "필요한거 잇음 검색은 해봐",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042517.1.5",
          "form": "그럼 지금 당근 켜봐(?",
          "original_form": "그럼 지금 당근 켜봐(?",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042517.1.6",
          "form": "치즈 어쩌구? ㅋㅋㅋ",
          "original_form": "치즈 어쩌구? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042517.1.7",
          "form": "당근? ㅇㅇ",
          "original_form": "당근? ㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042517.1.8",
          "form": "뭐 찾을거 있어?",
          "original_form": "뭐 찾을거 있어?",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042517.1.9",
          "form": "근데 우리 동네는 뭐가 잘 없어",
          "original_form": "근데 우리 동네는 뭐가 잘 없어",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042517.1.10",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042517.1.11",
          "form": "전에 우체국쪽에서 검색햇는데",
          "original_form": "전에 우체국쪽에서 검색햇는데",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042517.1.12",
          "form": "안나왓다고 그랫지",
          "original_form": "안나왓다고 그랫지",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042517.1.13",
          "form": "식사권~~!!!",
          "original_form": "식사권~~!!!",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042517.1.14",
          "form": "ㅋㅋㅋㅋㅋ ㅇㅎ 안나올거 같은데.....",
          "original_form": "ㅋㅋㅋㅋㅋ ㅇㅎ 안나올거 같은데.....",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042517.1.15",
          "form": "우체국쪽이 사람 가장 많이 사는 동네였거든 함 뒤져볼겡",
          "original_form": "우체국쪽이 사람 가장 많이 사는 동네였거든 함 뒤져볼겡",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042517.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042517.1.17",
          "form": "신라호텔",
          "original_form": "신라호텔",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042517.1.18",
          "form": "이나",
          "original_form": "이나",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042517.1.19",
          "form": "더 파.크뷰",
          "original_form": "더 파.크뷰",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042517.1.20",
          "form": "검색하면 나와",
          "original_form": "검색하면 나와",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042517.1.21",
          "form": "점 빼고....ㅎ...",
          "original_form": "점 빼고....ㅎ...",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042517.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ응ㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ응ㅇ",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042517.1.23",
          "form": "아까 중고나라에 안전결제 가능한 물건 나왔는데",
          "original_form": "아까 중고나라에 안전결제 가능한 물건 나왔는데",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042517.1.24",
          "form": "고민하다가 놓쳐버림 ㅠ",
          "original_form": "고민하다가 놓쳐버림 ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042517.1.25",
          "form": "ㅠㅠㅠ 아이고...",
          "original_form": "ㅠㅠㅠ 아이고...",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.26",
          "form": "지금 음.. 일년전인것밖에 안보인다.....",
          "original_form": "지금 음.. 일년전인것밖에 안보인다.....",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.27",
          "form": "근처 동네는 일단 없는거 같고..",
          "original_form": "근처 동네는 일단 없는거 같고..",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.28",
          "form": "동구까지 가보겠소",
          "original_form": "동구까지 가보겠소",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.29",
          "form": "앗.....ㅋㅋㅋㅋ ㅠ",
          "original_form": "앗.....ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.30",
          "form": "그래....",
          "original_form": "그래....",
          "speaker_id": "1",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042517.1.31",
          "form": "이거 검색은 위치등록 안해도 돼...",
          "original_form": "이거 검색은 위치등록 안해도 돼...",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042517.1.32",
          "form": "앗 그렇군..",
          "original_form": "앗 그렇군..",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042517.1.33",
          "form": "에코백은..엄청 많당...",
          "original_form": "에코백은..엄청 많당...",
          "speaker_id": "2",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042517.1.34",
          "form": "아 ㅋㅋㅋㅋ 너도 보고잇니...?",
          "original_form": "아 ㅋㅋㅋㅋ 너도 보고잇니...?",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042517.1.35",
          "form": "에코백 거래 잘돼...?",
          "original_form": "에코백 거래 잘돼...?",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042517.1.36",
          "form": "그거나 사다 팔까...ㅎ...",
          "original_form": "그거나 사다 팔까...ㅎ...",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042517.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 모르겠어 팔린거에 많긴 한데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 모르겠어 팔린거에 많긴 한데...",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042517.1.38",
          "form": "제주에코백이 제일 많아",
          "original_form": "제주에코백이 제일 많아",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042517.1.39",
