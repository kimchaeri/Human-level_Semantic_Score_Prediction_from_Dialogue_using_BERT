      "utterance": [
        {
          "id": "MDRW2100013663.1.1",
          "form": "즐겨찾는 커뮤니티 있으세요.",
          "original_form": "즐겨찾는 커뮤니티 있으세요.",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013663.1.2",
          "form": "아뇨없네요",
          "original_form": "아뇨없네요",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013663.1.3",
          "form": "뭐하시는거 있으시나요?",
          "original_form": "뭐하시는거 있으시나요?",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013663.1.4",
          "form": "저는 네이버카페요 ㅎㅎ",
          "original_form": "저는 네이버카페요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013663.1.5",
          "form": "정보 얻으시는건가요?",
          "original_form": "정보 얻으시는건가요?",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013663.1.6",
          "form": "네 아파트커뮤니티도 하고 직장내커뮤니티도 자주 들어가요",
          "original_form": "네 아파트커뮤니티도 하고 직장내커뮤니티도 자주 들어가요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013663.1.7",
          "form": "아파트커뮤니티가 있었군요",
          "original_form": "아파트커뮤니티가 있었군요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013663.1.8",
          "form": "나눔도 하고 좋더라고요",
          "original_form": "나눔도 하고 좋더라고요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013663.1.9",
          "form": "네 지역 맛집공유도 되고 물품 나눔이나 공동구매도 하죠",
          "original_form": "네 지역 맛집공유도 되고 물품 나눔이나 공동구매도 하죠",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013663.1.10",
          "form": "정모 나가신적있으시나요?",
          "original_form": "정모 나가신적있으시나요?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013663.1.11",
          "form": "없는것같아요",
          "original_form": "없는것같아요",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013663.1.12",
          "form": "커뮤니티에서도 거의 눈팅이지 제가 글을 쓰고 그러진않아요",
          "original_form": "커뮤니티에서도 거의 눈팅이지 제가 글을 쓰고 그러진않아요",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013663.1.13",
          "form": "정모하면 쑥스러울거같아요",
          "original_form": "정모하면 쑥스러울거같아요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013663.1.14",
          "form": "이름 부르기도 그렇고 ㅋㅋ 아뒤로 불러야하는지 ㅋㅋ",
          "original_form": "이름 부르기도 그렇고 ㅋㅋ 아뒤로 불러야하는지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013663.1.15",
          "form": "어른들은 잘하는거같아요",
          "original_form": "어른들은 잘하는거같아요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013663.1.16",
          "form": "아 그렇죠 어르신들 ㅎㅎ 등산가도 커뮤니티 모임으로 보이는 분들 계시더라고요",
          "original_form": "아 그렇죠 어르신들 ㅎㅎ 등산가도 커뮤니티 모임으로 보이는 분들 계시더라고요",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013663.1.17",
          "form": "넉살이 생기나봐요",
          "original_form": "넉살이 생기나봐요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013663.1.18",
          "form": "네 저희 부모님만 봐도 ㅎㅎㅎ동네분들 쉽게 말 거시고 ㅎ",
          "original_form": "네 저희 부모님만 봐도 ㅎㅎㅎ동네분들 쉽게 말 거시고 ㅎ",
          "speaker_id": "1",
