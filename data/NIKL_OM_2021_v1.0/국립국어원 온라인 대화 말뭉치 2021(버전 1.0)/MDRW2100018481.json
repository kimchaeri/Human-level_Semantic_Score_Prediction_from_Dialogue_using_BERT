      "utterance": [
        {
          "id": "MDRW2100018481.1.1",
          "form": "보통 위에 오빠 있으면 남자 친구들이랑 사이 좋던데",
          "original_form": "보통 위에 오빠 있으면 남자 친구들이랑 사이 좋던데",
          "speaker_id": "1",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100018481.1.2",
          "form": "맞아요 꽤 그렇다고 생각해요",
          "original_form": "맞아요 꽤 그렇다고 생각해요",
          "speaker_id": "2",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100018481.1.3",
          "form": "위에 오빠가 있어서 그런지 남저인 친구, 동기들이랑도 어색하지 않고 즐거워요!",
          "original_form": "위에 오빠가 있어서 그런지 남저인 친구, 동기들이랑도 어색하지 않고 즐거워요!",
          "speaker_id": "2",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100018481.1.4",
          "form": "좋은 일이네요!",
          "original_form": "좋은 일이네요!",
          "speaker_id": "1",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100018481.1.5",
          "form": "그럼 연애는 어떻게 잘 하고 계신가요?",
          "original_form": "그럼 연애는 어떻게 잘 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210827 02:52"
        },
        {
          "id": "MDRW2100018481.1.6",
          "form": "전 이미 결혼해서요 ㅋㅋ",
          "original_form": "전 이미 결혼해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:52"
        },
        {
          "id": "MDRW2100018481.1.7",
          "form": "아쉽게도 지금은 애인이 없답니다ㅎㅎ",
          "original_form": "아쉽게도 지금은 애인이 없답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:52"
        },
        {
          "id": "MDRW2100018481.1.8",
          "form": "결혼하면 같이 사는거잖아요 뭔가 신기해요ㅋㅋ",
          "original_form": "결혼하면 같이 사는거잖아요 뭔가 신기해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:53"
        },
        {
          "id": "MDRW2100018481.1.9",
          "form": "아 애인이 없다니...ㅠㅠ",
          "original_form": "아 애인이 없다니...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:54"
        },
        {
          "id": "MDRW2100018481.1.10",
          "form": "꽃다운 나이실텐데 ㅎㅎ 이상형은 어떻게 되세요?",
          "original_form": "꽃다운 나이실텐데 ㅎㅎ 이상형은 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210827 02:54"
        },
        {
          "id": "MDRW2100018481.1.11",
          "form": "결혼하면 처음에는 떨리는데 점점 익숙해져요!",
          "original_form": "결혼하면 처음에는 떨리는데 점점 익숙해져요!",
          "speaker_id": "1",
          "time": "20210827 02:54"
        },
        {
          "id": "MDRW2100018481.1.12",
          "form": "제 이상형은 다정한 남자에요!!",
          "original_form": "제 이상형은 다정한 남자에요!!",
          "speaker_id": "2",
          "time": "20210827 02:54"
        },
        {
          "id": "MDRW2100018481.1.13",
          "form": "다정하구 예의바른 분!",
          "original_form": "다정하구 예의바른 분!",
          "speaker_id": "2",
          "time": "20210827 02:55"
        },
        {
          "id": "MDRW2100018481.1.14",
          "form": "오오 아무래도 따뜻한 사람이 좋죠ㅎ",
          "original_form": "오오 아무래도 따뜻한 사람이 좋죠ㅎ",
          "speaker_id": "1",
          "time": "20210827 02:55"
        },
        {
          "id": "MDRW2100018481.1.15",
          "form": "제 이상형도 포근한 빨래같은 사람이었어요",
          "original_form": "제 이상형도 포근한 빨래같은 사람이었어요",
          "speaker_id": "1",
          "time": "20210827 02:55"
        },
        {
          "id": "MDRW2100018481.1.16",
          "form": "좀 독특하죠ㅋㅋㅋㅋ",
          "original_form": "좀 독특하죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:56"
        },
        {
          "id": "MDRW2100018481.1.17",
          "form": "앗 그런데 무슨 말인지 알거 같아요",
          "original_form": "앗 그런데 무슨 말인지 알거 같아요",
          "speaker_id": "2",
          "time": "20210827 02:56"
        },
        {
          "id": "MDRW2100018481.1.18",
          "form": "포근포근 다정한 분...",
          "original_form": "포근포근 다정한 분...",
          "speaker_id": "2",
          "time": "20210827 02:56"
        },
        {
          "id": "MDRW2100018481.1.19",
          "form": "너무 연에하고 싶네요ㅠㅠ",
          "original_form": "너무 연에하고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:57"
        },
        {
          "id": "MDRW2100018481.1.20",
          "form": "연애는 많이 할수록 좋습니다",
          "original_form": "연애는 많이 할수록 좋습니다",
          "speaker_id": "1",
          "time": "20210827 02:57"
        },
        {
          "id": "MDRW2100018481.1.21",
          "form": "소개팅도 팍팍 받아보셔요",
          "original_form": "소개팅도 팍팍 받아보셔요",
          "speaker_id": "1",
          "time": "20210827 02:57"
        },
        {
          "id": "MDRW2100018481.1.22",
          "form": "피같은 조언 감사합니다ㅋㅋㅋㅋ",
          "original_form": "피같은 조언 감사합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:57"
        },
        {
          "id": "MDRW2100018481.1.23",
          "form": "저도 얼른 연애하고 싶어요",
          "original_form": "저도 얼른 연애하고 싶어요",
          "speaker_id": "2",
          "time": "20210827 02:57"
        },
        {
          "id": "MDRW2100018481.1.24",
          "form": "코로나 나아지면 미팅 게속 잡아야죠",
          "original_form": "코로나 나아지면 미팅 게속 잡아야죠",
          "speaker_id": "2",
          "time": "20210827 02:58"
        },
        {
          "id": "MDRW2100018481.1.25",
          "form": "좋네요 전 남편이랑 데이트나 해야겠어요",
          "original_form": "좋네요 전 남편이랑 데이트나 해야겠어요",
          "speaker_id": "1",
          "time": "20210827 02:58"
        },
        {
          "id": "MDRW2100018481.1.26",
          "form": "데이트로 영화 보려구요",
          "original_form": "데이트로 영화 보려구요",
          "speaker_id": "1",
          "time": "20210827 02:59"
        },
        {
          "id": "MDRW2100018481.1.27",
          "form": "영화관 데이트도 좋죠 두근두근합니다 ㅎㄹ",
          "original_form": "영화관 데이트도 좋죠 두근두근합니다 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210827 02:59"
        },
        {
          "id": "MDRW2100018481.1.28",
          "form": "전 남친 생기면 놀이공원으로 데이트 가고 싶어요",
          "original_form": "전 남친 생기면 놀이공원으로 데이트 가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 02:59"
        },
        {
          "id": "MDRW2100018481.1.29",
          "form": "놀이공원도 로맨틱하죠ㅠㅠ",
          "original_form": "놀이공원도 로맨틱하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 03:00"
        },
        {
          "id": "MDRW2100018481.1.30",
          "form": "하루 빨리 솔로 탈출하시길 바랄게요ㅎ",
          "original_form": "하루 빨리 솔로 탈출하시길 바랄게요ㅎ",
          "speaker_id": "1",
          "time": "20210827 03:01"
        },
        {
          "id": "MDRW2100018481.1.31",
          "form": "앗 감사합니다ㅋㅋㅋㅋ 얼른 솔로 탈출 해야죠",
          "original_form": "앗 감사합니다ㅋㅋㅋㅋ 얼른 솔로 탈출 해야죠",
          "speaker_id": "2",
          "time": "20210827 03:01"
        }
      ]
