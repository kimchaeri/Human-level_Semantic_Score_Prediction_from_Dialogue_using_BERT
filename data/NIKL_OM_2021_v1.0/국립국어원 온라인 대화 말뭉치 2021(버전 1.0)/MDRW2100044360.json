      "utterance": [
        {
          "id": "MDRW2100044360.1.1",
          "form": "무슨커피 잴 조아하세요",
          "original_form": "무슨커피 잴 조아하세요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044360.1.2",
          "form": "전 원래 아메리카노 좋아했는데 라떼 만들어 먹어서 맛들렸어요 님은요?",
          "original_form": "전 원래 아메리카노 좋아했는데 라떼 만들어 먹어서 맛들렸어요 님은요?",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044360.1.3",
          "form": "저도 라떼 아아 조아해요",
          "original_form": "저도 라떼 아아 조아해요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044360.1.4",
          "form": "산미조아하세요?",
          "original_form": "산미조아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044360.1.5",
          "form": "전 탄맛 좋아해요!",
          "original_form": "전 탄맛 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044360.1.6",
          "form": "오 저랑 반대네요 ㅋㅋㅋㅋ",
          "original_form": "오 저랑 반대네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044360.1.7",
          "form": "스벅 조아하시나용",
          "original_form": "스벅 조아하시나용",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044360.1.8",
          "form": "스벅은 그럭저럭요 기프티콘 있으면 가요",
          "original_form": "스벅은 그럭저럭요 기프티콘 있으면 가요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044360.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044360.1.10",
          "form": "ㅋㅋㅋ 전 별때문에 자주가요",
          "original_form": "ㅋㅋㅋ 전 별때문에 자주가요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044360.1.11",
          "form": "어디카페가세요 주로",
          "original_form": "어디카페가세요 주로",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044360.1.12",
          "form": "저는 봄봄이나 컴포즈 가요 스벅은 공부할때 가요",
          "original_form": "저는 봄봄이나 컴포즈 가요 스벅은 공부할때 가요",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044360.1.13",
          "form": "오 둘다안가밧어요 못봤는뎅",
          "original_form": "오 둘다안가밧어요 못봤는뎅",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044360.1.14",
          "form": "맛잇어요?",
          "original_form": "맛잇어요?",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044360.1.15",
          "form": "둘다 생각보단 없더라구요",
          "original_form": "둘다 생각보단 없더라구요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044360.1.16",
          "form": "가보고싶네요",
          "original_form": "가보고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044360.1.17",
          "form": "디저트는요?",
          "original_form": "디저트는요?",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044360.1.18",
          "form": "가끔 메가커피 와플요 님은요?",
          "original_form": "가끔 메가커피 와플요 님은요?",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044360.1.19",
          "form": "그거마싯어요?",
          "original_form": "그거마싯어요?",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044360.1.20",
          "form": "투썸 케익요",
          "original_form": "투썸 케익요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044360.1.21",
          "form": "와플 곡물 반죽이라 맛있어요",
          "original_form": "와플 곡물 반죽이라 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044360.1.22",
          "form": "투썸 케이크가 유명하죠!",
          "original_form": "투썸 케이크가 유명하죠!",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044360.1.23",
          "form": "오 가바야겟다",
          "original_form": "오 가바야겟다",
          "speaker_id": "1",
          "time": "20210907 22:56"
        }
      ]
