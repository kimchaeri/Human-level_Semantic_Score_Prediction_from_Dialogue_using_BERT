      "utterance": [
        {
          "id": "MDRW2100037626.1.1",
          "form": "점심은 뭐드셨나요",
          "original_form": "점심은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100037626.1.2",
          "form": "어제 먹다남은 부대찌개 마저 먹었어요",
          "original_form": "어제 먹다남은 부대찌개 마저 먹었어요",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100037626.1.3",
          "form": "맛있으셨겠어요\r\n전 시리얼이요ㅋㅋ",
          "original_form": "맛있으셨겠어요\r\n전 시리얼이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037626.1.4",
          "form": "시리얼만 드시면 허전하지 않으세요?",
          "original_form": "시리얼만 드시면 허전하지 않으세요?",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037626.1.5",
          "form": "금방 배고프긴 하죠\r\n오래앉아있으니까 많이먹으면 힘들어서..",
          "original_form": "금방 배고프긴 하죠\r\n오래앉아있으니까 많이먹으면 힘들어서..",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037626.1.6",
          "form": "아 계속 앉아계시다면 어쩔수 없죠...",
          "original_form": "아 계속 앉아계시다면 어쩔수 없죠...",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037626.1.7",
          "form": "대신 배고프시면 조그마한 거라도 챙겨드세요 꼭",
          "original_form": "대신 배고프시면 조그마한 거라도 챙겨드세요 꼭",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037626.1.8",
          "form": "오늘도 시리얼과 토마토주스 먹었어요",
          "original_form": "오늘도 시리얼과 토마토주스 먹었어요",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100037626.1.9",
          "form": "아이고ㅠ 잘 챙겨드세요",
          "original_form": "아이고ㅠ 잘 챙겨드세요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100037626.1.10",
          "form": "아무리 시리얼에 영양소가 잘 들어가 있다지만...",
          "original_form": "아무리 시리얼에 영양소가 잘 들어가 있다지만...",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100037626.1.11",
          "form": "하도 그렇게 먹엇더니 점심 마니먹으면 힘들더라고요",
          "original_form": "하도 그렇게 먹엇더니 점심 마니먹으면 힘들더라고요",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100037626.1.12",
          "form": "이미 적응해버리셨군요...ㅠ",
          "original_form": "이미 적응해버리셨군요...ㅠ",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100037626.1.13",
          "form": "한시간이면 배고파지기는 하는데 더먹음 힘들고ㅋㅋ",
          "original_form": "한시간이면 배고파지기는 하는데 더먹음 힘들고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100037626.1.14",
          "form": "그럴땐 간식거리가 답이죠",
          "original_form": "그럴땐 간식거리가 답이죠",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100037626.1.15",
          "form": "네 작은거 한두개 먹으면 좋더라고요",
          "original_form": "네 작은거 한두개 먹으면 좋더라고요",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100037626.1.16",
          "form": "그럴때 어떤거 주로 드세요?",
          "original_form": "그럴때 어떤거 주로 드세요?",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100037626.1.17",
          "form": "몽셀같은거요",
          "original_form": "몽셀같은거요",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100037626.1.18",
          "form": "몽셸 좋죠",
          "original_form": "몽셸 좋죠",
          "speaker_id": "2",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100037626.1.19",
          "form": "저는 에너지바 먹었었는데ㅋㅋ",
          "original_form": "저는 에너지바 먹었었는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100037626.1.20",
          "form": "그것도 좋아요\r\n초코바는 넘달어서 별로지만요",
          "original_form": "그것도 좋아요\r\n초코바는 넘달어서 별로지만요",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100037626.1.21",
          "form": "초코바는 역시 호불호가 갈리더라구요",
          "original_form": "초코바는 역시 호불호가 갈리더라구요",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100037626.1.22",
          "form": "저는 있으면 먹습니다",
          "original_form": "저는 있으면 먹습니다",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100037626.1.23",
          "form": "저도 눈압에잇음 먹긴해욬ㅋ",
          "original_form": "저도 눈압에잇음 먹긴해욬ㅋ",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100037626.1.24",
          "form": "ㅋㅋ 눈앞에 보이는 순간 불가항력이죠",
          "original_form": "ㅋㅋ 눈앞에 보이는 순간 불가항력이죠",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100037626.1.25",
          "form": "일부러사진 않지만 있으면 못참아요ㅋㅋ",
          "original_form": "일부러사진 않지만 있으면 못참아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100037626.1.26",
          "form": "ㅋㅋㅋ 저랑 너무 똑같으시네요",
          "original_form": "ㅋㅋㅋ 저랑 너무 똑같으시네요",
          "speaker_id": "2",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100037626.1.27",
          "form": "근데 거의 그러겠져\r\n앞에 있는데 어케 참아요ㅋ",
          "original_form": "근데 거의 그러겠져\r\n앞에 있는데 어케 참아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 17:01"
        }
      ]
    }
