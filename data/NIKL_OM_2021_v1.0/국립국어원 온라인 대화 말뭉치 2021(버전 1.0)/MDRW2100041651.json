      "utterance": [
        {
          "id": "MDRW2100041651.1.1",
          "form": "네 쇼핑좋아합니다",
          "original_form": "네 쇼핑좋아합니다",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041651.1.2",
          "form": "최근엔 그릇을 좀 샀네요",
          "original_form": "최근엔 그릇을 좀 샀네요",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041651.1.3",
          "form": "오 그릇 저도 사고는픈뎅",
          "original_form": "오 그릇 저도 사고는픈뎅",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041651.1.4",
          "form": "관리를 못해서 ㅠㅠ",
          "original_form": "관리를 못해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041651.1.5",
          "form": "그래서 싼거사요ㅋㅋㅋ",
          "original_form": "그래서 싼거사요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041651.1.6",
          "form": "깨먹어도 아깝지않게ㅜㅜ",
          "original_form": "깨먹어도 아깝지않게ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041651.1.7",
          "form": "ㅋㅋㅋㅋ요즘엔 그릇도 진짜 다양하더라구용",
          "original_form": "ㅋㅋㅋㅋ요즘엔 그릇도 진짜 다양하더라구용",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100041651.1.8",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100041651.1.9",
          "form": "중고거래 경험 있으세요?",
          "original_form": "중고거래 경험 있으세요?",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041651.1.10",
          "form": "예ㅔㅔ전에 네이버카페같은데선 사봤는데",
          "original_form": "예ㅔㅔ전에 네이버카페같은데선 사봤는데",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100041651.1.11",
          "form": "당근은 아직도 안해봤어요!",
          "original_form": "당근은 아직도 안해봤어요!",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100041651.1.12",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100041651.1.13",
          "form": "저도 아직 ㅠㅠ 요즘엔 다들 하더라구용",
          "original_form": "저도 아직 ㅠㅠ 요즘엔 다들 하더라구용",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041651.1.14",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041651.1.15",
          "form": "가입까진했는데",
          "original_form": "가입까진했는데",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041651.1.16",
          "form": "선뜻뭐를사진못했어요ㅋㅋ",
          "original_form": "선뜻뭐를사진못했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100041651.1.17",
          "form": "오 전 아직 가입돜ㅋㅋㅋㅋ...",
          "original_form": "오 전 아직 가입돜ㅋㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100041651.1.18",
          "form": "사고싶은 물품 있나용",
          "original_form": "사고싶은 물품 있나용",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100041651.1.19",
          "form": "닌텐도 한창 열심히 할땐",
          "original_form": "닌텐도 한창 열심히 할땐",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041651.1.20",
          "form": "게임관련팩같은거사고싶었는데",
          "original_form": "게임관련팩같은거사고싶었는데",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041651.1.21",
          "form": "이젠집에있는거를 내다팔고싶어요ㅋㅋ",
          "original_form": "이젠집에있는거를 내다팔고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041651.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ닌텐도 재밌나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ닌텐도 재밌나요?",
          "speaker_id": "1",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041651.1.23",
          "form": "처음나왔을땐 자는시간도 줄여가면서했는데",
          "original_form": "처음나왔을땐 자는시간도 줄여가면서했는데",
          "speaker_id": "2",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041651.1.24",
          "form": "이제는ㅜㅜㅋㅋㅋ",
          "original_form": "이제는ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:26"
        }
      ]
