      "utterance": [
        {
          "id": "MDRW2100039534.1.1",
          "form": "저녁 먹는중ㅎ",
          "original_form": "저녁 먹는중ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039534.1.2",
          "form": "맞나 뭐 먹어??",
          "original_form": "맞나 뭐 먹어??",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039534.1.3",
          "form": "파김치랑 짜파게티, 파인애플",
          "original_form": "파김치랑 짜파게티, 파인애플",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039534.1.4",
          "form": "짜파게티 내가 끓였는데\r\n맛있어서 놀람ㅎ",
          "original_form": "짜파게티 내가 끓였는데\r\n맛있어서 놀람ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039534.1.5",
          "form": "저녁먹음?",
          "original_form": "저녁먹음?",
          "speaker_id": "2",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039534.1.6",
          "form": "ㅇㅇ 나는 메밀비빔면!",
          "original_form": "ㅇㅇ 나는 메밀비빔면!",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039534.1.7",
          "form": "우정본가에서 나온건데 가게파는것보다 더 맛있엉",
          "original_form": "우정본가에서 나온건데 가게파는것보다 더 맛있엉",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039534.1.8",
          "form": "안먹어본듯ㅎ\r\n맛있다니 먹어보고 싶닼",
          "original_form": "안먹어본듯ㅎ\r\n맛있다니 먹어보고 싶닼",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039534.1.9",
          "form": "ㅇㅇ가격도 괜춘해 6인분에 5900원했던거같아",
          "original_form": "ㅇㅇ가격도 괜춘해 6인분에 5900원했던거같아",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039534.1.10",
          "form": "소스도준당",
          "original_form": "소스도준당",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039534.1.11",
          "form": "와 가격좋다",
          "original_form": "와 가격좋다",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100039534.1.12",
          "form": "ㅇㅇ가격 괜춘",
          "original_form": "ㅇㅇ가격 괜춘",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039534.1.13",
          "form": "가족 한끼로 괜츈하네",
          "original_form": "가족 한끼로 괜츈하네",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039534.1.14",
          "form": "ㅇㅇ 새콤달콤 맛있당 그리고 밀가루면보다 메밀면 좋아해서",
          "original_form": "ㅇㅇ 새콤달콤 맛있당 그리고 밀가루면보다 메밀면 좋아해서",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039534.1.15",
          "form": "맞어 밀가루면보다 메밀면이 좋지",
          "original_form": "맞어 밀가루면보다 메밀면이 좋지",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039534.1.16",
          "form": "뭔가 더 고소한맛이남 ㅋㅋ",
          "original_form": "뭔가 더 고소한맛이남 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039534.1.17",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039534.1.18",
          "form": "밀가루보다 우리지역 곡물이 맛나ㅎ",
          "original_form": "밀가루보다 우리지역 곡물이 맛나ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039534.1.19",
          "form": "ㅋㅋ 메밀은 메밀면 끓인면수도 구수한갑더라",
          "original_form": "ㅋㅋ 메밀은 메밀면 끓인면수도 구수한갑더라",
          "speaker_id": "1",
          "time": "20210903 23:16"
        }
