      "utterance": [
        {
          "id": "MDRW2100008079.1.1",
          "form": "뉴스보니 부동산 수수료 때문에 난리네",
          "original_form": "뉴스보니 부동산 수수료 때문에 난리네",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008079.1.2",
          "form": "나 지금 리모콘 잡았다ㅋ",
          "original_form": "나 지금 리모콘 잡았다ㅋ",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008079.1.3",
          "form": "오~ 좋아요",
          "original_form": "오~ 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008079.1.4",
          "form": "해방타운으로 채널 돌리면 됨?",
          "original_form": "해방타운으로 채널 돌리면 됨?",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008079.1.5",
          "form": "아는형님 하고 있는데",
          "original_form": "아는형님 하고 있는데",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008079.1.6",
          "form": "보고 싶은걸로 봐라 드라마는 내가 안보니 잘 모르겠다 솔직히",
          "original_form": "보고 싶은걸로 봐라 드라마는 내가 안보니 잘 모르겠다 솔직히",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008079.1.7",
          "form": "나도 요새 보는 드라마가 없다",
          "original_form": "나도 요새 보는 드라마가 없다",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008079.1.8",
          "form": "아는형님 펜싱단 나왔네",
          "original_form": "아는형님 펜싱단 나왔네",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008079.1.9",
          "form": "4명이 다 나왔네",
          "original_form": "4명이 다 나왔네",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008079.1.10",
          "form": "응 김희철 머리 너무 한거 아니가",
          "original_form": "응 김희철 머리 너무 한거 아니가",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008079.1.11",
          "form": "지금 선수가 기획사 들어간 선수다",
          "original_form": "지금 선수가 기획사 들어간 선수다",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008079.1.12",
          "form": "아 찐짜 보께",
          "original_form": "아 찐짜 보께",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008079.1.13",
          "form": "음 그정도는 아닌데 뭘로 할려고",
          "original_form": "음 그정도는 아닌데 뭘로 할려고",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008079.1.14",
          "form": "잘 생기긴 했는데...  저렇게 바로 기획사에서 연락오나",
          "original_form": "잘 생기긴 했는데...  저렇게 바로 기획사에서 연락오나",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008079.1.15",
          "form": "그러게 .....그냥 아주 작은 기획사인가보다",
          "original_form": "그러게 .....그냥 아주 작은 기획사인가보다",
