      "utterance": [
        {
          "id": "MDRW2100036855.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036855.1.2",
          "form": "안녕하세요 점심 맛있는거 드셨나요",
          "original_form": "안녕하세요 점심 맛있는거 드셨나요",
          "speaker_id": "2",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100036855.1.3",
          "form": "빵 먹었어요!",
          "original_form": "빵 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100036855.1.4",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100036855.1.5",
          "form": "저는 ㅜ 다이어트중이라",
          "original_form": "저는 ㅜ 다이어트중이라",
          "speaker_id": "2",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100036855.1.6",
          "form": "디톡스주스 마셨어요ㅜ",
          "original_form": "디톡스주스 마셨어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100036855.1.7",
          "form": "아이고ㅜㅜ 3끼 다 주스로 드시나요ㅠㅠ",
          "original_form": "아이고ㅜㅜ 3끼 다 주스로 드시나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100036855.1.8",
          "form": "이게 이름이 48시간 레디톡인데 일단 48시간은 그렇게ㅜㅜ",
          "original_form": "이게 이름이 48시간 레디톡인데 일단 48시간은 그렇게ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036855.1.9",
          "form": "아 레몬디톡스 인가요??",
          "original_form": "아 레몬디톡스 인가요??",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036855.1.10",
          "form": "네이버에 레디톡치면 나와요",
          "original_form": "네이버에 레디톡치면 나와요",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036855.1.11",
          "form": "자몽맛이요 ㅎㅎ",
          "original_form": "자몽맛이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036855.1.12",
          "form": "이거 일주일 하는 분들도 있나봐요 워우",
          "original_form": "이거 일주일 하는 분들도 있나봐요 워우",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100036855.1.13",
          "form": "이런 거 하시는 분들 의지가 너무 멋져요",
          "original_form": "이런 거 하시는 분들 의지가 너무 멋져요",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100036855.1.14",
          "form": "세상에 저오늘 첫날인데 아까 짜증나서 죽을뻔했는데 ㅋㅋ",
          "original_form": "세상에 저오늘 첫날인데 아까 짜증나서 죽을뻔했는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100036855.1.15",
          "form": "이거 근데 가격이 넘비싸요 ㄷㄷ",
          "original_form": "이거 근데 가격이 넘비싸요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100036855.1.16",
          "form": "엇 저도 아까 찾아보고 그렇게 생각했어요 ㅎㅎ",
          "original_form": "엇 저도 아까 찾아보고 그렇게 생각했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100036855.1.17",
          "form": "이런게 핑크텍스인가 싶고 ㅎㅎ",
          "original_form": "이런게 핑크텍스인가 싶고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100036855.1.18",
          "form": "그래도 효과 보시면 좋죠!",
          "original_form": "그래도 효과 보시면 좋죠!",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100036855.1.19",
          "form": "저는 사실 동생이 주고간거라",
          "original_form": "저는 사실 동생이 주고간거라",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100036855.1.20",
          "form": "가격은 그렇다쳐도",
          "original_form": "가격은 그렇다쳐도",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100036855.1.21",
          "form": "성분이 좀 어이없어요",
          "original_form": "성분이 좀 어이없어요",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100036855.1.22",
          "form": "진짜 별거없달까",
          "original_form": "진짜 별거없달까",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100036855.1.23",
          "form": "그러게요 그냥 비타민음료 섞어 놓은 구성이네요",
          "original_form": "그러게요 그냥 비타민음료 섞어 놓은 구성이네요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        }
      ]
    }
  ]
