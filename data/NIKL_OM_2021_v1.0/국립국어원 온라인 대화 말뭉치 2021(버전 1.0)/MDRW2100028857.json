      "utterance": [
        {
          "id": "MDRW2100028857.1.1",
          "form": "어 계속 온대",
          "original_form": "어 계속 온대",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028857.1.2",
          "form": "아 비가 그쳐야 창틀 닦는데",
          "original_form": "아 비가 그쳐야 창틀 닦는데",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028857.1.3",
          "form": "니 동생은 오늘 닦았대",
          "original_form": "니 동생은 오늘 닦았대",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.4",
          "form": "알아",
          "original_form": "알아",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.5",
          "form": "나도 며칠전에 닦았는데",
          "original_form": "나도 며칠전에 닦았는데",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.6",
          "form": "비 와서 도루묵 됐어",
          "original_form": "비 와서 도루묵 됐어",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.7",
          "form": "그래 비 그치면 닦아라",
          "original_form": "그래 비 그치면 닦아라",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.8",
          "form": "한번씩 제습기 돌리고",
          "original_form": "한번씩 제습기 돌리고",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.9",
          "form": "ㅇㅇ근데 제습기 돌림 넘 더움",
          "original_form": "ㅇㅇ근데 제습기 돌림 넘 더움",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028857.1.10",
          "form": "이제 날 좀 선선해져서 괜찮을걸?",
          "original_form": "이제 날 좀 선선해져서 괜찮을걸?",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.11",
          "form": "몰라 여름보단 낫겠지",
          "original_form": "몰라 여름보단 낫겠지",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.12",
          "form": "그래도 아직 더워ㅠㅜ",
          "original_form": "그래도 아직 더워ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.13",
          "form": "방에 켜놓고",
          "original_form": "방에 켜놓고",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.14",
          "form": "나와있어",
          "original_form": "나와있어",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.15",
          "form": "ㅇㅇ그래야될듯",
          "original_form": "ㅇㅇ그래야될듯",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028857.1.16",
          "form": "자주 돌려야 돼",
          "original_form": "자주 돌려야 돼",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.17",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.18",
          "form": "습하면 좀벌레나",
          "original_form": "습하면 좀벌레나",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.19",
          "form": "그거 나무에 다 있다며",
          "original_form": "그거 나무에 다 있다며",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.20",
          "form": "많아진다",
          "original_form": "많아진다",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.21",
          "form": "극",
          "original_form": "극",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.22",
          "form": "혐",
          "original_form": "혐",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028857.1.23",
          "form": "그니까 자주 돌리라고",
          "original_form": "그니까 자주 돌리라고",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028857.1.24",
          "form": "좀약 넣어놓길",
          "original_form": "좀약 넣어놓길",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
