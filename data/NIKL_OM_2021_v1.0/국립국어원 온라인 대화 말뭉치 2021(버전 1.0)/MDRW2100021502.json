      "utterance": [
        {
          "id": "MDRW2100021502.1.1",
          "form": "저 사실 깜짝고백할게 있습니다",
          "original_form": "저 사실 깜짝고백할게 있습니다",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021502.1.2",
          "form": "헉 뭔데요",
          "original_form": "헉 뭔데요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021502.1.3",
          "form": "다음 카페에서 name2 보고 왔어요",
          "original_form": "다음 카페에서 &name2& 보고 왔어요",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021502.1.4",
          "form": "ㅋㅋㅋㅋ닉네임 검색해서..",
          "original_form": "ㅋㅋㅋㅋ닉네임 검색해서..",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021502.1.5",
          "form": "아 진짜요? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜요? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021502.1.6",
          "form": "이럴수가 너무 반갑잖아요",
          "original_form": "이럴수가 너무 반갑잖아요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021502.1.7",
          "form": "아 원래는 트위터 하셨어요?",
          "original_form": "아 원래는 트위터 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021502.1.8",
          "form": "네 그것도 좀 오래 전에 하다가 피곤해서 접었다가",
          "original_form": "네 그것도 좀 오래 전에 하다가 피곤해서 접었다가",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021502.1.9",
          "form": "비혼 커뮤니티 소식을 듣고 놓칠 수 없어서 헐레벌떡",
          "original_form": "비혼 커뮤니티 소식을 듣고 놓칠 수 없어서 헐레벌떡",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021502.1.10",
          "form": "가입하고 다시 접었어요",
          "original_form": "가입하고 다시 접었어요",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021502.1.11",
          "form": "오 그래도 소식은 잘 듣고 오셨네요ㅋㅋㅋㅋㅋ",
          "original_form": "오 그래도 소식은 잘 듣고 오셨네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021502.1.12",
          "form": "ㅋㅋㅋㅋㅋ진짜 기회 놓치면",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 기회 놓치면",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021502.1.13",
          "form": "다시는 클린한 모임에 들어갈 수 없을 것만 같아서",
          "original_form": "다시는 클린한 모임에 들어갈 수 없을 것만 같아서",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021502.1.14",
          "form": "저는 처음 시작할 때부터 멤버였는데 무슨 재인증하는 기간 놓쳤다가 한참 못 들어왔었어요ㅜㅜ",
          "original_form": "저는 처음 시작할 때부터 멤버였는데 무슨 재인증하는 기간 놓쳤다가 한참 못 들어왔었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021502.1.15",
          "form": "아! 저도 초반에 가입했었던 멤버예요!",
          "original_form": "아! 저도 초반에 가입했었던 멤버예요!",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021502.1.16",
          "form": "아마 사이트가 별도였을 때",
          "original_form": "아마 사이트가 별도였을 때",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021502.1.17",
          "form": "name1이랑 거기서 뵈었을수도 있겠네요",
          "original_form": "&name1&이랑 거기서 뵈었을수도 있겠네요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021502.1.18",
          "form": "호호",
          "original_form": "호호",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021502.1.19",
          "form": "저도 재인증 놓칠 뻔했어요",
          "original_form": "저도 재인증 놓칠 뻔했어요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021502.1.20",
          "form": "한동안 인터넷 줄이려고 안 들어간 텀이 있는데",
          "original_form": "한동안 인터넷 줄이려고 안 들어간 텀이 있는데",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021502.1.21",
          "form": "다시 들어가보니 무슨 일로 발칵 뒤집혀있는거예요",
          "original_form": "다시 들어가보니 무슨 일로 발칵 뒤집혀있는거예요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021502.1.22",
          "form": "어 뭐지 커뮤니티 아예 사라지는 건가 싶어서",
          "original_form": "어 뭐지 커뮤니티 아예 사라지는 건가 싶어서",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021502.1.23",
          "form": "망연자실해있는데",
          "original_form": "망연자실해있는데",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021502.1.24",
          "form": "다행히 카페로 옮길 수 있어서 바로 따라들어갔습죠",
          "original_form": "다행히 카페로 옮길 수 있어서 바로 따라들어갔습죠",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021502.1.25",
          "form": "저는 그 타이밍에 놓쳤었어요ㅜㅜ  쫓겨나는 줄 알았죠",
          "original_form": "저는 그 타이밍에 놓쳤었어요ㅜㅜ  쫓겨나는 줄 알았죠",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021502.1.26",
          "form": "진짜 놀라셨겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 놀라셨겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021502.1.27",
          "form": "맞아요 진짜로ㅋㅋㅋㅋ 이런 커뮤가 또 없잖아요...",
          "original_form": "맞아요 진짜로ㅋㅋㅋㅋ 이런 커뮤가 또 없잖아요...",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021502.1.28",
          "form": "맞아요 여기 말고 마음 놓고 얘기 틀 공간이 없어요",
          "original_form": "맞아요 여기 말고 마음 놓고 얘기 틀 공간이 없어요",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100021502.1.29",
          "form": "경제공부는 다른 데서 할 수 있어도 비혼이랑 결혼 생각 있는 사람들이랑은 애초에 계획이 다르잖아요",
