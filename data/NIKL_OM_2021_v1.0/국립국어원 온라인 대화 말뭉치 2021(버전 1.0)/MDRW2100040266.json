      "utterance": [
        {
          "id": "MDRW2100040266.1.1",
          "form": "맞아요 감자튀김도 같이 주는데요",
          "original_form": "맞아요 감자튀김도 같이 주는데요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040266.1.2",
          "form": "감자튀김 너무 맛있어요",
          "original_form": "감자튀김 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040266.1.3",
          "form": "순살로 시켰는데요 한 마리 사면 반 마리 추가로 구매 가능하거든요",
          "original_form": "순살로 시켰는데요 한 마리 사면 반 마리 추가로 구매 가능하거든요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040266.1.4",
          "form": "그래서 1.5 인분 샀어요",
          "original_form": "그래서 1.5 인분 샀어요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040266.1.5",
          "form": "몇 명이서 사먹는데요",
          "original_form": "몇 명이서 사먹는데요",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.6",
          "form": "프라이드 양념 간장 이렇게 세 가지 맛으로요",
          "original_form": "프라이드 양념 간장 이렇게 세 가지 맛으로요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.7",
          "form": "그런데 간장은 생각보다 별로고 깐풍기 맛있었어요",
          "original_form": "그런데 간장은 생각보다 별로고 깐풍기 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.8",
          "form": "노랑 통닭도 깐풍이 맛있어요",
          "original_form": "노랑 통닭도 깐풍이 맛있어요",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.9",
          "form": "생맥 또 사갔습니다",
          "original_form": "생맥 또 사갔습니다",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.10",
          "form": "치킨에 맥주 아니겠네요",
          "original_form": "치킨에 맥주 아니겠네요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.11",
          "form": "치킨에는 무조건 맥주 죠",
          "original_form": "치킨에는 무조건 맥주 죠",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.12",
          "form": "콜라도 받았죠",
          "original_form": "콜라도 받았죠",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.13",
          "form": "근데 와인도 괜찮아요",
          "original_form": "근데 와인도 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040266.1.14",
          "form": "전에는 1.5 인분 살 때 물을 두 개를 주셨는데",
          "original_form": "전에는 1.5 인분 살 때 물을 두 개를 주셨는데",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040266.1.15",
          "form": "치킨에 와인을 합쳐서 치와와 치와와라고 해요",
          "original_form": "치킨에 와인을 합쳐서 치와와 치와와라고 해요",
          "speaker_id": "1",
          "time": "20210904 15:18"
