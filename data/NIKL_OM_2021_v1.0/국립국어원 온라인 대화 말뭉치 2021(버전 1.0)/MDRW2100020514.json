      "utterance": [
        {
          "id": "MDRW2100020514.1.1",
          "form": "세상에 넘 힘드시겠어요",
          "original_form": "세상에 넘 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210828 08:51"
        },
        {
          "id": "MDRW2100020514.1.2",
          "form": "갑각류를 못먹어요",
          "original_form": "갑각류를 못먹어요",
          "speaker_id": "1",
          "time": "20210828 08:51"
        },
        {
          "id": "MDRW2100020514.1.3",
          "form": "너무 슬퍼요",
          "original_form": "너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210828 08:51"
        },
        {
          "id": "MDRW2100020514.1.4",
          "form": "중국집에서 또 뭐 좋아하시나요",
          "original_form": "중국집에서 또 뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 08:51"
        },
        {
          "id": "MDRW2100020514.1.5",
          "form": "헉 ㅠㅠ 제친구도 갑각류 엄청 좋아하는네 못먹어서 안타깝더라구요",
          "original_form": "헉 ㅠㅠ 제친구도 갑각류 엄청 좋아하는네 못먹어서 안타깝더라구요",
          "speaker_id": "2",
          "time": "20210828 08:51"
        },
        {
          "id": "MDRW2100020514.1.6",
          "form": "찹쌀탕수육을 좋아합니다",
          "original_form": "찹쌀탕수육을 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.7",
          "form": "탕수육은 찹쌀이죠",
          "original_form": "탕수육은 찹쌀이죠",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.8",
          "form": "탕수육은 찹쌀이죠",
          "original_form": "탕수육은 찹쌀이죠",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.9",
          "form": "소오름",
          "original_form": "소오름",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.10",
          "form": "약간 쫀득 바삭 너무좋잖아요",
          "original_form": "약간 쫀득 바삭 너무좋잖아요",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.11",
          "form": "먹메이트를 만났네요",
          "original_form": "먹메이트를 만났네요",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.12",
          "form": "그럼 부먹 찍먹",
          "original_form": "그럼 부먹 찍먹",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.13",
          "form": "그쵸 혁신적이에요",
          "original_form": "그쵸 혁신적이에요",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.14",
          "form": "찍먹이랍니다",
          "original_form": "찍먹이랍니다",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.15",
          "form": "참고로 저는 막먹입니다 ㅎ",
          "original_form": "참고로 저는 막먹입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.16",
          "form": "오 올라운더시군요",
          "original_form": "오 올라운더시군요",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.17",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.18",
          "form": "저는 확신의 찍먹이에요",
          "original_form": "저는 확신의 찍먹이에요",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.19",
          "form": "실수하지않고 맞춰드릴 수 있게됐습니다",
          "original_form": "실수하지않고 맞춰드릴 수 있게됐습니다",
          "speaker_id": "1",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100020514.1.20",
          "form": "배려깊으신 분이로군요",
          "original_form": "배려깊으신 분이로군요",
          "speaker_id": "2",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.21",
          "form": "약간 찹쌀이 소스랑 만나면",
          "original_form": "약간 찹쌀이 소스랑 만나면",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.23",
          "form": "뭐라해야하지 제가 좀배려가 깊습니다",
          "original_form": "뭐라해야하지 제가 좀배려가 깊습니다",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.24",
          "form": "그 좀 더 쫀득해지잖아요",
          "original_form": "그 좀 더 쫀득해지잖아요",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.25",
          "form": "맞아요 쫀득함이 더해지죠",
          "original_form": "맞아요 쫀득함이 더해지죠",
          "speaker_id": "2",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.26",
          "form": "와 진짜",
          "original_form": "와 진짜",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.27",
          "form": "가방끈 너무기시다",
          "original_form": "가방끈 너무기시다",
          "speaker_id": "1",
          "time": "20210828 08:53"
        },
        {
          "id": "MDRW2100020514.1.28",
          "form": "어휴.. 타자를 칠 필요가 없네요",
          "original_form": "어휴.. 타자를 칠 필요가 없네요",
          "speaker_id": "2",
          "time": "20210828 08:53"
        }
      ]
    }
  ]
