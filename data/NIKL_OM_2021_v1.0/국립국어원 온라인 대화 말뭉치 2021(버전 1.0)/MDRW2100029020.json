      "utterance": [
        {
          "id": "MDRW2100029020.1.1",
          "form": "으으",
          "original_form": "으으",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029020.1.2",
          "form": "역시 탈비덥이답이었어",
          "original_form": "역시 탈비덥이답이었어",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029020.1.3",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029020.1.4",
          "form": "이러나저러나휘인이가10년가까이있던곳이고마마무만든곳이고지금도단체는알비덥에서하니까",
          "original_form": "이러나저러나휘인이가10년가까이있던곳이고마마무만든곳이고지금도단체는알비덥에서하니까",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.5",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.6",
          "form": "너무비교는하지말아야지이랬는데",
          "original_form": "너무비교는하지말아야지이랬는데",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.7",
          "form": "아니.....",
          "original_form": "아니.....",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.8",
          "form": "그래도 이건 너무하잖아",
          "original_form": "그래도 이건 너무하잖아",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.9",
          "form": "작작해야지.....",
          "original_form": "작작해야지.....",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029020.1.10",
          "form": "아니 안할래야안할수업게만드는건",
          "original_form": "아니 안할래야안할수업게만드는건",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029020.1.11",
          "form": "비덥이선빵을치네",
          "original_form": "비덥이선빵을치네",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029020.1.12",
          "form": "그니까.....",
          "original_form": "그니까.....",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029020.1.13",
          "form": "하나하나얘기하고싶진않지만",
          "original_form": "하나하나얘기하고싶진않지만",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029020.1.14",
          "form": "해외팬들이제발공지올릴때영어도같이해달라고몇년을얘기했는데",
          "original_form": "해외팬들이제발공지올릴때영어도같이해달라고몇년을얘기했는데",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029020.1.15",
          "form": "그것도안해주더니",
          "original_form": "그것도안해주더니",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029020.1.16",
          "form": "더라이브는얘기나온지이틀만에영어추가해주더라....",
          "original_form": "더라이브는얘기나온지이틀만에영어추가해주더라....",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029020.1.17",
          "form": "진짜.... 알비덥 너무하다",
          "original_form": "진짜.... 알비덥 너무하다",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029020.1.18",
          "form": "기본은해야지",
          "original_form": "기본은해야지",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029020.1.19",
          "form": "뭐어려운거바라냐고.....",
          "original_form": "뭐어려운거바라냐고.....",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029020.1.20",
          "form": "영어공지솔까요즘돌판이면필요한거아님?",
          "original_form": "영어공지솔까요즘돌판이면필요한거아님?",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029020.1.21",
          "form": "공지나자컨자막에영어달아달라는얘기가나닙덕하고부터니까몇년이야....",
          "original_form": "공지나자컨자막에영어달아달라는얘기가나닙덕하고부터니까몇년이야....",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029020.1.22",
          "form": "4년이네",
          "original_form": "4년이네",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029020.1.23",
          "form": ".........어후",
          "original_form": ".........어후",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029020.1.24",
          "form": "진짜왜그러냐",
          "original_form": "진짜왜그러냐",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029020.1.25",
          "form": "해외팬도많은데",
          "original_form": "해외팬도많은데",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029020.1.26",
          "form": "예전이야소속돌도우리뿐이니",
          "original_form": "예전이야소속돌도우리뿐이니",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.27",
          "form": "번역하는사람고용하거나하는거애매했다쳐도",
          "original_form": "번역하는사람고용하거나하는거애매했다쳐도",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.28",
          "form": "요즘은4팀이나되는대",
          "original_form": "요즘은4팀이나되는대",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.29",
          "form": "왜안해줘.....",
          "original_form": "왜안해줘.....",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.30",
          "form": "ㅁㅇ.....",
          "original_form": "ㅁㅇ.....",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.31",
          "form": "아니 더안크고싶냐",
          "original_form": "아니 더안크고싶냐",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.32",
          "form": "진우야",
          "original_form": "진우야",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.33",
          "form": "몰랐는데해외팬들트럭도한번갈겼다더만",
          "original_form": "몰랐는데해외팬들트럭도한번갈겼다더만",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029020.1.34",
          "form": "답이없다...",
          "original_form": "답이없다...",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029020.1.35",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029020.1.36",
          "form": "트럭갈겼는데ㄷ이러냐",
          "original_form": "트럭갈겼는데ㄷ이러냐",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029020.1.37",
          "form": "해외팬덤세력이",
          "original_form": "해외팬덤세력이",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029020.1.38",
          "form": "클텐데",
          "original_form": "클텐데",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029020.1.39",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029020.1.40",
          "form": "적은편은아님",
          "original_form": "적은편은아님",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029020.1.41",
          "form": "몰라....니네가알아서해라",
          "original_form": "몰라....니네가알아서해라",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029020.1.42",
          "form": "계속이러는데태도가바뀔거같지도않고",
          "original_form": "계속이러는데태도가바뀔거같지도않고",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029020.1.43",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029020.1.44",
          "form": "하여간 ㅡㅡ알비덥",
          "original_form": "하여간 ㅡㅡ알비덥",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029020.1.45",
          "form": "고생이많다....",
          "original_form": "고생이많다....",
          "speaker_id": "1",
