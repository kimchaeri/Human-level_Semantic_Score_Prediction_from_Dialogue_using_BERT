      "utterance": [
        {
          "id": "MDRW2100033637.1.1",
          "form": "뭐 사고싶은 거 있으세요?",
          "original_form": "뭐 사고싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100033637.1.2",
          "form": "저는 사고싶은건많아요ㅋㅋ",
          "original_form": "저는 사고싶은건많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100033637.1.3",
          "form": "백도 사고싶고ㅋㅋ",
          "original_form": "백도 사고싶고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100033637.1.4",
          "form": "옷도 사고싶네요ㅋ",
          "original_form": "옷도 사고싶네요ㅋ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100033637.1.5",
          "form": "저도여ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도여ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100033637.1.6",
          "form": "어떤 가방",
          "original_form": "어떤 가방",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100033637.1.7",
          "form": "사고싶으세요",
          "original_form": "사고싶으세요",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100033637.1.8",
          "form": "그냥 마음은 ㅋㅋㅋ명품백사고싶네요ㅋㅋ",
          "original_form": "그냥 마음은 ㅋㅋㅋ명품백사고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100033637.1.9",
          "form": "샤, ..넬.. ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "샤, ..넬.. ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100033637.1.10",
          "form": "마음만요 히히",
          "original_form": "마음만요 히히",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100033637.1.11",
          "form": "어머 저두요",
          "original_form": "어머 저두요",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100033637.1.12",
          "form": "샤넬 진짜",
          "original_form": "샤넬 진짜",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100033637.1.13",
          "form": "예뻐요",
          "original_form": "예뻐요",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100033637.1.14",
          "form": "ㅋㅋㅋㅋㅋ맞아요. 예쁘죠ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요. 예쁘죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100033637.1.15",
          "form": "원하시는 색상있나요?",
          "original_form": "원하시는 색상있나요?",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100033637.1.16",
          "form": "전 블랙이요",
          "original_form": "전 블랙이요",
          "speaker_id": "2",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100033637.1.17",
          "form": "샤넬 호보백 너무 갖고싶어요 ㅎ",
          "original_form": "샤넬 호보백 너무 갖고싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100033637.1.18",
          "form": "저도 블랙이요ㅋ",
          "original_form": "저도 블랙이요ㅋ",
          "speaker_id": "1",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100033637.1.19",
          "form": "호보백 이쁘죠ㅋㅋ",
          "original_form": "호보백 이쁘죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100033637.1.20",
          "form": "네 엄청이요 ㅠ",
          "original_form": "네 엄청이요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100033637.1.21",
          "form": "구하기도 힘들죠",
          "original_form": "구하기도 힘들죠",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033637.1.22",
          "form": "그렇죠ㅠㅜ",
          "original_form": "그렇죠ㅠㅜ",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033637.1.23",
          "form": "사기힘들어요.",
          "original_form": "사기힘들어요.",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033637.1.24",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033637.1.25",
          "form": "사려먄 오픈런도 백화점에서",
          "original_form": "사려먄 오픈런도 백화점에서",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033637.1.26",
          "form": "뛰어야하고.. 충격..",
          "original_form": "뛰어야하고.. 충격..",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033637.1.27",
          "form": "돈있어도 못사죠ㅜㅋㅋ",
          "original_form": "돈있어도 못사죠ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033637.1.28",
          "form": "맞아요 ㅎ ㅎㅎ",
          "original_form": "맞아요 ㅎ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100033637.1.29",
          "form": "사기 힘든놈 ㅠㅠ",
          "original_form": "사기 힘든놈 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100033637.1.30",
          "form": "비싸요.ㅜㅜㅋㅋ",
          "original_form": "비싸요.ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:10"
        }
      ]
    }
