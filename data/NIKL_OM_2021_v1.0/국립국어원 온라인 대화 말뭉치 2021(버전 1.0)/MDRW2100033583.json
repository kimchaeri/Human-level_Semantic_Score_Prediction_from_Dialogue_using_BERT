        },
        {
          "id": "MDRW2100033583.1.13",
          "form": "2000라지 감자튀김  ㄷㄷ",
          "original_form": "2000라지 감자튀김  ㄷㄷ",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100033583.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100033583.1.15",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100033583.1.16",
          "form": "3900의 행복이었음",
          "original_form": "3900의 행복이었음",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100033583.1.17",
          "form": "노브랜드 감튀 소금 전혀 안친 맛임 ㅋㅋ",
          "original_form": "노브랜드 감튀 소금 전혀 안친 맛임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100033583.1.18",
          "form": "나름 건강한건가?ㅋㅋ",
          "original_form": "나름 건강한건가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100033583.1.19",
          "form": "음 케첩 찍어먹으면..거기서 거기?",
          "original_form": "음 케첩 찍어먹으면..거기서 거기?",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100033583.1.20",
          "form": "구매 희망 물건은 갖고싶은거 넘마늠ㅜ",
          "original_form": "구매 희망 물건은 갖고싶은거 넘마늠ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100033583.1.21",
          "form": "나는 내방 갖고싶음ㅋㅋㅋ",
          "original_form": "나는 내방 갖고싶음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100033583.1.22",
          "form": "비새는방 말고",
          "original_form": "비새는방 말고",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100033583.1.23",
          "form": "모든 소시민의 꿈",
          "original_form": "모든 소시민의 꿈",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100033583.1.24",
          "form": "마이홈 ㅋㅋㅋ",
          "original_form": "마이홈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100033583.1.25",
          "form": "너무나 비싼 구매희망물건이군요 ㅋㅋ",
          "original_form": "너무나 비싼 구매희망물건이군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100033583.1.26",
          "form": "젠장 서울에 자가 있는 부모님께 태어났어야했음",
          "original_form": "젠장 서울에 자가 있는 부모님께 태어났어야했음",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100033583.1.27",
          "form": "글게 ㅋㅋ",
          "original_form": "글게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033583.1.28",
          "form": "인구가 줄어든다는데 왜 전 집이없죠 ㅋㅋ",
          "original_form": "인구가 줄어든다는데 왜 전 집이없죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033583.1.29",
          "form": "그러게요 ㅋㅋ",
          "original_form": "그러게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033583.1.30",
          "form": "난  우리아파트 40년 다되가서 불안함",
          "original_form": "난  우리아파트 40년 다되가서 불안함",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033583.1.31",
          "form": "접때 미국아파트도 40년되니까 붕괴되드만",
          "original_form": "접때 미국아파트도 40년되니까 붕괴되드만",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033583.1.32",
          "form": "울 부모님 아파트도 오래됐는데ㅜㅜ",
          "original_form": "울 부모님 아파트도 오래됐는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100033583.1.33",
          "form": "사고싶은건 지금 저녁밥이군",
          "original_form": "사고싶은건 지금 저녁밥이군",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100033583.1.34",
          "form": "ㅋㅋㅋ 밖에 비안옴? 나가는것도일일듯",
          "original_form": "ㅋㅋㅋ 밖에 비안옴? 나가는것도일일듯",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
