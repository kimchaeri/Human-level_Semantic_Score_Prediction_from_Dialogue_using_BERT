        },
        {
          "id": "MDRW2100033583.1.8",
          "form": "오 당근가네",
          "original_form": "오 당근가네",
          "speaker_id": "2",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100033583.1.9",
          "form": "최근 구매 물건",
          "original_form": "최근 구매 물건",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100033583.1.10",
