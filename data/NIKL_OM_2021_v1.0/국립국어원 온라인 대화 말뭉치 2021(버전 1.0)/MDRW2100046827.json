        },
        {
          "id": "MDRW2100046827.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20211104 19:56"
        },
        {
          "id": "MDRW2100046827.1.3",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 19:56"
        },
        {
          "id": "MDRW2100046827.1.4",
          "form": "요즘 들어서 즐기고 계시는 취미가 있으시나요?",
          "original_form": "요즘 들어서 즐기고 계시는 취미가 있으시나요?",
          "speaker_id": "2",
          "time": "20211104 19:56"
        },
        {
          "id": "MDRW2100046827.1.5",
          "form": "헬스요",
          "original_form": "헬스요",
          "speaker_id": "1",
          "time": "20211104 19:57"
        },
        {
          "id": "MDRW2100046827.1.6",
          "form": "취미있나여",
          "original_form": "취미있나여",
          "speaker_id": "1",
          "time": "20211104 19:57"
        },
        {
          "id": "MDRW2100046827.1.7",
          "form": "헬스가 취미라니 ㅎㅎ",
          "original_form": "헬스가 취미라니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 19:57"
        },
        {
          "id": "MDRW2100046827.1.8",
          "form": "건강하신 분이군요..",
          "original_form": "건강하신 분이군요..",
          "speaker_id": "2",
          "time": "20211104 19:57"
        },
        {
          "id": "MDRW2100046827.1.9",
          "form": "ㅎㅎㅎㅎ;취미뭐 에영?",
          "original_form": "ㅎㅎㅎㅎ;취미뭐 에영?",
          "speaker_id": "1",
          "time": "20211104 19:58"
        },
        {
          "id": "MDRW2100046827.1.10",
          "form": "저는 뮤지컬 관람이 취미입니다",
          "original_form": "저는 뮤지컬 관람이 취미입니다",
          "speaker_id": "2",
          "time": "20211104 19:58"
        },
        {
          "id": "MDRW2100046827.1.11",
          "form": "고급 취미네요",
          "original_form": "고급 취미네요",
          "speaker_id": "1",
          "time": "20211104 19:58"
        },
        {
          "id": "MDRW2100046827.1.12",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "1",
          "time": "20211104 19:58"
        },
        {
          "id": "MDRW2100046827.1.13",
          "form": "네.. 돈이 엄청 들어서",
          "original_form": "네.. 돈이 엄청 들어서",
          "speaker_id": "2",
          "time": "20211104 20:00"
        },
        {
          "id": "MDRW2100046827.1.14",
          "form": "자주는 맛보는데",
          "original_form": "자주는 맛보는데",
          "speaker_id": "2",
          "time": "20211104 20:00"
        },
        {
          "id": "MDRW2100046827.1.15",
          "form": "못보는데",
          "original_form": "못보는데",
          "speaker_id": "2",
          "time": "20211104 20:00"
        },
        {
          "id": "MDRW2100046827.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.17",
          "form": "볼때마다 눈물 흘리고 옵니다..",
          "original_form": "볼때마다 눈물 흘리고 옵니다..",
          "speaker_id": "2",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.18",
          "form": "감동적일거같아요",
          "original_form": "감동적일거같아요",
          "speaker_id": "1",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.19",
          "form": "돈 많이 벌어야겠다고 다짐하게 된달까 ㅎㅎ",
          "original_form": "돈 많이 벌어야겠다고 다짐하게 된달까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.20",
          "form": "뮤지컬 보신 적 있으세요?",
          "original_form": "뮤지컬 보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046827.1.22",
          "form": "옷 연말에 한 번 봐주세요",
          "original_form": "옷 연말에 한 번 봐주세요",
          "speaker_id": "2",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.23",
          "form": "지킬 앤 하이드나",
          "original_form": "지킬 앤 하이드나",
          "speaker_id": "2",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.24",
          "form": "레베카 추천드려요 ㅎㅎ",
          "original_form": "레베카 추천드려요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.25",
          "form": "네 그럴게요",
          "original_form": "네 그럴게요",
          "speaker_id": "1",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.26",
          "form": "레베카",
          "original_form": "레베카",
          "speaker_id": "1",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.27",
          "form": "볼게요 ㅋㅋ",
          "original_form": "볼게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.28",
          "form": "ㅎㅎㅎ 네네 옥주현 배우님이 유명하시긴한데",
          "original_form": "ㅎㅎㅎ 네네 옥주현 배우님이 유명하시긴한데",
          "speaker_id": "2",
          "time": "20211104 20:04"
        },
        {
          "id": "MDRW2100046827.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20211104 20:05"
        },
        {
          "id": "MDRW2100046827.1.30",
          "form": "신영숙 배우님도 강추드려요",
          "original_form": "신영숙 배우님도 강추드려요",
          "speaker_id": "2",
          "time": "20211104 20:05"
        },
        {
          "id": "MDRW2100046827.1.31",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 20:05"
        },
        {
          "id": "MDRW2100046827.1.32",
          "form": "아주 좋아요 ㅋㅋㅋㅌ",
          "original_form": "아주 좋아요 ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20211104 20:05"
        },
        {
          "id": "MDRW2100046827.1.33",
          "form": "사실 어떤 배우님으로 봐도 좋으실거에요 ㅎㅎㅋㅋ",
