      "utterance": [
        {
          "id": "MDRW2100042687.1.1",
          "form": "오늘 저녁 뭐드셨어요",
          "original_form": "오늘 저녁 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210907 01:24"
        },
        {
          "id": "MDRW2100042687.1.2",
          "form": "전 떡볶이랑 빵 먹었어요!",
          "original_form": "전 떡볶이랑 빵 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 08:09"
        },
        {
          "id": "MDRW2100042687.1.3",
          "form": "떡볶이 어디떡볶이여",
          "original_form": "떡볶이 어디떡볶이여",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100042687.1.4",
          "form": "백화점 지하에서 사온 거라 어디떡볶이라고 하기엔 애매하네요...ㅎㅎ",
          "original_form": "백화점 지하에서 사온 거라 어디떡볶이라고 하기엔 애매하네요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100042687.1.5",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100042687.1.6",
          "form": "떡볶이 밀떡파세요 쌀떡파세요",
          "original_form": "떡볶이 밀떡파세요 쌀떡파세요",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100042687.1.7",
          "form": "저는 쌀떡파요!",
          "original_form": "저는 쌀떡파요!",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100042687.1.8",
          "form": "오 전 밀떡파여 소스가 베이는걸좋아해서",
          "original_form": "오 전 밀떡파여 소스가 베이는걸좋아해서",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100042687.1.9",
          "form": "쌀떡 이유는여",
          "original_form": "쌀떡 이유는여",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100042687.1.10",
          "form": "저는 쫄깃쫄깃한 식감이 너무 좋아요",
          "original_form": "저는 쫄깃쫄깃한 식감이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100042687.1.11",
          "form": "아 그쳐 ㅋㅋㅋ쌀떡특유의",
          "original_form": "아 그쳐 ㅋㅋㅋ쌀떡특유의",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100042687.1.12",
          "form": "네네 그 식감이 너무 좋더라구요",
          "original_form": "네네 그 식감이 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100042687.1.13",
          "form": "맞아요 쫀득하고쫄깃",
          "original_form": "맞아요 쫀득하고쫄깃",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100042687.1.14",
          "form": "매운거 잘드시나여",
          "original_form": "매운거 잘드시나여",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100042687.1.15",
          "form": "잘 먹진 않는데 좋아하긴 해요",
          "original_form": "잘 먹진 않는데 좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100042687.1.16",
          "form": "전 완전맵찔이라 떡볶이는 무조건 순한맛이거든여 ㅋㅋㅋㅋ달다구리한걸로",
          "original_form": "전 완전맵찔이라 떡볶이는 무조건 순한맛이거든여 ㅋㅋㅋㅋ달다구리한걸로",
          "speaker_id": "1",
