      "utterance": [
        {
          "id": "MDRW2100023806.1.1",
          "form": "안녕하세요 혹시 교정이나 라식 해보셧나요",
          "original_form": "안녕하세요 혹시 교정이나 라식 해보셧나요",
          "speaker_id": "1",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100023806.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023806.1.3",
          "form": "저는 둘 다 안 해봤어요",
          "original_form": "저는 둘 다 안 해봤어요",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023806.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023806.1.5",
          "form": "저도 둘다 안해봣는데",
          "original_form": "저도 둘다 안해봣는데",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023806.1.6",
          "form": "둘다 관심이 잇어서요!",
          "original_form": "둘다 관심이 잇어서요!",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023806.1.7",
          "form": "치열이 고르신가요",
          "original_form": "치열이 고르신가요",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023806.1.8",
          "form": "엄청 정갈하진 않은데 교정할 필요는 없는것같아요",
          "original_form": "엄청 정갈하진 않은데 교정할 필요는 없는것같아요",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100023806.1.9",
          "form": "오 잘됏네요",
          "original_form": "오 잘됏네요",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.10",
          "form": "name1은 치열 고르세요?",
          "original_form": "&name1&은 치열 고르세요?",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.11",
          "form": "저는 덧니가 있어서 병원에서 교정을 권했는데",
          "original_form": "저는 덧니가 있어서 병원에서 교정을 권했는데",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.12",
          "form": "교정하려면 이빨 네개를 뽑아야 한대요!",
          "original_form": "교정하려면 이빨 네개를 뽑아야 한대요!",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.13",
          "form": "헤엑",
          "original_form": "헤엑",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.14",
          "form": "아프겠네요",
          "original_form": "아프겠네요",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.15",
          "form": "너무 무서워서 일단 안할거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "너무 무서워서 일단 안할거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.16",
          "form": "교정을 할 때는",
          "original_form": "교정을 할 때는",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.17",
          "form": "진짜 너무 무서운데요??",
          "original_form": "진짜 너무 무서운데요??",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.18",
          "form": "그 작은 어금니를 뽑는 거래요",
          "original_form": "그 작은 어금니를 뽑는 거래요",
          "speaker_id": "1",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100023806.1.19",
          "form": "그쵸 생니를 네개나",
          "original_form": "그쵸 생니를 네개나",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023806.1.20",
          "form": "생니를 네개나..",
          "original_form": "생니를 네개나..",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023806.1.21",
          "form": "짝수로 뽑아야 한대요",
          "original_form": "짝수로 뽑아야 한대요",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023806.1.22",
          "form": "작은 어금니요??",
          "original_form": "작은 어금니요??",
          "speaker_id": "2",
          "time": "20210830 17:56"
        }
      ]
