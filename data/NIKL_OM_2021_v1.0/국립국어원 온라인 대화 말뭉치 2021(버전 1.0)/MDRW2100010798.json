      "utterance": [
        {
          "id": "MDRW2100010798.1.1",
          "form": "로봇청소기 사용하세요?",
          "original_form": "로봇청소기 사용하세요?",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010798.1.2",
          "form": "네ㅎㅎ 근데 괜히 산거같은ㅎㅎ",
          "original_form": "네ㅎㅎ 근데 괜히 산거같은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010798.1.3",
          "form": "잘 안 쓰게 되나요?",
          "original_form": "잘 안 쓰게 되나요?",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010798.1.4",
          "form": "네 그냥 청소기만 못하더라구요",
          "original_form": "네 그냥 청소기만 못하더라구요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010798.1.5",
          "form": "저희 집은 아직 유선청소기 쓰고 있어요",
          "original_form": "저희 집은 아직 유선청소기 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010798.1.6",
          "form": "저도 그냥 일반 청소기로 돌아갈거같아요ㅋㅋ",
          "original_form": "저도 그냥 일반 청소기로 돌아갈거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:04"
        },
        {
          "id": "MDRW2100010798.1.7",
          "form": "무선청소기 사고 싶긴 한데 매장 가서 들어보면 다 은근 무겁더라구요",
          "original_form": "무선청소기 사고 싶긴 한데 매장 가서 들어보면 다 은근 무겁더라구요",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010798.1.8",
          "form": "소파에 끼어 있는거 건지다가 허리다칠뻔했어요ㅋㅋ 은근 무게있어요",
          "original_form": "소파에 끼어 있는거 건지다가 허리다칠뻔했어요ㅋㅋ 은근 무게있어요",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010798.1.9",
          "form": "어쩌다 소파에 ㅋㅋㅠㅠ\r\n집안일은 하자면 끝이 없는 것 같아요. 바쁘면 대충 미룰 때도 많지만요 ㅋㅋ",
          "original_form": "어쩌다 소파에 ㅋㅋㅠㅠ\r\n집안일은 하자면 끝이 없는 것 같아요. 바쁘면 대충 미룰 때도 많지만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010798.1.10",
          "form": "맞아요 하는건 티가 안나는데 안하면 티가나요ㅠㅠ",
          "original_form": "맞아요 하는건 티가 안나는데 안하면 티가나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010798.1.11",
          "form": "기름때는 왜 이렇게 잘 생기는지ㅋㅋ 바로바로 안 하면 지우기까지 난이도가 상승하더라구요",
          "original_form": "기름때는 왜 이렇게 잘 생기는지ㅋㅋ 바로바로 안 하면 지우기까지 난이도가 상승하더라구요",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010798.1.12",
          "form": "맞아요 세제 아무리 좋아도 그때그때 지우는게 제일 좋은ㅠㅜㅜ",
          "original_form": "맞아요 세제 아무리 좋아도 그때그때 지우는게 제일 좋은ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010798.1.13",
          "form": "설거지도 그렇고 바로바로 하는 게 낫더라구요 ㅠ",
          "original_form": "설거지도 그렇고 바로바로 하는 게 낫더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 22:23"
        }
      ]
