        {
          "id": "MDRW2100031100.1.1",
          "form": "name2님 먹는 것도 좋아하시나요?",
          "original_form": "&name2&님 먹는 것도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031100.1.2",
          "form": "고럼요 ㅎㅎ",
          "original_form": "고럼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031100.1.3",
          "form": "약속 잡히면 맛집 조사하는 게 낙입니다",
          "original_form": "약속 잡히면 맛집 조사하는 게 낙입니다",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031100.1.4",
          "form": "우왁 어떤 음식 좋아하세요!",
          "original_form": "우왁 어떤 음식 좋아하세요!",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031100.1.5",
          "form": "국적 재료 안 가립니다ㅋㅋㅋ",
          "original_form": "국적 재료 안 가립니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031100.1.6",
          "form": "베트남 인도 멕시코 일본 중국",
          "original_form": "베트남 인도 멕시코 일본 중국",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031100.1.7",
          "form": "헛 편식 안 하시나봐요!",
          "original_form": "헛 편식 안 하시나봐요!",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031100.1.8",
          "form": "혿시 마라탕 좋아하세요!?",
          "original_form": "혿시 마라탕 좋아하세요!?",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031100.1.9",
          "form": "엄청 좋아하진 않는데 한번씩 먹고 싶더라구요ㅎㅎ",
          "original_form": "엄청 좋아하진 않는데 한번씩 먹고 싶더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031100.1.10",
          "form": "헉 전 완전 좋아해서 맨날 먹거든요..!",
          "original_form": "헉 전 완전 좋아해서 맨날 먹거든요..!",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031100.1.11",
          "form": "그럼 혹시 떡볶이는요!?",
          "original_form": "그럼 혹시 떡볶이는요!?",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031100.1.12",
          "form": "떡볶이는 소울푸드요",
          "original_form": "떡볶이는 소울푸드요",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031100.1.13",
          "form": "분식집 메뉴들",
          "original_form": "분식집 메뉴들",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031100.1.14",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031100.1.15",
          "form": "헛 저도요! 저 그 당면만두 좋아해요 ㅎ.ㅎ",
          "original_form": "헛 저도요! 저 그 당면만두 좋아해요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031100.1.16",
          "form": "name2님 최애 떡볶이 브랜드 뭔가요!?",
          "original_form": "&name2&님 최애 떡볶이 브랜드 뭔가요!?",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031100.1.17",
          "form": "신참 괜찮더라구요, 고구마떡볶이 좋았어요",
          "original_form": "신참 괜찮더라구요, 고구마떡볶이 좋았어요",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031100.1.18",
          "form": "걸작도 먹어보고 싶은데",
          "original_form": "걸작도 먹어보고 싶은데",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031100.1.19",
          "form": "집근처에 없어서 지인 집에 가서",
          "original_form": "집근처에 없어서 지인 집에 가서",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031100.1.20",
          "form": "먹으려구요ㅎㅎ",
          "original_form": "먹으려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031100.1.21",
          "form": "걸작은 먹어봤는데 신참은 처음 들어봐요",
          "original_form": "걸작은 먹어봤는데 신참은 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031100.1.22",
          "form": "약간 달달한 떡볶이인가요 아님 카레맛 나는 떡볶이?!",
          "original_form": "약간 달달한 떡볶이인가요 아님 카레맛 나는 떡볶이?!",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031100.1.23",
          "form": "카레맛은 아니에요, 고구마떡볶이는 달달하고",
          "original_form": "카레맛은 아니에요, 고구마떡볶이는 달달하고",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100031100.1.24",
          "form": "일반떡볶이는 다른 프랜차이즈 국물떡볶이랑 비슷한 느낌이에요",
          "original_form": "일반떡볶이는 다른 프랜차이즈 국물떡볶이랑 비슷한 느낌이에요",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100031100.1.25",
          "form": "우왁!!!! 저도 집 주변에 있나 찾아봐야겠구만요 ㅎ.ㅎ !",
          "original_form": "우왁!!!! 저도 집 주변에 있나 찾아봐야겠구만요 ㅎ.ㅎ !",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100031100.1.26",
          "form": "name2님은 떡볶이 사이드로는 뭐 좋아하세요!?",
          "original_form": "&name2&님은 떡볶이 사이드로는 뭐 좋아하세요!?",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100031100.1.27",
          "form": "김밥 짱 좋아하고 순대나 감자튀김, 야채튀김도 가리지않아요",
          "original_form": "김밥 짱 좋아하고 순대나 감자튀김, 야채튀김도 가리지않아요",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100031100.1.28",
          "form": "헐헐 저도 김밥 짱 좋아해요!!!",
          "original_form": "헐헐 저도 김밥 짱 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100031100.1.29",
          "form": "저도 김밥 완전 좋아해요!",
          "original_form": "저도 김밥 완전 좋아해요!",
