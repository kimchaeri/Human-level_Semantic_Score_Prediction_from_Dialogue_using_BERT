      "utterance": [
        {
          "id": "MDRW2100025323.1.1",
          "form": "참여중인 동아리 있으세요?",
          "original_form": "참여중인 동아리 있으세요?",
          "speaker_id": "1",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025323.1.2",
          "form": "ㅋㅋㅋ 아녀 없어요 대리님은 있으세여?",
          "original_form": "ㅋㅋㅋ 아녀 없어요 대리님은 있으세여?",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100025323.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아웃겨",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025323.1.4",
          "form": "제가 얘기했을껄요? 영어동아리요",
          "original_form": "제가 얘기했을껄요? 영어동아리요",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025323.1.5",
          "form": "어 그거 되게 전에 한다구 들은 거 같은데 아직도 하세여?",
          "original_form": "어 그거 되게 전에 한다구 들은 거 같은데 아직도 하세여?",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025323.1.6",
          "form": "영어학원 최근에 3개월댕긴데서 스터디모임하나 만들었어요ㅋㅋ",
          "original_form": "영어학원 최근에 3개월댕긴데서 스터디모임하나 만들었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025323.1.7",
          "form": "영어학원은 안댕김",
          "original_form": "영어학원은 안댕김",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025323.1.8",
          "form": "지금은요",
          "original_form": "지금은요",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025323.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025323.1.10",
          "form": "저도 뭐 하나 하구 싶어서 기웃댔거든요",
          "original_form": "저도 뭐 하나 하구 싶어서 기웃댔거든요",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025323.1.11",
          "form": "운동모임 그런더 ㅋㅋㅋㅋ 근데 넘 본격적이구 다들... 퇴근 후에 뭘 한다는 게 쉽지 않더라고요",
          "original_form": "운동모임 그런더 ㅋㅋㅋㅋ 근데 넘 본격적이구 다들... 퇴근 후에 뭘 한다는 게 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025323.1.12",
          "form": "주말에도 누워있기바쁨",
          "original_form": "주말에도 누워있기바쁨",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025323.1.13",
          "form": "그러긴해요",
          "original_form": "그러긴해요",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025323.1.14",
          "form": "사실정말귀찮음ㅋㅋㅋ",
          "original_form": "사실정말귀찮음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025323.1.15",
          "form": "ㅋㅋㅋㅋㅋ 그래도 영어능력치 쑥쑥",
          "original_form": "ㅋㅋㅋㅋㅋ 그래도 영어능력치 쑥쑥",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025323.1.16",
          "form": "만나서 머ㅓ 하세요?",
          "original_form": "만나서 머ㅓ 하세요?",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025323.1.17",
          "form": "프리토킹 아님 토익?",
          "original_form": "프리토킹 아님 토익?",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025323.1.18",
          "form": "토익따위 공부안합니다ㅋㅋ",
          "original_form": "토익따위 공부안합니다ㅋㅋ",
