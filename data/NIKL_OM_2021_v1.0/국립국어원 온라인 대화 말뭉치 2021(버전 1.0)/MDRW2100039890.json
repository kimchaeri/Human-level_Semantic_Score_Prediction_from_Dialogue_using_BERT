      "utterance": [
        {
          "id": "MDRW2100039890.1.1",
          "form": "소개팅한번받아봐요\r\n아니받아봣자나!!!",
          "original_form": "소개팅한번받아봐요\r\n아니받아봣자나!!!",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039890.1.2",
          "form": "그냥그랫자나힝",
          "original_form": "그냥그랫자나힝",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039890.1.3",
          "form": "소개팅ㅠㅠ",
          "original_form": "소개팅ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039890.1.4",
          "form": "전 이제 헤어지면 그냥 혼자사는거에요",
          "original_form": "전 이제 헤어지면 그냥 혼자사는거에요",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039890.1.5",
          "form": "전 약간 근처에서 찾는 스탈인거같아요",
          "original_form": "전 약간 근처에서 찾는 스탈인거같아요",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039890.1.6",
          "form": "소개팅 노노",
          "original_form": "소개팅 노노",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039890.1.7",
          "form": "name2님은 어떠세요",
          "original_form": "&name2&님은 어떠세요",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039890.1.8",
          "form": "헤어지면 뭘 혼자 살아요",
          "original_form": "헤어지면 뭘 혼자 살아요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039890.1.9",
          "form": "그런말하지마요",
          "original_form": "그런말하지마요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039890.1.10",
          "form": "걱정이많은 밤이메요",
          "original_form": "걱정이많은 밤이메요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039890.1.11",
          "form": "한치앞도 알수없는 인생 ㅋㅋ",
          "original_form": "한치앞도 알수없는 인생 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039890.1.12",
          "form": "참 어렵다 연애결혼",
          "original_form": "참 어렵다 연애결혼",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039890.1.13",
          "form": "괴롭다괴로워",
          "original_form": "괴롭다괴로워",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039890.1.14",
          "form": "인연이면 할거고 아니라면 더 좋은 인연 오겠죠",
          "original_form": "인연이면 할거고 아니라면 더 좋은 인연 오겠죠",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039890.1.15",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039890.1.16",
          "form": "나이들수록어렵다",
          "original_form": "나이들수록어렵다",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039890.1.17",
          "form": "조건도많아져",
          "original_form": "조건도많아져",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039890.1.18",
          "form": "맞아요 그런거같아요",
          "original_form": "맞아요 그런거같아요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039890.1.19",
          "form": "조건많아져서될문제가아닌데",
          "original_form": "조건많아져서될문제가아닌데",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039890.1.20",
          "form": "ㅋㅋㄱㅜㅜ후",
          "original_form": "ㅋㅋㄱㅜㅜ후",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039890.1.21",
          "form": "그러데요 ㅋㅋㅋ",
          "original_form": "그러데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039890.1.22",
          "form": "조건을 줄야봅시다",
          "original_form": "조건을 줄야봅시다",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039890.1.23",
          "form": "줄이자니 이상한놈 만날카봐무서워요",
          "original_form": "줄이자니 이상한놈 만날카봐무서워요",
