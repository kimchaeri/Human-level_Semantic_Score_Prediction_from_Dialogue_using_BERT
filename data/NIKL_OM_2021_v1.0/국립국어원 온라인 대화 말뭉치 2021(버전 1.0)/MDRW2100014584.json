      "utterance": [
        {
          "id": "MDRW2100014584.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014584.1.2",
          "form": "직관 가보신적있으신가요?",
          "original_form": "직관 가보신적있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014584.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014584.1.4",
          "form": "야구보러 딱 한 번 가봤네요",
          "original_form": "야구보러 딱 한 번 가봤네요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014584.1.5",
          "form": "직관 자주 하세요?",
          "original_form": "직관 자주 하세요?",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014584.1.6",
          "form": "저는 치맥하러 야구장갔어요",
          "original_form": "저는 치맥하러 야구장갔어요",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014584.1.7",
          "form": "아니요 프로 방콕녀라",
          "original_form": "아니요 프로 방콕녀라",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014584.1.8",
          "form": "저도 집순이라 동생따라 처음 가봤어요",
          "original_form": "저도 집순이라 동생따라 처음 가봤어요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014584.1.9",
          "form": "경기는 편하게 집에서 보는게 최고!",
          "original_form": "경기는 편하게 집에서 보는게 최고!",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014584.1.10",
          "form": "코시국때라 경기보면서는 못 먹고요",
          "original_form": "코시국때라 경기보면서는 못 먹고요",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014584.1.11",
          "form": "어떤 스포츠좋아하세요?",
          "original_form": "어떤 스포츠좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014584.1.12",
          "form": "그래도 직관 되게 신나더라구요",
          "original_form": "그래도 직관 되게 신나더라구요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.13",
          "form": "전 축구",
          "original_form": "전 축구",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.14",
          "form": "사실 스포츠는 잘 몰라서 남 보는 거 같이 보는 편이에요",
          "original_form": "사실 스포츠는 잘 몰라서 남 보는 거 같이 보는 편이에요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.15",
          "form": "야구를 별로 안좋아해서 전 별로였어요",
          "original_form": "야구를 별로 안좋아해서 전 별로였어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.16",
          "form": "그냥 신가했어요",
          "original_form": "그냥 신가했어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.17",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.18",
          "form": "주변사람들이",
          "original_form": "주변사람들이",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014584.1.19",
          "form": "저도 처음이라 신기했어요",
          "original_form": "저도 처음이라 신기했어요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014584.1.20",
          "form": "응원도 다들 똑같이 율동도 하고 해서 신기했고요",
          "original_form": "응원도 다들 똑같이 율동도 하고 해서 신기했고요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014584.1.21",
          "form": "축구는 어느 팀 좋아하시나요?",
          "original_form": "축구는 어느 팀 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014584.1.22",
          "form": "그것도 이젠 추억으로",
          "original_form": "그것도 이젠 추억으로",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014584.1.23",
          "form": "코로나가 모든 일상을 빼앗아서",
          "original_form": "코로나가 모든 일상을 빼앗아서",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014584.1.24",
          "form": "성남이요",
          "original_form": "성남이요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014584.1.25",
          "form": "막 팬은 아니구요",
          "original_form": "막 팬은 아니구요",
          "speaker_id": "1",
          "time": "20210825 19:59"
        }
      ]
    }
  ]
