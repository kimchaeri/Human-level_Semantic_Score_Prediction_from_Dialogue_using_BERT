      "utterance": [
        {
          "id": "MDRW2100005489.1.1",
          "form": "이상형 있어?",
          "original_form": "이상형 있어?",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005489.1.2",
          "form": "오... 이상형",
          "original_form": "오... 이상형",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005489.1.3",
          "form": "뭔가 다양한데",
          "original_form": "뭔가 다양한데",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005489.1.4",
          "form": "오 생각나는 거 말해봐",
          "original_form": "오 생각나는 거 말해봐",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005489.1.5",
          "form": "음 일단",
          "original_form": "음 일단",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005489.1.6",
          "form": "뱀파이어 같은",
          "original_form": "뱀파이어 같은",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005489.1.7",
          "form": "메플 데몬어벤져랑 웹툰 화이트블러드",
          "original_form": "메플 데몬어벤져랑 웹툰 화이트블러드",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005489.1.8",
          "form": "인외 실화야",
          "original_form": "인외 실화야",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005489.1.9",
          "form": "하지만 매력적인 건 맞지",
          "original_form": "하지만 매력적인 건 맞지",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005489.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인외 ㄹㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인외 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005489.1.11",
          "form": "피부톤도 약간",
          "original_form": "피부톤도 약간",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005489.1.12",
          "form": "잿빛 그런 거 좋아하는 듯",
          "original_form": "잿빛 그런 거 좋아하는 듯",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005489.1.13",
          "form": "아 창백한 거 너무 좋아",
          "original_form": "아 창백한 거 너무 좋아",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005489.1.14",
          "form": "연애와 결혼인데 진짜 웃기다",
          "original_form": "연애와 결혼인데 진짜 웃기다",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005489.1.15",
          "form": "하지만 이게 현실이지",
          "original_form": "하지만 이게 현실이지",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005489.1.16",
          "form": "이상형이 인간일 수가 있나?",
          "original_form": "이상형이 인간일 수가 있나?",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005489.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005489.1.18",
          "form": "투디에 빠질 수밖에 없음",
          "original_form": "투디에 빠질 수밖에 없음",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005489.1.19",
          "form": "현실인간은... 응...",
          "original_form": "현실인간은... 응...",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005489.1.20",
          "form": "넘웃겨",
          "original_form": "넘웃겨",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005489.1.21",
          "form": "결혼관이 어떻게 되시나요",
          "original_form": "결혼관이 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005489.1.22",
          "form": "전 비혼이요",
          "original_form": "전 비혼이요",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005489.1.23",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005489.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005489.1.25",
          "form": "연애관은요?",
          "original_form": "연애관은요?",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005489.1.26",
          "form": "전 비연애주의요",
          "original_form": "전 비연애주의요",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005489.1.27",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005489.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ아나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아나",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005489.1.29",
          "form": "저 하나 챙기기도 힘들더라고요",
          "original_form": "저 하나 챙기기도 힘들더라고요",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005489.1.30",
          "form": "나혼자잘산다 찍어야지",
          "original_form": "나혼자잘산다 찍어야지",
          "speaker_id": "2",
          "time": "20210821 23:06"
        }
      ]
    }
