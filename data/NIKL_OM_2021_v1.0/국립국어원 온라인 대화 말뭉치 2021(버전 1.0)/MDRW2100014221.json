      "utterance": [
        {
          "id": "MDRW2100014221.1.1",
          "form": "저녁은 또 뭘 먹나",
          "original_form": "저녁은 또 뭘 먹나",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014221.1.2",
          "form": "파스타나 해먹을까 싶은데 귀찮기도 ㅋㅋㅋ",
          "original_form": "파스타나 해먹을까 싶은데 귀찮기도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014221.1.3",
          "form": "오 파스타 좋은디?",
          "original_form": "오 파스타 좋은디?",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014221.1.4",
          "form": "무슨 파스타 해먹을라구",
          "original_form": "무슨 파스타 해먹을라구",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014221.1.5",
          "form": "선드라이 토마토 오일에 재워둔거 넣고",
          "original_form": "선드라이 토마토 오일에 재워둔거 넣고",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014221.1.6",
          "form": "오오오 뭔가 맛집의 냄새가 난다ㅋㅋㅋ",
          "original_form": "오오오 뭔가 맛집의 냄새가 난다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014221.1.7",
          "form": "근데 귀찮 오늘 습해서 쩜 더운 느낌",
          "original_form": "근데 귀찮 오늘 습해서 쩜 더운 느낌",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014221.1.8",
          "form": "응 오늘 가스불 켜기 쉽지않을듯ㅋㅋ",
          "original_form": "응 오늘 가스불 켜기 쉽지않을듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014221.1.9",
          "form": "난 사실 아까 꽈배기 두개 먹어서 저녁 안먹을까 싶기도 하고ㅋㅋ",
          "original_form": "난 사실 아까 꽈배기 두개 먹어서 저녁 안먹을까 싶기도 하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014221.1.10",
          "form": "제습기 좀 돌려야겠다",
          "original_form": "제습기 좀 돌려야겠다",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014221.1.11",
          "form": "꽈배기 맛있겠",
          "original_form": "꽈배기 맛있겠",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014221.1.12",
          "form": "응 그 유명한 인절미 꽈배기ㅋㅋ",
          "original_form": "응 그 유명한 인절미 꽈배기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014221.1.13",
          "form": "난 기본 꽈배기 파라 ㅋㅋㅋ",
          "original_form": "난 기본 꽈배기 파라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014221.1.14",
          "form": "콩개루 맛있다규!",
          "original_form": "콩개루 맛있다규!",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014221.1.15",
          "form": "좋아하는 사람이 많이 먹으면 되지 ㅋㅋㅋ",
          "original_form": "좋아하는 사람이 많이 먹으면 되지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014221.1.16",
          "form": "응 그래서 많이 먹었다ㅋㅋㅋ",
          "original_form": "응 그래서 많이 먹었다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014221.1.17",
          "form": "요즘 호떡도 계속 먹고싶었는데 5일장에 계속 비와서ㅋㅋㅋ",
          "original_form": "요즘 호떡도 계속 먹고싶었는데 5일장에 계속 비와서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014221.1.18",
          "form": "두개가... 많이니...",
          "original_form": "두개가... 많이니...",
          "speaker_id": "2",
