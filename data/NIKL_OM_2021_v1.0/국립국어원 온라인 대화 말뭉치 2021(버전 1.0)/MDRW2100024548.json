      "utterance": [
        {
          "id": "MDRW2100024548.1.1",
          "form": "안녕하세요!!!혹시 반려동물 있으신가요??",
          "original_form": "안녕하세요!!!혹시 반려동물 있으신가요??",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024548.1.2",
          "form": "아뇨!!! name1은 동물 키우시나요?",
          "original_form": "아뇨!!! &name1&은 동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024548.1.3",
          "form": "아 네넹",
          "original_form": "아 네넹",
          "speaker_id": "1",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024548.1.4",
          "form": "저는 고양이 키워요!",
          "original_form": "저는 고양이 키워요!",
          "speaker_id": "1",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024548.1.5",
          "form": "오 고양이 키우기 어때요? 안힘드세요?",
          "original_form": "오 고양이 키우기 어때요? 안힘드세요?",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024548.1.6",
          "form": "전 가족이랑 같이 살아서 저 혼자만 키우는 게 아니니까 괜찮더라구요",
          "original_form": "전 가족이랑 같이 살아서 저 혼자만 키우는 게 아니니까 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100024548.1.7",
          "form": "근데 자취하면 힘들것같긴해요",
          "original_form": "근데 자취하면 힘들것같긴해요",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100024548.1.8",
          "form": "혹시 알러지는 없으신가요???",
          "original_form": "혹시 알러지는 없으신가요???",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024548.1.9",
          "form": "고양이가 가구 긁거나 하진 않나요?",
          "original_form": "고양이가 가구 긁거나 하진 않나요?",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024548.1.10",
          "form": "네 알러지는 없눈거같아요",
          "original_form": "네 알러지는 없눈거같아요",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024548.1.11",
          "form": "가구가 남아나지않죠ㅠㅠ",
          "original_form": "가구가 남아나지않죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100024548.1.12",
          "form": "전에 손님 오셨는데 본인도 모르는",
          "original_form": "전에 손님 오셨는데 본인도 모르는",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100024548.1.13",
          "form": "알러지가 있으시더라구요ㅠㅠ",
          "original_form": "알러지가 있으시더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100024548.1.14",
          "form": "가게하시는거에요?",
          "original_form": "가게하시는거에요?",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100024548.1.15",
          "form": "앗 아뇨ㅎㅎ 집에 손님이 오셨던거에요!",
          "original_form": "앗 아뇨ㅎㅎ 집에 손님이 오셨던거에요!",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100024548.1.16",
          "form": "손님은 자기가 고양이 괜찮은 줄 아셨는데",
          "original_form": "손님은 자기가 고양이 괜찮은 줄 아셨는데",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100024548.1.17",
          "form": "막상 만지니까 알러지가 있다는 걸 그때서야 발견하셨어요ㅠㅠ",
          "original_form": "막상 만지니까 알러지가 있다는 걸 그때서야 발견하셨어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100024548.1.18",
          "form": "name2두 조심하세요ㅠㅠ",
          "original_form": "&name2&두 조심하세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100024548.1.19",
          "form": "잠깐 있는거랑 정말 반려로 있는거랑 다른가봐요",
          "original_form": "잠깐 있는거랑 정말 반려로 있는거랑 다른가봐요",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024548.1.20",
          "form": "앗 그렇군요 알려주셔서 감사해요",
          "original_form": "앗 그렇군요 알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024548.1.21",
          "form": "고양이 좋아하시는 종 있으세요???",
          "original_form": "고양이 좋아하시는 종 있으세요???",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024548.1.22",
          "form": "블루러시아??",
          "original_form": "블루러시아??",
          "speaker_id": "2",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100024548.1.23",
          "form": "아 러시안블루요 그거 귀엽던데요",
          "original_form": "아 러시안블루요 그거 귀엽던데요",
