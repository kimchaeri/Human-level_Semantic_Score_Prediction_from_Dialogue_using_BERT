      "utterance": [
        {
          "id": "MDRW2100023447.1.1",
          "form": "엄청 좋아하지",
          "original_form": "엄청 좋아하지",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023447.1.2",
          "form": "내가 요즘 봉사를 하는데",
          "original_form": "내가 요즘 봉사를 하는데",
          "speaker_id": "1",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023447.1.3",
          "form": "무슨 관계가 있지",
          "original_form": "무슨 관계가 있지",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.4",
          "form": "전시회봉사를 해",
          "original_form": "전시회봉사를 해",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.5",
          "form": "어떤 일을 정확히 하는 거야",
          "original_form": "어떤 일을 정확히 하는 거야",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.6",
          "form": "전시회가 면 그림 지키고 있는 사람들 있지",
          "original_form": "전시회가 면 그림 지키고 있는 사람들 있지",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.7",
          "form": "알아본 적 있어",
          "original_form": "알아본 적 있어",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.8",
          "form": "내가 그 사람이 되어보는거야",
          "original_form": "내가 그 사람이 되어보는거야",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.9",
          "form": "나는 그런 건 봉사하는 사람이 아니라 큐레이터인 줄 알았는데",
          "original_form": "나는 그런 건 봉사하는 사람이 아니라 큐레이터인 줄 알았는데",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.10",
          "form": "나도 그런 줄 알았는데 봉사자를 모집하더라",
          "original_form": "나도 그런 줄 알았는데 봉사자를 모집하더라",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.11",
          "form": "몇 번이나 해봤어",
          "original_form": "몇 번이나 해봤어",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.12",
          "form": "두 번 해봤는데 무척 보람차더라",
          "original_form": "두 번 해봤는데 무척 보람차더라",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.13",
          "form": "나도 그런 봉사 있으면 해보고 싶다",
          "original_form": "나도 그런 봉사 있으면 해보고 싶다",
          "speaker_id": "2",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023447.1.14",
          "form": "같이 봉사자주 하면 좋을 거 같아",
          "original_form": "같이 봉사자주 하면 좋을 거 같아",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023447.1.15",
          "form": "봉사 시간도 채우고 마음도 행복해지고",
          "original_form": "봉사 시간도 채우고 마음도 행복해지고",
          "speaker_id": "2",
          "time": "20210830 03:12"
        }
