      "utterance": [
        {
          "id": "MDRW2100021772.1.1",
          "form": "여행 계획 있으세요?",
          "original_form": "여행 계획 있으세요?",
          "speaker_id": "1",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100021772.1.2",
          "form": "계획은... 음 가을에 제주도 짧게 다녀올까 생각만 하고잇어욤",
          "original_form": "계획은... 음 가을에 제주도 짧게 다녀올까 생각만 하고잇어욤",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100021772.1.3",
          "form": "계획잇으신가요",
          "original_form": "계획잇으신가요",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100021772.1.4",
          "form": "좀 선선해지면 경주 가고싶어요",
          "original_form": "좀 선선해지면 경주 가고싶어요",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100021772.1.5",
          "form": "경주 좋져 ㅎ",
          "original_form": "경주 좋져 ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100021772.1.6",
          "form": "가을에 가믄 딱이겟네욤",
          "original_form": "가을에 가믄 딱이겟네욤",
          "speaker_id": "2",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100021772.1.7",
          "form": "그쵸 별 쏟아지는 곳도 가보고싶어요",
          "original_form": "그쵸 별 쏟아지는 곳도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100021772.1.8",
          "form": "낭만적이에요 !!",
          "original_form": "낭만적이에요 !!",
          "speaker_id": "2",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100021772.1.9",
          "form": "안반데기까지가야하는데 너무 머네요",
          "original_form": "안반데기까지가야하는데 너무 머네요",
          "speaker_id": "1",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100021772.1.10",
          "form": "가을장마가 제대로네요",
          "original_form": "가을장마가 제대로네요",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100021772.1.11",
          "form": "비가많이와요",
          "original_form": "비가많이와요",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100021772.1.12",
          "form": "주말에 드라이브가고싶은데ㅜ",
          "original_form": "주말에 드라이브가고싶은데ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100021772.1.13",
          "form": "부산도 이제 비내리네요",
          "original_form": "부산도 이제 비내리네요",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100021772.1.14",
          "form": "낮에 너무 더워서 머리 잘랐는데 갑자기 추워졌어요 ㅎㅎ",
          "original_form": "낮에 너무 더워서 머리 잘랐는데 갑자기 추워졌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100021772.1.15",
          "form": "오 저는 내일 퇴근하구",
          "original_form": "오 저는 내일 퇴근하구",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100021772.1.16",
          "form": "미용실예약햇어요",
          "original_form": "미용실예약햇어요",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100021772.1.17",
          "form": "파마!!",
          "original_form": "파마!!",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100021772.1.18",
          "form": "우와정말요 셋팅 하실거에요?",
          "original_form": "우와정말요 셋팅 하실거에요?",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100021772.1.19",
          "form": "머리하고 여행가요!---",
          "original_form": "머리하고 여행가요!---",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100021772.1.20",
          "form": "머리하고 여행가시나용?",
          "original_form": "머리하고 여행가시나용?",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100021772.1.21",
          "form": "저는 그냥 급 충동적으로 어제 예약해뿟어요ㅋㅋ",
          "original_form": "저는 그냥 급 충동적으로 어제 예약해뿟어요ㅋㅋ",
