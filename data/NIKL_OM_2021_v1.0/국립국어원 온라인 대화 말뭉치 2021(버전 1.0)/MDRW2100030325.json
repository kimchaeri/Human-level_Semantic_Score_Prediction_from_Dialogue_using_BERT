      "utterance": [
        {
          "id": "MDRW2100030325.1.1",
          "form": "헉 그런가?",
          "original_form": "헉 그런가?",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030325.1.2",
          "form": "나는 좀 신경쓰이는데",
          "original_form": "나는 좀 신경쓰이는데",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030325.1.3",
          "form": "뭐ㅠ요즘 마스쿠 쑤니까 괜찮울지도?",
          "original_form": "뭐ㅠ요즘 마스쿠 쑤니까 괜찮울지도?",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030325.1.4",
          "form": "그래서 우리 내일 고기 먹어?",
          "original_form": "그래서 우리 내일 고기 먹어?",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030325.1.5",
          "form": "언제 가지?",
          "original_form": "언제 가지?",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030325.1.6",
          "form": "점심에 갈까?",
          "original_form": "점심에 갈까?",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030325.1.7",
          "form": "너 언제 시간 괜찮아?",
          "original_form": "너 언제 시간 괜찮아?",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030325.1.8",
          "form": "나 학원 2시에 끝나는데ㅜㅠ 그 이후 괜찮니?",
          "original_form": "나 학원 2시에 끝나는데ㅜㅠ 그 이후 괜찮니?",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030325.1.9",
          "form": "헉 그럼 살짝 애매한데ㅠㅠ",
          "original_form": "헉 그럼 살짝 애매한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030325.1.10",
          "form": "아님 일요일...! 일요일은 다 됩니당",
          "original_form": "아님 일요일...! 일요일은 다 됩니당",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030325.1.11",
          "form": "일요일은 할머니댁에 가서ㅠㅠ",
          "original_form": "일요일은 할머니댁에 가서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030325.1.12",
          "form": "어쩌지..?",
          "original_form": "어쩌지..?",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030325.1.13",
          "form": "구럼 어쩔 수 없지ㅜㅜㅜ 고기는 담주에 먹자...",
          "original_form": "구럼 어쩔 수 없지ㅜㅜㅜ 고기는 담주에 먹자...",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030325.1.14",
          "form": "아니면 토요일 저녁은 안돼? 좀 일찍 먹는 저녁느낌...5시? 정도에",
          "original_form": "아니면 토요일 저녁은 안돼? 좀 일찍 먹는 저녁느낌...5시? 정도에",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030325.1.15",
          "form": "최대한 시간 비워볼게",
          "original_form": "최대한 시간 비워볼게",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030325.1.16",
          "form": "내일 알려줄게",
          "original_form": "내일 알려줄게",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030325.1.17",
          "form": "아니 오늘이구나",
          "original_form": "아니 오늘이구나",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030325.1.18",
          "form": "ㅋㅋㅋㅋㅋ 알았어 그럼 시간 나면 고기 먹우로 가는거로!!",
          "original_form": "ㅋㅋㅋㅋㅋ 알았어 그럼 시간 나면 고기 먹우로 가는거로!!",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030325.1.19",
          "form": "야호 고기!!",
          "original_form": "야호 고기!!",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030325.1.20",
          "form": "고기에 김치 구워 먹자",
          "original_form": "고기에 김치 구워 먹자",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030325.1.21",
          "form": "아 맞다 김치 안 좋아하지ㅠㅠ",
          "original_form": "아 맞다 김치 안 좋아하지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030325.1.22",
          "form": "그럼 쌈장에 먹자ㅋㅋㅋ",
          "original_form": "그럼 쌈장에 먹자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030325.1.23",
          "form": "ㅋㅋㅋㅋㅋ 너 먹고 싶은 구워 먹어도 되요",
          "original_form": "ㅋㅋㅋㅋㅋ 너 먹고 싶은 구워 먹어도 되요",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030325.1.24",
          "form": "고기에만 안 닿으면 상관 없어...",
          "original_form": "고기에만 안 닿으면 상관 없어...",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030325.1.25",
          "form": "쌈장 좋지~~",
          "original_form": "쌈장 좋지~~",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030325.1.26",
          "form": "아 근데 내가 7시에 수업이 있어서",
          "original_form": "아 근데 내가 7시에 수업이 있어서",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030325.1.27",
          "form": "늦은 점심이 더 좋긴 한데",
          "original_form": "늦은 점심이 더 좋긴 한데",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030325.1.28",
          "form": "그럼 3시나 4시 나 2시 30분",
          "original_form": "그럼 3시나 4시 나 2시 30분",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030325.1.29",
          "form": "3시에 볼까?",
          "original_form": "3시에 볼까?",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030325.1.30",
          "form": "아 근데 시간이 너무 늦다ㅠㅠ",
          "original_form": "아 근데 시간이 너무 늦다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030325.1.31",
          "form": "그럼 고기는 나중에 먹ㅈ자ㅜㅠㅠ",
          "original_form": "그럼 고기는 나중에 먹ㅈ자ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        }
      ]
