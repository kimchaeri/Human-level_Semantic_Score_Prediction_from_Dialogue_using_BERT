      "utterance": [
        {
          "id": "MDRW2100008225.1.1",
          "form": "랑종봤어?",
          "original_form": "랑종봤어?",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008225.1.2",
          "form": "나는 쫄보니까 안봤지만",
          "original_form": "나는 쫄보니까 안봤지만",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008225.1.3",
          "form": "넌 공포영화 자주보길래 궁금해서 물어봄",
          "original_form": "넌 공포영화 자주보길래 궁금해서 물어봄",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008225.1.4",
          "form": "예?? 제가요???",
          "original_form": "예?? 제가요???",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008225.1.5",
          "form": "저 무섭고 잔인한거 못봐서",
          "original_form": "저 무섭고 잔인한거 못봐서",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008225.1.6",
          "form": "공포영화 못보는데욧... 번짓수를 잘못찾으심",
          "original_form": "공포영화 못보는데욧... 번짓수를 잘못찾으심",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008225.1.7",
          "form": "너 곡성도 보고...",
          "original_form": "너 곡성도 보고...",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008225.1.8",
          "form": "막 피튀기는 영화도 봤고...",
          "original_form": "막 피튀기는 영화도 봤고...",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008225.1.9",
          "form": "못본다 하면서 본거 많길래 ㅋㅋㅋㅋ",
          "original_form": "못본다 하면서 본거 많길래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.10",
          "form": "예...? 그거 다",
          "original_form": "예...? 그거 다",
          "speaker_id": "2",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.11",
          "form": "억지로 끌려가서 본거고여",
          "original_form": "억지로 끌려가서 본거고여",
          "speaker_id": "2",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.12",
          "form": "제가 먼저 찾아본건 없어요...",
          "original_form": "제가 먼저 찾아본건 없어요...",
          "speaker_id": "2",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.13",
          "form": "그럼 머지않아 랑종도 보러",
          "original_form": "그럼 머지않아 랑종도 보러",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.14",
          "form": "끌려가는거 아니야...?",
          "original_form": "끌려가는거 아니야...?",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008225.1.15",
          "form": "너 유전도 보고 그랬잖아 ㅋㅋㅋ 주위에 매니아가 있나본데",
          "original_form": "너 유전도 보고 그랬잖아 ㅋㅋㅋ 주위에 매니아가 있나본데",
          "speaker_id": "1",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008225.1.16",
          "form": "그럴 일은 없습니다...",
          "original_form": "그럴 일은 없습니다...",
          "speaker_id": "2",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008225.1.17",
          "form": "그리고 놀랍게도 전부 다른 사람에게",
          "original_form": "그리고 놀랍게도 전부 다른 사람에게",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008225.1.18",
          "form": "끌려가서 본거랍니다... 저 쫄보인거 주변에 유명해서 앵간해선 안그래욬ㅋㅋㅋㅋ",
          "original_form": "끌려가서 본거랍니다... 저 쫄보인거 주변에 유명해서 앵간해선 안그래욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008225.1.19",
          "form": "ㅋㅋㅋㅋㅋ 쫄보인거 유명한데",
          "original_form": "ㅋㅋㅋㅋㅋ 쫄보인거 유명한데",
          "speaker_id": "1",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008225.1.20",
          "form": "왜 유명한 공포영화 도장깨기 했냐구",
          "original_form": "왜 유명한 공포영화 도장깨기 했냐구",
          "speaker_id": "1",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008225.1.21",
          "form": "이제 머지않아 랑종보자고 연락오겠네...",
          "original_form": "이제 머지않아 랑종보자고 연락오겠네...",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008225.1.22",
          "form": "그럴 일 없다니까욬ㅋㅋㅋㅋ",
          "original_form": "그럴 일 없다니까욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.23",
          "form": "랑종은 특히 잔인하다고 들어서",
          "original_form": "랑종은 특히 잔인하다고 들어서",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.24",
          "form": "거절합니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "거절합니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.25",
          "form": "아 맞아 잔인하긴 하다더라",
          "original_form": "아 맞아 잔인하긴 하다더라",
          "speaker_id": "1",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.26",
          "form": "나도 보지는 못하고 후기만 들었는데",
          "original_form": "나도 보지는 못하고 후기만 들었는데",
          "speaker_id": "1",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.27",
          "form": "잔인하다못해 징그럽고 불쾌하대",
          "original_form": "잔인하다못해 징그럽고 불쾌하대",
          "speaker_id": "1",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008225.1.28",
          "form": "저..  다른건 몰라도 잔인한건 진짜 못봐서 ㅎㅎ",
          "original_form": "저..  다른건 몰라도 잔인한건 진짜 못봐서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008225.1.29",
          "form": "제 주변에 저한테 예전에",
          "original_form": "제 주변에 저한테 예전에",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008225.1.30",
          "form": "피튀기는 뮤비 추천해줬다가 맞은 친구 있어서 ㅋㅋㅋㅋ 걔는 진짜 그 이후 저한테 그런 장난 안쳐욬ㅋㅋ",
          "original_form": "피튀기는 뮤비 추천해줬다가 맞은 친구 있어서 ㅋㅋㅋㅋ 걔는 진짜 그 이후 저한테 그런 장난 안쳐욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008225.1.31",
          "form": "ㅋㅋㅋㅋ그런 당신에게",
          "original_form": "ㅋㅋㅋㅋ그런 당신에게",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008225.1.32",
          "form": "피 한컵도 안나오는",
          "original_form": "피 한컵도 안나오는",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008225.1.33",
          "form": "파라노말 액티비티 보쉴?",
          "original_form": "파라노말 액티비티 보쉴?",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008225.1.34",
          "form": "사양합니다",
          "original_form": "사양합니다",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008225.1.35",
          "form": "그런건 보고싶지도 않아욧",
          "original_form": "그런건 보고싶지도 않아욧",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008225.1.36",
          "form": "언니야 말루...유전 보실...? ㅎㅎ",
          "original_form": "언니야 말루...유전 보실...? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008225.1.37",
          "form": "ㅎㅎ... 거절할게",
          "original_form": "ㅎㅎ... 거절할게",
          "speaker_id": "1",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008225.1.38",
          "form": "초반부터 아주 충격적인 장면이 나오고",
          "original_form": "초반부터 아주 충격적인 장면이 나오고",
          "speaker_id": "1",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008225.1.39",
          "form": "엄마역 배우분이 미처날뛴다면서",
          "original_form": "엄마역 배우분이 미처날뛴다면서",
          "speaker_id": "1",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008225.1.40",
          "form": "배우들 다 연기 잘해요",
          "original_form": "배우들 다 연기 잘해요",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008225.1.41",
          "form": "후반엔 판타지가 되지만",
          "original_form": "후반엔 판타지가 되지만",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008225.1.42",
          "form": "중반까진 스토리도 굳입니다",
          "original_form": "중반까진 스토리도 굳입니다",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008225.1.43",
          "form": "공포영화가 스토리 굳이라는건",
          "original_form": "공포영화가 스토리 굳이라는건",
          "speaker_id": "1",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008225.1.44",
          "form": "엄청 무섭단 소리잖아...",
          "original_form": "엄청 무섭단 소리잖아...",
          "speaker_id": "1",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008225.1.45",
          "form": "안볼래 그냥...",
          "original_form": "안볼래 그냥...",
          "speaker_id": "1",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008225.1.46",
          "form": "엥 무서운쪽 스토리 말고",
          "original_form": "엥 무서운쪽 스토리 말고",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008225.1.47",
          "form": "가족관의 갈등관계나 그런 양상들을",
          "original_form": "가족관의 갈등관계나 그런 양상들을",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008225.1.48",
          "form": "엄청 잘 풀었어요 ㅋㅋㅋㅋㅋㅋㅋ 서양 공포라 기승전 악마됐지만...",
          "original_form": "엄청 잘 풀었어요 ㅋㅋㅋㅋㅋㅋㅋ 서양 공포라 기승전 악마됐지만...",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008225.1.49",
          "form": "기승전 악마 아님 좀비말고",
          "original_form": "기승전 악마 아님 좀비말고",
          "speaker_id": "1",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008225.1.50",
          "form": "서양 공포영화가 뭐 있나..?",
          "original_form": "서양 공포영화가 뭐 있나..?",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008225.1.51",
          "form": "컬트종교 의식? 미드소마같은거?",
          "original_form": "컬트종교 의식? 미드소마같은거?",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008225.1.52",
          "form": "악마...좀비...음...",
          "original_form": "악마...좀비...음...",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008225.1.53",
          "form": "유령...?",
          "original_form": "유령...?",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008225.1.54",
          "form": "사실 저 공포영화 안봐서 알못이예요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 저 공포영화 안봐서 알못이예요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008225.1.55",
          "form": "앗 너도? 나도 ㅋㅋㅋㅋ",
          "original_form": "앗 너도? 나도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
