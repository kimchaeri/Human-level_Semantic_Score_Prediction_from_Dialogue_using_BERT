      "utterance": [
        {
          "id": "MDRW2100005601.1.1",
          "form": "안녕하세요! 학생이세요?",
          "original_form": "안녕하세요! 학생이세요?",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005601.1.2",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100005601.1.3",
          "form": "네 고등학생 입니다!",
          "original_form": "네 고등학생 입니다!",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100005601.1.4",
          "form": "얼마 전에 개학했겠네요",
          "original_form": "얼마 전에 개학했겠네요",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100005601.1.5",
          "form": "학교생활 어때요??",
          "original_form": "학교생활 어때요??",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100005601.1.6",
          "form": "힘들어요..",
          "original_form": "힘들어요..",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100005601.1.7",
          "form": "공부 안하고 싶어요",
          "original_form": "공부 안하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100005601.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100005601.1.9",
          "form": "동아리, 독서 봉사 수상 이런게 진짜 짜증나요",
          "original_form": "동아리, 독서 봉사 수상 이런게 진짜 짜증나요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100005601.1.10",
          "form": "저도 정말 고생 많이 했었어요ㅜㅜ",
          "original_form": "저도 정말 고생 많이 했었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100005601.1.11",
          "form": "상도 요즘은 생기부에 하나만 넣을 수 있다면서요",
          "original_form": "상도 요즘은 생기부에 하나만 넣을 수 있다면서요",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100005601.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100005601.1.13",
          "form": "한학기레 하나씩 넣을수있어요",
          "original_form": "한학기레 하나씩 넣을수있어요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100005601.1.14",
          "form": "근디 저는 그마저도 넣을게없네요..",
          "original_form": "근디 저는 그마저도 넣을게없네요..",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100005601.1.15",
          "form": "ㅜㅜ 생기부 정말 골치 아프겠어요",
          "original_form": "ㅜㅜ 생기부 정말 골치 아프겠어요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100005601.1.16",
          "form": "저도 수시로 대학 가서 생기부 엄청 신경 많이 썼거든요",
          "original_form": "저도 수시로 대학 가서 생기부 엄청 신경 많이 썼거든요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100005601.1.17",
          "form": "수시 생각하시나봐요?",
          "original_form": "수시 생각하시나봐요?",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100005601.1.18",
          "form": "네..... 일단은요",
          "original_form": "네..... 일단은요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100005601.1.19",
          "form": "에휴 힘드시겠어요",
          "original_form": "에휴 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100005601.1.20",
          "form": "생각하는 전공 있어요?",
          "original_form": "생각하는 전공 있어요?",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100005601.1.21",
          "form": "저 식품공학과 가고싶어요! 근데 사실잘모르겠어요.. 일단 그렇게 써두긴했는데....",
          "original_form": "저 식품공학과 가고싶어요! 근데 사실잘모르겠어요.. 일단 그렇게 써두긴했는데....",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100005601.1.22",
          "form": "원래 다 모르죠 뭐ㅋㅋㅋㅋ 바뀔 수도 있는 거고",
          "original_form": "원래 다 모르죠 뭐ㅋㅋㅋㅋ 바뀔 수도 있는 거고",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100005601.1.23",
          "form": "식품공학과에서는 어떤 걸 배우나요?",
          "original_form": "식품공학과에서는 어떤 걸 배우나요?",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100005601.1.24",
          "form": "영양학?인가요 아니면 식품을 만드는 건가요?",
          "original_form": "영양학?인가요 아니면 식품을 만드는 건가요?",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100005601.1.25",
          "form": "글쎄요.. 아직 안가봐서 잘 모르겠는디 영양학도 다루긴 하겠지만 그건 식품영양학과에서 위주로 하고 식품공학과 에서는 만들고 가공하는거를 위주로 배우는 거 같아요",
          "original_form": "글쎄요.. 아직 안가봐서 잘 모르겠는디 영양학도 다루긴 하겠지만 그건 식품영양학과에서 위주로 하고 식품공학과 에서는 만들고 가공하는거를 위주로 배우는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 13:43"
        }
      ]
    }
  ]
