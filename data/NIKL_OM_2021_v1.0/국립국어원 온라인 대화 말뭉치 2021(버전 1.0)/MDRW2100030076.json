      "utterance": [
        {
          "id": "MDRW2100030076.1.1",
          "form": "부럽네요..왕",
          "original_form": "부럽네요..왕",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030076.1.2",
          "form": "신랑있어서 저는 왕은 못돼요",
          "original_form": "신랑있어서 저는 왕은 못돼요",
          "speaker_id": "2",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030076.1.3",
          "form": "평소에 동호회활동은 하세요?",
          "original_form": "평소에 동호회활동은 하세요?",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030076.1.4",
          "form": "애들이랑",
          "original_form": "애들이랑",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030076.1.5",
          "form": "같이할수있는게 별로없죠..",
          "original_form": "같이할수있는게 별로없죠..",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030076.1.6",
          "form": "아뇨. 애가 어려서요 ㅠ",
          "original_form": "아뇨. 애가 어려서요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030076.1.7",
          "form": "연탄 봉사 말고 또 하시는거 있나요?",
          "original_form": "연탄 봉사 말고 또 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030076.1.8",
          "form": "가끔 무료급식 나눠주고",
          "original_form": "가끔 무료급식 나눠주고",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030076.1.9",
          "form": "아이들봉사갈때  가보기도하구요",
          "original_form": "아이들봉사갈때  가보기도하구요",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030076.1.10",
          "form": "무료급식이요?",
          "original_form": "무료급식이요?",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030076.1.11",
          "form": "네..노인들 무료급식요",
          "original_form": "네..노인들 무료급식요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030076.1.12",
          "form": "저희집 근처에도 그런게 있긴 하더라고요",
          "original_form": "저희집 근처에도 그런게 있긴 하더라고요",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030076.1.13",
          "form": "그런봉사 해봤네요..끝나고 밥먹고",
          "original_form": "그런봉사 해봤네요..끝나고 밥먹고",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030076.1.14",
          "form": "아이들도 같이요?",
          "original_form": "아이들도 같이요?",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030076.1.15",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030076.1.16",
          "form": "초등학교6학년때쯤",
          "original_form": "초등학교6학년때쯤",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030076.1.17",
          "form": "코로나로 그후론 못했어요..",
          "original_form": "코로나로 그후론 못했어요..",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030076.1.18",
          "form": "연탄은 그냥 계속하구요..무료급식은 안해요",
          "original_form": "연탄은 그냥 계속하구요..무료급식은 안해요",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030076.1.19",
          "form": "지금은 무료급식 자체가 안하는거죠?",
          "original_form": "지금은 무료급식 자체가 안하는거죠?",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100030076.1.20",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100030076.1.21",
          "form": "아무래도 위험하니까요 ㅠ",
          "original_form": "아무래도 위험하니까요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030076.1.22",
          "form": "전 요즘에도 연탄 쓰는지 몰랐네요",
          "original_form": "전 요즘에도 연탄 쓰는지 몰랐네요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030076.1.23",
          "form": "아직도쓰는곳이 있더라구요",
          "original_form": "아직도쓰는곳이 있더라구요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030076.1.24",
          "form": "여긴대전인데 있어요..완전시골도아닌데",
          "original_form": "여긴대전인데 있어요..완전시골도아닌데",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030076.1.25",
          "form": "그런곳은 도움이 손길이 필요하겠어요",
          "original_form": "그런곳은 도움이 손길이 필요하겠어요",
          "speaker_id": "1",
          "time": "20210904 15:25"
        }
