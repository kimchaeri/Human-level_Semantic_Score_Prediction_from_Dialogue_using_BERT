      "utterance": [
        {
          "id": "MDRW2100022041.1.1",
          "form": "name1님은 일어때요?",
          "original_form": "&name1&님은 일어때요?",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022041.1.2",
          "form": "전에 안힘들다고하셨는데 ㅎㅎ 그냥궁금해서ㅎㅎ",
          "original_form": "전에 안힘들다고하셨는데 ㅎㅎ 그냥궁금해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100022041.1.3",
          "form": "전 그냥 저냥 무난무난  ㅋ\r\n남피해 주지도 않고\r\n피해도 안입고",
          "original_form": "전 그냥 저냥 무난무난  ㅋ\r\n남피해 주지도 않고\r\n피해도 안입고",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022041.1.4",
          "form": "맨탈도 강한편이고",
          "original_form": "맨탈도 강한편이고",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022041.1.5",
          "form": "받아치기더 꽤 하구 ㅋ",
          "original_form": "받아치기더 꽤 하구 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:18"
        },
        {
          "id": "MDRW2100022041.1.6",
          "form": "ㅎㅎ 그러신것같애요ㅎ",
          "original_form": "ㅎㅎ 그러신것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022041.1.7",
          "form": "그래도 상처는 받는편이에요?",
          "original_form": "그래도 상처는 받는편이에요?",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022041.1.8",
          "form": "뭐 내유외강? 스타일?",
          "original_form": "뭐 내유외강? 스타일?",
          "speaker_id": "2",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022041.1.9",
          "form": "생각보단 여린편이긴 해요",
          "original_form": "생각보단 여린편이긴 해요",
          "speaker_id": "1",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022041.1.10",
          "form": "티를 안내죠 ㅋ",
          "original_form": "티를 안내죠 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:19"
        },
        {
          "id": "MDRW2100022041.1.11",
          "form": "상사가 야! 다시해와! 이게뭐야? 이러면?ㅎㅎ",
          "original_form": "상사가 야! 다시해와! 이게뭐야? 이러면?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022041.1.12",
          "form": "네 하고 그냥 가요 ㅋ",
          "original_form": "네 하고 그냥 가요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022041.1.13",
          "form": "상사 무안하게 왜그래요?ㅎㅎ",
          "original_form": "상사 무안하게 왜그래요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022041.1.14",
          "form": "근데 언제부터 일하신거에요?",
          "original_form": "근데 언제부터 일하신거에요?",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.15",
          "form": "ㅡㅁ ㅡ ㅋㅋㅋ 무안 해요?",
          "original_form": "ㅡㅁ ㅡ ㅋㅋㅋ 무안 해요?",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.16",
          "form": "원래 피아노전공이라고하셨잖아요?ㅎ",
          "original_form": "원래 피아노전공이라고하셨잖아요?ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.17",
          "form": "저는 좀 독특하긴 해요",
          "original_form": "저는 좀 독특하긴 해요",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.18",
          "form": "일한 시기도",
          "original_form": "일한 시기도",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.19",
          "form": "전공은 아니구나ㅎ 고등학교 때 하셨으니",
          "original_form": "전공은 아니구나ㅎ 고등학교 때 하셨으니",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.20",
          "form": "독특?ㅎ",
          "original_form": "독특?ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.21",
          "form": "전공은 못햇죠 ㅎㅎ",
          "original_form": "전공은 못햇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022041.1.22",
          "form": "대합 입학 2번",
          "original_form": "대합 입학 2번",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022041.1.23",
          "form": "2번다 졸업 안함",
          "original_form": "2번다 졸업 안함",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022041.1.24",
          "form": "첫일은 21살부터 했는데",
          "original_form": "첫일은 21살부터 했는데",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022041.1.25",
          "form": "중간에 학교 다니고",
          "original_form": "중간에 학교 다니고",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022041.1.26",
          "form": "뭐 이런  ㅎㅎ",
          "original_form": "뭐 이런  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022041.1.27",
          "form": "뭔가 복잡하네요ㅎㅎ",
          "original_form": "뭔가 복잡하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:23"
        }
      ]
    }
