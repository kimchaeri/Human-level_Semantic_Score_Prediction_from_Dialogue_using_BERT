      "utterance": [
        {
          "id": "MDRW2100043336.1.1",
          "form": "오늘 어떤 패션이야?",
          "original_form": "오늘 어떤 패션이야?",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043336.1.2",
          "form": "츄리닝",
          "original_form": "츄리닝",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043336.1.3",
          "form": "오 츄리닝 편하고 좋지",
          "original_form": "오 츄리닝 편하고 좋지",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043336.1.4",
          "form": "난 셔츠랑 치마",
          "original_form": "난 셔츠랑 치마",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043336.1.5",
          "form": "불편했어ㅠ",
          "original_form": "불편했어ㅠ",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043336.1.6",
          "form": "어디가느라",
          "original_form": "어디가느라",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043336.1.7",
          "form": "글케입었어",
          "original_form": "글케입었어",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043336.1.8",
          "form": "아 회사갈때 그렇게 입었어",
          "original_form": "아 회사갈때 그렇게 입었어",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043336.1.9",
          "form": "몇살인뎅",
          "original_form": "몇살인뎅",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043336.1.10",
          "form": "나 30살이야 ㅋㅋㅋ",
          "original_form": "나 30살이야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043336.1.11",
          "form": "넌 몇살이여",
          "original_form": "넌 몇살이여",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.12",
          "form": "21살이여",
          "original_form": "21살이여",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.13",
          "form": "대학생",
          "original_form": "대학생",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.14",
          "form": "와 나랑 거의 10살 차이네",
          "original_form": "와 나랑 거의 10살 차이네",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.15",
          "form": "대학생 조케따 부러워",
          "original_form": "대학생 조케따 부러워",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.16",
          "form": "언니",
          "original_form": "언니",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043336.1.17",
          "form": "근데 저",
          "original_form": "근데 저",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043336.1.18",
          "form": "비대면이라 학교를안가여",
          "original_form": "비대면이라 학교를안가여",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043336.1.19",
          "form": "와 그러네",
          "original_form": "와 그러네",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043336.1.20",
          "form": "나도 회사다니면서 대학원 다니는데",
          "original_form": "나도 회사다니면서 대학원 다니는데",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.21",
          "form": "난 오히려 비대면이라 편해",
          "original_form": "난 오히려 비대면이라 편해",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.22",
          "form": "와 어케 둘다",
          "original_form": "와 어케 둘다",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.23",
          "form": "같이다녀요",
          "original_form": "같이다녀요",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.24",
          "form": "너는 슬프겠다ㅠㅠ",
          "original_form": "너는 슬프겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.25",
          "form": "그래서 나 점점 늙오가고 잇어",
          "original_form": "그래서 나 점점 늙오가고 잇어",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.26",
          "form": "대학원이 특수대학원이라 수업이 저녁 7시야",
          "original_form": "대학원이 특수대학원이라 수업이 저녁 7시야",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.27",
          "form": "그래서가능해",
          "original_form": "그래서가능해",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.28",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043336.1.29",
          "form": "전 대학도 비대면인데 귀찮아서 대충다니는중",
          "original_form": "전 대학도 비대면인데 귀찮아서 대충다니는중",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043336.1.30",
          "form": "나도 그래 ㅋㅋㅋ",
          "original_form": "나도 그래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043336.1.31",
          "form": "차라리 비대면이라 편해",
          "original_form": "차라리 비대면이라 편해",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043336.1.32",
          "form": "하지만 엉덩이아파 수업 3시간이야ㅠ",
          "original_form": "하지만 엉덩이아파 수업 3시간이야ㅠ",
          "speaker_id": "2",
          "time": "20210907 18:09"
        }
      ]
    }
  ]
