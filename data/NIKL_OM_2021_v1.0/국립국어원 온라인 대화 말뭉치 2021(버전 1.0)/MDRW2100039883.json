      "utterance": [
        {
          "id": "MDRW2100039883.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039883.1.2",
          "form": "네 화이자 완료에요",
          "original_form": "네 화이자 완료에요",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039883.1.3",
          "form": "2차때 3키로가 빠졌어요",
          "original_form": "2차때 3키로가 빠졌어요",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039883.1.4",
          "form": "앗.. 앓으시느라 빠지신거예요?",
          "original_form": "앗.. 앓으시느라 빠지신거예요?",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039883.1.5",
          "form": "네 저는 배가 아파서 뭘 못먹겠도라고요",
          "original_form": "네 저는 배가 아파서 뭘 못먹겠도라고요",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039883.1.6",
          "form": "배탈처럼요?",
          "original_form": "배탈처럼요?",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039883.1.7",
          "form": "네 두통도 있었는데 원래도 위장이 안좋거든요",
          "original_form": "네 두통도 있었는데 원래도 위장이 안좋거든요",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039883.1.8",
          "form": "부스터샷은 안맞고싶을것 같아요",
          "original_form": "부스터샷은 안맞고싶을것 같아요",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039883.1.9",
          "form": "그렇겠네요 ㅠ",
          "original_form": "그렇겠네요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039883.1.10",
          "form": "전 별 이벤트 없이 넘어갔거든요",
          "original_form": "전 별 이벤트 없이 넘어갔거든요",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039883.1.11",
          "form": "접종 완료 하셨네요",
          "original_form": "접종 완료 하셨네요",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039883.1.12",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039883.1.13",
          "form": "워낙 안좋은 아야기가 많아서..뇌사 등등 ㅜ",
          "original_form": "워낙 안좋은 아야기가 많아서..뇌사 등등 ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039883.1.14",
          "form": "2차는 각오하라고 해서 엄청 무서웠는데",
          "original_form": "2차는 각오하라고 해서 엄청 무서웠는데",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039883.1.15",
          "form": "너무 멀쩡해요",
          "original_form": "너무 멀쩡해요",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039883.1.16",
          "form": "와 그것도 복 아닌가요 ㅎㅎ",
          "original_form": "와 그것도 복 아닌가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039883.1.17",
          "form": "그니까요~ 의사가 저녁에 자기전엔 진통제 먹으라거 해서 한알 먹었거든요",
          "original_form": "그니까요~ 의사가 저녁에 자기전엔 진통제 먹으라거 해서 한알 먹었거든요",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039883.1.18",
          "form": "저는 오히려 못먹게 해서 당황했어요",
          "original_form": "저는 오히려 못먹게 해서 당황했어요",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039883.1.19",
          "form": "진짜요?? 누가요?",
          "original_form": "진짜요?? 누가요?",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039883.1.20",
          "form": "같이 간 동료에게는 1차때 약 먹어서 말짱 꽝이라고했대요 연구결과냐 의사분의 사견이냐고 따지려다가말았대요",
          "original_form": "같이 간 동료에게는 1차때 약 먹어서 말짱 꽝이라고했대요 연구결과냐 의사분의 사견이냐고 따지려다가말았대요",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039883.1.21",
          "form": "오잉???",
          "original_form": "오잉???",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039883.1.22",
          "form": "그게 부르펜 계열을 먹었을때 항체 형성을 방해할 수도 있다는거지 항체 형성이 안된다는게 아니잖아요",
          "original_form": "그게 부르펜 계열을 먹었을때 항체 형성을 방해할 수도 있다는거지 항체 형성이 안된다는게 아니잖아요",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039883.1.23",
          "form": "그 의사샘은 백신 맞고 약 안드셨겠군요",
          "original_form": "그 의사샘은 백신 맞고 약 안드셨겠군요",
