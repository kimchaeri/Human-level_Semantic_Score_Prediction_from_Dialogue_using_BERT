      "utterance": [
        {
          "id": "MDRW2100039785.1.1",
          "form": "안녕하세요 최근에 뭐 사셨나요~?",
          "original_form": "안녕하세요 최근에 뭐 사셨나요~?",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039785.1.2",
          "form": "최근에 음",
          "original_form": "최근에 음",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039785.1.3",
          "form": "전동칫솔을 샀습니다",
          "original_form": "전동칫솔을 샀습니다",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039785.1.4",
          "form": "와 칫솔 어디꺼 사셨나요?",
          "original_form": "와 칫솔 어디꺼 사셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039785.1.5",
          "form": "오아 거요!",
          "original_form": "오아 거요!",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039785.1.6",
          "form": "전동칫솔 쓰시나여",
          "original_form": "전동칫솔 쓰시나여",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039785.1.7",
          "form": "아니요 저는 치실과 칫솔써요..ㅋㅋㅋㅋ",
          "original_form": "아니요 저는 치실과 칫솔써요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039785.1.8",
          "form": "전동칫솔 원래 쓰셨던거 구매하신건가요?",
          "original_form": "전동칫솔 원래 쓰셨던거 구매하신건가요?",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039785.1.9",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039785.1.10",
          "form": "저도 처음 구매해봤어요",
          "original_form": "저도 처음 구매해봤어요",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039785.1.11",
          "form": "1+1하길래",
          "original_form": "1+1하길래",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039785.1.12",
          "form": "친구랑 나눠서 샀습니당",
          "original_form": "친구랑 나눠서 샀습니당",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039785.1.13",
          "form": "와 원쁠원 좋네요 얼마 정도 하나요?",
          "original_form": "와 원쁠원 좋네요 얼마 정도 하나요?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039785.1.14",
          "form": "음 개당 3만원정도에 산 것 같아요",
          "original_form": "음 개당 3만원정도에 산 것 같아요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039785.1.15",
          "form": "최근에 뭐 사셨나여",
          "original_form": "최근에 뭐 사셨나여",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039785.1.16",
          "form": "저는 최근에 갤탭 북커버요",
          "original_form": "저는 최근에 갤탭 북커버요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039785.1.17",
          "form": "태블릿피씨를 처음 사서 너무 설레요",
          "original_form": "태블릿피씨를 처음 사서 너무 설레요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039785.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039785.1.19",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039785.1.20",
          "form": "어떤 기능이 마음에 드시나요",
          "original_form": "어떤 기능이 마음에 드시나요",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039785.1.21",
          "form": "저는 역시 쓰는거요",
          "original_form": "저는 역시 쓰는거요",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039785.1.22",
          "form": "필기감 진짜 너무 좋고",
          "original_form": "필기감 진짜 너무 좋고",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039785.1.23",
          "form": "화면도 커서 좋아요",
          "original_form": "화면도 커서 좋아요",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039785.1.24",
          "form": "만족!",
          "original_form": "만족!",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039785.1.25",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039785.1.26",
