      "utterance": [
        {
          "id": "MDRW2100019157.1.1",
          "form": "점심은 뭐 드셨나요?",
          "original_form": "점심은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019157.1.2",
          "form": "저는 갈비탕이요",
          "original_form": "저는 갈비탕이요",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019157.1.3",
          "form": "그리고 디저트로 다쿠아즈 한개",
          "original_form": "그리고 디저트로 다쿠아즈 한개",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019157.1.4",
          "form": "name2님은 뭐드셨어요?",
          "original_form": "&name2&님은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019157.1.5",
          "form": "우와 디저트까지 완벽 그 자체네요!",
          "original_form": "우와 디저트까지 완벽 그 자체네요!",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019157.1.6",
          "form": "전 라면 먹었어요 ㅎㅎ",
          "original_form": "전 라면 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019157.1.7",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019157.1.8",
          "form": "아뇨 ㅠ name1님은 잘하시나요?",
          "original_form": "아뇨 ㅠ &name1&님은 잘하시나요?",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019157.1.9",
          "form": "잘 못해요",
          "original_form": "잘 못해요",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019157.1.10",
          "form": "그런데 얼마전에 집에 두부랑 감자를 소진하려고",
          "original_form": "그런데 얼마전에 집에 두부랑 감자를 소진하려고",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019157.1.11",
          "form": "요즘은 배달 문화도 잘되어있고해서 ㅎㅎ",
          "original_form": "요즘은 배달 문화도 잘되어있고해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019157.1.12",
          "form": "감자수프랑 두부강정을 했었어요",
          "original_form": "감자수프랑 두부강정을 했었어요",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019157.1.13",
          "form": "배달 좋죠ㅎㅎㅎ",
          "original_form": "배달 좋죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019157.1.14",
          "form": "와 금손이세요! 두부강정 굉장히 어려울거같은데요?",
          "original_form": "와 금손이세요! 두부강정 굉장히 어려울거같은데요?",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019157.1.15",
          "form": "유튜브보고 했는데 어렵진 않았어요",
          "original_form": "유튜브보고 했는데 어렵진 않았어요",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019157.1.16",
          "form": "뒷정리가 힝들었어요",
          "original_form": "뒷정리가 힝들었어요",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019157.1.17",
          "form": "맞아요 요리도 요리지만 뒷정리가 헬이죠 ㅠㅠ",
          "original_form": "맞아요 요리도 요리지만 뒷정리가 헬이죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019157.1.18",
          "form": "전 간맞추기가 너무 힘든거같아요! 요리의 생명은 간인거같은데",
          "original_form": "전 간맞추기가 너무 힘든거같아요! 요리의 생명은 간인거같은데",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019157.1.19",
          "form": "알려주는데로 계량하니까 되더라고요",
          "original_form": "알려주는데로 계량하니까 되더라고요",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019157.1.20",
          "form": "커피는 좋아하세요?",
          "original_form": "커피는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 16:32"
        }
