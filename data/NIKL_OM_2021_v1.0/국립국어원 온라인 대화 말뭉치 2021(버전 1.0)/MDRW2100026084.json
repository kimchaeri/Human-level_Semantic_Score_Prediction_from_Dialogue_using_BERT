      "utterance": [
        {
          "id": "MDRW2100026084.1.1",
          "form": "제주사시나요!",
          "original_form": "제주사시나요!",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026084.1.2",
          "form": "네 2주전에 왔어여",
          "original_form": "네 2주전에 왔어여",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026084.1.3",
          "form": "저는 2주전에 제주에서 서울로 올라왔거든요",
          "original_form": "저는 2주전에 제주에서 서울로 올라왔거든요",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026084.1.4",
          "form": "우와 여름 다보내고 올라가셨네요",
          "original_form": "우와 여름 다보내고 올라가셨네요",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026084.1.5",
          "form": "선배님이시네요",
          "original_form": "선배님이시네요",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026084.1.6",
          "form": "잌ㅋㅋㅋㅋ선뱈ㅋㅋㅋ 부끄럽네요",
          "original_form": "잌ㅋㅋㅋㅋ선뱈ㅋㅋㅋ 부끄럽네요",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026084.1.7",
          "form": "저는 남원읍에서 2주 제주시에서 1주 있다가 올라왔어요",
          "original_form": "저는 남원읍에서 2주 제주시에서 1주 있다가 올라왔어요",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026084.1.8",
          "form": "오 저는 내년 3월까지 있으려구 아예 연세승계했어요",
          "original_form": "오 저는 내년 3월까지 있으려구 아예 연세승계했어요",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026084.1.9",
          "form": "오늘 전입신고해서 이제 도민혜택을 받으며 여행다니려구요 비자림 만장굴 다 공짜예여",
          "original_form": "오늘 전입신고해서 이제 도민혜택을 받으며 여행다니려구요 비자림 만장굴 다 공짜예여",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026084.1.10",
          "form": "아예 오래계시는군요 대박",
          "original_form": "아예 오래계시는군요 대박",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026084.1.11",
          "form": "일년간 여행다니시는건가요?",
          "original_form": "일년간 여행다니시는건가요?",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026084.1.12",
          "form": "서울에서 다시 일자리 찾으신거예요?",
          "original_form": "서울에서 다시 일자리 찾으신거예요?",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026084.1.13",
          "form": "저 초등학교에서 일해서 방학에 제주 내려갔다가 개학하고 다시 올라왔어요 ㅋㅋㅋ ㅜ",
          "original_form": "저 초등학교에서 일해서 방학에 제주 내려갔다가 개학하고 다시 올라왔어요 ㅋㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.14",
          "form": "10월까지는 여행하고 그 뒤로는 모르겠어요",
          "original_form": "10월까지는 여행하고 그 뒤로는 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.15",
          "form": "프리랜서라",
          "original_form": "프리랜서라",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.16",
          "form": "아하 그럼 제주에서도 일하실 수 있나요",
          "original_form": "아하 그럼 제주에서도 일하실 수 있나요",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.17",
          "form": "네 3월부터 한달에 한번씩 왔었다가",
          "original_form": "네 3월부터 한달에 한번씩 왔었다가",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.18",
          "form": "집을 빌리는 게 더 하겠다 싶어서요",
          "original_form": "집을 빌리는 게 더 하겠다 싶어서요",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026084.1.19",
          "form": "아 그렇네욬ㅋㅋㅋ 한달에 한번이면..",
          "original_form": "아 그렇네욬ㅋㅋㅋ 한달에 한번이면..",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026084.1.20",
          "form": "제주 여행지 추천해주세여",
          "original_form": "제주 여행지 추천해주세여",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026084.1.21",
          "form": "저는 사실 거기서 운전면허 땄어요 ㅋㅋㅋ",
          "original_form": "저는 사실 거기서 운전면허 땄어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026084.1.22",
          "form": "하하 실용적인 여행이네요 아무래도 시골이다보니",
          "original_form": "하하 실용적인 여행이네요 아무래도 시골이다보니",
          "speaker_id": "2",
          "time": "20210901 19:45"
        }
      ]
    }
