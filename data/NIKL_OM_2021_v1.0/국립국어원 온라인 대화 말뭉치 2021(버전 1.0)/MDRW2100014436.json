      "utterance": [
        {
          "id": "MDRW2100014436.1.1",
          "form": "중고 거래 몇건하면 금새 쏠쏠해요",
          "original_form": "중고 거래 몇건하면 금새 쏠쏠해요",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014436.1.2",
          "form": "만원 이만원 모여 큰돈 되죠",
          "original_form": "만원 이만원 모여 큰돈 되죠",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014436.1.3",
          "form": "아뒤가 account던데 남자분이신가봐요",
          "original_form": "아뒤가 &account&던데 남자분이신가봐요",
          "speaker_id": "1",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014436.1.4",
          "form": "퍼즐값은 적정하게 받으셨어요?",
          "original_form": "퍼즐값은 적정하게 받으셨어요?",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014436.1.5",
          "form": "그분이요?",
          "original_form": "그분이요?",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.6",
          "form": "네 상대가요",
          "original_form": "네 상대가요",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.7",
          "form": "야간근무시라고 내일아침에 오신다는데",
          "original_form": "야간근무시라고 내일아침에 오신다는데",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.9",
          "form": "저는 출근해야한다고",
          "original_form": "저는 출근해야한다고",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.10",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.11",
          "form": "아침은 못하죠",
          "original_form": "아침은 못하죠",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.12",
          "form": "무인택배함에 넣어두고 찾아가시라고 했어요",
          "original_form": "무인택배함에 넣어두고 찾아가시라고 했어요",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014436.1.13",
          "form": "출근하는데 ㅜㅡ",
          "original_form": "출근하는데 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014436.1.14",
          "form": "오 그런방법 좋네요 ㅎㅎ",
          "original_form": "오 그런방법 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014436.1.15",
          "form": "방금 무인택배함에 넣어두고 집으로 가는길이요 ㅎ",
          "original_form": "방금 무인택배함에 넣어두고 집으로 가는길이요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014436.1.16",
          "form": "오 돈은받으셨죠 ?",
          "original_form": "오 돈은받으셨죠 ?",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014436.1.17",
          "form": "입금받고 비번알려드렸어요",
          "original_form": "입금받고 비번알려드렸어요",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014436.1.18",
          "form": "오 완전좋네요",
          "original_form": "오 완전좋네요",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.19",
          "form": "전 무인택배함 못봤어요 ㅜㅜ",
          "original_form": "전 무인택배함 못봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.20",
          "form": "저 촌사람 ㅋㅋ",
          "original_form": "저 촌사람 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.21",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.22",
          "form": "아 아파트단지에 있어서",
          "original_form": "아 아파트단지에 있어서",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.23",
          "form": "종종 이렇게이용해요",
          "original_form": "종종 이렇게이용해요",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.24",
          "form": "와 좋은 아파트네요 ㅎㅎ",
          "original_form": "와 좋은 아파트네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014436.1.25",
          "form": "정말 좋은거 같아요",
          "original_form": "정말 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014436.1.26",
          "form": "가끔 럴굴보기 그럴때 있는데",
          "original_form": "가끔 럴굴보기 그럴때 있는데",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014436.1.27",
          "form": "안마주쳐도되고 좋아요",
          "original_form": "안마주쳐도되고 좋아요",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014436.1.28",
          "form": "요거 딱이네요",
          "original_form": "요거 딱이네요",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014436.1.29",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014436.1.30",
          "form": "남자분은 좀 얼굴보기가 ㅋㅋ",
          "original_form": "남자분은 좀 얼굴보기가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:05"
        }
      ]
    }
  ]
