      "utterance": [
        {
          "id": "MDRW2100043037.1.1",
          "form": "업무스트레스 잇으신가용",
          "original_form": "업무스트레스 잇으신가용",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043037.1.2",
          "form": "아무래도 약간의 스트레스는 있죠",
          "original_form": "아무래도 약간의 스트레스는 있죠",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043037.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043037.1.4",
          "form": "근데 스트레스 받다가",
          "original_form": "근데 스트레스 받다가",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043037.1.5",
          "form": "일이 잘풀리거나 특히 월급날은",
          "original_form": "일이 잘풀리거나 특히 월급날은",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043037.1.6",
          "form": "기분 좋아지고 그래요ㅎㅎ",
          "original_form": "기분 좋아지고 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043037.1.7",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043037.1.8",
          "form": "월급날 달달하져",
          "original_form": "월급날 달달하져",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043037.1.9",
          "form": "네 월급날의 기쁨",
          "original_form": "네 월급날의 기쁨",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043037.1.10",
          "form": "저는 업무스트레스는 없는데",
          "original_form": "저는 업무스트레스는 없는데",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043037.1.11",
          "form": "워낙 집순이라서 출근과동시에",
          "original_form": "워낙 집순이라서 출근과동시에",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043037.1.12",
          "form": "방전됨요",
          "original_form": "방전됨요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043037.1.13",
          "form": "아 저도 출근때가 젤 피곤한 상태",
          "original_form": "아 저도 출근때가 젤 피곤한 상태",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043037.1.14",
          "form": "월요일에 출근하면서 주말 기다려요ㅋㅋ",
          "original_form": "월요일에 출근하면서 주말 기다려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043037.1.15",
          "form": "하 저두요 지금도",
          "original_form": "하 저두요 지금도",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043037.1.16",
          "form": "침대에 누워서 행복해요",
          "original_form": "침대에 누워서 행복해요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043037.1.17",
          "form": "네 퇴근하고 누우면 정말 행복~",
          "original_form": "네 퇴근하고 누우면 정말 행복~",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043037.1.18",
          "form": "퇴근 몇시에 하시나여",
          "original_form": "퇴근 몇시에 하시나여",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043037.1.19",
          "form": "전 5시쯤이요",
          "original_form": "전 5시쯤이요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043037.1.20",
          "form": "ㅋㅋ저도 다섯시요",
          "original_form": "ㅋㅋ저도 다섯시요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043037.1.21",
          "form": "네시반부터 튀어나갈 준비 완료하고잇어여",
          "original_form": "네시반부터 튀어나갈 준비 완료하고잇어여",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043037.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043037.1.23",
          "form": "오옷 퇴근시간이 비슷하네요ㅋㅋ",
          "original_form": "오옷 퇴근시간이 비슷하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:25"
        }
