      "utterance": [
        {
          "id": "MDRW2100034986.1.1",
          "form": "요즘 보시는 콘텐츠 있나요",
          "original_form": "요즘 보시는 콘텐츠 있나요",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034986.1.2",
          "form": "대탈출 보고있어요!",
          "original_form": "대탈출 보고있어요!",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100034986.1.3",
          "form": "보신적 있으신가요?",
          "original_form": "보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100034986.1.4",
          "form": "아 본적없어요ㅠ 담에 볼게용 !",
          "original_form": "아 본적없어요ㅠ 담에 볼게용 !",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100034986.1.5",
          "form": "갯마을 차차차 봤어요",
          "original_form": "갯마을 차차차 봤어요",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100034986.1.6",
          "form": "오 새로 하는거죠?",
          "original_form": "오 새로 하는거죠?",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100034986.1.7",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100034986.1.8",
          "form": "맞아요 어제 1화는 쏘쏘했어요 그냥 비주얼 파티",
          "original_form": "맞아요 어제 1화는 쏘쏘했어요 그냥 비주얼 파티",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100034986.1.9",
          "form": "크으 얼굿맛집이죠",
          "original_form": "크으 얼굿맛집이죠",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100034986.1.10",
          "form": "저 신민아 엄청 좋아해요",
          "original_form": "저 신민아 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100034986.1.11",
          "form": "오오저도요 김선호도 좋아해요",
          "original_form": "오오저도요 김선호도 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100034986.1.12",
          "form": "오오 저번에 스타트업에서 최고였죠",
          "original_form": "오오 저번에 스타트업에서 최고였죠",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100034986.1.13",
          "form": "웃는 입매가 진짜 매력이예요 둘 다",
          "original_form": "웃는 입매가 진짜 매력이예요 둘 다",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100034986.1.14",
          "form": "맞아요 둘다 보조개ㅠㅠ",
          "original_form": "맞아요 둘다 보조개ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100034986.1.15",
          "form": "진짜 이뻐요",
          "original_form": "진짜 이뻐요",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100034986.1.16",
          "form": "보조개빠에요ㅋㅋㅋ",
          "original_form": "보조개빠에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100034986.1.17",
          "form": "저도그래요 ㅋㅋㅋ",
          "original_form": "저도그래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100034986.1.18",
          "form": "수술로 만들고싶네요 ㅋㅋㅋ",
          "original_form": "수술로 만들고싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100034986.1.19",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100034986.1.20",
          "form": "왜 전 없는건지..ㅋㅋㅋ",
          "original_form": "왜 전 없는건지..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100034986.1.21",
          "form": "주름 지으면 생기긴한데 그런 이쁜 보조개가 아니에요ㅠㅋㅋ",
          "original_form": "주름 지으면 생기긴한데 그런 이쁜 보조개가 아니에요ㅠㅋㅋ",
