      "utterance": [
        {
          "id": "MDRW2100000452.1.1",
          "form": "name3이 가방 짝퉁인듯",
          "original_form": "&name3&이 가방 짝퉁인듯",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000452.1.2",
          "form": "엄마 name3이 가방 어떻게생각해?",
          "original_form": "엄마 &name3&이 가방 어떻게생각해?",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000452.1.3",
          "form": "증말 돈주고 산 쓰레기 같아",
          "original_form": "증말 돈주고 산 쓰레기 같아",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000452.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000452.1.5",
          "form": "언니랑 나랑 짝퉁삿다고",
          "original_form": "언니랑 나랑 짝퉁삿다고",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000452.1.6",
          "form": "하나씩 검색해봄 ㅌㅋㅋㅋ",
          "original_form": "하나씩 검색해봄 ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000452.1.7",
          "form": "난 정말 안물어보고 사는 그거 진짜 맘에 안들어",
          "original_form": "난 정말 안물어보고 사는 그거 진짜 맘에 안들어",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000452.1.8",
          "form": "ㅋㅋㅋㅋ우린 보자마자 ...했는데도",
          "original_form": "ㅋㅋㅋㅋ우린 보자마자 ...했는데도",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000452.1.9",
          "form": "좀 남의 의견도 좀 물어보면 어디 닷나냐?",
          "original_form": "좀 남의 의견도 좀 물어보면 어디 닷나냐?",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000452.1.10",
          "form": "지가 사고 싶어서 삿다하니까...",
          "original_form": "지가 사고 싶어서 삿다하니까...",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.11",
          "form": "지돈주고 산거라 뭐라못함",
          "original_form": "지돈주고 산거라 뭐라못함",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.12",
          "form": "돈이 썩어나",
          "original_form": "돈이 썩어나",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇게 돈쓰는법도 배우는거지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇게 돈쓰는법도 배우는거지",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.14",
          "form": "그렇게 생각 하는데",
          "original_form": "그렇게 생각 하는데",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.15",
          "form": "과해",
          "original_form": "과해",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000452.1.16",
          "form": "뭐가 그리 쉽냐? 걘",
          "original_form": "뭐가 그리 쉽냐? 걘",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.17",
          "form": "근데 겁없이 턱턱 사는건 있어",
          "original_form": "근데 겁없이 턱턱 사는건 있어",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.18",
          "form": "갤탭샀을때 좀 느낌",
          "original_form": "갤탭샀을때 좀 느낌",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.19",
          "form": "난 아직도 뭐하나 살려면 후덜덜 한데",
          "original_form": "난 아직도 뭐하나 살려면 후덜덜 한데",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.20",
          "form": "원래 mz세대가",
          "original_form": "원래 mz세대가",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.21",
          "form": "돈생기면 바로 쓰는 타입인가봐",
          "original_form": "돈생기면 바로 쓰는 타입인가봐",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.22",
          "form": "갤탭 뿐이야?",
          "original_form": "갤탭 뿐이야?",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.23",
          "form": "피티도",
          "original_form": "피티도",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000452.1.24",
          "form": "돈 무서운줄을 몰라",
          "original_form": "돈 무서운줄을 몰라",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000452.1.25",
