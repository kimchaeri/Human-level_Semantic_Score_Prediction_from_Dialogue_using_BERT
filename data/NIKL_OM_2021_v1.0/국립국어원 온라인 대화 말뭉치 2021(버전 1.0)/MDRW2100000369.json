      "utterance": [
        {
          "id": "MDRW2100000369.1.1",
          "form": "이상형이 어떻게 되나요",
          "original_form": "이상형이 어떻게 되나요",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000369.1.2",
          "form": "자상하고 다정하면 좋겠지요",
          "original_form": "자상하고 다정하면 좋겠지요",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000369.1.3",
          "form": "아직 미혼인가요",
          "original_form": "아직 미혼인가요",
          "speaker_id": "1",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000369.1.4",
          "form": "결혼했어요",
          "original_form": "결혼했어요",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000369.1.5",
          "form": "성실하고 책임감 있는 사람이 최고에요",
          "original_form": "성실하고 책임감 있는 사람이 최고에요",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000369.1.6",
          "form": "너무부럽네요 전 아직 미혼입니다",
          "original_form": "너무부럽네요 전 아직 미혼입니다",
          "speaker_id": "1",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000369.1.7",
          "form": "네~ 진짜 내 사람 같은 사람이 때 되면 나타나더라고요",
          "original_form": "네~ 진짜 내 사람 같은 사람이 때 되면 나타나더라고요",
          "speaker_id": "2",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000369.1.8",
          "form": "늦기전에 저도 어서 만나야하는데",
          "original_form": "늦기전에 저도 어서 만나야하는데",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000369.1.9",
          "form": "자연스러운 만남을 추구 하는편이라",
          "original_form": "자연스러운 만남을 추구 하는편이라",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000369.1.10",
          "form": "쉽지가 않네요ㅎ",
          "original_form": "쉽지가 않네요ㅎ",
          "speaker_id": "1",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000369.1.11",
          "form": "일부러 만나려고 조바심 안내도 인연이 있는 것 같아요",
          "original_form": "일부러 만나려고 조바심 안내도 인연이 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000369.1.12",
          "form": "네 저도 소개팅으로 만나긴 했지만 성격이 일단 잘 맞아야죠 ㅎㅎ",
          "original_form": "네 저도 소개팅으로 만나긴 했지만 성격이 일단 잘 맞아야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000369.1.13",
          "form": "나중에 좋아하는 사람이 생기면 결혼할 생각이 있으세요?",
          "original_form": "나중에 좋아하는 사람이 생기면 결혼할 생각이 있으세요?",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000369.1.14",
          "form": "저는 지금 당장이라도 하고싶어요ㅎ",
          "original_form": "저는 지금 당장이라도 하고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000369.1.15",
          "form": "하지만 현실은 너무 벽이 높네요",
          "original_form": "하지만 현실은 너무 벽이 높네요",
          "speaker_id": "1",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000369.1.16",
          "form": "점점 결혼하기가 어려워지죠 ㅠ 근데 모든걸 완벽하게 준비하곤 할 수 없어요",
          "original_form": "점점 결혼하기가 어려워지죠 ㅠ 근데 모든걸 완벽하게 준비하곤 할 수 없어요",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000369.1.17",
          "form": "같이 이뤄가는 거죠",
          "original_form": "같이 이뤄가는 거죠",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000369.1.18",
          "form": "그걸 같이 타협하며 채워갈 동반자를 만나면 좋겠네요",
          "original_form": "그걸 같이 타협하며 채워갈 동반자를 만나면 좋겠네요",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000369.1.19",
          "form": "꼭 만나실 거에요. name1님 생각을 이해해주실 분",
          "original_form": "꼭 만나실 거에요. &name1&님 생각을 이해해주실 분",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000369.1.20",
          "form": "저희도 결혼할 때 흙수저라 아무것도 없이 시작했어요",
          "original_form": "저희도 결혼할 때 흙수저라 아무것도 없이 시작했어요",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000369.1.21",
          "form": "그냥 마음만 가지고",
          "original_form": "그냥 마음만 가지고",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000369.1.22",
          "form": "제가 원하는게 딱 그런거 에요",
          "original_form": "제가 원하는게 딱 그런거 에요",
          "speaker_id": "1",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000369.1.23",
          "form": "근데 주변에는 전부 돈, 집 이게 제일 중요하더군요",
          "original_form": "근데 주변에는 전부 돈, 집 이게 제일 중요하더군요",
