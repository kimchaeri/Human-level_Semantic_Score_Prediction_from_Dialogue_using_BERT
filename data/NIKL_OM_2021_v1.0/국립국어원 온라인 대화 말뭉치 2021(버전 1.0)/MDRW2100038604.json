      "utterance": [
        {
          "id": "MDRW2100038604.1.1",
          "form": "판자 하나하나 방향 맞춰 세워가면서 집짓는 오픈월드겜도 봤음",
          "original_form": "판자 하나하나 방향 맞춰 세워가면서 집짓는 오픈월드겜도 봤음",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038604.1.2",
          "form": "과금도도 마찬가지임",
          "original_form": "과금도도 마찬가지임",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038604.1.3",
          "form": "땅부터 고르고 시작하고",
          "original_form": "땅부터 고르고 시작하고",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038604.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038604.1.5",
          "form": "솔직히 그리고 이겜",
          "original_form": "솔직히 그리고 이겜",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038604.1.6",
          "form": "5성 일정이상 뽑으면",
          "original_form": "5성 일정이상 뽑으면",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038604.1.7",
          "form": "굳이 과금할필요",
          "original_form": "굳이 과금할필요",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038604.1.8",
          "form": "읪어보이는데",
          "original_form": "읪어보이는데",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038604.1.9",
          "form": "그것도 모바일 일반가챠 대비 혁신이지",
          "original_form": "그것도 모바일 일반가챠 대비 혁신이지",
          "speaker_id": "1",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.10",
          "form": "ㅇㅇ낚시도감 하우징 이런건",
          "original_form": "ㅇㅇ낚시도감 하우징 이런건",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.11",
          "form": "플스형 겜은",
          "original_form": "플스형 겜은",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.12",
          "form": "보통 플스겜은 많아야 10만원 정도면 풀패키지 사는데요...",
          "original_form": "보통 플스겜은 많아야 10만원 정도면 풀패키지 사는데요...",
          "speaker_id": "1",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.13",
          "form": "당연한거임",
          "original_form": "당연한거임",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.14",
          "form": "당장 동숲만봐도 그런거 다있어",
          "original_form": "당장 동숲만봐도 그런거 다있어",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038604.1.15",
          "form": "그치 ㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100038604.1.16",
          "form": "플스겜이었으면 요리 재료도 그냥 생선 고기 이런거 아니고",
          "original_form": "플스겜이었으면 요리 재료도 그냥 생선 고기 이런거 아니고",
          "speaker_id": "2",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100038604.1.17",
          "form": "종류 싹 나눠야했음",
          "original_form": "종류 싹 나눠야했음",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038604.1.18",
          "form": "반성하고",
          "original_form": "반성하고",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038604.1.19",
          "form": "모바일로 돌아와라",
          "original_form": "모바일로 돌아와라",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038604.1.20",
          "form": "니넨 플스로가면 그냥 씹덕형 아류작이야",
          "original_form": "니넨 플스로가면 그냥 씹덕형 아류작이야",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038604.1.21",
          "form": "라이덴 궁 모션도",
          "original_form": "라이덴 궁 모션도",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038604.1.22",
          "form": "또 표절이라고",
          "original_form": "또 표절이라고",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038604.1.23",
          "form": "난리났더만",
          "original_form": "난리났더만",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038604.1.24",
          "form": "무슨 컴게임을 하겠다고",
          "original_form": "무슨 컴게임을 하겠다고",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038604.1.25",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.26",
          "form": "플스는 그리고",
          "original_form": "플스는 그리고",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.27",
          "form": "캐디부터 그림체가 다른데",
          "original_form": "캐디부터 그림체가 다른데",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.28",
          "form": "당장 에일로이가 원래 어떤 디자인인지",
          "original_form": "당장 에일로이가 원래 어떤 디자인인지",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.29",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ플스로 가면",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ플스로 가면",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.30",
          "form": "그래픽 개선",
          "original_form": "그래픽 개선",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.31",
          "form": "1500배는",
          "original_form": "1500배는",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038604.1.32",
          "form": "해야함",
          "original_form": "해야함",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.33",
          "form": "그리고 조연들 얼굴도",
          "original_form": "그리고 조연들 얼굴도",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.34",
          "form": "당연히 다 달라야지",
          "original_form": "당연히 다 달라야지",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.35",
          "form": "여캐 옷부터",
          "original_form": "여캐 옷부터",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.36",
          "form": "지금처럼 엑스트라 모델링 돌려쓰기?",
          "original_form": "지금처럼 엑스트라 모델링 돌려쓰기?",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.37",
          "form": "싹 갈아엎어야함",
          "original_form": "싹 갈아엎어야함",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.38",
          "form": "있을수없음",
          "original_form": "있을수없음",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.39",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ현실 인간형 쓸디에",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ현실 인간형 쓸디에",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.40",
          "form": "어디 사방팔방에 되도않게 구멍낸 옷을 ㅋㅋㅋㅋ",
          "original_form": "어디 사방팔방에 되도않게 구멍낸 옷을 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.41",
          "form": "그따위로 입히면",
          "original_form": "그따위로 입히면",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038604.1.42",
          "form": "진짜 너무",
          "original_form": "진짜 너무",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038604.1.43",
          "form": "개구린 겜인거",
          "original_form": "개구린 겜인거",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038604.1.44",
          "form": "티 확나서",
          "original_form": "티 확나서",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038604.1.45",
          "form": "아무도 안함",
          "original_form": "아무도 안함",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038604.1.46",
          "form": "그리고 플스는 서양쪽에서도 손 많이 대서 이제 여캐한테 옷 그따위로 안입히는 추세잖아",
          "original_form": "그리고 플스는 서양쪽에서도 손 많이 대서 이제 여캐한테 옷 그따위로 안입히는 추세잖아",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038604.1.47",
          "form": "ㅇㅇ 일애니 오타쿠같은 디자인",
          "original_form": "ㅇㅇ 일애니 오타쿠같은 디자인",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100038604.1.48",
          "form": "유행 지난지 백만년인데",
          "original_form": "유행 지난지 백만년인데",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100038604.1.49",
          "form": "모바일이니까 가챠겜 하던 인간들이 붙어잇는거지",
          "original_form": "모바일이니까 가챠겜 하던 인간들이 붙어잇는거지",
          "speaker_id": "1",
          "time": "20210902 16:58"
        }
      ]
