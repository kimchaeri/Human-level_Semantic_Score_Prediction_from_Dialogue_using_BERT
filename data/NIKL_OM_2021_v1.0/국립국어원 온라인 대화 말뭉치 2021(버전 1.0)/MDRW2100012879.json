      "utterance": [
        {
          "id": "MDRW2100012879.1.1",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 06:20"
        },
        {
          "id": "MDRW2100012879.1.2",
          "form": "혹시 야구 보시나요?",
          "original_form": "혹시 야구 보시나요?",
          "speaker_id": "1",
          "time": "20210825 06:20"
        },
        {
          "id": "MDRW2100012879.1.3",
          "form": "아니요 두산 알지못합니다...",
          "original_form": "아니요 두산 알지못합니다...",
          "speaker_id": "2",
          "time": "20210825 06:21"
        },
        {
          "id": "MDRW2100012879.1.4",
          "form": "아 저는 기아 알지 못합니다",
          "original_form": "아 저는 기아 알지 못합니다",
          "speaker_id": "1",
          "time": "20210825 06:21"
        },
        {
          "id": "MDRW2100012879.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 06:21"
        },
        {
          "id": "MDRW2100012879.1.6",
          "form": "ㅋㅋㅋ아버지는 어디 팬이세요?",
          "original_form": "ㅋㅋㅋ아버지는 어디 팬이세요?",
          "speaker_id": "2",
          "time": "20210825 06:21"
        },
        {
          "id": "MDRW2100012879.1.7",
          "form": "아버지가 기아 팬이세요",
          "original_form": "아버지가 기아 팬이세요",
          "speaker_id": "1",
          "time": "20210825 06:21"
        },
        {
          "id": "MDRW2100012879.1.8",
          "form": "야구 좋아하시면 유니폼 같은 것도 구매하시나요?",
          "original_form": "야구 좋아하시면 유니폼 같은 것도 구매하시나요?",
          "speaker_id": "1",
          "time": "20210825 06:22"
        },
        {
          "id": "MDRW2100012879.1.9",
          "form": "아니요..야구경기는 욕을 안할수가 없어서. 흠흠",
          "original_form": "아니요..야구경기는 욕을 안할수가 없어서. 흠흠",
          "speaker_id": "2",
          "time": "20210825 06:23"
        },
        {
          "id": "MDRW2100012879.1.10",
          "form": "그건 그래요.. ㅋㅋㅋ",
          "original_form": "그건 그래요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 06:24"
        },
        {
          "id": "MDRW2100012879.1.11",
          "form": "가끔 보면  쟤들 지금 뭐하나 생각들 때 있어요",
          "original_form": "가끔 보면  쟤들 지금 뭐하나 생각들 때 있어요",
          "speaker_id": "1",
          "time": "20210825 06:24"
        },
        {
          "id": "MDRW2100012879.1.12",
          "form": "그 연봉 양궁이나 여자배구 줘야해요....",
          "original_form": "그 연봉 양궁이나 여자배구 줘야해요....",
          "speaker_id": "2",
          "time": "20210825 06:24"
        },
        {
          "id": "MDRW2100012879.1.13",
          "form": "그니까요..그 연봉을 받고 그 자리에서서 공 바라볼 때",
          "original_form": "그니까요..그 연봉을 받고 그 자리에서서 공 바라볼 때",
          "speaker_id": "1",
          "time": "20210825 06:25"
        },
        {
          "id": "MDRW2100012879.1.14",
          "form": "올림픽 경기중 껌씹는 거 중계로 보는 국민이 제일 슬픔.",
          "original_form": "올림픽 경기중 껌씹는 거 중계로 보는 국민이 제일 슬픔.",
          "speaker_id": "2",
          "time": "20210825 06:25"
        },
        {
          "id": "MDRW2100012879.1.15",
          "form": "아.. 그거 진짜",
          "original_form": "아.. 그거 진짜",
          "speaker_id": "1",
          "time": "20210825 06:26"
        },
        {
          "id": "MDRW2100012879.1.16",
          "form": "진짜 너무했어요",
          "original_form": "진짜 너무했어요",
          "speaker_id": "1",
          "time": "20210825 06:26"
        },
        {
          "id": "MDRW2100012879.1.17",
          "form": "다른 종목 선수들은.. 진짜 죽기살기로 하는데",
          "original_form": "다른 종목 선수들은.. 진짜 죽기살기로 하는데",
          "speaker_id": "1",
          "time": "20210825 06:26"
        },
        {
          "id": "MDRW2100012879.1.18",
          "form": "올림픽이 장난이니..",
          "original_form": "올림픽이 장난이니..",
          "speaker_id": "1",
          "time": "20210825 06:27"
        },
        {
          "id": "MDRW2100012879.1.19",
          "form": "하고 묻고싶더라는..",
          "original_form": "하고 묻고싶더라는..",
          "speaker_id": "1",
          "time": "20210825 06:27"
        },
        {
          "id": "MDRW2100012879.1.20",
          "form": "진짜 배가 불러서.. 전 야구 나라는 잊고 상대팀 응원했어요ㅠㅠ",
          "original_form": "진짜 배가 불러서.. 전 야구 나라는 잊고 상대팀 응원했어요ㅠㅠ",
          "speaker_id": "2",
