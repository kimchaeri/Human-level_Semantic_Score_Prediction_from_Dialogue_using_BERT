      "utterance": [
        {
          "id": "MDRW2100027513.1.1",
          "form": "책 그만 사려고 했는데 또 샀어",
          "original_form": "책 그만 사려고 했는데 또 샀어",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027513.1.2",
          "form": "무슨 책 샀어",
          "original_form": "무슨 책 샀어",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027513.1.3",
          "form": "만화책인데 한 번 사면 우르르르 사게 되니까",
          "original_form": "만화책인데 한 번 사면 우르르르 사게 되니까",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027513.1.4",
          "form": "순정만화도 사고 원서도 사고",
          "original_form": "순정만화도 사고 원서도 사고",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027513.1.5",
          "form": "눈 뒤집혀서 사고 나니까 이제 텅장",
          "original_form": "눈 뒤집혀서 사고 나니까 이제 텅장",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027513.1.6",
          "form": "얼마나 많이 산 거야ㅋㅋㅋㅋㅋㅋ",
          "original_form": "얼마나 많이 산 거야ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027513.1.7",
          "form": "책은 한 번 사면 10만원 이상 결제하지 않아? 요즘 너무 비싸잖아 ㅠㅠㅠ",
          "original_form": "책은 한 번 사면 10만원 이상 결제하지 않아? 요즘 너무 비싸잖아 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027513.1.8",
          "form": "그것도 맞아... 책값 되게 비싸짐. 이북이면 싸게 팔기라도 하등가",
          "original_form": "그것도 맞아... 책값 되게 비싸짐. 이북이면 싸게 팔기라도 하등가",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027513.1.9",
          "form": "이북은 손에 남는 게 없는데",
          "original_form": "이북은 손에 남는 게 없는데",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.10",
          "form": "왜!! 그리! 비싸!",
          "original_form": "왜!! 그리! 비싸!",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.11",
          "form": "ㅁㅈ... 되팔램도 안되는디",
          "original_form": "ㅁㅈ... 되팔램도 안되는디",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.12",
          "form": "회사 도산하면 내 책.다 날라가는겨!",
          "original_form": "회사 도산하면 내 책.다 날라가는겨!",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.13",
          "form": "내 책을 위해 계속 리디를 이용해야 한다는 것...¿",
          "original_form": "내 책을 위해 계속 리디를 이용해야 한다는 것...¿",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.14",
          "form": "현물 책보다는 이북이 편해서",
          "original_form": "현물 책보다는 이북이 편해서",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.15",
          "form": "어쩔수없이 이용한다",
          "original_form": "어쩔수없이 이용한다",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027513.1.16",
          "form": "난 책이 더 좋은데",
          "original_form": "난 책이 더 좋은데",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027513.1.17",
          "form": "만화책 좀 정리해야하는데 귀찮다",
          "original_form": "만화책 좀 정리해야하는데 귀찮다",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027513.1.18",
          "form": "책은 많이 사면",
          "original_form": "책은 많이 사면",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027513.1.19",
          "form": "둘 곳이 없잖여",
          "original_form": "둘 곳이 없잖여",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027513.1.20",
          "form": "이북은 내 손에 작은 도서관이구",
          "original_form": "이북은 내 손에 작은 도서관이구",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027513.1.21",
          "form": "그래도 책이 더 조아?",
          "original_form": "그래도 책이 더 조아?",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.22",
          "form": "근데 종이책이 집중도 잘되고 안 잊어먹엉",
          "original_form": "근데 종이책이 집중도 잘되고 안 잊어먹엉",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.23",
          "form": "그건 그렇긴하다",
          "original_form": "그건 그렇긴하다",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.24",
          "form": "종이책으로 보면 특정 장면 보고싶으면 얼추 기억난다구",
          "original_form": "종이책으로 보면 특정 장면 보고싶으면 얼추 기억난다구",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.25",
          "form": "언제 종이책을 읽었는지 가물가물하다만",
          "original_form": "언제 종이책을 읽었는지 가물가물하다만",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.26",
          "form": "페이지?",
          "original_form": "페이지?",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.27",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027513.1.28",
          "form": "후루룩 넘겨서 여기 이 페이지다!",
          "original_form": "후루룩 넘겨서 여기 이 페이지다!",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.29",
          "form": "하고 펴는고야?",
          "original_form": "하고 펴는고야?",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.30",
          "form": "앞은 이만한 두께였다...!",
          "original_form": "앞은 이만한 두께였다...!",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.31",
          "form": "ㅇㅇ 대충 이쯔음 라고 펼치는 듯",
          "original_form": "ㅇㅇ 대충 이쯔음 라고 펼치는 듯",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.32",
          "form": "손의 감촉!",
          "original_form": "손의 감촉!",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.33",
          "form": "나중에 만화책도",
          "original_form": "나중에 만화책도",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027513.1.34",
          "form": "만화책도",
          "original_form": "만화책도",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.35",
          "form": "몇권에서 무슨 장면 나왔지 이걸 10년 후에도 기억하드라",
          "original_form": "몇권에서 무슨 장면 나왔지 이걸 10년 후에도 기억하드라",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.36",
          "form": "인상깊은 장면이라 그릏지만..",
          "original_form": "인상깊은 장면이라 그릏지만..",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.37",
          "form": "헐 난 기억 못할듯",
          "original_form": "헐 난 기억 못할듯",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.38",
          "form": "그 작품에 이런 게 잇었지 <까지는 가능",
          "original_form": "그 작품에 이런 게 잇었지 <까지는 가능",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.39",
          "form": "몇권<모름",
          "original_form": "몇권<모름",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027513.1.40",
          "form": "내가 이북으로 읽어서 그런가요",
          "original_form": "내가 이북으로 읽어서 그런가요",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027513.1.41",
          "form": "옛날에는 대여점에서 책 빌려봤잖아",
          "original_form": "옛날에는 대여점에서 책 빌려봤잖아",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027513.1.42",
          "form": "그시절에도 기억이 안나 너무 옛날인가",
          "original_form": "그시절에도 기억이 안나 너무 옛날인가",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027513.1.43",
          "form": "너무 예전일 수도 있고...",
          "original_form": "너무 예전일 수도 있고...",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027513.1.44",
          "form": "언니는 기억나지요?",
          "original_form": "언니는 기억나지요?",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027513.1.45",
          "form": "ㅇㅇ 네에 그렇습니다. 이 작가가 저거 그렸다 이런 거 너모 재밋어",
          "original_form": "ㅇㅇ 네에 그렇습니다. 이 작가가 저거 그렸다 이런 거 너모 재밋어",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027513.1.46",
          "form": "좀 관심사가 달라서 뇌를 쓰는 방식이 다른덧ㅋㅋㅋㅋㅋ",
          "original_form": "좀 관심사가 달라서 뇌를 쓰는 방식이 다른덧ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027513.1.47",
          "form": "그냥 아 잼따 ㅎ 로 끝나는 듯 나는.... 작가....작가가 누구지?수준ㅋㅋㅋ",
          "original_form": "그냥 아 잼따 ㅎ 로 끝나는 듯 나는.... 작가....작가가 누구지?수준ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027513.1.48",
          "form": "뇌에 저장해두기 ^ㅡ^ 그래서 나는 종이책이 더 좋은듯",
          "original_form": "뇌에 저장해두기 ^ㅡ^ 그래서 나는 종이책이 더 좋은듯",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027513.1.49",
          "form": "믈론 웹툰도 기억하면 기분 째지지만...",
          "original_form": "믈론 웹툰도 기억하면 기분 째지지만...",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027513.1.50",
          "form": "나도 만약에 우리집에 커다란 서재가ㅡ있고 내가 부자였다면 읽은 책 모듀모두 모아둘거",
          "original_form": "나도 만약에 우리집에 커다란 서재가ㅡ있고 내가 부자였다면 읽은 책 모듀모두 모아둘거",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027513.1.51",
          "form": "지금은 그지니까 이북",
          "original_form": "지금은 그지니까 이북",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027513.1.52",
          "form": "나중에 방 하나를 책방으로 만들고 싶지ㅋㅋㅋㅋㅋ",
          "original_form": "나중에 방 하나를 책방으로 만들고 싶지ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027513.1.53",
          "form": "만화방처럼 막 옆으로 넘기는 책장에",
          "original_form": "만화방처럼 막 옆으로 넘기는 책장에",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027513.1.54",
          "form": "빼곡하게 책 쌓아서 ㅋㅋㅋㅋ",
          "original_form": "빼곡하게 책 쌓아서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027513.1.55",
          "form": "근데 그렇게 차리면 부실한 집은 무너지겠지... 책이 무거우니까",
          "original_form": "근데 그렇게 차리면 부실한 집은 무너지겠지... 책이 무거우니까",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027513.1.56",
          "form": "책 많은 집은 공사를 한다카더라",
          "original_form": "책 많은 집은 공사를 한다카더라",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027513.1.57",
          "form": "우리집도 이사할 때 내가 책 좀 정리했는데",
          "original_form": "우리집도 이사할 때 내가 책 좀 정리했는데",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027513.1.58",
          "form": "책 많다고 추가 요금나옴ㅋㅋㅋㅋ",
