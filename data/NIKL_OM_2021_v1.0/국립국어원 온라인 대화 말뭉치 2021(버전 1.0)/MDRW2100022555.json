      "utterance": [
        {
          "id": "MDRW2100022555.1.1",
          "form": "겜이 뭐 하세요?",
          "original_form": "겜이 뭐 하세요?",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022555.1.2",
          "form": "저 쿠킹덤이요",
          "original_form": "저 쿠킹덤이요",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022555.1.3",
          "form": "님은요?ㅎㅎ",
          "original_form": "님은요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022555.1.4",
          "form": "전 요즘하는건 없어요..",
          "original_form": "전 요즘하는건 없어요..",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022555.1.5",
          "form": "아ㅠ",
          "original_form": "아ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022555.1.6",
          "form": "쿠키 재밋어여? 노가다 많은가요? ㅋㅋ",
          "original_form": "쿠키 재밋어여? 노가다 많은가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022555.1.7",
          "form": "어릴때하시던건??",
          "original_form": "어릴때하시던건??",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022555.1.8",
          "form": "ㅋㅋ노가다엄청나요 고인물이라서 걍 하는거ㅠ",
          "original_form": "ㅋㅋ노가다엄청나요 고인물이라서 걍 하는거ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022555.1.9",
          "form": "초창기에 뛰어드셧나보네요? ㅋㅋㅋ",
          "original_form": "초창기에 뛰어드셧나보네요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022555.1.10",
          "form": "ㅋㅋ맞아요 런칭했을때부터!!",
          "original_form": "ㅋㅋ맞아요 런칭했을때부터!!",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022555.1.11",
          "form": "직후는아니구 한 2주?",
          "original_form": "직후는아니구 한 2주?",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022555.1.12",
          "form": "오... 고렙?",
          "original_form": "오... 고렙?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022555.1.13",
          "form": "순위도있나여?",
          "original_form": "순위도있나여?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022555.1.14",
          "form": "아레나라고 순위컨텐츠도 있는데",
          "original_form": "아레나라고 순위컨텐츠도 있는데",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022555.1.15",
          "form": "그냥 쿠키모으고 이런 순위는 없더라고요ㅠ",
          "original_form": "그냥 쿠키모으고 이런 순위는 없더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022555.1.16",
          "form": "첨에 해보고 전 못하겠어서 포기.. ㅋㅋㅋ",
          "original_form": "첨에 해보고 전 못하겠어서 포기.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022555.1.17",
          "form": "뭔가 복잡",
          "original_form": "뭔가 복잡",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022555.1.18",
          "form": "ㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022555.1.19",
          "form": "그럼 깔아는 보셨네요!!!",
          "original_form": "그럼 깔아는 보셨네요!!!",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022555.1.20",
          "form": "폰게임 어떤거하셧어용",
          "original_form": "폰게임 어떤거하셧어용",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022555.1.21",
          "form": "네.. 마지막으로 한게.. 뭐더라..",
          "original_form": "네.. 마지막으로 한게.. 뭐더라..",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022555.1.22",
          "form": "좀비 지키키 같은거 였는데.. \r\n막 건물 세우고 농장 만들고 그런류요..",
          "original_form": "좀비 지키키 같은거 였는데.. \r\n막 건물 세우고 농장 만들고 그런류요..",
