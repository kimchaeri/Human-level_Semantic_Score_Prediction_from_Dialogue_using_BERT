      "utterance": [
        {
          "id": "MDRW2100032219.1.1",
          "form": "ㅋㅋ공식적으로는4500입니당~~",
          "original_form": "ㅋㅋ공식적으로는4500입니당~~",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032219.1.2",
          "form": "근데 제 생각과는 다른결과가 나오긴해요ㅋㅋ",
          "original_form": "근데 제 생각과는 다른결과가 나오긴해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032219.1.3",
          "form": "4500이라니 올수리시군요ㅋㅋ",
          "original_form": "4500이라니 올수리시군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032219.1.4",
          "form": "ㅋㅋㅋ넹ㅋㅋ붙박이몇개빼고올수리요",
          "original_form": "ㅋㅋㅋ넹ㅋㅋ붙박이몇개빼고올수리요",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032219.1.5",
          "form": "근더남편은아쉬워하더라구요",
          "original_form": "근더남편은아쉬워하더라구요",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032219.1.6",
          "form": "붙박이문은교체했는데, 그러다보니 구멍이 많이 뚤려있다고;;;",
          "original_form": "붙박이문은교체했는데, 그러다보니 구멍이 많이 뚤려있다고;;;",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032219.1.7",
          "form": "그부분이 싫은가봐요ㅋ",
          "original_form": "그부분이 싫은가봐요ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032219.1.8",
          "form": "아 아예붙박이를바꿀껄하고?",
          "original_form": "아 아예붙박이를바꿀껄하고?",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.9",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.10",
          "form": "그 구멍이 은근신경쓰이니까요ㅜㅠ",
          "original_form": "그 구멍이 은근신경쓰이니까요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.11",
          "form": "통으로바꿀껄하고요..",
          "original_form": "통으로바꿀껄하고요..",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.12",
          "form": "그래도환경오염에...ㅋㅋ",
          "original_form": "그래도환경오염에...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.13",
          "form": "굳이싶어서안바꿨는데ㅋㅋ",
          "original_form": "굳이싶어서안바꿨는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.14",
          "form": "그러게요 하는김에다했음좋았지만",
          "original_form": "그러게요 하는김에다했음좋았지만",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.15",
          "form": "저도은근히후회중ㅋ",
          "original_form": "저도은근히후회중ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032219.1.16",
          "form": "환경을 위해선 잘하신 소비였어요",
          "original_form": "환경을 위해선 잘하신 소비였어요",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100032219.1.17",
          "form": "특히신발장...ㅋ",
          "original_form": "특히신발장...ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100032219.1.18",
          "form": "잘했어요ㅋㅋ",
          "original_form": "잘했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100032219.1.19",
          "form": "남의발냄새가고스란히나는것같은기분ㅇㅋ",
          "original_form": "남의발냄새가고스란히나는것같은기분ㅇㅋ",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100032219.1.20",
          "form": "신발장도 문짝만교체하나요?",
