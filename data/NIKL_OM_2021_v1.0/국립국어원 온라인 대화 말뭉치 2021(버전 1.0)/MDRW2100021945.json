      "utterance": [
        {
          "id": "MDRW2100021945.1.1",
          "form": "자바칩은맛잇어",
          "original_form": "자바칩은맛잇어",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021945.1.2",
          "form": "나도 자바칩 좋아해",
          "original_form": "나도 자바칩 좋아해",
          "speaker_id": "1",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021945.1.3",
          "form": "쿠앤쿠 프라프치노에 자바칩 추가한게 최고지",
          "original_form": "쿠앤쿠 프라프치노에 자바칩 추가한게 최고지",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021945.1.4",
          "form": "쿠앤크도 맛나",
          "original_form": "쿠앤크도 맛나",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021945.1.5",
          "form": "컴포즈 커피  거기가 진짜 맛잇오ㅎㅎ",
          "original_form": "컴포즈 커피  거기가 진짜 맛잇오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021945.1.6",
          "form": "나 거기 한 번도 안 먹어 봤는데 맛있어?",
          "original_form": "나 거기 한 번도 안 먹어 봤는데 맛있어?",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100021945.1.7",
          "form": "나는 스타벅스 맛있었어",
          "original_form": "나는 스타벅스 맛있었어",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021945.1.8",
          "form": "그런데 비싸ㅠㅠ",
          "original_form": "그런데 비싸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021945.1.9",
          "form": "나는 스탁벅스 불매,,? 자야",
          "original_form": "나는 스탁벅스 불매,,? 자야",
          "speaker_id": "2",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021945.1.10",
          "form": "컴포즈 자바칩이 진짜 맛잇어",
          "original_form": "컴포즈 자바칩이 진짜 맛잇어",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100021945.1.11",
          "form": "시켜먹어바",
          "original_form": "시켜먹어바",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100021945.1.12",
          "form": "컴포즈? 눈에 보이면 들어가 볼게",
          "original_form": "컴포즈? 눈에 보이면 들어가 볼게",
          "speaker_id": "1",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100021945.1.13",
          "form": "눈에 보이기 전에 찾아 먹어도 후회하지 않을고야",
          "original_form": "눈에 보이기 전에 찾아 먹어도 후회하지 않을고야",
          "speaker_id": "2",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100021945.1.14",
          "form": "오랜만에 봄봄에서 오레오 프라푸치노? 먹고 싶은데 주변에 봄봄이 없어 ㅠㅠ",
          "original_form": "오랜만에 봄봄에서 오레오 프라푸치노? 먹고 싶은데 주변에 봄봄이 없어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100021945.1.15",
          "form": "봄봄  의령에만 잇는거지?",
          "original_form": "봄봄  의령에만 잇는거지?",
          "speaker_id": "2",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100021945.1.16",
          "form": "나도 봄봄이 없어,,",
          "original_form": "나도 봄봄이 없어,,",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.17",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.18",
          "form": "그거 프랜차이즈 아니야?",
          "original_form": "그거 프랜차이즈 아니야?",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.19",
          "form": "배민에 안나와,,",
          "original_form": "배민에 안나와,,",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.20",
          "form": "경남에만 있대..",
          "original_form": "경남에만 있대..",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.21",
          "form": "나 몰랐어",
          "original_form": "나 몰랐어",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100021945.1.22",
          "form": "그게무슨,,,,,,,,,",
          "original_form": "그게무슨,,,,,,,,,",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100021945.1.23",
          "form": "부산에도 없는거네,,,,,",
          "original_form": "부산에도 없는거네,,,,,",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100021945.1.24",
          "form": "수도권에도 주라,,,",
          "original_form": "수도권에도 주라,,,",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100021945.1.25",
          "form": "부산에 있어",
          "original_form": "부산에 있어",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100021945.1.26",
          "form": "아 제주도에도 있네?",
          "original_form": "아 제주도에도 있네?",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100021945.1.27",
          "form": "봄봄 서울에는 없어?",
          "original_form": "봄봄 서울에는 없어?",
          "speaker_id": "1",
          "time": "20210829 01:15"
        }
      ]
    }
