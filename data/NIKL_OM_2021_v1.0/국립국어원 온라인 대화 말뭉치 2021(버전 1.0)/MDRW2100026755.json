      "utterance": [
        {
          "id": "MDRW2100026755.1.1",
          "form": "딱 저한테 이상적인 계절은 가을인데",
          "original_form": "딱 저한테 이상적인 계절은 가을인데",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026755.1.2",
          "form": "벌레를... 뺐음 좋겠어요",
          "original_form": "벌레를... 뺐음 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026755.1.3",
          "form": "가을 너무 좋은데 요즘 가을이 잇긴한가요 ㅠㅠ",
          "original_form": "가을 너무 좋은데 요즘 가을이 잇긴한가요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026755.1.4",
          "form": "벌레는 항상 없었으면해요 ㅜㅜ",
          "original_form": "벌레는 항상 없었으면해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026755.1.5",
          "form": "잠깐 가을은 스쳐지나가죠...",
          "original_form": "잠깐 가을은 스쳐지나가죠...",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026755.1.6",
          "form": "곧 추워지겠죠?ㅠㅠ",
          "original_form": "곧 추워지겠죠?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026755.1.7",
          "form": "네.. 혹독한 겨울이 찾아오겠죠 ㅠ",
          "original_form": "네.. 혹독한 겨울이 찾아오겠죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026755.1.8",
          "form": "패딩.. 꺼내야겠네요ㅠㅠ",
          "original_form": "패딩.. 꺼내야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026755.1.9",
          "form": "ㅠㅠㅠㅠ패딩 ㅜㅜㅜ",
          "original_form": "ㅠㅠㅠㅠ패딩 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026755.1.10",
          "form": "벌써 패딩 ㅠㅠㅠ",
          "original_form": "벌써 패딩 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026755.1.11",
          "form": "반팔 넣고 얼마뒤면 패딩 꺼내야죠ㅠㅠ",
          "original_form": "반팔 넣고 얼마뒤면 패딩 꺼내야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026755.1.12",
          "form": "맞아요 ㅠㅠㅠ 금새 옵니다",
          "original_form": "맞아요 ㅠㅠㅠ 금새 옵니다",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026755.1.13",
          "form": "이번 겨울은 혹독하지 않았으면...ㅠㅠ",
          "original_form": "이번 겨울은 혹독하지 않았으면...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026755.1.14",
          "form": "추운거 싫어요ㅠㅠ",
          "original_form": "추운거 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026755.1.15",
          "form": "저는 눈만 많이왔으면...",
          "original_form": "저는 눈만 많이왔으면...",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026755.1.16",
          "form": "오 눈 좋아하시나봐요",
          "original_form": "오 눈 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026755.1.17",
          "form": "네 ㅎㅎ 근데 부산 살아서 눈을 못봐요...",
          "original_form": "네 ㅎㅎ 근데 부산 살아서 눈을 못봐요...",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026755.1.18",
          "form": "눈 보는게 손에 꼽을 정도..",
          "original_form": "눈 보는게 손에 꼽을 정도..",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026755.1.19",
          "form": "저는 대구 사는데 여기도 많이 안와요..",
          "original_form": "저는 대구 사는데 여기도 많이 안와요..",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026755.1.20",
          "form": "아..대구살면.. 안오죠 ㅋㅋㅋㅋㅋㅋㅋ 경상도는 안와요 ㅠㅠㅠ",
          "original_form": "아..대구살면.. 안오죠 ㅋㅋㅋㅋㅋㅋㅋ 경상도는 안와요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026755.1.21",
          "form": "눈오면 우린 이벤트잖아요ㅋㅋㅋㅋ",
          "original_form": "눈오면 우린 이벤트잖아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026755.1.22",
          "form": "와봤자 얼마 안쌓여서ㅋㅋㅋㅋ",
          "original_form": "와봤자 얼마 안쌓여서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026755.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026755.1.24",
          "form": "그냥 내리는거보는게 이벤트",
          "original_form": "그냥 내리는거보는게 이벤트",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026755.1.25",
          "form": "쌓이는 날이없죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "쌓이는 날이없죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026755.1.26",
          "form": "허리까지 쌓이고 그런걸 바라진 않지만",
          "original_form": "허리까지 쌓이고 그런걸 바라진 않지만",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026755.1.27",
          "form": "보면 신기할것같아욬ㅋ",
          "original_form": "보면 신기할것같아욬ㅋ",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026755.1.28",
          "form": "너무 보고싶어서 작년엔 그냥 눈오는 날 맞춰서 윗지방으로 갔어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 보고싶어서 작년엔 그냥 눈오는 날 맞춰서 윗지방으로 갔어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026755.1.29",
          "form": "오 멋있는데요? 안해봤는데 좋다",
          "original_form": "오 멋있는데요? 안해봤는데 좋다",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026755.1.30",
          "form": "호캉스 잡아놓고 눈오는 거 보고싶어졌어요",
          "original_form": "호캉스 잡아놓고 눈오는 거 보고싶어졌어요",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026755.1.31",
          "form": "오우...그건진짜 눈이온다는거 확정내리고 가야",
          "original_form": "오우...그건진짜 눈이온다는거 확정내리고 가야",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.32",
          "form": "사실 호캉스고뭐고 그냥 겁나 너무 많이와서",
          "original_form": "사실 호캉스고뭐고 그냥 겁나 너무 많이와서",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.33",
          "form": "허리까지오는데가고싶어요",
          "original_form": "허리까지오는데가고싶어요",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.35",
          "form": "철원 이런데 가야할까요ㅋㅋㅋㅋㅋ",
          "original_form": "철원 이런데 가야할까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.36",
          "form": "아님 스키장ㅋㅋㅋㅋㅋ",
          "original_form": "아님 스키장ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026755.1.37",
          "form": "태백..? 영월..?",
          "original_form": "태백..? 영월..?",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026755.1.38",
          "form": "뭐든 강원도면 좋을거같아요",
          "original_form": "뭐든 강원도면 좋을거같아요",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026755.1.39",
          "form": "그러고보니 전 스키장도 안가봐서",
          "original_form": "그러고보니 전 스키장도 안가봐서",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026755.1.40",
          "form": "쌓인 눈을 본적이 없어요 거의ㅋㅋ",
          "original_form": "쌓인 눈을 본적이 없어요 거의ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026755.1.41",
          "form": "저도 스키장은 안가봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 스키장은 안가봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026755.1.42",
          "form": "그러니 눈이 좋죠 ㅠㅠㅠ",
