      "utterance": [
        {
          "id": "MDRW2100021147.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100021147.1.2",
          "form": "음료뭐조아하세요?",
          "original_form": "음료뭐조아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021147.1.3",
          "form": "저는 두유좋아해여 ㅋㅋㅋ",
          "original_form": "저는 두유좋아해여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100021147.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021147.1.5",
          "form": "저도 검은콩 우유 맛나요",
          "original_form": "저도 검은콩 우유 맛나요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021147.1.6",
          "form": "오~ 마져요 두유는 검은콩이 더 고소하니 맛나더라구요!",
          "original_form": "오~ 마져요 두유는 검은콩이 더 고소하니 맛나더라구요!",
          "speaker_id": "1",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100021147.1.7",
          "form": "저는 탄산수좋아해요",
          "original_form": "저는 탄산수좋아해요",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100021147.1.8",
          "form": "카페가서도 탄산수시키는경우많아요",
          "original_form": "카페가서도 탄산수시키는경우많아요",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100021147.1.9",
          "form": "카페가면뭐시키세요?",
          "original_form": "카페가면뭐시키세요?",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100021147.1.10",
          "form": "아정말요?? 저는 주로 티나 에이드 자주시켜먹어요 !",
          "original_form": "아정말요?? 저는 주로 티나 에이드 자주시켜먹어요 !",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100021147.1.11",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100021147.1.12",
          "form": "커피못드시나요?",
          "original_form": "커피못드시나요?",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100021147.1.13",
          "form": "커피 못먹는건아닌데 맛없어서 안먹어요 ㅜ 아직 커피의참맛을 몰라요 ㅋㅋ",
          "original_form": "커피 못먹는건아닌데 맛없어서 안먹어요 ㅜ 아직 커피의참맛을 몰라요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100021147.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100021147.1.15",
          "form": "저도 카페 알바하면서 알게됐어요",
          "original_form": "저도 카페 알바하면서 알게됐어요",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100021147.1.16",
          "form": "한번마시면 못끊으니깐 마시지마세요ㅜ",
          "original_form": "한번마시면 못끊으니깐 마시지마세요ㅜ",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100021147.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100021147.1.18",
          "form": "아핰ㅋㅋㅋㅋ 다들그렇게말하시네욮ㅋㅋㅋㅋ 앞으로 입에도안댈작정이에요~ㅎㅎ",
          "original_form": "아핰ㅋㅋㅋㅋ 다들그렇게말하시네욮ㅋㅋㅋㅋ 앞으로 입에도안댈작정이에요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100021147.1.19",
          "form": "좋은생각입니다",
          "original_form": "좋은생각입니다",
          "speaker_id": "2",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100021147.1.20",
          "form": "티좋아하는거있나요?",
          "original_form": "티좋아하는거있나요?",
          "speaker_id": "2",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100021147.1.21",
          "form": "저는 얼그레이티랑 녹차빼곤 거의다좋아해여~",
          "original_form": "저는 얼그레이티랑 녹차빼곤 거의다좋아해여~",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100021147.1.22",
          "form": "요새는 페퍼민트티 즐겨먹어요 ㅎㅎ",
          "original_form": "요새는 페퍼민트티 즐겨먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100021147.1.23",
          "form": "오 페퍼민트맛있죠",
          "original_form": "오 페퍼민트맛있죠",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100021147.1.24",
          "form": "저도 얼그레이맛없어요ㅜ",
          "original_form": "저도 얼그레이맛없어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:48"
