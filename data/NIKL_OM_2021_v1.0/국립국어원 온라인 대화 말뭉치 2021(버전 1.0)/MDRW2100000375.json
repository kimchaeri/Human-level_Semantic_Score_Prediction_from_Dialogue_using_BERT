      "utterance": [
        {
          "id": "MDRW2100000375.1.1",
          "form": "남친이나 여친있어 name2?",
          "original_form": "남친이나 여친있어 &name2&?",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000375.1.2",
          "form": "아니. 지금은 싱글이야",
          "original_form": "아니. 지금은 싱글이야",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000375.1.3",
          "form": "연애 너무 지겨워",
          "original_form": "연애 너무 지겨워",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000375.1.4",
          "form": "name1는?",
          "original_form": "&name1&는?",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000375.1.5",
          "form": "연애가 지겨워? 나는 외로움을 많이타서,, 항상누가있어야 편하더라",
          "original_form": "연애가 지겨워? 나는 외로움을 많이타서,, 항상누가있어야 편하더라",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000375.1.6",
          "form": "지난 연애가 너무 힘들어서 지겨운 느낌이 있나봐!",
          "original_form": "지난 연애가 너무 힘들어서 지겨운 느낌이 있나봐!",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000375.1.7",
          "form": "맞아. 연애하면 그런 부분은 좋지!",
          "original_form": "맞아. 연애하면 그런 부분은 좋지!",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000375.1.8",
          "form": "제일 오래 연애했던게 얼마나돼?",
          "original_form": "제일 오래 연애했던게 얼마나돼?",
          "speaker_id": "1",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000375.1.9",
          "form": "지난 연애를 8년 했어!",
          "original_form": "지난 연애를 8년 했어!",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000375.1.10",
          "form": "name1는?",
          "original_form": "&name1&는?",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000375.1.11",
          "form": "와 진짜오래했다",
          "original_form": "와 진짜오래했다",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000375.1.12",
          "form": "나는 전 연애는 3년 10개월정도? 지금은 내일모레가 300일이야 ㅎㅎ",
          "original_form": "나는 전 연애는 3년 10개월정도? 지금은 내일모레가 300일이야 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000375.1.13",
          "form": "300일!",
          "original_form": "300일!",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000375.1.14",
          "form": "한창 행복하겠다",
          "original_form": "한창 행복하겠다",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000375.1.15",
          "form": "결혼 생각 있어?",
          "original_form": "결혼 생각 있어?",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000375.1.16",
          "form": "나는 결혼 빨리 하고싶은데 남자친구가 내년 1월에 캐나다로 유학가 ㅎㅎㅎㅎ",
          "original_form": "나는 결혼 빨리 하고싶은데 남자친구가 내년 1월에 캐나다로 유학가 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000375.1.17",
          "form": "절망이야 ㅠ",
          "original_form": "절망이야 ㅠ",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000375.1.18",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000375.1.19",
          "form": "롱디구나..",
          "original_form": "롱디구나..",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000375.1.20",
          "form": "내가 그래서 헤어졌어",
          "original_form": "내가 그래서 헤어졌어",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000375.1.21",
          "form": "지역이 어디였는데? 해외롱디였어?",
          "original_form": "지역이 어디였는데? 해외롱디였어?",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000375.1.22",
          "form": "놀랍게도 캐나다!",
          "original_form": "놀랍게도 캐나다!",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000375.1.23",
          "form": "진짜로? 신기하다",
          "original_form": "진짜로? 신기하다",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000375.1.24",
          "form": "내남자친구랑 같은학교인거아냐?ㅋㅋㅋ",
          "original_form": "내남자친구랑 같은학교인거아냐?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000375.1.25",
          "form": "아닐 거야ㅋㅋㅋ그 사람은 캐나다 사람이었거든",
          "original_form": "아닐 거야ㅋㅋㅋ그 사람은 캐나다 사람이었거든",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000375.1.26",
          "form": "결혼하고 싶었는데 둘 중 하나는 가족+친구들하고 떨어져야 하니까",
          "original_form": "결혼하고 싶었는데 둘 중 하나는 가족+친구들하고 떨어져야 하니까",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000375.1.27",
          "form": "앗! 외국인이였구나",
          "original_form": "앗! 외국인이였구나",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000375.1.28",
          "form": "아이고 진짜 고민많았겠다 ㅠㅠ",
          "original_form": "아이고 진짜 고민많았겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000375.1.29",
          "form": "그래서 연애만 하자고 그랬는데",
          "original_form": "그래서 연애만 하자고 그랬는데",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000375.1.30",
          "form": "그러다 보니 나이가 들고",
          "original_form": "그러다 보니 나이가 들고",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000375.1.31",
          "form": "선택을 해야 하는 순간이 오더라고",
          "original_form": "선택을 해야 하는 순간이 오더라고",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000375.1.32",
          "form": "그러면 어쩔수가없었네 아깝다",
          "original_form": "그러면 어쩔수가없었네 아깝다",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000375.1.33",
          "form": "인연이라면 다시만나지않을까? 그리고 더 좋은사람 많을거야",
          "original_form": "인연이라면 다시만나지않을까? 그리고 더 좋은사람 많을거야",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000375.1.34",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000375.1.35",
          "form": "롱디는 어떻게 준비하고 있어?",
          "original_form": "롱디는 어떻게 준비하고 있어?",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.36",
          "form": "연락이 진짜 중요한 것 같아!",
          "original_form": "연락이 진짜 중요한 것 같아!",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.37",
          "form": "지금도 내가대전이고 남친이 서울이라 롱디긴한데",
          "original_form": "지금도 내가대전이고 남친이 서울이라 롱디긴한데",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.38",
          "form": "연락을 진짜잘해줘! 그래서 다행인거같아",
          "original_form": "연락을 진짜잘해줘! 그래서 다행인거같아",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.39",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.40",
          "form": "믿지않겠지만 하루에 3시간넘게 통화하는거같아 기본 4시간?",
          "original_form": "믿지않겠지만 하루에 3시간넘게 통화하는거같아 기본 4시간?",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000375.1.41",
          "form": "연락만 잘 돼도 문제가 반은 줄지!",
          "original_form": "연락만 잘 돼도 문제가 반은 줄지!",
