      "utterance": [
        {
          "id": "MDRW2100007088.1.1",
          "form": "닭똥집은좋아하시나요",
          "original_form": "닭똥집은좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007088.1.2",
          "form": "넘 좋죠 ㅋㅋㅋ",
          "original_form": "넘 좋죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007088.1.3",
          "form": "닭똥집도 안느끼하게",
          "original_form": "닭똥집도 안느끼하게",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007088.1.4",
          "form": "양념인가요ㅠㅠ",
          "original_form": "양념인가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.5",
          "form": "닭똥집마저...",
          "original_form": "닭똥집마저...",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.6",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.7",
          "form": "혹시막창두양념인가요",
          "original_form": "혹시막창두양념인가요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.8",
          "form": "반죽에다가",
          "original_form": "반죽에다가",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.9",
          "form": "튀기면 ㅋㅋ 안느끼하고 맛나죠",
          "original_form": "튀기면 ㅋㅋ 안느끼하고 맛나죠",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.10",
          "form": "저 침고였어요",
          "original_form": "저 침고였어요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007088.1.11",
          "form": "헉ㅋㅋ직접 튀기기도하시나요 멋져요",
          "original_form": "헉ㅋㅋ직접 튀기기도하시나요 멋져요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.12",
          "form": "청,양.고.추",
          "original_form": "청,양.고.추",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.13",
          "form": "를 넣어서 ㅋㅋㅋㅋㅋ",
          "original_form": "를 넣어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.14",
          "form": "아...고.추가",
          "original_form": "아...고.추가",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.15",
          "form": "튀긴집이 ㅋㅋ 진짜 마싰는데",
          "original_form": "튀긴집이 ㅋㅋ 진짜 마싰는데",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.16",
          "form": "매콤한걸말하는건데...ㅋㅋㅋ",
          "original_form": "매콤한걸말하는건데...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.17",
          "form": "안느끼하고",
          "original_form": "안느끼하고",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.18",
          "form": "직접 튀기진 않아요 ㅋㅋㅋㅋㅋ",
          "original_form": "직접 튀기진 않아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007088.1.19",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007088.1.20",
          "form": "막창는 불막창 !!!",
          "original_form": "막창는 불막창 !!!",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007088.1.21",
          "form": "와",
