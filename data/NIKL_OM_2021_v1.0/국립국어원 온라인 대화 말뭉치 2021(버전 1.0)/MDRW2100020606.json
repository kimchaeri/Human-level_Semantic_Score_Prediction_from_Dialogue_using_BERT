      "utterance": [
        {
          "id": "MDRW2100020606.1.1",
          "form": "다이어트 해본 적 잇어요?",
          "original_form": "다이어트 해본 적 잇어요?",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020606.1.2",
          "form": "지금도 다이어트해요 ㅎㅎ",
          "original_form": "지금도 다이어트해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020606.1.3",
          "form": "저 방금 백신맞고  왔어오",
          "original_form": "저 방금 백신맞고  왔어오",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020606.1.4",
          "form": "1차 백신이요?",
          "original_form": "1차 백신이요?",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020606.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020606.1.6",
          "form": "1차요",
          "original_form": "1차요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020606.1.7",
          "form": "화이자? 모더나? 뭐 맞으셨어요?",
          "original_form": "화이자? 모더나? 뭐 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020606.1.8",
          "form": "화이자요",
          "original_form": "화이자요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020606.1.9",
          "form": "name1님은 맞으셨어요?",
          "original_form": "&name1&님은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020606.1.10",
          "form": "네네 저는 곧 2차 맞아요",
          "original_form": "네네 저는 곧 2차 맞아요",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020606.1.11",
          "form": "1차는 맞을만 하죠?ㅎㅎ",
          "original_form": "1차는 맞을만 하죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020606.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020606.1.13",
          "form": "맞으셨구나",
          "original_form": "맞으셨구나",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020606.1.14",
          "form": "빠르시네요",
          "original_form": "빠르시네요",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020606.1.15",
          "form": "ㅎㅎㅎ 원래 더 빨랐어야 했는데... ㅜㅜ",
          "original_form": "ㅎㅎㅎ 원래 더 빨랐어야 했는데... ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020606.1.16",
          "form": "미뤄졌네요..",
          "original_form": "미뤄졌네요..",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020606.1.17",
          "form": "강제 6주",
          "original_form": "강제 6주",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020606.1.18",
          "form": "그쵸 ㅜㅜ",
          "original_form": "그쵸 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020606.1.19",
          "form": "어이 없어요.....",
          "original_form": "어이 없어요.....",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020606.1.20",
          "form": "모더나 수급이 안되는게 왜 화이자도 미루는지..",
          "original_form": "모더나 수급이 안되는게 왜 화이자도 미루는지..",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020606.1.21",
          "form": "1차 접종률에 목숨 걸고",
          "original_form": "1차 접종률에 목숨 걸고",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020606.1.22",
          "form": "2차를 미뤄버리는",
          "original_form": "2차를 미뤄버리는",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020606.1.23",
          "form": "이상한짓...",
          "original_form": "이상한짓...",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020606.1.24",
          "form": "그니까요 ㅜㅜ 안그랬음 좋겠는데",
          "original_form": "그니까요 ㅜㅜ 안그랬음 좋겠는데",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020606.1.25",
          "form": "접종주기를 고무줄처럼 늘렸다 줄였다 뭐하는 짓인지",
          "original_form": "접종주기를 고무줄처럼 늘렸다 줄였다 뭐하는 짓인지",
          "speaker_id": "1",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020606.1.26",
          "form": "제약사에서 고지한건 화이자 3주에요",
          "original_form": "제약사에서 고지한건 화이자 3주에요",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020606.1.27",
          "form": "근데 이걸 6주로 바꿔버리니",
          "original_form": "근데 이걸 6주로 바꿔버리니",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020606.1.28",
          "form": "맞아요 그게 말이 되냐구요",
          "original_form": "맞아요 그게 말이 되냐구요",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020606.1.29",
          "form": "전국민이 마루타 됐어요 아주그냥",
          "original_form": "전국민이 마루타 됐어요 아주그냥",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020606.1.30",
          "form": "미국으로 장기출장간 친구",
          "original_form": "미국으로 장기출장간 친구",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020606.1.31",
          "form": "7월1일에 갔는데 7월말에 2차까지 다맞았다고",
          "original_form": "7월1일에 갔는데 7월말에 2차까지 다맞았다고",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020606.1.32",
          "form": "연락오던데",
          "original_form": "연락오던데",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020606.1.33",
          "form": "이러다 접종한거 효과없으면",
          "original_form": "이러다 접종한거 효과없으면",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020606.1.34",
          "form": "책임지겠죠? 배째라 하려나",
          "original_form": "책임지겠죠? 배째라 하려나",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020606.1.35",
          "form": "ㅋㅋㅋㅋ책임 안질걸요?",
          "original_form": "ㅋㅋㅋㅋ책임 안질걸요?",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020606.1.36",
          "form": "지금 12주까지 늘리면 오히려 더 효과있다고 언플하던데",
          "original_form": "지금 12주까지 늘리면 오히려 더 효과있다고 언플하던데",
          "speaker_id": "1",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020606.1.37",
          "form": "제약사든 정부든 이젠 못믿게 됐어요",
          "original_form": "제약사든 정부든 이젠 못믿게 됐어요",
          "speaker_id": "1",
          "time": "20210828 11:39"
        }
      ]
    }
