      "utterance": [
        {
          "id": "MDRW2100035703.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100035703.1.2",
          "form": "아니여!",
          "original_form": "아니여!",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035703.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035703.1.4",
          "form": "아니여! 아직이요",
          "original_form": "아니여! 아직이요",
          "speaker_id": "1",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035703.1.5",
          "form": "만약 키우신다면 어떤동물키우고 싶으세요?",
          "original_form": "만약 키우신다면 어떤동물키우고 싶으세요?",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035703.1.6",
          "form": "음 저는 강아지요!",
          "original_form": "음 저는 강아지요!",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035703.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035703.1.8",
          "form": "저는 고양이요",
          "original_form": "저는 고양이요",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035703.1.9",
          "form": "강아지 왜 키우고싶으신가요?",
          "original_form": "강아지 왜 키우고싶으신가요?",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035703.1.10",
          "form": "앗 딱히 이유를 생각해본 적은 없는데...",
          "original_form": "앗 딱히 이유를 생각해본 적은 없는데...",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100035703.1.11",
          "form": "귀여움인가요?",
          "original_form": "귀여움인가요?",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100035703.1.12",
          "form": "근데 또 고양이도 귀엽죠..",
          "original_form": "근데 또 고양이도 귀엽죠..",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100035703.1.13",
          "form": "고양이 키우고 싶은 이유가 있으세요?",
          "original_form": "고양이 키우고 싶은 이유가 있으세요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100035703.1.14",
          "form": "고양이는 같이 잘 지내다가도 혼자만의 시간을 잘지낸다고 해야할까요?",
          "original_form": "고양이는 같이 잘 지내다가도 혼자만의 시간을 잘지낸다고 해야할까요?",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100035703.1.15",
          "form": "그점이 매력적이예요",
          "original_form": "그점이 매력적이예요",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100035703.1.16",
          "form": "앗 맞네요",
          "original_form": "앗 맞네요",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100035703.1.17",
          "form": "혼자만의 시간을 즐길줄 아는 냥이",
          "original_form": "혼자만의 시간을 즐길줄 아는 냥이",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100035703.1.18",
          "form": "그게 습성이라 매일 붙어있지않는게 좋아요",
          "original_form": "그게 습성이라 매일 붙어있지않는게 좋아요",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100035703.1.19",
          "form": "강아지 있는 친구들집가면",
          "original_form": "강아지 있는 친구들집가면",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100035703.1.20",
          "form": "귀엽고 좋은데 매번 붙어있더라구요",
          "original_form": "귀엽고 좋은데 매번 붙어있더라구요",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100035703.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100035703.1.22",
          "form": "가족들 돌아올때까지 현관 지킨다는 이야기들을 들으면 마음이 아프더라고요",
          "original_form": "가족들 돌아올때까지 현관 지킨다는 이야기들을 들으면 마음이 아프더라고요",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100035703.1.23",
          "form": "그죠...매순간 곁에 있지 못사니까요... ㅠ",
          "original_form": "그죠...매순간 곁에 있지 못사니까요... ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:56"
        }
      ]
