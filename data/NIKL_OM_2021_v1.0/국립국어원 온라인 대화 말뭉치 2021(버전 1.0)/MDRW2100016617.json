      "utterance": [
        {
          "id": "MDRW2100016617.1.1",
          "form": "놀러가고싶다",
          "original_form": "놀러가고싶다",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016617.1.2",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016617.1.3",
          "form": "제주도랑 북유럽?",
          "original_form": "제주도랑 북유럽?",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.4",
          "form": "난 싱가폴가보고싶다 홍콩이랑",
          "original_form": "난 싱가폴가보고싶다 홍콩이랑",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.5",
          "form": "근데 홍콩은 글렀어",
          "original_form": "근데 홍콩은 글렀어",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.6",
          "form": "코로나만 없었어도ㅠㅠ",
          "original_form": "코로나만 없었어도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.7",
          "form": "홍콩은 좀...ㅇㅇ",
          "original_form": "홍콩은 좀...ㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.8",
          "form": "이제 우리가 아는 홍콩느낌아닐듯",
          "original_form": "이제 우리가 아는 홍콩느낌아닐듯",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016617.1.9",
          "form": "그럴것같아 홍콩 아니면 미국?",
          "original_form": "그럴것같아 홍콩 아니면 미국?",
          "speaker_id": "2",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016617.1.10",
          "form": "중국이 차지하기 전에 가볼걸 넘 아쉽다",
          "original_form": "중국이 차지하기 전에 가볼걸 넘 아쉽다",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016617.1.11",
          "form": "미국도 땅 넓어서 볼데 먆을듯",
          "original_form": "미국도 땅 넓어서 볼데 먆을듯",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016617.1.12",
          "form": "근데 홍콩은 쇼핑하러 가는 데라 여행으로도 좋을지는 모르겠다",
          "original_form": "근데 홍콩은 쇼핑하러 가는 데라 여행으로도 좋을지는 모르겠다",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016617.1.13",
          "form": "미국은 그 사막 가보고싶다",
          "original_form": "미국은 그 사막 가보고싶다",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016617.1.14",
          "form": "홍콩영화가 원래 핫했잖아",
          "original_form": "홍콩영화가 원래 핫했잖아",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016617.1.15",
          "form": "영국도 가보고싶어",
          "original_form": "영국도 가보고싶어",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016617.1.16",
          "form": "볼거 많았을듯",
          "original_form": "볼거 많았을듯",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016617.1.17",
          "form": "영국은 인종차별심해서 무서움ㅠㅜ",
          "original_form": "영국은 인종차별심해서 무서움ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.18",
          "form": "해리포터 놀이공원같은데 있잖아",
          "original_form": "해리포터 놀이공원같은데 있잖아",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.19",
          "form": "앙ㅋㅋㄱㅋ",
          "original_form": "앙ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.20",
          "form": "그건그래..",
          "original_form": "그건그래..",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.21",
          "form": "인종차별 무서워",
          "original_form": "인종차별 무서워",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.22",
          "form": "런던아이도 유명함",
          "original_form": "런던아이도 유명함",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016617.1.23",
          "form": "코로나 끝나도 한동안 사려야 할듯",
          "original_form": "코로나 끝나도 한동안 사려야 할듯",
