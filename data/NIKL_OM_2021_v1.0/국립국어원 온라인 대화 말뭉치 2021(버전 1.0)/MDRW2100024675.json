        {
          "id": "MDRW2100024675.1.1",
          "form": "요즘 퍼즐겜에 빠졌어요",
          "original_form": "요즘 퍼즐겜에 빠졌어요",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024675.1.2",
          "form": "저도 겜 한적 없다가 요즘 무료해서 해요",
          "original_form": "저도 겜 한적 없다가 요즘 무료해서 해요",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024675.1.3",
          "form": "게임은 진짜 누구나 중독되는 맛이 있어요ㅜㅜ",
          "original_form": "게임은 진짜 누구나 중독되는 맛이 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024675.1.4",
          "form": "전 레벨업이 안되면 그만 두어요",
          "original_form": "전 레벨업이 안되면 그만 두어요",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024675.1.5",
          "form": "무료게임 하신다구요?",
          "original_form": "무료게임 하신다구요?",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024675.1.6",
          "form": "넹 모바일게임만",
          "original_form": "넹 모바일게임만",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024675.1.7",
          "form": "인생이 심심해서요",
          "original_form": "인생이 심심해서요",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024675.1.8",
          "form": "레벨업은 끈기가 필요해요",
          "original_form": "레벨업은 끈기가 필요해요",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100024675.1.9",
          "form": "넹 전 그 끈기가 없어 갈아타요",
          "original_form": "넹 전 그 끈기가 없어 갈아타요",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024675.1.10",
          "form": "게임은 끈기가\u0000 필수입니다",
          "original_form": "게임은 끈기가\u0000 필수입니다",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024675.1.11",
          "form": "어떤 게임 좋아하세요",
          "original_form": "어떤 게임 좋아하세요",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024675.1.12",
          "form": "그럼 게임은 가볍게만 하세요",
          "original_form": "그럼 게임은 가볍게만 하세요",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024675.1.13",
          "form": "퍼즐겜만 하세요?",
          "original_form": "퍼즐겜만 하세요?",
