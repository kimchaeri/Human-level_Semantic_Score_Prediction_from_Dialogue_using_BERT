      "utterance": [
        {
          "id": "MDRW2100044057.1.1",
          "form": "최재성 나왔네",
          "original_form": "최재성 나왔네",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044057.1.2",
          "form": "아 그래요?  몰랐네",
          "original_form": "아 그래요?  몰랐네",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044057.1.3",
          "form": "강석우는 없다",
          "original_form": "강석우는 없다",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044057.1.4",
          "form": "최재성 젊을때 잘생겼던데",
          "original_form": "최재성 젊을때 잘생겼던데",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044057.1.5",
          "form": "우묵배미의 사랑 이건 유명",
          "original_form": "우묵배미의 사랑 이건 유명",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044057.1.6",
          "form": "아 들어봤어요",
          "original_form": "아 들어봤어요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044057.1.7",
          "form": "사랑과 전쟁 많나 여기서 장난이 아니엿지",
          "original_form": "사랑과 전쟁 많나 여기서 장난이 아니엿지",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044057.1.8",
          "form": "사랑과 전쟁에 남주 누구였어요?",
          "original_form": "사랑과 전쟁에 남주 누구였어요?",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.9",
          "form": "박중훈이 상대역",
          "original_form": "박중훈이 상대역",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.10",
          "form": "오 본듯 해요ㅋ",
          "original_form": "오 본듯 해요ㅋ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.11",
          "form": "최재성이랑 박상원 아니엿나",
          "original_form": "최재성이랑 박상원 아니엿나",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.12",
          "form": "아 모르겠어요",
          "original_form": "아 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.13",
          "form": "밀월이라는 영화도 찍고",
          "original_form": "밀월이라는 영화도 찍고",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044057.1.14",
          "form": "밀월은 김윤진 아니예요?",
          "original_form": "밀월은 김윤진 아니예요?",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044057.1.15",
          "form": "옛날 영화는 제목이 왜 이래",
          "original_form": "옛날 영화는 제목이 왜 이래",
          "speaker_id": "2",
