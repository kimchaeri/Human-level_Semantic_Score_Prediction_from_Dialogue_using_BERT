          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.6",
          "form": "영화에 나오는 중세풍 성",
          "original_form": "영화에 나오는 중세풍 성",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.7",
          "form": "옛날 성?",
          "original_form": "옛날 성?",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.8",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.10",
          "form": "그런거 의외로 얼마안한재",
          "original_form": "그런거 의외로 얼마안한재",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.11",
          "form": "한대",
          "original_form": "한대",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.12",
          "form": "한 십몇억이면 살수있대",
          "original_form": "한 십몇억이면 살수있대",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.13",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.14",
          "form": "서울집값보다 싼거 아니가 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "서울집값보다 싼거 아니가 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.15",
          "form": "우리나라 집값에 익숙해져서 그런가",
          "original_form": "우리나라 집값에 익숙해져서 그런가",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032496.1.16",
          "form": "당장 백만원도없는데",
          "original_form": "당장 백만원도없는데",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.17",
          "form": "걍 거기가서 살아야겠누",
          "original_form": "걍 거기가서 살아야겠누",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.18",
          "form": "ㄱ  ㅐㅐ싸보임",
          "original_form": "ㄱ  ㅐㅐ싸보임",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.22",
          "form": "원래 그렇지 뭐",
          "original_form": "원래 그렇지 뭐",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.23",
          "form": "근데 계단 많고 그래서",
          "original_form": "근데 계단 많고 그래서",
          "speaker_id": "2",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.24",
          "form": "높은곳에있어서",
          "original_form": "높은곳에있어서",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100032496.1.25",
          "form": "ㅇㅇ교통도 안좋고",
          "original_form": "ㅇㅇ교통도 안좋고",
