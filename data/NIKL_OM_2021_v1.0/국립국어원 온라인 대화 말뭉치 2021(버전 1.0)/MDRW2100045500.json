        {
          "id": "MDRW2100045500.1.2",
          "form": "네 학생이에요~",
          "original_form": "네 학생이에요~",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045500.1.3",
          "form": "대학생이신거죠?",
          "original_form": "대학생이신거죠?",
          "speaker_id": "1",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045500.1.4",
          "form": "네 ㅎㅎ 이번학기는 휴학했어요",
          "original_form": "네 ㅎㅎ 이번학기는 휴학했어요",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045500.1.5",
          "form": "자유영혼!",
          "original_form": "자유영혼!",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.6",
          "form": "코로나때매 학교 노잼이죠!",
          "original_form": "코로나때매 학교 노잼이죠!",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.7",
          "form": "네ㅠㅠ 아무래도 다 비대면으로 진행하다보니까",
          "original_form": "네ㅠㅠ 아무래도 다 비대면으로 진행하다보니까",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.8",
          "form": "그리고 과제가 예전보다 많아서 넘 힘들어요~~ ㅎㅎ",
          "original_form": "그리고 과제가 예전보다 많아서 넘 힘들어요~~ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.9",
          "form": "등록금도 냈는데",
          "original_form": "등록금도 냈는데",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.10",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.11",
          "form": "몇학년이세요?",
          "original_form": "몇학년이세요?",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045500.1.12",
          "form": "저 4학년인데 휴학했어요 졸업 준비가 덜 되어서 ㅎㅎ",
          "original_form": "저 4학년인데 휴학했어요 졸업 준비가 덜 되어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045500.1.13",
          "form": "아 저도 한학기남기고 휴학했었어요",
          "original_form": "아 저도 한학기남기고 휴학했었어요",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045500.1.14",
