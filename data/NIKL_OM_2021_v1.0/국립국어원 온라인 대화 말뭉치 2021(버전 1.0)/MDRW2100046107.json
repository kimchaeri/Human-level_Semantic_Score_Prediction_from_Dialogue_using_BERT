      "utterance": [
        {
          "id": "MDRW2100046107.1.1",
          "form": "걍 어피치처럼만 하지ㅜㅜ",
          "original_form": "걍 어피치처럼만 하지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.2",
          "form": "라이언부터 디자이너가 달라서",
          "original_form": "라이언부터 디자이너가 달라서",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.3",
          "form": "어피치 그거 그린분 퇴사한걸루알고있는데",
          "original_form": "어피치 그거 그린분 퇴사한걸루알고있는데",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.4",
          "form": "헐ㅜㅜㅜㅜㅜㅜ",
          "original_form": "헐ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.5",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.6",
          "form": "회사저작권이라",
          "original_form": "회사저작권이라",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.7",
          "form": "돈도별로못받았다고...",
          "original_form": "돈도별로못받았다고...",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.8",
          "form": "헐......",
          "original_form": "헐......",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046107.1.9",
          "form": "제가 다 속상해요",
          "original_form": "제가 다 속상해요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.10",
          "form": "개인 저작권이었으면",
          "original_form": "개인 저작권이었으면",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.11",
          "form": "그니까요 ㅠㅜ여자분이셨는데",
          "original_form": "그니까요 ㅠㅜ여자분이셨는데",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.12",
          "form": "차도 사고 집도 사고",
          "original_form": "차도 사고 집도 사고",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.13",
          "form": "ㅜㅜㅜㅠㅠㅠㅜㅠㅜㅠㅠ",
          "original_form": "ㅜㅜㅜㅠㅠㅠㅜㅠㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.14",
          "form": "진짜 때돈벌었조",
          "original_form": "진짜 때돈벌었조",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.15",
          "form": "진짜 일확천금인데ㅣㅠㅠㅠ",
          "original_form": "진짜 일확천금인데ㅣㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.16",
          "form": "저처럼",
          "original_form": "저처럼",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.17",
          "form": "춘식인 근데 진짜 어디서 튀어나온 걸까요",
          "original_form": "춘식인 근데 진짜 어디서 튀어나온 걸까요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046107.1.18",
          "form": "춘식이를 싫어하는 분을봐서",
          "original_form": "춘식이를 싫어하는 분을봐서",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
