      "utterance": [
        {
          "id": "MDRW2100018862.1.1",
          "form": "점심은 뭐드시나요ㅎ",
          "original_form": "점심은 뭐드시나요ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018862.1.2",
          "form": "복숭아랑 집에있는 반찬으로",
          "original_form": "복숭아랑 집에있는 반찬으로",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018862.1.3",
          "form": "밥먹었어요",
          "original_form": "밥먹었어요",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018862.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018862.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.6",
          "form": "ㅎㅎ 샌드위치요",
          "original_form": "ㅎㅎ 샌드위치요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.7",
          "form": "다른분과 헷갈렸어요 ㅋㅋ",
          "original_form": "다른분과 헷갈렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.8",
          "form": "샌드위치 ㅋㅋ",
          "original_form": "샌드위치 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.9",
          "form": "무슨 에그 샌드위치던데",
          "original_form": "무슨 에그 샌드위치던데",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.10",
          "form": "설거지 없는",
          "original_form": "설거지 없는",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.11",
          "form": "뭐라부르던데 까먹었네요",
          "original_form": "뭐라부르던데 까먹었네요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.12",
          "form": "앙 전 설거지감 만들었네요 ㅋㅋ",
          "original_form": "앙 전 설거지감 만들었네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.13",
          "form": "올해 복숭아 맛나더라고요",
          "original_form": "올해 복숭아 맛나더라고요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.15",
          "form": "요거트랑 먹는게 대세던데",
          "original_form": "요거트랑 먹는게 대세던데",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.16",
          "form": "저 엄청 먹었어요 ㅎㅎ",
          "original_form": "저 엄청 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.17",
          "form": "요거트 만들기도 한대요",
          "original_form": "요거트 만들기도 한대요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018862.1.18",
          "form": "샌드위치 배가 덜차지 않으세요?",
          "original_form": "샌드위치 배가 덜차지 않으세요?",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.19",
          "form": "약간의 죽도 나왔어요ㅋㅋ",
          "original_form": "약간의 죽도 나왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.20",
          "form": "어머 상콤 맜있겠어요 ㅎㅎ",
          "original_form": "어머 상콤 맜있겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.21",
          "form": "ㅋㅋㅋㅣㅣ",
          "original_form": "ㅋㅋㅋㅣㅣ",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.22",
          "form": "엥 ㅋㅋ",
          "original_form": "엥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.23",
          "form": "그건 좀",
          "original_form": "그건 좀",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018862.1.24",
