      "utterance": [
        {
          "id": "MDRW2100013745.1.1",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013745.1.2",
          "form": "넷플릭스 웨이브 티빙도요 ㅎ",
          "original_form": "넷플릭스 웨이브 티빙도요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013745.1.3",
          "form": "오 많이 보시네요 저도 딱 그렇게 봐요",
          "original_form": "오 많이 보시네요 저도 딱 그렇게 봐요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013745.1.4",
          "form": "넷플릭스는 콘텐츠가 많은데 너무 많아서 그런지 뭘 봐야할 지 모르겠어요",
          "original_form": "넷플릭스는 콘텐츠가 많은데 너무 많아서 그런지 뭘 봐야할 지 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013745.1.5",
          "form": "요즘 전 티빙을 많이 이용하는 것 같아요",
          "original_form": "요즘 전 티빙을 많이 이용하는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013745.1.6",
          "form": "맞아요 넷플 넘 많죠",
          "original_form": "맞아요 넷플 넘 많죠",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013745.1.7",
          "form": "넷플 오리지널 중에 재밌게 보신거 있으신가요?",
          "original_form": "넷플 오리지널 중에 재밌게 보신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013745.1.8",
          "form": "전 킹덤이오",
          "original_form": "전 킹덤이오",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013745.1.9",
          "form": "아 킹덤 재밌죠",
          "original_form": "아 킹덤 재밌죠",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013745.1.10",
          "form": "이번에 아신전도 보셨나요?",
          "original_form": "이번에 아신전도 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013745.1.11",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100013745.1.12",
          "form": "근데 아신은 좀 불편했어요",
          "original_form": "근데 아신은 좀 불편했어요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100013745.1.13",
          "form": "어떤 점에서요?",
          "original_form": "어떤 점에서요?",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100013745.1.14",
          "form": "조선인이 너무 악질이에요ㅠㅠ",
          "original_form": "조선인이 너무 악질이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100013745.1.15",
          "form": "역사적 사실인지는 모르겠으나...",
          "original_form": "역사적 사실인지는 모르겠으나...",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100013745.1.16",
          "form": "아 맞아요 진짜 나쁘게 그려지긴했어요",
          "original_form": "아 맞아요 진짜 나쁘게 그려지긴했어요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100013745.1.17",
          "form": "마지막에 그런 반전이 있을 줄은 몰랐어요\r\n아버지도 알고보니 살아계시고 마을사람들은 다 좀비 만들어놓고.. 얼른 시즌3 나왔으면 좋겠어요",
          "original_form": "마지막에 그런 반전이 있을 줄은 몰랐어요\r\n아버지도 알고보니 살아계시고 마을사람들은 다 좀비 만들어놓고.. 얼른 시즌3 나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100013745.1.18",
          "form": "맞아요 진짜 시즌3 ㅜㅜ",
          "original_form": "맞아요 진짜 시즌3 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100013745.1.19",
          "form": "아마 나오려면 한참 기다려야겠죠?ㅠㅠ",
          "original_form": "아마 나오려면 한참 기다려야겠죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100013745.1.20",
          "form": "그르게요 ㅎㅎ",
          "original_form": "그르게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100013745.1.21",
          "form": "다른거 봐야하나",
          "original_form": "다른거 봐야하나",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100013745.1.22",
          "form": "아무래도 당분간은 그래야 할 것 같아요",
          "original_form": "아무래도 당분간은 그래야 할 것 같아요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013745.1.23",
          "form": "곧 넷플릭스 오리지널드라마 오징어게임한대요",
          "original_form": "곧 넷플릭스 오리지널드라마 오징어게임한대요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013745.1.24",
          "form": "뭔가 좀 잔인한 것같다하네요",
          "original_form": "뭔가 좀 잔인한 것같다하네요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013745.1.25",
          "form": "아 그거 무슨 무도",
          "original_form": "아 그거 무슨 무도",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100013745.1.26",
          "form": "게임?인데",
          "original_form": "게임?인데",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100013745.1.27",
          "form": "잔인한 버젼이라고",
          "original_form": "잔인한 버젼이라고",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100013745.1.28",
          "form": "무도에서 한 게임이에요? 그건 몰랐어요 오징어게임이 있다는 것도 처음알았어요 ㅎㅎ",
          "original_form": "무도에서 한 게임이에요? 그건 몰랐어요 오징어게임이 있다는 것도 처음알았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:42"
        }
      ]
