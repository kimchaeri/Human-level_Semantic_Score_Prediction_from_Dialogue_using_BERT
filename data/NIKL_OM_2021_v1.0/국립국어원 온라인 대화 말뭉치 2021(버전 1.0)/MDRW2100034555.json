      "utterance": [
        {
          "id": "MDRW2100034555.1.1",
          "form": "기억에 남는 기념품 있으세요?ㅋㅋㅋ",
          "original_form": "기억에 남는 기념품 있으세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034555.1.2",
          "form": "저는 해외여행 가면 마그넷 사오는데",
          "original_form": "저는 해외여행 가면 마그넷 사오는데",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034555.1.3",
          "form": "그것들 보면 여행 갔다온 기억이 새록새록 나더라고요",
          "original_form": "그것들 보면 여행 갔다온 기억이 새록새록 나더라고요",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034555.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034555.1.5",
          "form": "오 마그넷! 전 보통 필기구 사오거든요",
          "original_form": "오 마그넷! 전 보통 필기구 사오거든요",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034555.1.6",
          "form": "연필이나 볼펜 ㅋㅋㅋ",
          "original_form": "연필이나 볼펜 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034555.1.7",
          "form": "쓰면서 새록새록 되돌아보고 그래요",
          "original_form": "쓰면서 새록새록 되돌아보고 그래요",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034555.1.8",
          "form": "오 필기구도 좋네요",
          "original_form": "오 필기구도 좋네요",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100034555.1.9",
          "form": "저는 그 지역 간식도 잘 사와요ㅎㅎ",
          "original_form": "저는 그 지역 간식도 잘 사와요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100034555.1.10",
          "form": "금방 먹고 없어지긴 하지만",
          "original_form": "금방 먹고 없어지긴 하지만",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100034555.1.11",
          "form": "마자요 간식이 젤 간단하고 젤 손이 많이가죠ㅋㅋㅋㅋ",
          "original_form": "마자요 간식이 젤 간단하고 젤 손이 많이가죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100034555.1.12",
          "form": "네네 주변에 선물하기도 좋고",
          "original_form": "네네 주변에 선물하기도 좋고",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034555.1.13",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034555.1.14",
          "form": "네네 좋아해요",
          "original_form": "네네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034555.1.15",
          "form": "주로 어디 지역으로 많이 여행하셨어요?",
          "original_form": "주로 어디 지역으로 많이 여행하셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100034555.1.16",
          "form": "선호하는 여행지?",
          "original_form": "선호하는 여행지?",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100034555.1.17",
          "form": "저는 주로 국내 여행 다녔는데",
          "original_form": "저는 주로 국내 여행 다녔는데",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100034555.1.18",
          "form": "이제 해외를 좀 다녀봐야겠다 하던 중에 코로나가 ㅠㅠㅠㅋㅋㅋ",
          "original_form": "이제 해외를 좀 다녀봐야겠다 하던 중에 코로나가 ㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100034555.1.19",
          "form": "아 저는 이제 국내여행 좀 다니려고요",
          "original_form": "아 저는 이제 국내여행 좀 다니려고요",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034555.1.20",
          "form": "국내도 좋은 곳 참 많아요",
          "original_form": "국내도 좋은 곳 참 많아요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100034555.1.21",
          "form": "국내여행 가이드북 샀어요",
          "original_form": "국내여행 가이드북 샀어요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100034555.1.22",
          "form": "오 괜찮은 가이드북 있나요?",
          "original_form": "오 괜찮은 가이드북 있나요?",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100034555.1.23",
          "form": "이번에 에이든 국내여행 가이드북 샀는데",
          "original_form": "이번에 에이든 국내여행 가이드북 샀는데",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100034555.1.24",
          "form": "계속 지도 만들던 회사라 나름 괜찮은 것 같아요",
          "original_form": "계속 지도 만들던 회사라 나름 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100034555.1.25",
          "form": "그렇군요 찾아볼게요",
          "original_form": "그렇군요 찾아볼게요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034555.1.26",
          "form": "이제 날 선선해서 가까운 국내여행지는 다닐만 할 듯해요",
          "original_form": "이제 날 선선해서 가까운 국내여행지는 다닐만 할 듯해요",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034555.1.27",
          "form": "마자요ㅠ가을되면 좀 한산한 곳으로 쏘다녀야겠어요",
          "original_form": "마자요ㅠ가을되면 좀 한산한 곳으로 쏘다녀야겠어요",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034555.1.28",
          "form": "네 여행 안다닌지 넘 오래됐어요ㅠ",
          "original_form": "네 여행 안다닌지 넘 오래됐어요ㅠ",
