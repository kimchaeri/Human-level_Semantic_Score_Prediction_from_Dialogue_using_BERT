      "utterance": [
        {
          "id": "MDRW2100021704.1.1",
          "form": "최근에 무슨 책을 읽으셨나요",
          "original_form": "최근에 무슨 책을 읽으셨나요",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021704.1.2",
          "form": "장류진 작가님의 달까지가자",
          "original_form": "장류진 작가님의 달까지가자",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021704.1.3",
          "form": "읽었어요",
          "original_form": "읽었어요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021704.1.4",
          "form": "아  장르가 뭔가요",
          "original_form": "아  장르가 뭔가요",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021704.1.5",
          "form": "소설이에요",
          "original_form": "소설이에요",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021704.1.6",
          "form": "세태소설이라해야하나",
          "original_form": "세태소설이라해야하나",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021704.1.7",
          "form": "아 검색하니 재미있을 것 같아요",
          "original_form": "아 검색하니 재미있을 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021704.1.8",
          "form": "국내 소설을 많이 안 읽었는데 도전해봐야겠어요",
          "original_form": "국내 소설을 많이 안 읽었는데 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021704.1.9",
          "form": "name1님은 최근에 무슨 책 읽으셨나요?",
          "original_form": "&name1&님은 최근에 무슨 책 읽으셨나요?",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021704.1.10",
          "form": "당신이 어머니의 집밥을 먹을 수 있는 횟수는 00번 남았습니다를 읽었어요",
          "original_form": "당신이 어머니의 집밥을 먹을 수 있는 횟수는 00번 남았습니다를 읽었어요",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021704.1.11",
          "form": "제목의 횟수가 기억이 안 나네요ㅜ",
          "original_form": "제목의 횟수가 기억이 안 나네요ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021704.1.12",
          "form": "오 무슨 내용인가요?",
          "original_form": "오 무슨 내용인가요?",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021704.1.13",
          "form": "어 이게 스포가 될수 있는 내용이라ㅜ",
          "original_form": "어 이게 스포가 될수 있는 내용이라ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021704.1.14",
          "form": "중요한 반전과 관계있나요?",
          "original_form": "중요한 반전과 관계있나요?",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021704.1.15",
          "form": "저는 원래 스포 좋아해서 웬만하면 괜찮아요",
          "original_form": "저는 원래 스포 좋아해서 웬만하면 괜찮아요",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021704.1.16",
          "form": "제목과 관련한 반전이 있어요",
          "original_form": "제목과 관련한 반전이 있어요",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021704.1.17",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021704.1.18",
          "form": "그 제목의 의미가 처음 봤을 때와 조금 달라요",
          "original_form": "그 제목의 의미가 처음 봤을 때와 조금 달라요",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021704.1.19",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021704.1.20",
          "form": "소설이죠?",
          "original_form": "소설이죠?",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021704.1.21",
          "form": "네네 단편소설들입니다",
          "original_form": "네네 단편소설들입니다",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021704.1.22",
          "form": "읽고 시간이 아깝지 않았어요",
