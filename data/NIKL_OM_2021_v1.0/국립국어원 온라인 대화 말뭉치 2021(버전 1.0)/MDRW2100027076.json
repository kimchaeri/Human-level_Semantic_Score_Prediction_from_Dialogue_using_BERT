        {
          "id": "MDRW2100027076.1.1",
          "form": "아 name1",
          "original_form": "아 &name1&",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027076.1.2",
          "form": "진 단장 파란 뽑기로 뽑은거지?",
          "original_form": "진 단장 파란 뽑기로 뽑은거지?",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027076.1.3",
          "form": "응ㅇ 그 처음 뽑으면 좀 싸게 주는거 그걸로 함 돌렸었거든",
          "original_form": "응ㅇ 그 처음 뽑으면 좀 싸게 주는거 그걸로 함 돌렸었거든",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027076.1.4",
          "form": "ㅇㅎ그거 뽑기 기록 보면",
          "original_form": "ㅇㅎ그거 뽑기 기록 보면",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027076.1.5",
          "form": "몇회 뽑았는지 볼수있거든?",
          "original_form": "몇회 뽑았는지 볼수있거든?",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027076.1.6",
          "form": "90연이 천장이야",
          "original_form": "90연이 천장이야",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027076.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027076.1.8",
          "form": "파란가챠랑 빨간가챠",
          "original_form": "파란가챠랑 빨간가챠",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027076.1.9",
          "form": "따로 세는거야",
          "original_form": "따로 세는거야",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027076.1.10",
          "form": "앗 그래? 음..일단 빨간가챠 돌리는 중인데",
          "original_form": "앗 그래? 음..일단 빨간가챠 돌리는 중인데",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027076.1.11",
          "form": "파란가챠를 돌려야 했나",
          "original_form": "파란가챠를 돌려야 했나",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027076.1.12",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027076.1.13",
          "form": "빨간가챠돌리는게 맞아",
          "original_form": "빨간가챠돌리는게 맞아",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027076.1.14",
          "form": "아 그렇군 조아조아",
          "original_form": "아 그렇군 조아조아",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027076.1.15",
          "form": "파란건 처음이라 좀 있길래 돌려본거였거든",
          "original_form": "파란건 처음이라 좀 있길래 돌려본거였거든",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027076.1.16",
          "form": "뿌듯",
          "original_form": "뿌듯",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027076.1.17",
          "form": "그거 10연째에만 4성 뜨는거니까",
          "original_form": "그거 10연째에만 4성 뜨는거니까",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027076.1.18",
          "form": "3성만 나온다고 너무 고민하지 말고",
          "original_form": "3성만 나온다고 너무 고민하지 말고",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027076.1.19",
          "form": "무기만 나와서",
          "original_form": "무기만 나와서",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027076.1.20",
          "form": "얘는 무기만 좋아하나봐 하고있어",
          "original_form": "얘는 무기만 좋아하나봐 하고있어",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027076.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.22",
          "form": "3성 캐릭터는",
          "original_form": "3성 캐릭터는",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.23",
          "form": "4성은 기대도 안하지만 먼가 사람을 보고싶어지는",
          "original_form": "4성은 기대도 안하지만 먼가 사람을 보고싶어지는",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.24",
          "form": "업기때문에",
          "original_form": "업기때문에",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.25",
          "form": "무기만 나오는게 맞아...",
          "original_form": "무기만 나오는게 맞아...",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.26",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.27",
          "form": "글쿤....",
          "original_form": "글쿤....",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.28",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.29",
          "form": "왜 다 4성뿐(급기야맨",
          "original_form": "왜 다 4성뿐(급기야맨",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.30",
          "form": "엠버랑 리사 성능이 3성급이긴 해",
          "original_form": "엠버랑 리사 성능이 3성급이긴 해",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027076.1.31",
          "form": "그치만 그친구들도 4성임",
          "original_form": "그치만 그친구들도 4성임",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027076.1.32",
          "form": "4성이 3성급인 막나가는",
          "original_form": "4성이 3성급인 막나가는",
