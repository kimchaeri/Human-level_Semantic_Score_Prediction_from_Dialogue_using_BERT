      "utterance": [
        {
          "id": "MDRW2100025227.1.1",
          "form": "요새 자주하는 게임있음??",
          "original_form": "요새 자주하는 게임있음??",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025227.1.2",
          "form": "나는 프메씨리즈",
          "original_form": "나는 프메씨리즈",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025227.1.3",
          "form": "스팀게임 다운받아서하는데",
          "original_form": "스팀게임 다운받아서하는데",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025227.1.4",
          "form": "위쳐 3 재밌드라",
          "original_form": "위쳐 3 재밌드라",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025227.1.5",
          "form": "스팀게임 나 하우스플리퍼하는데",
          "original_form": "스팀게임 나 하우스플리퍼하는데",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025227.1.6",
          "form": "그거는 하다가 멀미나더라고",
          "original_form": "그거는 하다가 멀미나더라고",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025227.1.7",
          "form": "ㅋㅋㅋㅋ그거 집청소하는 게임?",
          "original_form": "ㅋㅋㅋㅋ그거 집청소하는 게임?",
          "speaker_id": "2",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.8",
          "form": "ㅇㅇㅇ 집개보수해서 내다파는게임",
          "original_form": "ㅇㅇㅇ 집개보수해서 내다파는게임",
          "speaker_id": "1",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.9",
          "form": "맞아 어떤건 귀밑에 붙이고 해야됨",
          "original_form": "맞아 어떤건 귀밑에 붙이고 해야됨",
          "speaker_id": "2",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.10",
          "form": "그거 vr안경쓰고하면",
          "original_form": "그거 vr안경쓰고하면",
          "speaker_id": "1",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.11",
          "form": "우우욱",
          "original_form": "우우욱",
          "speaker_id": "2",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.12",
          "form": "아마 20분도못하고 토할듯",
          "original_form": "아마 20분도못하고 토할듯",
          "speaker_id": "1",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.13",
          "form": "너무 힘드러 멀미하면",
          "original_form": "너무 힘드러 멀미하면",
          "speaker_id": "2",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100025227.1.14",
          "form": "그래서 하다가 요새는 잠시 쉬는데",
          "original_form": "그래서 하다가 요새는 잠시 쉬는데",
          "speaker_id": "1",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100025227.1.15",
          "form": "하나하나 다 일일히 닦고 쓸고해야해서",
          "original_form": "하나하나 다 일일히 닦고 쓸고해야해서",
          "speaker_id": "1",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100025227.1.16",
          "form": "위쳐도 약간 시점이 막 바껴서 속이 좀 미식거림",
          "original_form": "위쳐도 약간 시점이 막 바껴서 속이 좀 미식거림",
          "speaker_id": "2",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100025227.1.17",
          "form": "플레이가 단조로움",
          "original_form": "플레이가 단조로움",
          "speaker_id": "1",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100025227.1.18",
          "form": "오 근데스토린어떤데",
          "original_form": "오 근데스토린어떤데",
          "speaker_id": "1",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100025227.1.19",
          "form": "어떤 여자 찾아다니는 남자 이야긴데 나름 재밌음",
          "original_form": "어떤 여자 찾아다니는 남자 이야긴데 나름 재밌음",
          "speaker_id": "2",
          "time": "20210901 01:25"
        },
        {
          "id": "MDRW2100025227.1.20",
          "form": "아 그 드라마로도만들어진거?",
          "original_form": "아 그 드라마로도만들어진거?",
          "speaker_id": "1",
          "time": "20210901 01:25"
        },
