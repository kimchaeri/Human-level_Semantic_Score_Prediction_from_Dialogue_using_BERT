      "utterance": [
        {
          "id": "MDRW2100012099.1.1",
          "form": "재택근무 하시나요?",
          "original_form": "재택근무 하시나요?",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012099.1.2",
          "form": "네 어쩌다 가끔 해요",
          "original_form": "네 어쩌다 가끔 해요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100012099.1.3",
          "form": "현장직 아니라 꼭 출근안해도 되는데 잘 안시켜주네요ㅜㅜ\r\n재택만 하면 좋을텐데",
          "original_form": "현장직 아니라 꼭 출근안해도 되는데 잘 안시켜주네요ㅜㅜ\r\n재택만 하면 좋을텐데",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100012099.1.4",
          "form": "얼마주기로 하시나요",
          "original_form": "얼마주기로 하시나요",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100012099.1.5",
          "form": "딱 정해진 주기가 없어요ㅜㅠ",
          "original_form": "딱 정해진 주기가 없어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.6",
          "form": "확진자수 증폭되거나",
          "original_form": "확진자수 증폭되거나",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.7",
          "form": "근처에 확진자 있을때는",
          "original_form": "근처에 확진자 있을때는",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.8",
          "form": "해도됩니다 이런 공지가",
          "original_form": "해도됩니다 이런 공지가",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.9",
          "form": "내려오고",
          "original_form": "내려오고",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.10",
          "form": "아님 그냥 출근.",
          "original_form": "아님 그냥 출근.",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100012099.1.11",
          "form": "출근이 디폴트에요",
          "original_form": "출근이 디폴트에요",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100012099.1.12",
          "form": "자주 해주면 좋을텐데요ㅠ",
          "original_form": "자주 해주면 좋을텐데요ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100012099.1.13",
          "form": "맞아요 관리자들은 눈 앞에 있길 원하는듯",
          "original_form": "맞아요 관리자들은 눈 앞에 있길 원하는듯",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100012099.1.14",
          "form": "장점과 단점 어떤게 있을까요 재택",
          "original_form": "장점과 단점 어떤게 있을까요 재택",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100012099.1.15",
          "form": "그러게요 뭐가 있을까요 신뢰의 문제만 해결된다면 오히려 더 생산적이고 효율적인거같아요",
          "original_form": "그러게요 뭐가 있을까요 신뢰의 문제만 해결된다면 오히려 더 생산적이고 효율적인거같아요",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100012099.1.16",
          "form": "제도가 잘 만들어졌음좋겠네요",
          "original_form": "제도가 잘 만들어졌음좋겠네요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100012099.1.17",
          "form": "진짜 그랬음 좋겠어요",
          "original_form": "진짜 그랬음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100012099.1.18",
          "form": "가가구님은 재택하세요?",
          "original_form": "가가구님은 재택하세요?",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100012099.1.19",
          "form": "넵 지금은 재택입니다",
          "original_form": "넵 지금은 재택입니다",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100012099.1.20",
          "form": "좋은 회사에 다니시네요",
          "original_form": "좋은 회사에 다니시네요",
          "speaker_id": "1",
          "time": "20210826 20:38"
        }
      ]
