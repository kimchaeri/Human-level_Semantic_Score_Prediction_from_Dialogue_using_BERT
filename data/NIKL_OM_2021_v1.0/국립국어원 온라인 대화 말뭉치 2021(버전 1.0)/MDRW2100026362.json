      "utterance": [
        {
          "id": "MDRW2100026362.1.1",
          "form": "으앗",
          "original_form": "으앗",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026362.1.2",
          "form": "저도 골목식당 돈가스집",
          "original_form": "저도 골목식당 돈가스집",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026362.1.3",
          "form": "네 그것도 팔던데여ㅋ 식사권",
          "original_form": "네 그것도 팔던데여ㅋ 식사권",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026362.1.4",
          "form": "줄서고싶었는데",
          "original_form": "줄서고싶었는데",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026362.1.5",
          "form": "이미 어플로 바뀌어서",
          "original_form": "이미 어플로 바뀌어서",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026362.1.6",
          "form": "아진짜요?ㅜㅜㅋ",
          "original_form": "아진짜요?ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.7",
          "form": "망했어요",
          "original_form": "망했어요",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.8",
          "form": "이틀도전했는데",
          "original_form": "이틀도전했는데",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.9",
          "form": "독하다 사람들ㅋㅋ",
          "original_form": "독하다 사람들ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.10",
          "form": "차라이 줄서는게 나을판...이었어요 ㅠㅠㅠ",
          "original_form": "차라이 줄서는게 나을판...이었어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.11",
          "form": "기회를 못잡아요 ㅠㅠㅠㅠㅠㅜ",
          "original_form": "기회를 못잡아요 ㅠㅠㅠㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.12",
          "form": "제주도 자주안가는데 ㅠㅜㅜ",
          "original_form": "제주도 자주안가는데 ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.13",
          "form": "꼭가고싶었는데 ㅠㅠㅜㅜ",
          "original_form": "꼭가고싶었는데 ㅠㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.14",
          "form": "그렇게 맛나대여?ㅜ",
          "original_form": "그렇게 맛나대여?ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.15",
          "form": "모방 식당 많을듯한데",
          "original_form": "모방 식당 많을듯한데",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026362.1.16",
          "form": "어플 당첨되면 비행기티켓 끊으세요ㅜㅟㅣ",
          "original_form": "어플 당첨되면 비행기티켓 끊으세요ㅜㅟㅣ",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.17",
          "form": "연돈 식사일이 제주도 여행날!",
          "original_form": "연돈 식사일이 제주도 여행날!",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.18",
          "form": "아..그게",
          "original_form": "아..그게",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.19",
          "form": "제주도에서만",
          "original_form": "제주도에서만",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.20",
          "form": "위치가 제주도이여야만",
          "original_form": "위치가 제주도이여야만",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.21",
          "form": "티켓팅이가능해요",
          "original_form": "티켓팅이가능해요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026362.1.22",
          "form": "그러니..뭐 거의못먹는다 봐여죠",
          "original_form": "그러니..뭐 거의못먹는다 봐여죠",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026362.1.23",
          "form": "와.",
          "original_form": "와.",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026362.1.24",
          "form": "머리썼네여ㅋㅋ",
          "original_form": "머리썼네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026362.1.25",
          "form": "아이디만들어서 도전해드리고 싶네요ㅋ",
          "original_form": "아이디만들어서 도전해드리고 싶네요ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026362.1.26",
          "form": "제주도 여행객 타겟 맛집이 많아져서",
          "original_form": "제주도 여행객 타겟 맛집이 많아져서",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.27",
          "form": "놀라워요",
          "original_form": "놀라워요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.28",
          "form": "한번해보세용!",
          "original_form": "한번해보세용!",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.29",
          "form": "당첨되면 진짜 운 좋으신거 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "당첨되면 진짜 운 좋으신거 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.30",
          "form": "저는 아마 제주도가서",
          "original_form": "저는 아마 제주도가서",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.31",
          "form": "맨날해도",
          "original_form": "맨날해도",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.32",
          "form": "제주돌빵? 진짜 돌덩이같아서 돌밭에 떨어지면 못찾겠던데여ㅋ",
          "original_form": "제주돌빵? 진짜 돌덩이같아서 돌밭에 떨어지면 못찾겠던데여ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.33",
          "form": "당첨안될거같아요",
          "original_form": "당첨안될거같아요",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026362.1.34",
          "form": "아 그거 인스타에서봤는데",
          "original_form": "아 그거 인스타에서봤는데",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026362.1.35",
          "form": "도전의지 활활",
          "original_form": "도전의지 활활",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026362.1.36",
          "form": "아니 안먹고싶게 안생겼어요??",
          "original_form": "아니 안먹고싶게 안생겼어요??",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026362.1.37",
          "form": "그 돌빵",
          "original_form": "그 돌빵",
          "speaker_id": "2",
          "time": "20210901 21:53"
        }
      ]
    }
  ]
