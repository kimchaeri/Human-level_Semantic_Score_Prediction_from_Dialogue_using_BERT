      "utterance": [
        {
          "id": "MDRW2100039708.1.1",
          "form": "색다른데 맛있ㄴㅔ용",
          "original_form": "색다른데 맛있ㄴㅔ용",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039708.1.2",
          "form": "저도 다음에 먹어봐야겠어요",
          "original_form": "저도 다음에 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039708.1.3",
          "form": "치킨브랜드 중에 땅땅 제일 좋아하세요?",
          "original_form": "치킨브랜드 중에 땅땅 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039708.1.4",
          "form": "저는 허니콤보나 맛쵸킹 좋아해요",
          "original_form": "저는 허니콤보나 맛쵸킹 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039708.1.5",
          "form": "님은어때요?",
          "original_form": "님은어때요?",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039708.1.6",
          "form": "저는 교촌 좋아해요",
          "original_form": "저는 교촌 좋아해요",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039708.1.7",
          "form": "교촌 감튀도 좋구요ㅎ",
          "original_form": "교촌 감튀도 좋구요ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039708.1.8",
          "form": "달콤짭잘 맛나더라구요 ㅎㅎ",
          "original_form": "달콤짭잘 맛나더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.9",
          "form": "감자박스는 쫌 사기치던데",
          "original_form": "감자박스는 쫌 사기치던데",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.10",
          "form": "그쵸 아 ㅜㅜㅜ",
          "original_form": "그쵸 아 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.11",
          "form": "밑에가 불룩한게",
          "original_form": "밑에가 불룩한게",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.12",
          "form": "양이 좀 작죠",
          "original_form": "양이 좀 작죠",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.13",
          "form": "너무해요",
          "original_form": "너무해요",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.14",
          "form": "그게 아쉬워요",
          "original_form": "그게 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039708.1.15",
          "form": "레드도 맛난데",
          "original_form": "레드도 맛난데",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039708.1.16",
          "form": "그래도 같이오는 칠리소스도 너무 맛있어요",
          "original_form": "그래도 같이오는 칠리소스도 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039708.1.17",
          "form": "왜 허니랑 반반 안해주죠  순살말고",
          "original_form": "왜 허니랑 반반 안해주죠  순살말고",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039708.1.18",
          "form": "허니 반반 없는건 처음 알았네요",
          "original_form": "허니 반반 없는건 처음 알았네요",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039708.1.19",
          "form": "허니는 별로 안좋아해가지고 ㅎㅎ",
          "original_form": "허니는 별로 안좋아해가지고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039708.1.20",
          "form": "허니 좋아하시나봐요",
          "original_form": "허니 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039708.1.21",
          "form": "달달해서 맛나더라규요 금방 물리긴해요",
          "original_form": "달달해서 맛나더라규요 금방 물리긴해요",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039708.1.22",
          "form": "맞아요 그래서 저도 한마리 다못먹어요ㅜ",
          "original_form": "맞아요 그래서 저도 한마리 다못먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039708.1.23",
          "form": "다른음식 좋아하시는건 있으세요?",
          "original_form": "다른음식 좋아하시는건 있으세요?",
          "speaker_id": "1",
          "time": "20210903 20:55"
        }
      ]
