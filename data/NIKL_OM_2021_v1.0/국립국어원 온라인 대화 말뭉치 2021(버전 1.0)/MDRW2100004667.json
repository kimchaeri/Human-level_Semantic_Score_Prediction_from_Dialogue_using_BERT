      "utterance": [
        {
          "id": "MDRW2100004667.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004667.1.2",
          "form": "여행 좋아해요!",
          "original_form": "여행 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004667.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004667.1.4",
          "form": "어디로 여행 가보셨어요?",
          "original_form": "어디로 여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004667.1.5",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004667.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004667.1.7",
          "form": "호주는 어땠어요??",
          "original_form": "호주는 어땠어요??",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004667.1.8",
          "form": "가본적 없는 곳이라서 궁금하네요",
          "original_form": "가본적 없는 곳이라서 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004667.1.9",
          "form": "청정지역이에요",
          "original_form": "청정지역이에요",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004667.1.10",
          "form": "호주에 동물 많잖아요",
          "original_form": "호주에 동물 많잖아요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004667.1.11",
          "form": "캥거루나 쿼카, 코알라등등",
          "original_form": "캥거루나 쿼카, 코알라등등",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004667.1.12",
          "form": "호주여행때 보셨어요?",
          "original_form": "호주여행때 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004667.1.13",
          "form": "캥거루 코알라 실제로요",
          "original_form": "캥거루 코알라 실제로요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004667.1.14",
          "form": "캥거루 싸움 장난 아님",
          "original_form": "캥거루 싸움 장난 아님",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004667.1.15",
          "form": "오우..",
          "original_form": "오우..",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004667.1.16",
          "form": "영상으로만 봤는데..",
          "original_form": "영상으로만 봤는데..",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004667.1.17",
          "form": "캥거루 싸움이 진짜였네요ㄷㄷ",
          "original_form": "캥거루 싸움이 진짜였네요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004667.1.18",
          "form": "네 무섭더라구요",
          "original_form": "네 무섭더라구요",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004667.1.19",
          "form": "호주에서 보신 것 중에",
          "original_form": "호주에서 보신 것 중에",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004667.1.20",
          "form": "좋았던거 있으세요?",
          "original_form": "좋았던거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004667.1.21",
          "form": "코알라요",
          "original_form": "코알라요",
          "speaker_id": "1",
          "time": "20210821 18:20"
        }
      ]
