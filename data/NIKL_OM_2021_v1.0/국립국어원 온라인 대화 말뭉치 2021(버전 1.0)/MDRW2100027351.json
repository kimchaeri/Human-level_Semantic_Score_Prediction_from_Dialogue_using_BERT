      "utterance": [
        {
          "id": "MDRW2100027351.1.1",
          "form": "라면좋아허새요?",
          "original_form": "라면좋아허새요?",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027351.1.2",
          "form": "부산진족발 짱맛나요",
          "original_form": "부산진족발 짱맛나요",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027351.1.3",
          "form": "고기양이많고",
          "original_form": "고기양이많고",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027351.1.4",
          "form": "고기가쫄깃하고 뼈가 많이없어요",
          "original_form": "고기가쫄깃하고 뼈가 많이없어요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.5",
          "form": "족발엔 막국수가 짱인데요",
          "original_form": "족발엔 막국수가 짱인데요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.6",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.7",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.8",
          "form": "같이 뇸뇸먹어줘야해요",
          "original_form": "같이 뇸뇸먹어줘야해요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.9",
          "form": "막국수넘좋아요",
          "original_form": "막국수넘좋아요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027351.1.10",
          "form": "며칠전에",
          "original_form": "며칠전에",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.11",
          "form": "족발에 국수싸서 먹기",
          "original_form": "족발에 국수싸서 먹기",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.12",
          "form": "후루룩",
          "original_form": "후루룩",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.13",
          "form": "완당집가서",
          "original_form": "완당집가서",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.14",
          "form": "발국수먹었어요",
          "original_form": "발국수먹었어요",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.15",
          "form": "완당부드러운데ㅋㅋ",
          "original_form": "완당부드러운데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.16",
          "form": "유부초밥이랑",
          "original_form": "유부초밥이랑",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.18",
          "form": "완당먹고싶었는데",
          "original_form": "완당먹고싶었는데",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.19",
          "form": "더워서",
          "original_form": "더워서",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.20",
          "form": "서대맛집 그 발국수집?",
          "original_form": "서대맛집 그 발국수집?",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.21",
          "form": "발국수로 먹었어요",
          "original_form": "발국수로 먹었어요",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.22",
          "form": "거긴 발국수가 짱이죠",
          "original_form": "거긴 발국수가 짱이죠",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.23",
          "form": "네 부민동에있어요",
          "original_form": "네 부민동에있어요",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.24",
          "form": "아니다 부민동이단ㆍㄴㆍ",
          "original_form": "아니다 부민동이단ㆍㄴㆍ",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027351.1.26",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027351.1.27",
          "form": "유부초밥도맛있어요",
          "original_form": "유부초밥도맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027351.1.28",
          "form": "남포동보다",
          "original_form": "남포동보다",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027351.1.29",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:44"
        }
      ]
