      "utterance": [
        {
          "id": "MDRW2100038958.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100038958.1.2",
          "form": "안녕하세요?^^",
          "original_form": "안녕하세요?^^",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100038958.1.3",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100038958.1.4",
          "form": "전 요즘 훠궈가 먹고 싶어요",
          "original_form": "전 요즘 훠궈가 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100038958.1.5",
          "form": "훠궈맛있죠ㅜ",
          "original_form": "훠궈맛있죠ㅜ",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100038958.1.6",
          "form": "ㅠㅠ코로나때매 못가고 있어요,,",
          "original_form": "ㅠㅠ코로나때매 못가고 있어요,,",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100038958.1.7",
          "form": "중국음식 좋아하세요?",
          "original_form": "중국음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100038958.1.8",
          "form": "대체로 좋아하는데 본토스타일중에는 좀 가리는게 있어요",
          "original_form": "대체로 좋아하는데 본토스타일중에는 좀 가리는게 있어요",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038958.1.9",
          "form": "저도 대체로 다 좋아해요",
          "original_form": "저도 대체로 다 좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038958.1.10",
          "form": "요즘엔 마라탕 먹고싶어요",
          "original_form": "요즘엔 마라탕 먹고싶어요",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038958.1.11",
          "form": "저도 마라탕도 좋아해요 그건 배달도 되니깐",
          "original_form": "저도 마라탕도 좋아해요 그건 배달도 되니깐",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038958.1.12",
          "form": "하지만 제가 좋아하는 마라탕집이 집에서 멀어서,,",
          "original_form": "하지만 제가 좋아하는 마라탕집이 집에서 멀어서,,",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100038958.1.13",
          "form": "맞아요 근데 저는 가서 먹는게 더 좋아요",
          "original_form": "맞아요 근데 저는 가서 먹는게 더 좋아요",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100038958.1.14",
          "form": "배달은 뭔가 더 비싼 느낌인것같아요",
          "original_form": "배달은 뭔가 더 비싼 느낌인것같아요",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100038958.1.15",
          "form": "네 배달비가 최소 5천원은 붙는 느낌?",
          "original_form": "네 배달비가 최소 5천원은 붙는 느낌?",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038958.1.16",
          "form": "저렴한 마라탕집 하나를 알아두면 끼니때우기 조았어요",
          "original_form": "저렴한 마라탕집 하나를 알아두면 끼니때우기 조았어요",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038958.1.17",
          "form": "맞아요 재료도 더 비싼거같고ㅠㅜ",
          "original_form": "맞아요 재료도 더 비싼거같고ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        }
      ]
