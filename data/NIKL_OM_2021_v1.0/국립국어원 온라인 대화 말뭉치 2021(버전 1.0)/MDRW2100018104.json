      "utterance": [
        {
          "id": "MDRW2100018104.1.1",
          "form": "운동은 하시나영? ㅋㅋㅋㅋ",
          "original_form": "운동은 하시나영? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018104.1.2",
          "form": "실내자전거 매일 타요 ㅋㅋㅋ",
          "original_form": "실내자전거 매일 타요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018104.1.3",
          "form": "먹으면서 타시나요? 먹고나서 타시나요? ㅋㅋㅋㅋ",
          "original_form": "먹으면서 타시나요? 먹고나서 타시나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018104.1.4",
          "form": "먹으면서 ㅋㅋㅋㅋ 탈수도 있나요? ㅋㅋㅋㅋㅋ",
          "original_form": "먹으면서 ㅋㅋㅋㅋ 탈수도 있나요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018104.1.5",
          "form": "ㅋㅋㅋ 시간이 생명인뎅..",
          "original_form": "ㅋㅋㅋ 시간이 생명인뎅..",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018104.1.6",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018104.1.7",
          "form": "전 등산 다니는데.먹으먄서 걸으니까\n. 오히려 살쪄요",
          "original_form": "전 등산 다니는데.먹으먄서 걸으니까\n. 오히려 살쪄요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018104.1.8",
          "form": "저는 공복에 탈 때도있고 먹고나서도 있어요",
          "original_form": "저는 공복에 탈 때도있고 먹고나서도 있어요",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018104.1.9",
          "form": "등산은 기력없으면 못할거같아요 ㅠ",
          "original_form": "등산은 기력없으면 못할거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018104.1.10",
          "form": "등산 경험 없으세용?",
          "original_form": "등산 경험 없으세용?",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018104.1.11",
          "form": "네 저는 등산하면 무섭더라구여 미끄럽고 ㅋㅋㅋㅋㅋ",
          "original_form": "네 저는 등산하면 무섭더라구여 미끄럽고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018104.1.12",
          "form": "하긴 험한데는 험하죠... \n근처 뒷동산은 없어요?",
          "original_form": "하긴 험한데는 험하죠... \n근처 뒷동산은 없어요?",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018104.1.13",
          "form": "있긴한데 밖에서 하는 운동은 너무 귀찮더라구요 발 떼기가 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "있긴한데 밖에서 하는 운동은 너무 귀찮더라구요 발 떼기가 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
