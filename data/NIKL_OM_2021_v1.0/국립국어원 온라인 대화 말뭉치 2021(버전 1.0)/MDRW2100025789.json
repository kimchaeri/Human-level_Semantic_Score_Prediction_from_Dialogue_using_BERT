      "utterance": [
        {
          "id": "MDRW2100025789.1.1",
          "form": "그렇게 맛없냐?",
          "original_form": "그렇게 맛없냐?",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025789.1.2",
          "form": "저거 한통 다 어케먹지?",
          "original_form": "저거 한통 다 어케먹지?",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025789.1.3",
          "form": "한통이야?",
          "original_form": "한통이야?",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025789.1.4",
          "form": "두통 아니었나?",
          "original_form": "두통 아니었나?",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025789.1.5",
          "form": "헐 한통도 골치아픈데 두통이라고?",
          "original_form": "헐 한통도 골치아픈데 두통이라고?",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025789.1.6",
          "form": "그런거 같은데",
          "original_form": "그런거 같은데",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.7",
          "form": "한번봐봐",
          "original_form": "한번봐봐",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.8",
          "form": "와 저거 어떻게 하냐 진짜",
          "original_form": "와 저거 어떻게 하냐 진짜",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.9",
          "form": "먹어야지",
          "original_form": "먹어야지",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.10",
          "form": "무나할까?",
          "original_form": "무나할까?",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.11",
          "form": "그러던가",
          "original_form": "그러던가",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.12",
          "form": "저거 먹음 엄마도 먹을거야?",
          "original_form": "저거 먹음 엄마도 먹을거야?",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025789.1.13",
          "form": "니가 먹어야지ㅋㅋㄱ",
          "original_form": "니가 먹어야지ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025789.1.14",
          "form": "헐 그럴 줄 알았다",
          "original_form": "헐 그럴 줄 알았다",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025789.1.15",
          "form": "당장 무나하자",
          "original_form": "당장 무나하자",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025789.1.16",
          "form": "당근할까",
          "original_form": "당근할까",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025789.1.17",
          "form": "저걸 누가 돈 주고 가져가겠어",
          "original_form": "저걸 누가 돈 주고 가져가겠어",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
