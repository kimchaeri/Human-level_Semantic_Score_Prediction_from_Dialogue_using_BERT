      "utterance": [
        {
          "id": "MDRW2100030596.1.1",
          "form": "이번 주말은 해가 나네요~",
          "original_form": "이번 주말은 해가 나네요~",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100030596.1.2",
          "form": "어디 나들이 계획 있나요?",
          "original_form": "어디 나들이 계획 있나요?",
          "speaker_id": "1",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100030596.1.3",
          "form": "여친만나고 오려구여 ㅋㅋ",
          "original_form": "여친만나고 오려구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030596.1.4",
          "form": "아침에 차 엔진오일 갈고왔어요",
          "original_form": "아침에 차 엔진오일 갈고왔어요",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030596.1.5",
          "form": "오~ 좋은 주말이네요",
          "original_form": "오~ 좋은 주말이네요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030596.1.6",
          "form": "날씨도 데이트하기 정말 좋아요",
          "original_form": "날씨도 데이트하기 정말 좋아요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030596.1.7",
          "form": "지금 한참 만나고 있겠네요~",
          "original_form": "지금 한참 만나고 있겠네요~",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030596.1.8",
          "form": "네 만나고있어오",
          "original_form": "네 만나고있어오",
          "speaker_id": "2",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100030596.1.9",
          "form": "즐거운 시간 보내요~^^",
          "original_form": "즐거운 시간 보내요~^^",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030596.1.10",
          "form": "지금 잠깐 쉬는중이에요 ㅋㅋㅋ",
          "original_form": "지금 잠깐 쉬는중이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100030596.1.11",
          "form": "공원에서 산책하다가 더워서",
          "original_form": "공원에서 산책하다가 더워서",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100030596.1.12",
          "form": "아직은 더위가 남아있죠?",
          "original_form": "아직은 더위가 남아있죠?",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100030596.1.13",
          "form": "환절기 감기 걸리면 안되니까 온도 관리 잘하세요",
          "original_form": "환절기 감기 걸리면 안되니까 온도 관리 잘하세요",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100030596.1.14",
          "form": "네 좀 덥더라구여",
          "original_form": "네 좀 덥더라구여",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100030596.1.15",
          "form": "저도 보니까 아침저녁 기온과 낮기온이 달라서 걸칠옷을 가지고 다녀요",
          "original_form": "저도 보니까 아침저녁 기온과 낮기온이 달라서 걸칠옷을 가지고 다녀요",
          "speaker_id": "1",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100030596.1.16",
          "form": "괜히 이럴 때 감기 걸려버리면 눈치 보이니까요",
          "original_form": "괜히 이럴 때 감기 걸려버리면 눈치 보이니까요",
          "speaker_id": "1",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100030596.1.17",
          "form": "근데 제가 좀 더위를 많이타거든요",
          "original_form": "근데 제가 좀 더위를 많이타거든요",
          "speaker_id": "2",
          "time": "20210905 08:24"
        },
        {
          "id": "MDRW2100030596.1.18",
          "form": "좀 격하게 움직이면 아직도 덥더라구여",
          "original_form": "좀 격하게 움직이면 아직도 덥더라구여",
          "speaker_id": "2",
          "time": "20210905 08:24"
        },
        {
          "id": "MDRW2100030596.1.19",
          "form": "네 아직은 초가을이라 낮에는 여름 같죠",
          "original_form": "네 아직은 초가을이라 낮에는 여름 같죠",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100030596.1.20",
          "form": "밤에는 선선한게 너무 좋더라구요",
          "original_form": "밤에는 선선한게 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100030596.1.21",
          "form": "밤에 더워서 못자는 일이 없으니 좋아요",
          "original_form": "밤에 더워서 못자는 일이 없으니 좋아요",
          "speaker_id": "1",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100030596.1.22",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100030596.1.23",
          "form": "여름을 너무 시러해서ㅜㅜ",
          "original_form": "여름을 너무 시러해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100030596.1.24",
          "form": "저도 더운거 진짜 싫어해요",
          "original_form": "저도 더운거 진짜 싫어해요",
          "speaker_id": "1",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100030596.1.25",
          "form": "전 겨울이 좋아요",
          "original_form": "전 겨울이 좋아요",
          "speaker_id": "2",
          "time": "20210905 09:32"
        }
      ]
    }
  ]
