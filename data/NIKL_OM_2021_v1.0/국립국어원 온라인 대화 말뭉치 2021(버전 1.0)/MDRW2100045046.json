      "utterance": [
        {
          "id": "MDRW2100045046.1.1",
          "form": "오 먹어봐야겠어요",
          "original_form": "오 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045046.1.2",
          "form": "스벅 제 최애는 허니자몽블랙티예요",
          "original_form": "스벅 제 최애는 허니자몽블랙티예요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045046.1.3",
          "form": "그거 유명하잖아요 저 이번주에 먹어보려구욤",
          "original_form": "그거 유명하잖아요 저 이번주에 먹어보려구욤",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045046.1.4",
          "form": "따로 뭐 추가하셔서 드세요ㅡ??",
          "original_form": "따로 뭐 추가하셔서 드세요ㅡ??",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045046.1.5",
          "form": "아뇨 그냥 그것만요!!",
          "original_form": "아뇨 그냥 그것만요!!",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045046.1.6",
          "form": "저도 추천 받아서 먹은건데 맛있더러구요..",
          "original_form": "저도 추천 받아서 먹은건데 맛있더러구요..",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045046.1.7",
          "form": "그러니까요 추천 되게 많이 하더라구요!!",
          "original_form": "그러니까요 추천 되게 많이 하더라구요!!",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045046.1.8",
          "form": "근데 설명부터 맛있어보여요..",
          "original_form": "근데 설명부터 맛있어보여요..",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045046.1.9",
          "form": "맞아요 ㅜㅜ 블랙티 좋아하세요?",
          "original_form": "맞아요 ㅜㅜ 블랙티 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045046.1.10",
          "form": "좋아하시면 강추입니다..",
          "original_form": "좋아하시면 강추입니다..",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045046.1.11",
          "form": "헉 안마셔봤는데 뭔가 제취향일것같더라구요..",
          "original_form": "헉 안마셔봤는데 뭔가 제취향일것같더라구요..",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045046.1.12",
          "form": "거기에 자몽조합이면 끝이죠ㅜ",
          "original_form": "거기에 자몽조합이면 끝이죠ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045046.1.13",
          "form": "꼭꼭 드셔보세요!",
          "original_form": "꼭꼭 드셔보세요!",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045046.1.14",
          "form": "스타벅스 디저트도 좋아하시나요?",
          "original_form": "스타벅스 디저트도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045046.1.15",
          "form": "카스타드밖에 안먹어봤는데 다들 맛있어보이더라구요",
          "original_form": "카스타드밖에 안먹어봤는데 다들 맛있어보이더라구요",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045046.1.16",
          "form": "드셔보셨어요??",
          "original_form": "드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045046.1.17",
          "form": "네네 맛있어여!",
          "original_form": "네네 맛있어여!",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045046.1.18",
          "form": "저는 치즈케이크나 피칸파이도 좋아해요",
          "original_form": "저는 치즈케이크나 피칸파이도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045046.1.19",
          "form": "오오오 치즈케이크 종류가 다양하던데 다 맛있나요??",
          "original_form": "오오오 치즈케이크 종류가 다양하던데 다 맛있나요??",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045046.1.20",
          "form": "네네 다는 안먹어봤는데.. 디저트가 평타이상은 다 하는 것 같아요",
          "original_form": "네네 다는 안먹어봤는데.. 디저트가 평타이상은 다 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045046.1.21",
          "form": "아하!! 혹시 슈크림바움쿠헨인가 그것도 드셔보셨어요??",
          "original_form": "아하!! 혹시 슈크림바움쿠헨인가 그것도 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045046.1.22",
          "form": "그건 못먹어봤네요 근데 이름이 맛있어보여요...",
          "original_form": "그건 못먹어봤네요 근데 이름이 맛있어보여요...",
          "speaker_id": "2",
          "time": "20210908 00:53"
        }
      ]
    }
