      "utterance": [
        {
          "id": "MDRW2100000812.1.1",
          "form": "싸이월드라고 아세요?",
          "original_form": "싸이월드라고 아세요?",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000812.1.2",
          "form": "알죠!! 초중딩때 열심히 했어요 ㅋㅋㅋ",
          "original_form": "알죠!! 초중딩때 열심히 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000812.1.3",
          "form": "오 대박 저도 옛날에 많이 했었어요",
          "original_form": "오 대박 저도 옛날에 많이 했었어요",
          "speaker_id": "1",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000812.1.4",
          "form": "다시 부활한다던데...! 잘 될까요",
          "original_form": "다시 부활한다던데...! 잘 될까요",
          "speaker_id": "1",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000812.1.5",
          "form": "ㅜㅜ 그러게요 근데 맨날 부활한다고 했다가 흐지부지됐다가 그러길래",
          "original_form": "ㅜㅜ 그러게요 근데 맨날 부활한다고 했다가 흐지부지됐다가 그러길래",
          "speaker_id": "2",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000812.1.6",
          "form": "진짜 부활 하는건 맞나..?!싶구..",
          "original_form": "진짜 부활 하는건 맞나..?!싶구..",
          "speaker_id": "2",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000812.1.7",
          "form": "그때가 재밌긴했는데 ㅋㅋㅋㅋ그쵸",
          "original_form": "그때가 재밌긴했는데 ㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000812.1.8",
          "form": "맞아요 벌써 십년도 더 지났네요",
          "original_form": "맞아요 벌써 십년도 더 지났네요",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000812.1.9",
          "form": "그땐 싸이월드가 인스타그램보다 핫했는데...",
          "original_form": "그땐 싸이월드가 인스타그램보다 핫했는데...",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000812.1.10",
          "form": "마자여....진짜 모두가 싸이월드하고",
          "original_form": "마자여....진짜 모두가 싸이월드하고",
          "speaker_id": "2",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000812.1.11",
          "form": "투데이 올리려고 새로고침도했어요",
          "original_form": "투데이 올리려고 새로고침도했어요",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000812.1.12",
          "form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000812.1.13",
          "form": "사람사는 거 다 비슷한가봐요",
          "original_form": "사람사는 거 다 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000812.1.14",
          "form": "저도:그랬었는데...ㅋㅋㅋ 그땐 투데이가 인기도나 마찬가지...",
          "original_form": "저도:그랬었는데...ㅋㅋㅋ 그땐 투데이가 인기도나 마찬가지...",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000812.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요..흑........",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요..흑........",
          "speaker_id": "2",
          "time": "20210819 19:47"
        },
        {
          "id": "MDRW2100000812.1.16",
          "form": "싸이월드 부활해도 요새 워낙 다른 대체제가 많아서 ㅠㅠ",
          "original_form": "싸이월드 부활해도 요새 워낙 다른 대체제가 많아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000812.1.17",
          "form": "맞아요 저라도 싸이 안항거같아요",
          "original_form": "맞아요 저라도 싸이 안항거같아요",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000812.1.18",
          "form": "진짜 획기적인 뭔갈 들고오면 모르겠지만 ㅋㅋㅋ",
          "original_form": "진짜 획기적인 뭔갈 들고오면 모르겠지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000812.1.19",
          "form": "저두 ㅜㅜ 안할고같아요",
          "original_form": "저두 ㅜㅜ 안할고같아요",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000812.1.20",
          "form": "그리고 안에 너무 옛날추억이...",
          "original_form": "그리고 안에 너무 옛날추억이...",
          "speaker_id": "1",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000812.1.21",
