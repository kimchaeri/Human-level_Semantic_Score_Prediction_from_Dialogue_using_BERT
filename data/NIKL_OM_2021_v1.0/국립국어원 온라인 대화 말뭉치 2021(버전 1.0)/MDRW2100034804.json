      "utterance": [
        {
          "id": "MDRW2100034804.1.1",
          "form": "티켓팅 해보셨어요?",
          "original_form": "티켓팅 해보셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034804.1.2",
          "form": "효자템으로 불린다는 나훈아 티켓팅을...",
          "original_form": "효자템으로 불린다는 나훈아 티켓팅을...",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.3",
          "form": "매년 도전했답니다...",
          "original_form": "매년 도전했답니다...",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.4",
          "form": "티켓팅 잘 하시나요 ㅎㅎ",
          "original_form": "티켓팅 잘 하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.5",
          "form": "와와아",
          "original_form": "와와아",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.6",
          "form": "성공하셨나요",
          "original_form": "성공하셨나요",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.7",
          "form": "전 맨날 실패하는 편인데 ㅋㅋㅋ",
          "original_form": "전 맨날 실패하는 편인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.8",
          "form": "나훈아 콘서트 뭔가 저도 가보고 싶어지더라구여? ㅋㅋㅋㅋㅋ",
          "original_form": "나훈아 콘서트 뭔가 저도 가보고 싶어지더라구여? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034804.1.9",
          "form": "몇 년전에 한 번 성공하긴 했는데 그때 저는 못 봐서",
          "original_form": "몇 년전에 한 번 성공하긴 했는데 그때 저는 못 봐서",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034804.1.10",
          "form": "추석 방송 이후로 제것도 도전중인데 쉽지않네요 ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "추석 방송 이후로 제것도 도전중인데 쉽지않네요 ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034804.1.11",
          "form": "엄마들만 그 재밌는걸 보고 ㅋㅋㅋㅋㅋ",
          "original_form": "엄마들만 그 재밌는걸 보고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034804.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ그니까요 너무 충격받았어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그니까요 너무 충격받았어요",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034804.1.13",
          "form": "전 덕질하는 친구 용병으로 한번씩 참전하는데 늘 참패하고 돌아와요",
          "original_form": "전 덕질하는 친구 용병으로 한번씩 참전하는데 늘 참패하고 돌아와요",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034804.1.14",
          "form": "진짜 이쯤되면 티켓팅은 그냥 운인 것 같아요",
          "original_form": "진짜 이쯤되면 티켓팅은 그냥 운인 것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034804.1.15",
          "form": "다들 이제 기술은 다 있고 운이에요 운ㅋㅋㅋㅋㅋ",
          "original_form": "다들 이제 기술은 다 있고 운이에요 운ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034804.1.16",
          "form": "그니까요 다른 사람이 선택하지 않은 좌석을 선택할 운..ㅜㅜ",
          "original_form": "그니까요 다른 사람이 선택하지 않은 좌석을 선택할 운..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034804.1.17",
          "form": "올해 나훈아 콘서트 연기 된다는 말이 있던데",
          "original_form": "올해 나훈아 콘서트 연기 된다는 말이 있던데",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034804.1.18",
          "form": "취소표 나오면 냉큼 주워야 겠어요 ㅋㅋㅋ",
          "original_form": "취소표 나오면 냉큼 주워야 겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034804.1.19",
          "form": "아 마자요 저희 지역에 하려던 거 연기됐더라구요",
          "original_form": "아 마자요 저희 지역에 하려던 거 연기됐더라구요",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034804.1.20",
          "form": "할수 있으려나 모르겠어요",
          "original_form": "할수 있으려나 모르겠어요",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034804.1.21",
          "form": "그건 그래요ㅠ 막연하게 기다리기만 해야하고ㅠㅠ",
          "original_form": "그건 그래요ㅠ 막연하게 기다리기만 해야하고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100034804.1.22",
          "form": "그래서 이번 추석때 나훈아 콘서트 한번 더 해주면 좋겠네요 ㅋㅋㅋㅋ",
          "original_form": "그래서 이번 추석때 나훈아 콘서트 한번 더 해주면 좋겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034804.1.23",
          "form": "이번 추석에는 심수봉 콘서트래요 ㅋㅋㅋㅋ",
          "original_form": "이번 추석에는 심수봉 콘서트래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034804.1.24",
          "form": "kbs 재미 본거 같아요 ㅋㅋㅋㅋ",
          "original_form": "kbs 재미 본거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034804.1.25",
          "form": "앗 심수봉이었군요!",
          "original_form": "앗 심수봉이었군요!",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034804.1.26",
          "form": "그분도 명곡 진짜 많으시던데 재밌겠네요ㅋㅋㅋ",
          "original_form": "그분도 명곡 진짜 많으시던데 재밌겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034804.1.27",
          "form": "맞아요 신나는 노래 별로 없을 것 같지만, 이번에는 분위기 잡고 보려고 미러볼도 샀어요 ㅋㅋㅋㅋ",
