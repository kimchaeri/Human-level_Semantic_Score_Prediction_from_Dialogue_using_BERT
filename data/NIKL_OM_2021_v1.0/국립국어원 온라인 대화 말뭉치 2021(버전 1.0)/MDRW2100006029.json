      "utterance": [
        {
          "id": "MDRW2100006029.1.1",
          "form": "코로나 백신은 맞으셧어요?",
          "original_form": "코로나 백신은 맞으셧어요?",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006029.1.2",
          "form": "네 저는 의료인이라서",
          "original_form": "네 저는 의료인이라서",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006029.1.3",
          "form": "진작맞앗어여",
          "original_form": "진작맞앗어여",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006029.1.4",
          "form": "의료인이셧구나",
          "original_form": "의료인이셧구나",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006029.1.5",
          "form": "그럼 일찍 맞으셧겟네요",
          "original_form": "그럼 일찍 맞으셧겟네요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006029.1.6",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006029.1.7",
          "form": "맞으셧나여",
          "original_form": "맞으셧나여",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006029.1.8",
          "form": "전 아직 1차만 맞은 상태예요",
          "original_form": "전 아직 1차만 맞은 상태예요",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100006029.1.9",
          "form": "화이자맞으셧나용",
          "original_form": "화이자맞으셧나용",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100006029.1.10",
          "form": "모더나 맞앗어요",
          "original_form": "모더나 맞앗어요",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100006029.1.11",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100006029.1.12",
          "form": "저는 화이자 맞앗어용",
          "original_form": "저는 화이자 맞앗어용",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100006029.1.13",
          "form": "이제 위드코로나 인가요 흑흑",
          "original_form": "이제 위드코로나 인가요 흑흑",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100006029.1.14",
          "form": "ㅠㅠ 코로나 없던 시절로 완전히 돌아갈수 없을거라는게 너무 슬퍼요",
          "original_form": "ㅠㅠ 코로나 없던 시절로 완전히 돌아갈수 없을거라는게 너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006029.1.15",
          "form": "화이자 많이 아프셧어요?",
          "original_form": "화이자 많이 아프셧어요?",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006029.1.16",
          "form": "저는 안아팟어요ㅎㅎ",
          "original_form": "저는 안아팟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006029.1.17",
          "form": "튼튼한가바여",
          "original_form": "튼튼한가바여",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006029.1.18",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006029.1.19",
          "form": "전 2차 불안해요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "전 2차 불안해요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100006029.1.20",
          "form": "ㅜㅜ타이레놀 구비해놓고 푹쉬는수밖에ㅜㅜㅜㅜ",
          "original_form": "ㅜㅜ타이레놀 구비해놓고 푹쉬는수밖에ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100006029.1.21",
          "form": "일단 타이레놀은 잔뜩 사놧어요 ㅋㅋㅋㅋㅋ",
          "original_form": "일단 타이레놀은 잔뜩 사놧어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100006029.1.22",
          "form": "1차맞앗을때 열은 없엇는데 피부가 많이 간지러웟엇거든요",
          "original_form": "1차맞앗을때 열은 없엇는데 피부가 많이 간지러웟엇거든요",
