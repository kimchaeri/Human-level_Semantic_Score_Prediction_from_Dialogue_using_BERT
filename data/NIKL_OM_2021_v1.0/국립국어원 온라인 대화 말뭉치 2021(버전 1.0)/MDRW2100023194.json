          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023194.1.7",
          "form": "그거 한댓지",
          "original_form": "그거 한댓지",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023194.1.8",
          "form": "13일이나 14일정도?",
          "original_form": "13일이나 14일정도?",
