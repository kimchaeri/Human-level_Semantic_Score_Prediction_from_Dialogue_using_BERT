        {
          "id": "MDRW2100035318.1.1",
          "form": "집안일 하시나요,",
          "original_form": "집안일 하시나요,",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035318.1.2",
          "form": "집안일 하는게 힘들어요ㅜㅜ",
          "original_form": "집안일 하는게 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035318.1.3",
          "form": "어떤거 하세요? 분담하시나요?",
          "original_form": "어떤거 하세요? 분담하시나요?",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035318.1.4",
          "form": "자취를해요",
          "original_form": "자취를해요",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035318.1.5",
          "form": "아..그럼 오롯이 혼자 다 하셔야겠어요 ㅠㅠ",
          "original_form": "아..그럼 오롯이 혼자 다 하셔야겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:37"
        },
        {
          "id": "MDRW2100035318.1.6",
          "form": "집안일 해야되는데...",
          "original_form": "집안일 해야되는데...",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035318.1.7",
          "form": "잘안하게 되더라구요",
          "original_form": "잘안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035318.1.8",
          "form": "쉬엄쉬엄 하셔도 되겠어요 혼자니까~^^",
          "original_form": "쉬엄쉬엄 하셔도 되겠어요 혼자니까~^^",
          "speaker_id": "1",
          "time": "20210830 05:44"
        },
        {
          "id": "MDRW2100035318.1.9",
          "form": "집에 있으면 쉬고싶어서",
          "original_form": "집에 있으면 쉬고싶어서",
          "speaker_id": "2",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035318.1.10",
          "form": "아무것도 하기가 싫네요ㅜㅜ",
          "original_form": "아무것도 하기가 싫네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035318.1.11",
          "form": "그렇죠 그래도 맞춰야 할 사람이 없어서 편할 것 같아요",
          "original_form": "그렇죠 그래도 맞춰야 할 사람이 없어서 편할 것 같아요",
          "speaker_id": "1",
          "time": "20210830 10:52"
        },
        {
          "id": "MDRW2100035318.1.12",
          "form": "같이 살면 불편한것도 있겠죠",
          "original_form": "같이 살면 불편한것도 있겠죠",
          "speaker_id": "2",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035318.1.13",
          "form": "ㅜㅜ",
