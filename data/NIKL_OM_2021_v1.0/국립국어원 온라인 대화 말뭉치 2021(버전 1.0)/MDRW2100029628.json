      "utterance": [
        {
          "id": "MDRW2100029628.1.1",
          "form": "혹시 일을 하고 있으신가요?",
          "original_form": "혹시 일을 하고 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029628.1.2",
          "form": "직업은 있습니다! 사장님 몰래 월루하고 있네요ㅋㅋ",
          "original_form": "직업은 있습니다! 사장님 몰래 월루하고 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029628.1.3",
          "form": "크 월루도 재능이에요",
          "original_form": "크 월루도 재능이에요",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029628.1.4",
          "form": "부럽습니다ㅠㅜ",
          "original_form": "부럽습니다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029628.1.5",
          "form": "근래 일이 너무 없어서 너무 머쓱한ㅜㅜ",
          "original_form": "근래 일이 너무 없어서 너무 머쓱한ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029628.1.6",
          "form": "자료 찾기만 하는 척..",
          "original_form": "자료 찾기만 하는 척..",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029628.1.7",
          "form": "그럴때 민망하죠..ㅎ",
          "original_form": "그럴때 민망하죠..ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029628.1.8",
          "form": "지나가실 때만 괜히 막 폴더 열었다 닫았다..ㅋㅋ",
          "original_form": "지나가실 때만 괜히 막 폴더 열었다 닫았다..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029628.1.9",
          "form": "혹시 어떤 일하시는 지 여쭤봐도 되나요?",
          "original_form": "혹시 어떤 일하시는 지 여쭤봐도 되나요?",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029628.1.10",
          "form": "고냥 평범한 사무직입니당ㅎㅎ name1님은요?",
          "original_form": "고냥 평범한 사무직입니당ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029628.1.11",
          "form": "저는 백수입니다ㅋㅋㅋㅋ",
          "original_form": "저는 백수입니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029628.1.12",
          "form": "헉 부러워... 저도 퇴사시켜주세요ㅠㅠㅜㅋㅋ",
          "original_form": "헉 부러워... 저도 퇴사시켜주세요ㅠㅠㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029628.1.13",
          "form": "선생님, 그럼 그 자리 저 주실래요?ㅎㅎ",
          "original_form": "선생님, 그럼 그 자리 저 주실래요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029628.1.14",
          "form": "월급받고 싶어요ㅠㅠㅠ",
          "original_form": "월급받고 싶어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029628.1.15",
          "form": "고통의 굴레같아요..",
          "original_form": "고통의 굴레같아요..",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029628.1.16",
          "form": "백수 > 일하고 싶음 > 직장인 > 퇴사하고 싶음",
          "original_form": "백수 > 일하고 싶음 > 직장인 > 퇴사하고 싶음",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029628.1.17",
          "form": "역시 로또 1등만이 답인걸로",
          "original_form": "역시 로또 1등만이 답인걸로",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029628.1.18",
          "form": "ㅋㅋㅋ로또 1등을 원하는데 막상 사기는 아까운ㅎ",
          "original_form": "ㅋㅋㅋ로또 1등을 원하는데 막상 사기는 아까운ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029628.1.19",
          "form": "ㅋㅋㅋㅋ맨날 입으로만 사요",
          "original_form": "ㅋㅋㅋㅋ맨날 입으로만 사요",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029628.1.20",
          "form": "한달에 한번 오천원 자동",
          "original_form": "한달에 한번 오천원 자동",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029628.1.21",
          "form": "그래도 구입은하시네요ㅎㅎ",
          "original_form": "그래도 구입은하시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029628.1.22",
          "form": "친구가 3등 당첨된걸 봐버려서 그만..",
          "original_form": "친구가 3등 당첨된걸 봐버려서 그만..",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029628.1.23",
          "form": "3등이 얼마였죠?",
          "original_form": "3등이 얼마였죠?",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029628.1.24",
          "form": "세금 제외하면 120? 정도 나오더라구요",
          "original_form": "세금 제외하면 120? 정도 나오더라구요",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029628.1.25",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029628.1.26",
          "form": "부럽네요ㅎ",
          "original_form": "부럽네요ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029628.1.27",
          "form": "덕분에 치킨 한마리 얻어먹고 좋았는데..",
          "original_form": "덕분에 치킨 한마리 얻어먹고 좋았는데..",
          "speaker_id": "2",
          "time": "20210903 11:36"
