      "utterance": [
        {
          "id": "MDRW2100035671.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035671.1.2",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035671.1.3",
          "form": "최후의 만찬 먹었어요ㅜㅜ",
          "original_form": "최후의 만찬 먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035671.1.4",
          "form": "아니 ㅜㅜ 그게 무슨 소리예요ㅜㅜㅜ",
          "original_form": "아니 ㅜㅜ 그게 무슨 소리예요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035671.1.5",
          "form": "월요일부터 다이어트할거에요",
          "original_form": "월요일부터 다이어트할거에요",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035671.1.6",
          "form": "이번엔 진짜로ㅠ",
          "original_form": "이번엔 진짜로ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035671.1.7",
          "form": "ㅜㅇㅜ! 아직 월요일까지 한시간반 남았어요",
          "original_form": "ㅜㅇㅜ! 아직 월요일까지 한시간반 남았어요",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035671.1.8",
          "form": "얼른 더 드세욧!",
          "original_form": "얼른 더 드세욧!",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035671.1.9",
          "form": "악 ㅋㅋ 고마워요",
          "original_form": "악 ㅋㅋ 고마워요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035671.1.10",
          "form": "미련없게 꼬북칩 먹고있어요",
          "original_form": "미련없게 꼬북칩 먹고있어요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035671.1.11",
          "form": "ㅎㅎ 최후의 만찬 메뉴는 뭔가요",
          "original_form": "ㅎㅎ 최후의 만찬 메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035671.1.12",
          "form": "ㅜㅇㅜ 그 바사삭 꼬북칩마저 끊으시려고요?",
          "original_form": "ㅜㅇㅜ 그 바사삭 꼬북칩마저 끊으시려고요?",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035671.1.13",
          "form": "일단 제 최애인 피자를 먹었어요",
          "original_form": "일단 제 최애인 피자를 먹었어요",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035671.1.14",
          "form": "고르곤졸라",
          "original_form": "고르곤졸라",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035671.1.15",
          "form": "아 꿀 찍어 드셨겠죠!",
          "original_form": "아 꿀 찍어 드셨겠죠!",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035671.1.16",
          "form": "맥주는요?",
          "original_form": "맥주는요?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035671.1.17",
          "form": "맥주랑",
          "original_form": "맥주랑",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035671.1.18",
          "form": "전 탄산파라",
          "original_form": "전 탄산파라",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035671.1.19",
          "form": "미련없이 콜라마시고",
          "original_form": "미련없이 콜라마시고",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035671.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035671.1.21",
          "form": "아ㅎㅎ 최후의 만찬 자격이 충분하네요ㅎㅎ",
          "original_form": "아ㅎㅎ 최후의 만찬 자격이 충분하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035671.1.22",
          "form": "내일부터는 그럼 술도 탄산도 끊나요ㅜ",
          "original_form": "내일부터는 그럼 술도 탄산도 끊나요ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035671.1.23",
          "form": "아 그리고 아쉬우니",
          "original_form": "아 그리고 아쉬우니",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100035671.1.24",
          "form": "피자 한판더 프로슈토라고 하몽올라간거로 ㅋㅋ",
          "original_form": "피자 한판더 프로슈토라고 하몽올라간거로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100035671.1.25",
          "form": "헉ㅇ하몽 내사랑ㅜㅜ",
          "original_form": "헉ㅇ하몽 내사랑ㅜㅜ",
          "speaker_id": "1",
