        {
          "id": "MDRW2100044099.1.1",
          "form": "최명길 드라마로 다시 돌아가서",
          "original_form": "최명길 드라마로 다시 돌아가서",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044099.1.2",
          "form": "ㅋㅋㅋ 어디 나왔어요?",
          "original_form": "ㅋㅋㅋ 어디 나왔어요?",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044099.1.3",
          "form": "94년 사랑은 없다",
          "original_form": "94년 사랑은 없다",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044099.1.4",
          "form": "또 처음 들어봄",
          "original_form": "또 처음 들어봄",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044099.1.5",
          "form": "sbs 33부작",
          "original_form": "sbs 33부작",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044099.1.6",
          "form": "절친한 두영자가 한남자를",
          "original_form": "절친한 두영자가 한남자를",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044099.1.7",
          "form": "짧아졋지만 기네요",
          "original_form": "짧아졋지만 기네요",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044099.1.8",
          "form": "또다른 여주는 누구예요?",
          "original_form": "또다른 여주는 누구예요?",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044099.1.9",
          "form": "90년대부터 짧아지기 시작한듯",
          "original_form": "90년대부터 짧아지기 시작한듯",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044099.1.10",
          "form": "너무 길면 배우도 힘들듯",
          "original_form": "너무 길면 배우도 힘들듯",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044099.1.11",
          "form": "황신혜",
          "original_form": "황신혜",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044099.1.12",
          "form": "이때부터 황신혜 나오기 시작하나요",
          "original_form": "이때부터 황신혜 나오기 시작하나요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044099.1.13",
          "form": "남주가 이효정 ㅋㅋ",
          "original_form": "남주가 이효정 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044099.1.14",
          "form": "이효정이 남자예요?",
          "original_form": "이효정이 남자예요?",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044099.1.15",
          "form": "엉 얼굴보면 알껄",
          "original_form": "엉 얼굴보면 알껄",
