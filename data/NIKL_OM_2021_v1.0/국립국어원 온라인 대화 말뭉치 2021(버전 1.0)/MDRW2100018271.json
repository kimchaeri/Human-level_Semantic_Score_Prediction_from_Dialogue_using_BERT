      "utterance": [
        {
          "id": "MDRW2100018271.1.1",
          "form": "울 강아지들 자는가",
          "original_form": "울 강아지들 자는가",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.2",
          "form": "그래 잔다",
          "original_form": "그래 잔다",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.3",
          "form": "양치 시켜줘야하는데 벌써 자는구만",
          "original_form": "양치 시켜줘야하는데 벌써 자는구만",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.4",
          "form": "이따 깨면 시켜줘라",
          "original_form": "이따 깨면 시켜줘라",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.5",
          "form": "알았당",
          "original_form": "알았당",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.6",
          "form": "양치 잘 시켜줘라",
          "original_form": "양치 잘 시켜줘라",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.7",
          "form": "엄마도 좀 시켜줘랏!",
          "original_form": "엄마도 좀 시켜줘랏!",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.8",
          "form": "니가 담당이잖아",
          "original_form": "니가 담당이잖아",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018271.1.9",
          "form": "그런게 어딨엉 다같이 관리해줘야지",
          "original_form": "그런게 어딨엉 다같이 관리해줘야지",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018271.1.10",
          "form": "난 산책담당",
          "original_form": "난 산책담당",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018271.1.11",
          "form": "흠 할흠 그렇긴 하다만 ㅋ",
          "original_form": "흠 할흠 그렇긴 하다만 ㅋ",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018271.1.12",
          "form": "ㅋㅋ 양치 몇분 안걸리잖아",
          "original_form": "ㅋㅋ 양치 몇분 안걸리잖아",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018271.1.13",
          "form": "그렇지 ㅠㅠ 귀찮지만 잘해줘야짓",
          "original_form": "그렇지 ㅠㅠ 귀찮지만 잘해줘야짓",
