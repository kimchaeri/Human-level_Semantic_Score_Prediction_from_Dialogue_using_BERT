      "utterance": [
        {
          "id": "MDRW2100005214.1.1",
          "form": "온라인커뮤니티 하는거 있으세요?",
          "original_form": "온라인커뮤니티 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.2",
          "form": "온라인커뮤",
          "original_form": "온라인커뮤",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.3",
          "form": "글은 안 쓰는데",
          "original_form": "글은 안 쓰는데",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.4",
          "form": "눈팅은 많이 해요",
          "original_form": "눈팅은 많이 해요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.5",
          "form": "더쿠 말씀이신가요?",
          "original_form": "더쿠 말씀이신가요?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.6",
          "form": "name1님은 왠지",
          "original_form": "&name1&님은 왠지",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.7",
          "form": "더쿠만 하시나요?",
          "original_form": "더쿠만 하시나요?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.8",
          "form": "댓글 좀 쓰실듯",
          "original_form": "댓글 좀 쓰실듯",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ맞슴니다 어떻게 아셨죠?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞슴니다 어떻게 아셨죠?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.10",
          "form": "웃대도 눈팅해요",
          "original_form": "웃대도 눈팅해요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.11",
          "form": "공평한(?) 시각을 가지기위해",
          "original_form": "공평한(?) 시각을 가지기위해",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.12",
          "form": "온라인커뮤에서 본건데",
          "original_form": "온라인커뮤에서 본건데",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005214.1.13",
          "form": "자기가 컴플렉스가 있는 부분을",
          "original_form": "자기가 컴플렉스가 있는 부분을",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005214.1.14",
          "form": "악플 달때 잘 단대요",
          "original_form": "악플 달때 잘 단대요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005214.1.15",
          "form": "아, 맞아요",
          "original_form": "아, 맞아요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005214.1.16",
          "form": "댓글에 빡대가리라고 욕하는 비중이 많아요",
          "original_form": "댓글에 빡대가리라고 욕하는 비중이 많아요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005214.1.17",
          "form": "웃대도 재미있는 커뮤라고 생각해요+",
          "original_form": "웃대도 재미있는 커뮤라고 생각해요+",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005214.1.18",
          "form": "옛날엔 베스티즈, 인스티즈, 네이트판, 오늘의유머",
          "original_form": "옛날엔 베스티즈, 인스티즈, 네이트판, 오늘의유머",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005214.1.19",
          "form": "도 봤는데",
          "original_form": "도 봤는데",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005214.1.20",
          "form": "더쿠가 좋더라구요",
          "original_form": "더쿠가 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005214.1.21",
          "form": "정착했어요",
          "original_form": "정착했어요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005214.1.22",
          "form": "이것저것 굉장히 많이 하셨네요",
          "original_form": "이것저것 굉장히 많이 하셨네요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005214.1.23",
          "form": "더쿠가 아무래도 여초커뮤중에서는",
          "original_form": "더쿠가 아무래도 여초커뮤중에서는",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005214.1.24",
          "form": "규모도 크고 좋은 것 같습니다",
          "original_form": "규모도 크고 좋은 것 같습니다",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005214.1.25",
          "form": "여성시대에 대해서는 어떻게 생각하시나요",
          "original_form": "여성시대에 대해서는 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005214.1.26",
          "form": "최근에 여성시대에 대해 말이 많았던것같아서요",
          "original_form": "최근에 여성시대에 대해 말이 많았던것같아서요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005214.1.27",
          "form": "온라인커뮤로한턴 더 하시죠",
          "original_form": "온라인커뮤로한턴 더 하시죠",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005214.1.28",
          "form": "저는 더쿠와 여성시대 눈팅을 하고있어요",
          "original_form": "저는 더쿠와 여성시대 눈팅을 하고있어요",
