      "utterance": [
        {
          "id": "MDRW2100031587.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031587.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031587.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031587.1.4",
          "form": "헉 넵넵 저는 태국음식을먹었어요",
          "original_form": "헉 넵넵 저는 태국음식을먹었어요",
          "speaker_id": "2",
          "time": "20210905 02:14"
        },
        {
          "id": "MDRW2100031587.1.5",
          "form": "맛좋더라구요... 맥주랑 같이먹었는데 정말 .. 너무맛있었어요",
          "original_form": "맛좋더라구요... 맥주랑 같이먹었는데 정말 .. 너무맛있었어요",
          "speaker_id": "2",
          "time": "20210905 02:15"
        },
        {
          "id": "MDRW2100031587.1.6",
          "form": "역시 기름진 면요리는 맥주와함께죠",
          "original_form": "역시 기름진 면요리는 맥주와함께죠",
          "speaker_id": "2",
          "time": "20210905 02:15"
        },
        {
          "id": "MDRW2100031587.1.7",
          "form": "우와 맛있었겠네요 어떤 태국요리 드셨나요?",
          "original_form": "우와 맛있었겠네요 어떤 태국요리 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 02:26"
        },
        {
          "id": "MDRW2100031587.1.8",
          "form": "이야 팟타이랑 쌀국수 친구랑 노나먹었어요",
          "original_form": "이야 팟타이랑 쌀국수 친구랑 노나먹었어요",
          "speaker_id": "2",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031587.1.9",
          "form": "팟타이가 기름진 음식인가요?",
          "original_form": "팟타이가 기름진 음식인가요?",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100031587.1.10",
          "form": "ㅋㅋㅋ뭔가 고추기름이라던가 많지 않던가요?!",
          "original_form": "ㅋㅋㅋ뭔가 고추기름이라던가 많지 않던가요?!",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100031587.1.11",
          "form": "어 그렇군요 저는 딱히 그런 생각을 안해서 몰랐던 것 같아요ㅋㅋㅋ",
          "original_form": "어 그렇군요 저는 딱히 그런 생각을 안해서 몰랐던 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100031587.1.12",
          "form": "그럼 좋아하시는 음식 있으신가요?",
          "original_form": "그럼 좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100031587.1.13",
          "form": "오오 저는 라자냐 이좋아해요",
          "original_form": "오오 저는 라자냐 이좋아해요",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100031587.1.14",
          "form": "맛난...라자냐...",
          "original_form": "맛난...라자냐...",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100031587.1.15",
          "form": "냠냠굿~",
          "original_form": "냠냠굿~",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100031587.1.16",
          "form": "라자냐는 어떤 음식인가요?",
          "original_form": "라자냐는 어떤 음식인가요?",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100031587.1.17",
          "form": "파스타의 종류라고생각하면되세요",
          "original_form": "파스타의 종류라고생각하면되세요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100031587.1.18",
          "form": "넓고 얇은 면인데",
          "original_form": "넓고 얇은 면인데",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100031587.1.19",
          "form": "그것과 다진고기와 토마토소스를 섞고 치즈를 층층히쎃아서 오븐에 구운요리에요",
          "original_form": "그것과 다진고기와 토마토소스를 섞고 치즈를 층층히쎃아서 오븐에 구운요리에요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100031587.1.20",
          "form": "맛이없을수가 없다구요~",
          "original_form": "맛이없을수가 없다구요~",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100031587.1.21",
          "form": "name1님은 어떤음식좋아하시나요~",
          "original_form": "&name1&님은 어떤음식좋아하시나요~",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100031587.1.22",
          "form": "오~~한번 먹어봐야겠네요\r\n저는 떡볶이요",
