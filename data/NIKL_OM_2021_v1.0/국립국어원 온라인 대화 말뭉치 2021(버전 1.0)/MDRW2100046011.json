      "utterance": [
        {
          "id": "MDRW2100046011.1.1",
          "form": "네네 전 너무 잘 봤어요",
          "original_form": "네네 전 너무 잘 봤어요",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046011.1.2",
          "form": "가수 누구 좋아하세요??",
          "original_form": "가수 누구 좋아하세요??",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046011.1.3",
          "form": "저는 음 가슈는 으으음",
          "original_form": "저는 음 가슈는 으으음",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046011.1.4",
          "form": "최근에 있지 영상 자주봤어요!",
          "original_form": "최근에 있지 영상 자주봤어요!",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046011.1.5",
          "form": "오옹 있지 노래 너무 신나요 ㅋㅋㅋㅋ",
          "original_form": "오옹 있지 노래 너무 신나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046011.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046011.1.7",
          "form": "이번에 마.피.아 는 쫌 충격이었지만..",
          "original_form": "이번에 마.피.아 는 쫌 충격이었지만..",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046011.1.8",
          "form": "나름 중독성있더라고요",
          "original_form": "나름 중독성있더라고요",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046011.1.9",
          "form": "마피아는 좀 독특햇나요? ㅋㅋ",
          "original_form": "마피아는 좀 독특햇나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046011.1.10",
          "form": "배우보다 더 배우",
          "original_form": "배우보다 더 배우",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046011.1.11",
          "form": "늑댈 가지고 노는 여우",
          "original_form": "늑댈 가지고 노는 여우",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046011.1.12",
          "form": "가 가사에요",
          "original_form": "가 가사에요",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046011.1.13",
          "form": "와.. 뮤슨 십년전 가사 같네요 ㅋㅋㅋㅋ",
          "original_form": "와.. 뮤슨 십년전 가사 같네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046011.1.14",
          "form": "그쳐...ㅋㅋㅋㅋㅋ",
          "original_form": "그쳐...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046011.1.15",
          "form": "제왑삐가 직접 프로듀싱했대요",
          "original_form": "제왑삐가 직접 프로듀싱했대요",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046011.1.16",
          "form": "와.. 가사말고 멜로디는 좋나요..?",
          "original_form": "와.. 가사말고 멜로디는 좋나요..?",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046011.1.17",
          "form": "넹 꽤나 중독적인 멜로디에요",
          "original_form": "넹 꽤나 중독적인 멜로디에요",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046011.1.18",
          "form": "우옼ㅋㅋ 다행이네욬ㅋㅋㅋㅋ",
          "original_form": "우옼ㅋㅋ 다행이네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046011.1.19",
          "form": "저는 방탄노래 좋아해요",
          "original_form": "저는 방탄노래 좋아해요",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046011.1.20",
          "form": "방탄 좋아하시는군요!!",
          "original_form": "방탄 좋아하시는군요!!",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046011.1.21",
          "form": "네넼ㅋㅋㅋ 방탄 노래 들으면 신나드라그요",
          "original_form": "네넼ㅋㅋㅋ 방탄 노래 들으면 신나드라그요",
          "speaker_id": "1",
          "time": "20210908 09:50"
        }
      ]
