      "utterance": [
        {
          "id": "MDRW2100041744.1.1",
          "form": "염색 머리 손상 안되는거 있었음 좋겠네요 ㅠ",
          "original_form": "염색 머리 손상 안되는거 있었음 좋겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041744.1.2",
          "form": "오징어먹물로 염색하면",
          "original_form": "오징어먹물로 염색하면",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041744.1.3",
          "form": "먹물된다",
          "original_form": "먹물된다",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041744.1.4",
          "form": "그거 감을때마다 먹나올텐데 ㅋㅋㅋㅋ",
          "original_form": "그거 감을때마다 먹나올텐데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041744.1.5",
          "form": "머리로 먹글씨도 쓸수있을듯",
          "original_form": "머리로 먹글씨도 쓸수있을듯",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.6",
          "form": "비싼거만 아니면 괜찮은데...",
          "original_form": "비싼거만 아니면 괜찮은데...",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.7",
          "form": "나중에 싸지면 사야지",
          "original_form": "나중에 싸지면 사야지",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.8",
          "form": "돈 많이 벌어서 사드릴게요 ㅎㅎ",
          "original_form": "돈 많이 벌어서 사드릴게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.9",
          "form": "얼른 취직이나 해라",
          "original_form": "얼른 취직이나 해라",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.10",
          "form": "졸업먼저 하구!!",
          "original_form": "졸업먼저 하구!!",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.11",
          "form": "너는 아직 새치없지",
          "original_form": "너는 아직 새치없지",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.12",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041744.1.13",
          "form": "저는 아직 20대니까...",
          "original_form": "저는 아직 20대니까...",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041744.1.14",
          "form": "벌써 흰머리 있으면 큰일이긴 해",
          "original_form": "벌써 흰머리 있으면 큰일이긴 해",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041744.1.15",
          "form": "그쵸 아직 23인데",
          "original_form": "그쵸 아직 23인데",
