      "utterance": [
        {
          "id": "MDRW2100045304.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.2",
          "form": "몰입확되게...",
          "original_form": "몰입확되게...",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.4",
          "form": "배우는 대단한거같아",
          "original_form": "배우는 대단한거같아",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.5",
          "form": "그래도 조는 조인가보ㅓ",
          "original_form": "그래도 조는 조인가보ㅓ",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.6",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.7",
          "form": "아 진짜 헤드윅같은건",
          "original_form": "아 진짜 헤드윅같은건",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.8",
          "form": "잘맞아..",
          "original_form": "잘맞아..",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045304.1.9",
          "form": "성악적 발성이 안어울리는거지...",
          "original_form": "성악적 발성이 안어울리는거지...",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.10",
          "form": "되게 얌전하고 그럴거같지만",
          "original_form": "되게 얌전하고 그럴거같지만",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.11",
          "form": "엄청나게 끼도잘부리고",
          "original_form": "엄청나게 끼도잘부리고",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ까불거리기도잘하고...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ까불거리기도잘하고...",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.13",
          "form": "왜 인기많은지 알겠어..",
          "original_form": "왜 인기많은지 알겠어..",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ성악발성 안돼도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ성악발성 안돼도",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.15",
          "form": "일단 노래 잘하면 되지 머..",
          "original_form": "일단 노래 잘하면 되지 머..",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.16",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.17",
          "form": "그 지킬앤하이드도 확실히",
          "original_form": "그 지킬앤하이드도 확실히",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.18",
          "form": "마라맛이어서",
          "original_form": "마라맛이어서",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045304.1.19",
          "form": "그런연기는 잘하는데",
          "original_form": "그런연기는 잘하는데",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.20",
          "form": "코어가 괜히 많은게 아니엇어",
          "original_form": "코어가 괜히 많은게 아니엇어",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.21",
          "form": "또 그런극은",
          "original_form": "또 그런극은",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.22",
          "form": "노래가 많이 부족한게 티가 잘나는 그이라",
          "original_form": "노래가 많이 부족한게 티가 잘나는 그이라",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.23",
          "form": "그게 단점인거지",
          "original_form": "그게 단점인거지",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.25",
          "form": "게다가 텐션이빨라가지곸ㅋㅋㅋ",
          "original_form": "게다가 텐션이빨라가지곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.26",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.27",
          "form": "이렇게 자기가 하고싶은 자유도가 높은 극이",
          "original_form": "이렇게 자기가 하고싶은 자유도가 높은 극이",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.28",
          "form": "잘맞음",
          "original_form": "잘맞음",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.29",
          "form": "하고싶은 말도많고",
          "original_form": "하고싶은 말도많고",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045304.1.30",
          "form": "대사도 자기식으로 수정이 많이들어가는 편이라서",
          "original_form": "대사도 자기식으로 수정이 많이들어가는 편이라서",
          "speaker_id": "1",
          "time": "20210908 01:47"
        }
      ]
    }
