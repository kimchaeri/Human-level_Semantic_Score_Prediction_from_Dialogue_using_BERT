      "utterance": [
        {
          "id": "MDRW2100006423.1.1",
          "form": "우리 로맥ㅜㅜ",
          "original_form": "우리 로맥ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006423.1.2",
          "form": "로맥은 이제 나이도 좀 있죠?",
          "original_form": "로맥은 이제 나이도 좀 있죠?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006423.1.3",
          "form": "네 많죠",
          "original_form": "네 많죠",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006423.1.4",
          "form": "에이징커브올때가 됐죠",
          "original_form": "에이징커브올때가 됐죠",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006423.1.5",
          "form": "지났나",
          "original_form": "지났나",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006423.1.6",
          "form": "5년쯤 있었던 거 같은데 장기 외인이랑 헤어지는 거 아쉽긴 해요",
          "original_form": "5년쯤 있었던 거 같은데 장기 외인이랑 헤어지는 거 아쉽긴 해요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100006423.1.7",
          "form": "이제 보내줘야죠",
          "original_form": "이제 보내줘야죠",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006423.1.8",
          "form": "뭐 나중에 서튼 감독처럼 리그 복귀할 수도 있고ㅋㅋ",
          "original_form": "뭐 나중에 서튼 감독처럼 리그 복귀할 수도 있고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006423.1.9",
          "form": "ㅋㅋㅋㅋ 타격코치",
          "original_form": "ㅋㅋㅋㅋ 타격코치",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006423.1.10",
          "form": "페이지라는 어플 아세요?",
          "original_form": "페이지라는 어플 아세요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006423.1.11",
          "form": "네알아요",
          "original_form": "네알아요",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006423.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006423.1.13",
          "form": "ㅋㅋ 야구만 봐도 출석체크 되니까 이것저것 받았어요",
          "original_form": "ㅋㅋ 야구만 봐도 출석체크 되니까 이것저것 받았어요",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006423.1.14",
          "form": "퍼주더라구요 ㅋㅋ",
          "original_form": "퍼주더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006423.1.15",
          "form": "아직 초기라 그런 것 같긴 해요ㅋㅋ",
          "original_form": "아직 초기라 그런 것 같긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100006423.1.16",
          "form": "좋죠 저희들이야ㅋㅋㅋ",
          "original_form": "좋죠 저희들이야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
