      "utterance": [
        {
          "id": "MDRW2100028114.1.1",
          "form": "혹시 모나카 라는 과자?아십니까?",
          "original_form": "혹시 모나카 라는 과자?아십니까?",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028114.1.2",
          "form": "알아요 당연히ㅎㅎ",
          "original_form": "알아요 당연히ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028114.1.3",
          "form": "스님이",
          "original_form": "스님이",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.4",
          "form": "제가 그 스님을 좋아했는데",
          "original_form": "제가 그 스님을 좋아했는데",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.5",
          "form": "갈때마다",
          "original_form": "갈때마다",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.6",
          "form": "간식을 주셨어요",
          "original_form": "간식을 주셨어요",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.7",
          "form": "헉 좋은 스님...",
          "original_form": "헉 좋은 스님...",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.8",
          "form": "착한 스님...",
          "original_form": "착한 스님...",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028114.1.9",
          "form": "그때 첨 먹어본 모나카였어요",
          "original_form": "그때 첨 먹어본 모나카였어요",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.10",
          "form": "너무 달고 맛있엇서요",
          "original_form": "너무 달고 맛있엇서요",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.11",
          "form": "시골 아이들",
          "original_form": "시골 아이들",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.12",
          "form": "단거 잘 못먹는데",
          "original_form": "단거 잘 못먹는데",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.13",
          "form": "모나카 커피랑 먹음 넘 좋죠ㅜ",
          "original_form": "모나카 커피랑 먹음 넘 좋죠ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.14",
          "form": "잘 챙겨주셧어요",
          "original_form": "잘 챙겨주셧어요",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.15",
          "form": "그 여자스님",
          "original_form": "그 여자스님",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028114.1.16",
          "form": "진정 자애로운 스님이군요!",
          "original_form": "진정 자애로운 스님이군요!",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028114.1.17",
          "form": "그래도 불교에 귀의 안하신 뚝심ㅎ",
          "original_form": "그래도 불교에 귀의 안하신 뚝심ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028114.1.18",
          "form": "저 절밥 궁금해서 관련 책도 샀었어욪ㅎ",
          "original_form": "저 절밥 궁금해서 관련 책도 샀었어욪ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028114.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028114.1.20",
          "form": "역시 다음 휴가땐 템플스테이를!",
          "original_form": "역시 다음 휴가땐 템플스테이를!",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028114.1.21",
          "form": "회장님 백종원입니까?",
          "original_form": "회장님 백종원입니까?",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028114.1.22",
          "form": "가고싶은 절 있으세요?",
          "original_form": "가고싶은 절 있으세요?",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028114.1.23",
          "form": "정말 다채롭습니다",
          "original_form": "정말 다채롭습니다",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028114.1.24",
          "form": "매력 쩌시네요",
          "original_form": "매력 쩌시네요",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028114.1.25",
          "form": "절밥 책이라.. 후아",
          "original_form": "절밥 책이라.. 후아",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028114.1.26",
          "form": "그냥 가까운 곳 가고 싶어요ㅎ",
          "original_form": "그냥 가까운 곳 가고 싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028114.1.27",
          "form": "아 전 원래 먹는거 다룬 거 잘 사요ㅎㅎ",
          "original_form": "아 전 원래 먹는거 다룬 거 잘 사요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028114.1.28",
          "form": "최근에 시노다과장의 삼시세끼란 책 샀는데",
          "original_form": "최근에 시노다과장의 삼시세끼란 책 샀는데",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028114.1.29",
          "form": "삼시세끼 먹을걸 집요하게 그림과 글로 매일 정리하시는 분임",
          "original_form": "삼시세끼 먹을걸 집요하게 그림과 글로 매일 정리하시는 분임",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028114.1.30",
          "form": "그걸 모아 출판했더라구요",
          "original_form": "그걸 모아 출판했더라구요",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028114.1.31",
          "form": "진정 먹는걸 사랑하신 분ㅋㅋ",
          "original_form": "진정 먹는걸 사랑하신 분ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028114.1.32",
          "form": "와 제가 농담조로 말씀드린거 심심한 사과의 말씀을 드립니다",
          "original_form": "와 제가 농담조로 말씀드린거 심심한 사과의 말씀을 드립니다",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028114.1.33",
          "form": "찐행복이시네요",
          "original_form": "찐행복이시네요",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028114.1.34",
          "form": "멋집니다",
          "original_form": "멋집니다",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028114.1.35",
          "form": "근처에 절이 있나요?",
          "original_form": "근처에 절이 있나요?",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028114.1.36",
          "form": "없어요.ㅎㅎ",
          "original_form": "없어요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028114.1.37",
          "form": "그나마 가까운 곳...도 멀어요ㅜ",
          "original_form": "그나마 가까운 곳...도 멀어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 18:00"
        }
      ]
    }
