      "utterance": [
        {
          "id": "MDRW2100023865.1.1",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023865.1.2",
          "form": "일반을 좋아해요 ㅋ",
          "original_form": "일반을 좋아해요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023865.1.3",
          "form": "매운떡볶이 좋아해요?",
          "original_form": "매운떡볶이 좋아해요?",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023865.1.4",
          "form": "엽떡빼고 일반매운건좋아해요",
          "original_form": "엽떡빼고 일반매운건좋아해요",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100023865.1.5",
          "form": "엽떡은 왜 싫어요?",
          "original_form": "엽떡은 왜 싫어요?",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023865.1.6",
          "form": "캡산이신맛이라서요",
          "original_form": "캡산이신맛이라서요",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100023865.1.7",
          "form": "매운음식좋아하세요?",
          "original_form": "매운음식좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023865.1.8",
          "form": "넹 좋아해요",
          "original_form": "넹 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023865.1.9",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023865.1.10",
          "form": "떡볶이나 치킨이요",
          "original_form": "떡볶이나 치킨이요",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023865.1.11",
          "form": "매운것있는거중에서요",
          "original_form": "매운것있는거중에서요",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023865.1.12",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023865.1.13",
          "form": "매운거 잘못먹어 매운치킨 뭐가 맛있어?",
          "original_form": "매운거 잘못먹어 매운치킨 뭐가 맛있어?",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100023865.1.14",
          "form": "치킨매니아 땡초치킨",
          "original_form": "치킨매니아 땡초치킨",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100023865.1.15",
          "form": "이름은 기억안나지만 고 추 들어간거",
          "original_form": "이름은 기억안나지만 고 추 들어간거",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100023865.1.16",
          "form": "맛있겠다 배달음식자주먹어?",
          "original_form": "맛있겠다 배달음식자주먹어?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100023865.1.17",
          "form": "족발로 ㅋ",
          "original_form": "족발로 ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023865.1.18",
          "form": "보쌈이나",
          "original_form": "보쌈이나",
