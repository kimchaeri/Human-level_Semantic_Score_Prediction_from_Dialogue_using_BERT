        },
        {
          "id": "MDRW2100018345.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018345.1.3",
          "form": "(좋은)아침입니다",
          "original_form": "(좋은)아침입니다",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018345.1.4",
          "form": "커뮤니티 하시는 것 있으세용?",
          "original_form": "커뮤니티 하시는 것 있으세용?",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018345.1.5",
          "form": "저 커뮤니티는 학교 커뮤니티 정도..?",
          "original_form": "저 커뮤니티는 학교 커뮤니티 정도..?",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018345.1.6",
          "form": "하시는 거 잇으세요?",
          "original_form": "하시는 거 잇으세요?",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018345.1.7",
          "form": "저는 그냥 전체적으로 인터넷 많이 돌아다녀요 ㅎㅎ 다음인기글같은것도 보고 에타도 보구용",
          "original_form": "저는 그냥 전체적으로 인터넷 많이 돌아다녀요 ㅎㅎ 다음인기글같은것도 보고 에타도 보구용",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018345.1.8",
          "form": "다음인기글 재밌죠",
          "original_form": "다음인기글 재밌죠",
          "speaker_id": "2",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018345.1.9",
          "form": "저는 다음카페앱 약간 빅데이터용으로 쓰는데",
          "original_form": "저는 다음카페앱 약간 빅데이터용으로 쓰는데",
          "speaker_id": "2",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018345.1.10",
          "form": "레시피 검색하다가 한시간 뚝딱..",
          "original_form": "레시피 검색하다가 한시간 뚝딱..",
          "speaker_id": "2",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018345.1.11",
          "form": "빅데이터용! 맞아요",
          "original_form": "빅데이터용! 맞아요",
          "speaker_id": "1",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018345.1.12",
          "form": "궁금한거 있을땐 네이버보다 다음카페같은게 빠른 것 같아요",
          "original_form": "궁금한거 있을땐 네이버보다 다음카페같은게 빠른 것 같아요",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018345.1.13",
          "form": "그쵸 ㅋㅋㅋ글고 찐 꿀팁 많아서",
          "original_form": "그쵸 ㅋㅋㅋ글고 찐 꿀팁 많아서",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018345.1.14",
          "form": "행복해요",
          "original_form": "행복해요",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018345.1.15",
          "form": "흠 그리고 학교커뮤니티에서 스터디 만들어서",
          "original_form": "흠 그리고 학교커뮤니티에서 스터디 만들어서",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018345.1.16",
          "form": "거기서 사람들 많이 만나는 것 같아요",
          "original_form": "거기서 사람들 많이 만나는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018345.1.17",
          "form": "관심사 맞는 사람들 만나는 거 너무좋고!",
          "original_form": "관심사 맞는 사람들 만나는 거 너무좋고!",
          "speaker_id": "2",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018345.1.18",
          "form": "엇 저 그거 본 적 있어요",
          "original_form": "엇 저 그거 본 적 있어요",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018345.1.19",
          "form": "에타에서 그 스터디어쩌구로 넘어가면",
          "original_form": "에타에서 그 스터디어쩌구로 넘어가면",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018345.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018345.1.21",
          "form": "나오는 스터디 동아리 이런거 말쌈하시는거죠",
          "original_form": "나오는 스터디 동아리 이런거 말쌈하시는거죠",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018345.1.22",
          "form": "그것도 재밌어보이더라구요 사진스터디 이런것도 있고 ㅎㅎ",
          "original_form": "그것도 재밌어보이더라구요 사진스터디 이런것도 있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018345.1.23",
          "form": "오 아뇨 저희 학교는 그냥 오카파서 많이들 모이더라고요",
          "original_form": "오 아뇨 저희 학교는 그냥 오카파서 많이들 모이더라고요",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018345.1.24",
          "form": "캠픽 말씀하시는 건가봐요! 저 캠픽은 안해봤어요",
          "original_form": "캠픽 말씀하시는 건가봐요! 저 캠픽은 안해봤어요",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018345.1.25",
          "form": "아 네 맞아요 전 캠픽!! 전 둘다 직접 해보지는 못했는데",
          "original_form": "아 네 맞아요 전 캠픽!! 전 둘다 직접 해보지는 못했는데",
          "speaker_id": "1",
          "time": "20210827 08:44"
        },
        {
          "id": "MDRW2100018345.1.26",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "1",
          "time": "20210827 08:44"
        },
        {
          "id": "MDRW2100018345.1.27",
          "form": "요즘 비대면이라 친구들도 잘 못사귀잖아요 그런걸로 학교사람들이랑 어울리는 것도 괜찮은 것 같아요",
          "original_form": "요즘 비대면이라 친구들도 잘 못사귀잖아요 그런걸로 학교사람들이랑 어울리는 것도 괜찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018345.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018345.1.29",
          "form": "혹시 여대세요?",
          "original_form": "혹시 여대세요?",
          "speaker_id": "2",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018345.1.30",
          "form": "여대이고싶은......이요ㅠ",
          "original_form": "여대이고싶은......이요ㅠ",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018345.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아.......",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아.......",
          "speaker_id": "2",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018345.1.32",
          "form": "아 그래서 에타...아....",
          "original_form": "아 그래서 에타...아....",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018345.1.33",
          "form": "앗 여대는 에타라구 안하나요..!!!?",
          "original_form": "앗 여대는 에타라구 안하나요..!!!?",
          "speaker_id": "1",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018345.1.34",
          "form": "아뇨아뇨아뇨",
          "original_form": "아뇨아뇨아뇨",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018345.1.35",
          "form": "공학은 에타 많이 안쓴다고 그러길래",
          "original_form": "공학은 에타 많이 안쓴다고 그러길래",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018345.1.36",
          "form": "직접 써본적없다고 하셔서",
          "original_form": "직접 써본적없다고 하셔서",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018345.1.37",
          "form": "그때 혹시 여댄지 여쭤본 거였어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그때 혹시 여댄지 여쭤본 거였어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018345.1.38",
          "form": "아~ 맞아요 ㅎ",
          "original_form": "아~ 맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018345.1.39",
          "form": "헷..",
          "original_form": "헷..",
          "speaker_id": "2",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018345.1.40",
          "form": "에타들어가도 기분 안좋을때 많아서 정보찾기용으로만 써요 ㅋㅋㅋ",
          "original_form": "에타들어가도 기분 안좋을때 많아서 정보찾기용으로만 써요 ㅋㅋㅋ",
          "speaker_id": "1",
