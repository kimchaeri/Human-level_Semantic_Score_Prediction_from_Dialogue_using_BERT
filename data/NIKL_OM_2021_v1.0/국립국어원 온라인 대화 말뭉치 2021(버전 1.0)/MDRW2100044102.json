      "utterance": [
        {
          "id": "MDRW2100044102.1.1",
          "form": "라이프 익스텐션 입니다",
          "original_form": "라이프 익스텐션 입니다",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044102.1.2",
          "form": "평조아서 샀는데",
          "original_form": "평조아서 샀는데",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044102.1.3",
          "form": ".....",
          "original_form": ".....",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044102.1.4",
          "form": "앗 이름만 보면 좋아보이는데",
          "original_form": "앗 이름만 보면 좋아보이는데",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044102.1.5",
          "form": "효과는 어때??",
          "original_form": "효과는 어때??",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044102.1.6",
          "form": "아 머리한지는 며칠 됐나오",
          "original_form": "아 머리한지는 며칠 됐나오",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044102.1.7",
          "form": "저 월요일ㅋㅋ 이제 하루 되었어오",
          "original_form": "저 월요일ㅋㅋ 이제 하루 되었어오",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044102.1.8",
          "form": "효과는...요즘 안묵음;;",
          "original_form": "효과는...요즘 안묵음;;",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044102.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044102.1.10",
          "form": "어제 비왔지 않나오",
          "original_form": "어제 비왔지 않나오",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044102.1.11",
          "form": "ㄴㄴ 오전에 갔어오",
          "original_form": "ㄴㄴ 오전에 갔어오",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.12",
          "form": "쉬는 날이라",
          "original_form": "쉬는 날이라",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.13",
          "form": "머리 맘에 드므ㅡㅡ",
          "original_form": "머리 맘에 드므ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.14",
          "form": "듬?",
          "original_form": "듬?",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.16",
          "form": "염색도 함??",
          "original_form": "염색도 함??",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.17",
          "form": "근데 오전에 갔는데 묭실 사람 꽤 있음",
          "original_form": "근데 오전에 갔는데 묭실 사람 꽤 있음",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.18",
          "form": "ㄴㄴ 염색은 안했어오",
          "original_form": "ㄴㄴ 염색은 안했어오",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.19",
          "form": "머리 맘에 들긴 하는데 머리 감고나서도 유지가 되어야...",
          "original_form": "머리 맘에 들긴 하는데 머리 감고나서도 유지가 되어야...",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044102.1.20",
          "form": "머리 언제 감아??",
          "original_form": "머리 언제 감아??",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044102.1.21",
          "form": "내일 감을려구요ㅋㅋ",
          "original_form": "내일 감을려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044102.1.22",
          "form": "님 머리 하면 몇일뒤에 감음??",
          "original_form": "님 머리 하면 몇일뒤에 감음??",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044102.1.23",
          "form": "나는 당일날 감음",
          "original_form": "나는 당일날 감음",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044102.1.24",
          "form": "찝찝해서...",
          "original_form": "찝찝해서...",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044102.1.25",
          "form": "내일 머리감을 때 린스 써도 됨???",
          "original_form": "내일 머리감을 때 린스 써도 됨???",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044102.1.26",
          "form": "ㅇㅇ디자니어쌤이 24시간만 지나면 감아도 된다고 하더라",
          "original_form": "ㅇㅇ디자니어쌤이 24시간만 지나면 감아도 된다고 하더라",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044102.1.27",
          "form": "린스 괜춘하데",
          "original_form": "린스 괜춘하데",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044102.1.28",
          "form": "님 묭실 가면 대화함,,,??",
          "original_form": "님 묭실 가면 대화함,,,??",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044102.1.29",
          "form": "웬만하면 안해",
          "original_form": "웬만하면 안해",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044102.1.30",
          "form": "묜실 대화 좋아함???",
          "original_form": "묜실 대화 좋아함???",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044102.1.31",
          "form": "ㄴㄴ 시러함ㅋㅋ",
          "original_form": "ㄴㄴ 시러함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044102.1.32",
          "form": "디자이너가 먼저 말을 걸거나",
          "original_form": "디자이너가 먼저 말을 걸거나",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044102.1.33",
          "form": "손님이 먼저 말을 걸거나 이 두종류가 있더라고",
          "original_form": "손님이 먼저 말을 걸거나 이 두종류가 있더라고",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044102.1.34",
          "form": "난 첨 갔을때 말을 안해서 그런가 슨생님이 말 안하셔서 넘 편해...",
          "original_form": "난 첨 갔을때 말을 안해서 그런가 슨생님이 말 안하셔서 넘 편해...",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044102.1.35",
          "form": "헐 좋은 선생님이다",
          "original_form": "헐 좋은 선생님이다",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044102.1.36",
          "form": "항상 그 선생님에게 받아??",
          "original_form": "항상 그 선생님에게 받아??",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044102.1.37",
          "form": "묭실대화는 돈 내고 불편함을 사는 기분이야",
          "original_form": "묭실대화는 돈 내고 불편함을 사는 기분이야",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044102.1.38",
          "form": "ㅁㅈ 내가 돈내고 머리 하는데 부담스러움...",
          "original_form": "ㅁㅈ 내가 돈내고 머리 하는데 부담스러움...",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044102.1.39",
          "form": "ㅇㅇ 이사가서 다른데 갔다가 데이고ㅋㅋ 그냥 갔던데 다시감",
          "original_form": "ㅇㅇ 이사가서 다른데 갔다가 데이고ㅋㅋ 그냥 갔던데 다시감",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044102.1.40",
          "form": "address역 주노헤어 임다ㅇㅇ",
          "original_form": "&address&역 주노헤어 임다ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044102.1.41",
          "form": "으악 데이기까지 함????",
          "original_form": "으악 데이기까지 함????",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
