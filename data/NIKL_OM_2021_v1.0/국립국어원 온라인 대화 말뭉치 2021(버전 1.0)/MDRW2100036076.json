      "utterance": [
        {
          "id": "MDRW2100036076.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 16:07"
        },
        {
          "id": "MDRW2100036076.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100036076.1.3",
          "form": "안녈하새요. 점심 맛잇게 드셧어여?",
          "original_form": "안녈하새요. 점심 맛잇게 드셧어여?",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100036076.1.4",
          "form": "네~ 간단하게 먹었어요",
          "original_form": "네~ 간단하게 먹었어요",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100036076.1.5",
          "form": "뭐드셧는데여",
          "original_form": "뭐드셧는데여",
          "speaker_id": "1",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100036076.1.6",
          "form": "비빔면에 삶은계란+비엔나 살짝 구웠어요",
          "original_form": "비빔면에 삶은계란+비엔나 살짝 구웠어요",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036076.1.7",
          "form": "오.. 그러시군요. 저는 며칠전에 비빔국수에\n오리고기랑 먹엇어요",
          "original_form": "오.. 그러시군요. 저는 며칠전에 비빔국수에\n오리고기랑 먹엇어요",
          "speaker_id": "1",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036076.1.8",
          "form": "오 맛있었겠어요",
          "original_form": "오 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036076.1.9",
          "form": "그런데 비빔국수 양념만들때 겨자를 너무 많이넣어서 고통스러웟어요",
          "original_form": "그런데 비빔국수 양념만들때 겨자를 너무 많이넣어서 고통스러웟어요",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036076.1.10",
          "form": "으 생각만해도 고통이네요",
          "original_form": "으 생각만해도 고통이네요",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036076.1.11",
          "form": "전 겨자를 좋아하지 않아 아얘 안 넣어요",
          "original_form": "전 겨자를 좋아하지 않아 아얘 안 넣어요",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036076.1.12",
          "form": "망했어요",
          "original_form": "망했어요",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036076.1.13",
          "form": "적당히는 좋은데\n욕심히 너무 과했네요",
          "original_form": "적당히는 좋은데\n욕심히 너무 과했네요",
          "speaker_id": "1",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036076.1.14",
          "form": "ㅜㅜ 그래도 다 드셨나요?",
          "original_form": "ㅜㅜ 그래도 다 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100036076.1.15",
          "form": "며칠전에 꾸역꾸역 다 먹고 속안좋아서 괴로웟어요.\n남은 양념장은 어떻게든 소비해보려고요",
          "original_form": "며칠전에 꾸역꾸역 다 먹고 속안좋아서 괴로웟어요.\n남은 양념장은 어떻게든 소비해보려고요",
          "speaker_id": "1",
          "time": "20210830 16:37"
        }
      ]
