      "utterance": [
        {
          "id": "MDRW2100001801.1.1",
          "form": "새벽에 문닫고 자야겠다 엄청 시원해",
          "original_form": "새벽에 문닫고 자야겠다 엄청 시원해",
          "speaker_id": "2",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001801.1.2",
          "form": "응 온도 많이 떨어졌어",
          "original_form": "응 온도 많이 떨어졌어",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001801.1.3",
          "form": "선풍기 안켜도 이렇게 시원하다니",
          "original_form": "선풍기 안켜도 이렇게 시원하다니",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001801.1.4",
          "form": "이럴 때 감기 안걸리게 조심해야 돼",
          "original_form": "이럴 때 감기 안걸리게 조심해야 돼",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001801.1.5",
          "form": "너무 시원하고 뽀송해서 좋아",
          "original_form": "너무 시원하고 뽀송해서 좋아",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001801.1.6",
          "form": "요즘 미세먼지도 없더라",
          "original_form": "요즘 미세먼지도 없더라",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001801.1.7",
          "form": "그러니 더 굿이지 완전 가을날씨",
          "original_form": "그러니 더 굿이지 완전 가을날씨",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.8",
          "form": "잠이 솔솔 와 요즘은",
          "original_form": "잠이 솔솔 와 요즘은",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.9",
          "form": "ㅎㅎ 낮에는 졸면 안돼",
          "original_form": "ㅎㅎ 낮에는 졸면 안돼",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.10",
          "form": "ㅋㅋ 밤에 말야",
          "original_form": "ㅋㅋ 밤에 말야",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.11",
          "form": "좀 시끄럽긴 해도 선풍기 에어컨 안켜도 되니 너무 좋아",
          "original_form": "좀 시끄럽긴 해도 선풍기 에어컨 안켜도 되니 너무 좋아",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.12",
          "form": "전기세 걱정 없고 ㅋㅋ",
          "original_form": "전기세 걱정 없고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001801.1.13",
          "form": "글치 ㅎㅎ 관리비 두려웡",
          "original_form": "글치 ㅎㅎ 관리비 두려웡",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001801.1.14",
          "form": "이런 가을 날씨 오래가면 좋겠다",
          "original_form": "이런 가을 날씨 오래가면 좋겠다",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001801.1.15",
          "form": "나두 ㅎㅎ 시원해서 너무 좋음",
          "original_form": "나두 ㅎㅎ 시원해서 너무 좋음",
