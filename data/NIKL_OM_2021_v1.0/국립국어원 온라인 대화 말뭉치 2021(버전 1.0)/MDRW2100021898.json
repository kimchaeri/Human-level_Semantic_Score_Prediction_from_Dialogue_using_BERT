      "utterance": [
        {
          "id": "MDRW2100021898.1.1",
          "form": "입맛에 안맞는데 양도 많으면 처리하기 곤란했겠네요",
          "original_form": "입맛에 안맞는데 양도 많으면 처리하기 곤란했겠네요",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021898.1.2",
          "form": "아 그래서",
          "original_form": "아 그래서",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021898.1.3",
          "form": "어떻게 했냐면",
          "original_form": "어떻게 했냐면",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.4",
          "form": "마라 비빔밥?",
          "original_form": "마라 비빔밥?",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.5",
          "form": "진라면 매운맛 스프를 넣어서 업그레이드 했오요",
          "original_form": "진라면 매운맛 스프를 넣어서 업그레이드 했오요",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.7",
          "form": "괜찮은 방법이네요 ㅋㅋㅋㅋ",
          "original_form": "괜찮은 방법이네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.8",
          "form": "재료도 살리구요",
          "original_form": "재료도 살리구요",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021898.1.9",
          "form": "글고 비에이치씨",
          "original_form": "글고 비에이치씨",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021898.1.10",
          "form": "맛초킹 1인분닭 있어서 먹었는제",
          "original_form": "맛초킹 1인분닭 있어서 먹었는제",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021898.1.11",
          "form": "제가 뿌링클 싫어해서",
          "original_form": "제가 뿌링클 싫어해서",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021898.1.12",
          "form": "잘 골랐더니",
          "original_form": "잘 골랐더니",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021898.1.13",
          "form": "맛있게 먹었어요",
          "original_form": "맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021898.1.14",
          "form": "비에이치씨 후라이드만 먹어봤지만 맛있을 것 같아요!",
          "original_form": "비에이치씨 후라이드만 먹어봤지만 맛있을 것 같아요!",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021898.1.15",
          "form": "제주도는 육지09090999 보다 식재로 물가가 비싸던데",
          "original_form": "제주도는 육지09090999 보다 식재로 물가가 비싸던데",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021898.1.16",
          "form": "프랜차이즈 음식가격은 같나요?",
          "original_form": "프랜차이즈 음식가격은 같나요?",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021898.1.17",
          "form": "프렌차이즈는 같구요",
          "original_form": "프렌차이즈는 같구요",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021898.1.18",
          "form": "과일 채소",
          "original_form": "과일 채소",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021898.1.19",
          "form": "제주산 아닌거는 비싼것 같아요",
          "original_form": "제주산 아닌거는 비싼것 같아요",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021898.1.20",
          "form": "외식비도 찬차만별이구요",
          "original_form": "외식비도 찬차만별이구요",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021898.1.21",
          "form": "우와 역시,, 잠깐 놀러가긴 좋지만 오래 살긴 살짝 애매하겠어요",
          "original_form": "우와 역시,, 잠깐 놀러가긴 좋지만 오래 살긴 살짝 애매하겠어요",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021898.1.22",
          "form": "제주도에 땅이 있다면,, 살고 싶겠지만,,",
          "original_form": "제주도에 땅이 있다면,, 살고 싶겠지만,,",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021898.1.23",
          "form": "근데 다 배타고 들어가니깐 재료가 비쌀수 밖에 없겠네요 울릉도는 쌀값도 비싸더라고요",
          "original_form": "근데 다 배타고 들어가니깐 재료가 비쌀수 밖에 없겠네요 울릉도는 쌀값도 비싸더라고요",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021898.1.24",
          "form": "네.맞아요 근데 또 관광지니까 식재료 비행기도 태워서도 오니까 이해해야죠",
          "original_form": "네.맞아요 근데 또 관광지니까 식재료 비행기도 태워서도 오니까 이해해야죠",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021898.1.25",
          "form": "최근에 청귤이 많이 나와서",
          "original_form": "최근에 청귤이 많이 나와서",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021898.1.26",
          "form": "유기농으로 좀 사서 청담글까 생각중이예여",
          "original_form": "유기농으로 좀 사서 청담글까 생각중이예여",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021898.1.27",
          "form": "청귤청도 좋아해요! 맛있겠어요",
          "original_form": "청귤청도 좋아해요! 맛있겠어요",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021898.1.28",
          "form": "기념품으로 주변에 나눠줘도 되겠네요 10월까지계신다니간,,",
          "original_form": "기념품으로 주변에 나눠줘도 되겠네요 10월까지계신다니간,,",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021898.1.29",
          "form": "제주사람들은 풋귤이라 하대요",
          "original_form": "제주사람들은 풋귤이라 하대요",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021898.1.30",
          "form": "푸른색일때 귤을따는거라서요",
          "original_form": "푸른색일때 귤을따는거라서요",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021898.1.31",
          "form": "네 다들 놀러온다해서",
          "original_form": "네 다들 놀러온다해서",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021898.1.32",
          "form": "조금 만들어보고 잘 만들어지면 대량으로 하려고요",
          "original_form": "조금 만들어보고 잘 만들어지면 대량으로 하려고요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021898.1.33",
          "form": "청귤청 맛있다 생각을 못했는데 워낙 몸에 좋다네요",
          "original_form": "청귤청 맛있다 생각을 못했는데 워낙 몸에 좋다네요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021898.1.34",
          "form": "비타민도 많구요",
          "original_form": "비타민도 많구요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021898.1.35",
          "form": "우와 제가 친구면 정말 좋을 것 같아요 ㅋㅋㅋ",
          "original_form": "우와 제가 친구면 정말 좋을 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100021898.1.36",
          "form": "제주놀러가니 귤청도 선물받구 하면 더 좋죠!",
          "original_form": "제주놀러가니 귤청도 선물받구 하면 더 좋죠!",
          "speaker_id": "1",
          "time": "20210828 23:49"
        }
      ]
    }
