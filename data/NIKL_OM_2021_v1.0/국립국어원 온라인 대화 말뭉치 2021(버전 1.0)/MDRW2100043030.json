      "utterance": [
        {
          "id": "MDRW2100043030.1.1",
          "form": "안녕하세요! 요즘 운동하시나요?",
          "original_form": "안녕하세요! 요즘 운동하시나요?",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043030.1.2",
          "form": "저 숨쉬기요",
          "original_form": "저 숨쉬기요",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043030.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043030.1.4",
          "form": "하..일핑계로 안하고잇네요..",
          "original_form": "하..일핑계로 안하고잇네요..",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043030.1.5",
          "form": "아 넘 웃겨요 근데 저도 마찬가지입니다... 일도.. 운동아닐까요",
          "original_form": "아 넘 웃겨요 근데 저도 마찬가지입니다... 일도.. 운동아닐까요",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100043030.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043030.1.7",
          "form": "글고 저는 출퇴근도 왕복 3시간이나 걸리거든요?",
          "original_form": "글고 저는 출퇴근도 왕복 3시간이나 걸리거든요?",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043030.1.8",
          "form": "근데 왜 살은 찔까요@!!????",
          "original_form": "근데 왜 살은 찔까요@!!????",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043030.1.9",
          "form": "대체 어디서 찌는 살일까요!!!!!!!!!!!",
          "original_form": "대체 어디서 찌는 살일까요!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043030.1.10",
          "form": "이건 정말 의문입니다...",
          "original_form": "이건 정말 의문입니다...",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043030.1.11",
          "form": "왕복 3시간이라니 ..",
          "original_form": "왕복 3시간이라니 ..",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043030.1.12",
          "form": "징글징글하죠..?",
          "original_form": "징글징글하죠..?",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043030.1.13",
          "form": "한시간정도는 앉아가는데",
          "original_form": "한시간정도는 앉아가는데",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043030.1.14",
          "form": "오래 앉아있는것도 힘들더라구요 허리아픔 ㅍ",
          "original_form": "오래 앉아있는것도 힘들더라구요 허리아픔 ㅍ",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043030.1.15",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043030.1.16",
          "form": "맞아요... 원래 차나 대중교통에 가만히 앉아있는게 더 힘들잖아요ㅠㅜ",
          "original_form": "맞아요... 원래 차나 대중교통에 가만히 앉아있는게 더 힘들잖아요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043030.1.17",
          "form": "허리가 정말 너무 아프더라구요",
          "original_form": "허리가 정말 너무 아프더라구요",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043030.1.18",
          "form": "근데 운동 너무 안해서 이젠 진짜 해여해요",
          "original_form": "근데 운동 너무 안해서 이젠 진짜 해여해요",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043030.1.19",
          "form": "진짜요ㅜㅜ 근데 뭘 해야할지 감이 안 잡히네요",
          "original_form": "진짜요ㅜㅜ 근데 뭘 해야할지 감이 안 잡히네요",
          "speaker_id": "1",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043030.1.20",
          "form": "배우고 싶은 운동 있으세요?",
          "original_form": "배우고 싶은 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043030.1.21",
          "form": "음 그냥 헬스 한 번 해보고 싶어요",
          "original_form": "음 그냥 헬스 한 번 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043030.1.22",
          "form": "헬스는 진짜 자기싸움인거같아요",
          "original_form": "헬스는 진짜 자기싸움인거같아요",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043030.1.23",
          "form": "저는 혼자하려니 너무 재미없더라구요 ㅠ",
          "original_form": "저는 혼자하려니 너무 재미없더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043030.1.24",
          "form": "만약 한다하면... 피티를 해야겠죠ㅠㅠ",
          "original_form": "만약 한다하면... 피티를 해야겠죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043030.1.25",
          "form": "맞아요 피티는 재밌더라구요",
          "original_form": "맞아요 피티는 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043030.1.26",
          "form": "저는 ㅋㅋㅋㅋ의지를 돈주고 사야하는 사람인지라",
          "original_form": "저는 ㅋㅋㅋㅋ의지를 돈주고 사야하는 사람인지라",
          "speaker_id": "2",
          "time": "20210907 13:55"
        }
      ]
