      "utterance": [
        {
          "id": "MDRW2100017516.1.1",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017516.1.2",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.3",
          "form": "근데 난 이 베이컨중에 2장만 쓰고싶어",
          "original_form": "근데 난 이 베이컨중에 2장만 쓰고싶어",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.4",
          "form": "녹였다가 다시 얼리면 문제가 없을까?",
          "original_form": "녹였다가 다시 얼리면 문제가 없을까?",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.5",
          "form": "끌과 망치가 필요하겠네",
          "original_form": "끌과 망치가 필요하겠네",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.6",
          "form": "녹였다가 얼리면",
          "original_form": "녹였다가 얼리면",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.7",
          "form": "맛없어",
          "original_form": "맛없어",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017516.1.8",
          "form": "베이컨깎는 노인..",
          "original_form": "베이컨깎는 노인..",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017516.1.9",
          "form": "깡... 깡...",
          "original_form": "깡... 깡...",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017516.1.10",
          "form": "장작패기",
          "original_form": "장작패기",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017516.1.11",
          "form": "베이컨패기",
          "original_form": "베이컨패기",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017516.1.12",
          "form": "다 녹이지 말고 살짝만 녹이면",
          "original_form": "다 녹이지 말고 살짝만 녹이면",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017516.1.13",
          "form": "떨어지지 않을까",
          "original_form": "떨어지지 않을까",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017516.1.14",
          "form": "상온에 잠깐 두는거야",
          "original_form": "상온에 잠깐 두는거야",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017516.1.15",
          "form": "열심히 만들고 있는건가",
          "original_form": "열심히 만들고 있는건가",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017516.1.16",
          "form": "열심히 만들고 먹고왔어",
          "original_form": "열심히 만들고 먹고왔어",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017516.1.17",
          "form": "와진짜 맛있었다",
          "original_form": "와진짜 맛있었다",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017516.1.18",
          "form": "베이컨 내놓으니까 금방 녹긴 했는데",
          "original_form": "베이컨 내놓으니까 금방 녹긴 했는데",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017516.1.19",
          "form": "떨어지는게 아니라 부숴지더라고",
          "original_form": "떨어지는게 아니라 부숴지더라고",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017516.1.20",
          "form": "그래서 통째로 세로로 잘라서 썼어..^^",
          "original_form": "그래서 통째로 세로로 잘라서 썼어..^^",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017516.1.21",
          "form": "썰어버렸구나",
          "original_form": "썰어버렸구나",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017516.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017516.1.23",
          "form": "후식도 먹었어?",
          "original_form": "후식도 먹었어?",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017516.1.24",
          "form": "후식은 안먹었어",
          "original_form": "후식은 안먹었어",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017516.1.25",
          "form": "왜냐면 이미 너무나 배가 불렀기 때문에",
          "original_form": "왜냐면 이미 너무나 배가 불렀기 때문에",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017516.1.26",
          "form": "얼마나 맛있었으면!",
          "original_form": "얼마나 맛있었으면!",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017516.1.27",
          "form": "이번에는 성공이었나보네",
          "original_form": "이번에는 성공이었나보네",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017516.1.28",
          "form": "엉 저번에는 물을 너무 많이 넣어서",
          "original_form": "엉 저번에는 물을 너무 많이 넣어서",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017516.1.29",
          "form": "이번에는 안탈정도로만 넣었어",
          "original_form": "이번에는 안탈정도로만 넣었어",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017516.1.30",
          "form": "근데 약간 짰어",
          "original_form": "근데 약간 짰어",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017516.1.31",
          "form": "치킨스톡이랑 스팸때문인듯 ㅋㅋㅋ",
          "original_form": "치킨스톡이랑 스팸때문인듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.32",
          "form": "짠게 두개나 들어갔어!",
          "original_form": "짠게 두개나 들어갔어!",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.33",
          "form": "지난번에는 소세지 넣고 했었어서",
          "original_form": "지난번에는 소세지 넣고 했었어서",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.34",
          "form": "한번만 더하면",
          "original_form": "한번만 더하면",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.35",
          "form": "완벽해지겠네",
          "original_form": "완벽해지겠네",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.36",
          "form": "ㅋㅋㅋㅋㅋ 간을 안봐서.. 그런것같은..",
          "original_form": "ㅋㅋㅋㅋㅋ 간을 안봐서.. 그런것같은..",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.37",
          "form": "엄마한테 한입 먹어보라고 했는데",
          "original_form": "엄마한테 한입 먹어보라고 했는데",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017516.1.38",
          "form": "엄마 밥먹고 왔는데 식빵 2장먹음",
          "original_form": "엄마 밥먹고 왔는데 식빵 2장먹음",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017516.1.39",
          "form": "엄마 : 너무 배부른데 먹는것을 멈출수없다",
          "original_form": "엄마 : 너무 배부른데 먹는것을 멈출수없다",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017516.1.40",
          "form": "그리고 계란 많이넣지 왜 2개만 넣었냐고 혼남",
          "original_form": "그리고 계란 많이넣지 왜 2개만 넣었냐고 혼남",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017516.1.41",
          "form": "아니.. 저만먹을건데 2개면 충분하지 않나여..",
          "original_form": "아니.. 저만먹을건데 2개면 충분하지 않나여..",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017516.1.42",
          "form": "계란은",
          "original_form": "계란은",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100017516.1.43",
          "form": "다다익선이긴해",
          "original_form": "다다익선이긴해",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100017516.1.44",
          "form": "그래도 2개 정도면",
          "original_form": "그래도 2개 정도면",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.45",
          "form": "그치 계란 많을수록 맛있긴하지",
          "original_form": "그치 계란 많을수록 맛있긴하지",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.46",
          "form": "라디오회관가고싶다",
          "original_form": "라디오회관가고싶다",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.47",
          "form": "적당하지 않았나!",
          "original_form": "적당하지 않았나!",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.48",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.49",
          "form": "두명이서먹을줄알았음",
          "original_form": "두명이서먹을줄알았음",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.50",
          "form": "라디오회관 안간지",
          "original_form": "라디오회관 안간지",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.51",
          "form": "세개넣을걸그랬어",
          "original_form": "세개넣을걸그랬어",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.52",
          "form": "1년 6개월..",
          "original_form": "1년 6개월..",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.53",
          "form": "하울세트가 아른아른...",
          "original_form": "하울세트가 아른아른...",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100017516.1.54",
          "form": "이사가기전에 명란크림우동 먹어야하는데",
          "original_form": "이사가기전에 명란크림우동 먹어야하는데",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100017516.1.55",
          "form": "모츠나베도 다시먹고싶어",
          "original_form": "모츠나베도 다시먹고싶어",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100017516.1.56",
          "form": "다음에는 토리도노야 가보자",
          "original_form": "다음에는 토리도노야 가보자",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100017516.1.57",
          "form": "거기 친구가 추천해준맛집인데 이번에 확장이전했거든",
          "original_form": "거기 친구가 추천해준맛집인데 이번에 확장이전했거든",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100017516.1.58",
          "form": "매일 웨이팅이 있어",
          "original_form": "매일 웨이팅이 있어",
          "speaker_id": "1",
