      "utterance": [
        {
          "id": "MDRW2100045773.1.1",
          "form": "그게 날듯요... 자꾸 살쪄서",
          "original_form": "그게 날듯요... 자꾸 살쪄서",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045773.1.2",
          "form": "큰일났다 차 시동안끔 ㅋㅋ",
          "original_form": "큰일났다 차 시동안끔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045773.1.3",
          "form": "설마요 ㄷㄷ",
          "original_form": "설마요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045773.1.4",
          "form": "손해 봤네요",
          "original_form": "손해 봤네요",
          "speaker_id": "1",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045773.1.5",
          "form": "조금 굶어도 되긴하네요",
          "original_form": "조금 굶어도 되긴하네요",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045773.1.6",
          "form": "물은 계속 화장실 물 드시나봐요",
          "original_form": "물은 계속 화장실 물 드시나봐요",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045773.1.7",
          "form": "그거도 무한일까요?",
          "original_form": "그거도 무한일까요?",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045773.1.8",
          "form": "아까 단수인거 보면 아닐듯",
          "original_form": "아까 단수인거 보면 아닐듯",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045773.1.9",
          "form": "감자칩 저도 먹고싶네요",
          "original_form": "감자칩 저도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045773.1.10",
          "form": "다시 샐러드 시작 ㅋㅋㅋ",
          "original_form": "다시 샐러드 시작 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045773.1.11",
          "form": "다이어트식 드시넹 ㅋㅋ",
          "original_form": "다이어트식 드시넹 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045773.1.12",
          "form": "현실과 똑같이 양이 작았네요",
          "original_form": "현실과 똑같이 양이 작았네요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045773.1.13",
          "form": "운동을 못하게 된건가요?",
          "original_form": "운동을 못하게 된건가요?",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045773.1.14",
          "form": "덜 먹어서 그런가봐요",
          "original_form": "덜 먹어서 그런가봐요",
