      "utterance": [
        {
          "id": "MDRW2100019842.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019842.1.2",
          "form": "예시 보니까 잘못한거 같네",
          "original_form": "예시 보니까 잘못한거 같네",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019842.1.3",
          "form": "ㅋㅋㅋㅋ 결혼생활은 없구나",
          "original_form": "ㅋㅋㅋㅋ 결혼생활은 없구나",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019842.1.4",
          "form": "내 결혼생활을 들으면 결혼하고 싶지 않을것 같은데",
          "original_form": "내 결혼생활을 들으면 결혼하고 싶지 않을것 같은데",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019842.1.5",
          "form": "연애만이라도 해~",
          "original_form": "연애만이라도 해~",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.6",
          "form": "그것도 사실 피곤하다",
          "original_form": "그것도 사실 피곤하다",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.7",
          "form": "무슨 얘기를 하시나여 행복한 분께서",
          "original_form": "무슨 얘기를 하시나여 행복한 분께서",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.8",
          "form": "그냥 혼자가 최고인것 같아",
          "original_form": "그냥 혼자가 최고인것 같아",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.11",
          "form": "역시 튜닝의 끝은 순정이지",
          "original_form": "역시 튜닝의 끝은 순정이지",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019842.1.12",
          "form": "행복하려고 결혼한건 아니고 뭐때문에 결혼을 한거지",
          "original_form": "행복하려고 결혼한건 아니고 뭐때문에 결혼을 한거지",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.13",
          "form": "연애하고 결혼해도 솔로로 돌아오는거야",
          "original_form": "연애하고 결혼해도 솔로로 돌아오는거야",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.14",
          "form": "갑자기 의문이드네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 의문이드네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.15",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.16",
          "form": "혼자왔다 혼자가믄 인생이잖아",
          "original_form": "혼자왔다 혼자가믄 인생이잖아",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.17",
          "form": "단체로 와도 홀로 가는 인생",
          "original_form": "단체로 와도 홀로 가는 인생",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.18",
          "form": "붙어있어도 혼자만의 시간이 필요하고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "붙어있어도 혼자만의 시간이 필요하고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019842.1.19",
          "form": "결혼관을 여쭤볼수도 없고",
          "original_form": "결혼관을 여쭤볼수도 없고",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019842.1.20",
          "form": "이런걸 초면인 사람이랑 어떻게 얘기하라는거에욧",
          "original_form": "이런걸 초면인 사람이랑 어떻게 얘기하라는거에욧",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019842.1.21",
          "form": "결혼하니까 의무만 늘어난것같아",
          "original_form": "결혼하니까 의무만 늘어난것같아",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019842.1.22",
          "form": "그렇긴하다 ㅋㅋㅋ",
          "original_form": "그렇긴하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019842.1.23",
          "form": "아니야 넌 결혼 전에도 의무가 많았어",
          "original_form": "아니야 넌 결혼 전에도 의무가 많았어",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.24",
          "form": "그냥 늘은거 뿐이야",
          "original_form": "그냥 늘은거 뿐이야",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.25",
          "form": "그러네 근데 더 늘어났어 응응",
          "original_form": "그러네 근데 더 늘어났어 응응",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.26",
          "form": "내가 봤기 때문에 ㅋㅋㅋㅋ",
          "original_form": "내가 봤기 때문에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.27",
          "form": "눈코뜰새가 없어",
          "original_form": "눈코뜰새가 없어",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.28",
          "form": "자고일어나면 일일일",
          "original_form": "자고일어나면 일일일",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.29",
          "form": "돌볼 어리지 않은 늙은 양들이 늘었을 뿐...",
          "original_form": "돌볼 어리지 않은 늙은 양들이 늘었을 뿐...",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.30",
          "form": "곧 어린 양도 생기겄지",
          "original_form": "곧 어린 양도 생기겄지",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.31",
          "form": "아이고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019842.1.32",
          "form": "결혼 행복하네...",
          "original_form": "결혼 행복하네...",
