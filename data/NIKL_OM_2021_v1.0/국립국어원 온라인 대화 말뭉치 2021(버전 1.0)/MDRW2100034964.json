      "utterance": [
        {
          "id": "MDRW2100034964.1.1",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034964.1.2",
          "form": "유용하네요",
          "original_form": "유용하네요",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034964.1.3",
          "form": "맘카페에도 유용한 정보 자주 올라와요",
          "original_form": "맘카페에도 유용한 정보 자주 올라와요",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034964.1.4",
          "form": "그런카페에는 물건많이 사야하는",
          "original_form": "그런카페에는 물건많이 사야하는",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034964.1.5",
          "form": "아이들있는집에 필요하겠네요",
          "original_form": "아이들있는집에 필요하겠네요",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.6",
          "form": "그래서 최근에 쌀 10kg 2만원 초반주고 샀어요ㅋㅋㅋㅋ",
          "original_form": "그래서 최근에 쌀 10kg 2만원 초반주고 샀어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.7",
          "form": "오 ㅇ사다",
          "original_form": "오 ㅇ사다",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.8",
          "form": "10키로면 얼마나 먹나요",
          "original_form": "10키로면 얼마나 먹나요",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.9",
          "form": "그쳐ㅋㅋㅋ 이런 특가딜 올라오면 바로 공유되서ㅋㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋ 이런 특가딜 올라오면 바로 공유되서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.10",
          "form": "저는 4키로 좀씩 먹는데",
          "original_form": "저는 4키로 좀씩 먹는데",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100034964.1.11",
          "form": "저는 밀가루나 다른거 많이 먹어서 10키로면 음... 꽤 오래먹어요",
          "original_form": "저는 밀가루나 다른거 많이 먹어서 10키로면 음... 꽤 오래먹어요",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.12",
          "form": "오 택배비 공짠가요",
          "original_form": "오 택배비 공짠가요",
          "speaker_id": "2",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.14",
          "form": "그리고 최근에는",
          "original_form": "그리고 최근에는",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.15",
          "form": "현대패션몰에서",
          "original_form": "현대패션몰에서",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.16",
          "form": "출석체크하면 매일 포인트주는데",
          "original_form": "출석체크하면 매일 포인트주는데",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.17",
          "form": "이거 모아서 슬리퍼 공짜로 샀어요ㅋㅋㅋ",
          "original_form": "이거 모아서 슬리퍼 공짜로 샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100034964.1.18",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034964.1.19",
          "form": "슬리퍼공짜네요",
          "original_form": "슬리퍼공짜네요",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034964.1.20",
          "form": "네네 타미힐피거 슬리퍼 샀는데",
          "original_form": "네네 타미힐피거 슬리퍼 샀는데",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034964.1.21",
          "form": "가볍고 좋더라구요",
          "original_form": "가볍고 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034964.1.22",
          "form": "와 출석만으로 득템 꿀인데요",
          "original_form": "와 출석만으로 득템 꿀인데요",
