      "utterance": [
        {
          "id": "MDRW2100002022.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002022.1.2",
          "form": "네 영화 좋아합니다!",
          "original_form": "네 영화 좋아합니다!",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002022.1.3",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002022.1.4",
          "form": "저는 주로 판타지나 액션좋아해요",
          "original_form": "저는 주로 판타지나 액션좋아해요",
          "speaker_id": "1",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100002022.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100002022.1.6",
          "form": "저는 스릴러, 액션 좋아해요!",
          "original_form": "저는 스릴러, 액션 좋아해요!",
          "speaker_id": "2",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100002022.1.7",
          "form": "가장 재밌게 본 영화는 뭔가요?",
          "original_form": "가장 재밌게 본 영화는 뭔가요?",
          "speaker_id": "2",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100002022.1.8",
          "form": "최근에 본 영화 중 재밋게 본건 모가디슈에요",
          "original_form": "최근에 본 영화 중 재밋게 본건 모가디슈에요",
          "speaker_id": "1",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100002022.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100002022.1.10",
          "form": "영화관에서 최근에 본건 분노의질주인데 재밌더라구요!",
          "original_form": "영화관에서 최근에 본건 분노의질주인데 재밌더라구요!",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002022.1.11",
          "form": "모가디슈 아직 못봤는데 추천하는 사람이 많네요!",
          "original_form": "모가디슈 아직 못봤는데 추천하는 사람이 많네요!",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100002022.1.12",
          "form": "신파가 없어서 맘에 들엇어요ㅎㅎ",
          "original_form": "신파가 없어서 맘에 들엇어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:31"
        },
        {
          "id": "MDRW2100002022.1.13",
          "form": "오 궁금하네요",
          "original_form": "오 궁금하네요",
          "speaker_id": "2",
          "time": "20210820 10:41"
        },
        {
          "id": "MDRW2100002022.1.14",
          "form": "빨리 가서 봐야겠어요",
          "original_form": "빨리 가서 봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 10:41"
        },
        {
          "id": "MDRW2100002022.1.15",
          "form": "유튜브는 자주 보시나요?",
          "original_form": "유튜브는 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002022.1.16",
          "form": "네 그냥 알고리즘에 뜨는거 보는 편이에요",
          "original_form": "네 그냥 알고리즘에 뜨는거 보는 편이에요",
          "speaker_id": "1",
          "time": "20210820 10:44"
        },
        {
          "id": "MDRW2100002022.1.17",
          "form": "오 저도 추천 영상 타고 계속 보다보면 시간 훅 가더라구요",
          "original_form": "오 저도 추천 영상 타고 계속 보다보면 시간 훅 가더라구요",
          "speaker_id": "2",
          "time": "20210820 10:47"
        },
        {
          "id": "MDRW2100002022.1.18",
          "form": "맞아요 드라마클립이나 영화소개 보면 시간 뚝딱가요",
          "original_form": "맞아요 드라마클립이나 영화소개 보면 시간 뚝딱가요",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002022.1.19",
          "form": "유튜브에 볼게 많아서 좋아요!",
          "original_form": "유튜브에 볼게 많아서 좋아요!",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002022.1.20",
          "form": "넷플릭스나 왓챠 같은 플랫폼도 이용하시나요?",
          "original_form": "넷플릭스나 왓챠 같은 플랫폼도 이용하시나요?",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002022.1.21",
          "form": "네 저는 넷플이랑 왓챠 다 이용중이에요",
          "original_form": "네 저는 넷플이랑 왓챠 다 이용중이에요",
          "speaker_id": "1",
          "time": "20210820 18:18"
        }
      ]
