      "utterance": [
        {
          "id": "MDRW2100011720.1.1",
          "form": "우리가 결혼한지도 5년이 넘었군",
          "original_form": "우리가 결혼한지도 5년이 넘었군",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011720.1.2",
          "form": "전 연애도 일 년 넘어본적이 없어요",
          "original_form": "전 연애도 일 년 넘어본적이 없어요",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011720.1.3",
          "form": "연애관이ㅡ이상했나보지",
          "original_form": "연애관이ㅡ이상했나보지",
          "speaker_id": "1",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011720.1.4",
          "form": "그런데 사랑하는 사람과 오년 넘게 살다니",
          "original_form": "그런데 사랑하는 사람과 오년 넘게 살다니",
          "speaker_id": "2",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011720.1.5",
          "form": "행복이군요",
          "original_form": "행복이군요",
          "speaker_id": "2",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011720.1.6",
          "form": "만난지 7년된게 놀라움",
          "original_form": "만난지 7년된게 놀라움",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011720.1.7",
          "form": "시간이 빨라",
          "original_form": "시간이 빨라",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011720.1.8",
          "form": "이제 여유좀 생기면 데이트도해야지",
          "original_form": "이제 여유좀 생기면 데이트도해야지",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011720.1.9",
          "form": "맞아요 점점 여유는 생기겠지요.",
          "original_form": "맞아요 점점 여유는 생기겠지요.",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011720.1.10",
          "form": "아이가 크면서..",
          "original_form": "아이가 크면서..",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011720.1.11",
          "form": "그렇지 직장에 27살 여사원에게 소개팅을 시켜줬는데",
          "original_form": "그렇지 직장에 27살 여사원에게 소개팅을 시켜줬는데",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011720.1.12",
          "form": "골프 코치를",
          "original_form": "골프 코치를",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011720.1.13",
          "form": "근데 잘 안되었나보더라",
          "original_form": "근데 잘 안되었나보더라",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011720.1.14",
          "form": "마음 맞기가 어디 쉽나",
          "original_form": "마음 맞기가 어디 쉽나",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011720.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011720.1.16",
          "form": "그렇지 특히 소개팅에선",
          "original_form": "그렇지 특히 소개팅에선",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011720.1.17",
          "form": "우리는 진짜 불같은 연애ㅋㅋ",
          "original_form": "우리는 진짜 불같은 연애ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011720.1.18",
          "form": "그랬나요 허허허",
          "original_form": "그랬나요 허허허",
          "speaker_id": "1",
          "time": "20210825 11:26"
        },
        {
          "id": "MDRW2100011720.1.19",
          "form": "기억이 잘...",
          "original_form": "기억이 잘...",
          "speaker_id": "1",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100011720.1.20",
          "form": "연애 근황은 어떠신가요",
          "original_form": "연애 근황은 어떠신가요",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100011720.1.21",
          "form": "연애가 먹는건가요",
          "original_form": "연애가 먹는건가요",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100011720.1.22",
          "form": "네 그렇습니다ㅋㅋ",
          "original_form": "네 그렇습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100011720.1.23",
          "form": "이상형이던 남자가 지나고보니 그냥 이상 이었어",
          "original_form": "이상형이던 남자가 지나고보니 그냥 이상 이었어",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100011720.1.24",
          "form": "ㅋㅋㅋㅋ 이상? 드림?",
          "original_form": "ㅋㅋㅋㅋ 이상? 드림?",
          "speaker_id": "2",
          "time": "20210825 12:02"
        },
