      "utterance": [
        {
          "id": "MDRW2100045126.1.1",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045126.1.2",
          "form": "5성 불 딜러는",
          "original_form": "5성 불 딜러는",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045126.1.3",
          "form": "둘이나 있으면서",
          "original_form": "둘이나 있으면서",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045126.1.4",
          "form": "1티어급으로",
          "original_form": "1티어급으로",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045126.1.5",
          "form": "다이루크 이제",
          "original_form": "다이루크 이제",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045126.1.6",
          "form": "은퇴했어...",
          "original_form": "은퇴했어...",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045126.1.7",
          "form": "언제적 딜러야...",
          "original_form": "언제적 딜러야...",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045126.1.8",
          "form": "아냐 다이루크 나름 평타 기준 4위야",
          "original_form": "아냐 다이루크 나름 평타 기준 4위야",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.9",
          "form": "흑흑 4위라니",
          "original_form": "흑흑 4위라니",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.10",
          "form": "어르신 분발하세요",
          "original_form": "어르신 분발하세요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.11",
          "form": "1위 소 2위 유라고",
          "original_form": "1위 소 2위 유라고",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.12",
          "form": "3위는 라이덴이 차지햇어",
          "original_form": "3위는 라이덴이 차지햇어",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.13",
          "form": "와 유라가 2위야??",
          "original_form": "와 유라가 2위야??",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045126.1.14",
          "form": "ㅇㅇ온리 평타만 보면",
          "original_form": "ㅇㅇ온리 평타만 보면",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045126.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045126.1.16",
          "form": "라이덴도 평타 3위면",
          "original_form": "라이덴도 평타 3위면",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045126.1.17",
          "form": "거의 메인딜러 급인데?",
          "original_form": "거의 메인딜러 급인데?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045126.1.18",
          "form": "그치만 라이덴은",
          "original_form": "그치만 라이덴은",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045126.1.19",
          "form": "평타 안써",
          "original_form": "평타 안써",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.20",
          "form": "걔도 좀 궁 원툴이야",
          "original_form": "걔도 좀 궁 원툴이야",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.22",
          "form": "그리고 내가 보기에 저 기준은",
          "original_form": "그리고 내가 보기에 저 기준은",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.24",
          "form": "풀돌같음ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "풀돌같음ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.25",
          "form": "풀돌기준?",
          "original_form": "풀돌기준?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.26",
          "form": "그럴줄 알앗지 ㅋㅋㅋㅋ",
          "original_form": "그럴줄 알앗지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.27",
          "form": "몰라 톱밥놈들이",
          "original_form": "몰라 톱밥놈들이",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.28",
          "form": "안써놨어",
          "original_form": "안써놨어",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045126.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왠지 풀돌일거같음....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왠지 풀돌일거같음....",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045126.1.30",
          "form": "명함이면 명함기준이라고 먼저 써놧을듯...",
          "original_form": "명함이면 명함기준이라고 먼저 써놧을듯...",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045126.1.31",
          "form": "그니까ㅋㅋㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045126.1.32",
