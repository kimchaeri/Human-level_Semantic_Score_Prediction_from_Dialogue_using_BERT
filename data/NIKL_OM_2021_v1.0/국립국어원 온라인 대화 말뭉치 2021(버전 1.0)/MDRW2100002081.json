      "utterance": [
        {
          "id": "MDRW2100002081.1.1",
          "form": "어떤 연예인 좋아하세요?",
          "original_form": "어떤 연예인 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002081.1.2",
          "form": "요즘에 연예인 너무 많아서 누가 누군지 잘 모르겠어요ㅋㅋㅋ옛날사람이라..",
          "original_form": "요즘에 연예인 너무 많아서 누가 누군지 잘 모르겠어요ㅋㅋㅋ옛날사람이라..",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002081.1.3",
          "form": "옛날 연예인 누구 좋아하세요?",
          "original_form": "옛날 연예인 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002081.1.4",
          "form": "옛날엔 빅뱅을 좋아했었어요..ㅎㅎ",
          "original_form": "옛날엔 빅뱅을 좋아했었어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002081.1.5",
          "form": "그럼 저랑 비슷한 나이대이신...",
          "original_form": "그럼 저랑 비슷한 나이대이신...",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002081.1.6",
          "form": "당시에 원더걸스도 나오던시절 아닌가요",
          "original_form": "당시에 원더걸스도 나오던시절 아닌가요",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002081.1.7",
          "form": "완전 맞아요",
          "original_form": "완전 맞아요",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002081.1.8",
          "form": "비스트와 동방신기 슈퍼주니어..",
          "original_form": "비스트와 동방신기 슈퍼주니어..",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002081.1.9",
          "form": "와 비스트 오랜만에 듣네요",
          "original_form": "와 비스트 오랜만에 듣네요",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002081.1.10",
          "form": "초등학생때 비가 데뷔했었죠",
          "original_form": "초등학생때 비가 데뷔했었죠",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002081.1.11",
          "form": "아~완전 핫했죠 월드스타 비",
          "original_form": "아~완전 핫했죠 월드스타 비",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002081.1.12",
          "form": "옛날 생각 나네요",
          "original_form": "옛날 생각 나네요",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002081.1.13",
          "form": "당시에 스펀지도 봤었는대",
          "original_form": "당시에 스펀지도 봤었는대",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002081.1.14",
          "form": "빛나라 지식의 별!",
          "original_form": "빛나라 지식의 별!",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002081.1.15",
          "form": "서프라이즈도 완전 잘봤었죠",
          "original_form": "서프라이즈도 완전 잘봤었죠",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002081.1.16",
          "form": "맞아요 일요일 12시",
          "original_form": "맞아요 일요일 12시",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002081.1.17",
          "form": "오 시간까지ㅋㅋㅋ",
          "original_form": "오 시간까지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002081.1.18",
          "form": "ㅋㅋㅋ 당시엔 외국인 연기자분들이 신기했어요",
