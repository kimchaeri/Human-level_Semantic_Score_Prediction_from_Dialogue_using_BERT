      "utterance": [
        {
          "id": "MDRW2100028671.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028671.1.2",
          "form": "안녕하세요 동호회나 봉사 하시나용?",
          "original_form": "안녕하세요 동호회나 봉사 하시나용?",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028671.1.3",
          "form": "길고양이 밥주는 봉사 하고 있습니디",
          "original_form": "길고양이 밥주는 봉사 하고 있습니디",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028671.1.4",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028671.1.5",
          "form": "사실 전 지금 하는 건 없구 예전엔 지역아동센터 교육봉사 다녔었어요",
          "original_form": "사실 전 지금 하는 건 없구 예전엔 지역아동센터 교육봉사 다녔었어요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028671.1.6",
          "form": "길고양이 밥ㅠㅠ 좋은 일 하시는구나",
          "original_form": "길고양이 밥ㅠㅠ 좋은 일 하시는구나",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028671.1.7",
          "form": "와 좋은일 하셨군요",
          "original_form": "와 좋은일 하셨군요",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028671.1.8",
          "form": "하고 싶은 봉사활동 있으신가요?",
          "original_form": "하고 싶은 봉사활동 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028671.1.9",
          "form": "저는 길고양이 밥주는 봉사를 하고 싶어졌네요...",
          "original_form": "저는 길고양이 밥주는 봉사를 하고 싶어졌네요...",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028671.1.10",
          "form": "어릴땐 벽화봉사같은 거 하고 싶었었는데",
          "original_form": "어릴땐 벽화봉사같은 거 하고 싶었었는데",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028671.1.11",
          "form": "벽화봉사도 좋은거 같아요",
          "original_form": "벽화봉사도 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028671.1.12",
          "form": "얼마전 밥주던 길고양이는 쉼터로 들어 갔어요",
          "original_form": "얼마전 밥주던 길고양이는 쉼터로 들어 갔어요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028671.1.13",
          "form": "지내는 곳이 공사를 해서",
          "original_form": "지내는 곳이 공사를 해서",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028671.1.14",
          "form": "우왕 쉼터도 있구나",
          "original_form": "우왕 쉼터도 있구나",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028671.1.15",
          "form": "공사하면 완전 위험한데ㅠㅜ 거의 구출하신거네요",
          "original_form": "공사하면 완전 위험한데ㅠㅜ 거의 구출하신거네요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028671.1.16",
          "form": "네 저번주에 쉼터 보내서 넘 다행인ㅠ",
          "original_form": "네 저번주에 쉼터 보내서 넘 다행인ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028671.1.17",
          "form": "하고 싶은 동호회는 있으세요?",
          "original_form": "하고 싶은 동호회는 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028671.1.18",
          "form": "오 전에 글쓰는 소모임은 했었는데",
          "original_form": "오 전에 글쓰는 소모임은 했었는데",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028671.1.19",
          "form": "사회인 동호회는 잘 모르겠어요... 요즘 자전거를 타보고 싶긴 한데",
          "original_form": "사회인 동호회는 잘 모르겠어요... 요즘 자전거를 타보고 싶긴 한데",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028671.1.20",
          "form": "자전거 동호회분들은 다들 너무 본격적이셔서 무섭더라구용",
          "original_form": "자전거 동호회분들은 다들 너무 본격적이셔서 무섭더라구용",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028671.1.21",
          "form": "맞아요 자전거 동호회 분들 가끔씩 보면 장비며 옷이 궁서체 이신...",
          "original_form": "맞아요 자전거 동호회 분들 가끔씩 보면 장비며 옷이 궁서체 이신...",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028671.1.22",
          "form": "저도 자전거 좋아하는 하는데 동호회는 뭔가 무섭습니다ㅎ",
          "original_form": "저도 자전거 좋아하는 하는데 동호회는 뭔가 무섭습니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028671.1.23",
          "form": "마자요ㅋㅋㅋㅋㅋㅋ너무 진지해서 국토 대종주 해야할 거 같고ㅠㅠㅠ",
          "original_form": "마자요ㅋㅋㅋㅋㅋㅋ너무 진지해서 국토 대종주 해야할 거 같고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028671.1.24",
          "form": "선글라스...무서워용...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "선글라스...무서워용...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028671.1.25",
          "form": "그냥 전 천변이나 좀... 타고 피크닉 즐기고 싶을 뿐인데",
          "original_form": "그냥 전 천변이나 좀... 타고 피크닉 즐기고 싶을 뿐인데",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028671.1.26",
          "form": "name1님은 하고싶은 동호회 있으세요?",
          "original_form": "&name1&님은 하고싶은 동호회 있으세요?",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028671.1.27",
          "form": "앗 일단 선글라스에 스피드를 내셔서",
          "original_form": "앗 일단 선글라스에 스피드를 내셔서",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028671.1.28",
          "form": "대화가 안될거 같은ㅠㅠㅋㅋㅋ",
          "original_form": "대화가 안될거 같은ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028671.1.29",
          "form": "저는 영화 동아리 뭔가 들고 싶어요",
          "original_form": "저는 영화 동아리 뭔가 들고 싶어요",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028671.1.30",
          "form": "ㅋㅋㅋㅋㅋ마자요 대화가 중요한데",
          "original_form": "ㅋㅋㅋㅋㅋ마자요 대화가 중요한데",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028671.1.31",
          "form": "와 영화동아리 좋네요 마음이 편해지는...",
          "original_form": "와 영화동아리 좋네요 마음이 편해지는...",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028671.1.32",
          "form": "저 중고등학교 내내 영화/다큐 동아리였는데 추억이 새록새록하네요",
          "original_form": "저 중고등학교 내내 영화/다큐 동아리였는데 추억이 새록새록하네요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028671.1.33",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
