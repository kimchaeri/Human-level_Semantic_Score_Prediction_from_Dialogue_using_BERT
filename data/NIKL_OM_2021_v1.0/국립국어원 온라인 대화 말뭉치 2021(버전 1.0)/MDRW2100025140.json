      "utterance": [
        {
          "id": "MDRW2100025140.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025140.1.2",
          "form": "네 주로 집에서 봐요",
          "original_form": "네 주로 집에서 봐요",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025140.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025140.1.4",
          "form": "코로나 전에는 영화관 자주 갔는데",
          "original_form": "코로나 전에는 영화관 자주 갔는데",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025140.1.5",
          "form": "아무래도 요새는 집에서 ㅎㅎ",
          "original_form": "아무래도 요새는 집에서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025140.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025140.1.7",
          "form": "극장 가본지가ㅠ",
          "original_form": "극장 가본지가ㅠ",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025140.1.8",
          "form": "가~끔 가긴 하는데",
          "original_form": "가~끔 가긴 하는데",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025140.1.9",
          "form": "막상 가면 사람 없어서 또 괜찮더라구요",
          "original_form": "막상 가면 사람 없어서 또 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100025140.1.10",
          "form": "오 괜찮네요",
          "original_form": "오 괜찮네요",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100025140.1.11",
          "form": "전용극장ㅎ",
          "original_form": "전용극장ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100025140.1.12",
          "form": "근데 요새 넷플릭스 티빙 웨이브 다 괜춘해서",
          "original_form": "근데 요새 넷플릭스 티빙 웨이브 다 괜춘해서",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025140.1.13",
          "form": "집도 좋은거 같습니다",
          "original_form": "집도 좋은거 같습니다",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025140.1.14",
          "form": "앗 저도 넷플릭스봐요",
          "original_form": "앗 저도 넷플릭스봐요",
          "speaker_id": "1",
          "time": "20210901 01:41"
        },
        {
          "id": "MDRW2100025140.1.15",
          "form": "뭐 보셨나요 최근에ㅎㅎ",
          "original_form": "뭐 보셨나요 최근에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:45"
        },
        {
          "id": "MDRW2100025140.1.16",
          "form": "전 금쪽이여",
          "original_form": "전 금쪽이여",
          "speaker_id": "1",
          "time": "20210901 01:46"
        },
        {
          "id": "MDRW2100025140.1.17",
          "form": "음 제가 안본거네요ㅎㅎ",
          "original_form": "음 제가 안본거네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:47"
        },
        {
          "id": "MDRW2100025140.1.18",
          "form": "전 킹덤아신전이요 ㅎㅎ",
          "original_form": "전 킹덤아신전이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:47"
        },
        {
          "id": "MDRW2100025140.1.19",
          "form": "아 재밌나요?",
          "original_form": "아 재밌나요?",
          "speaker_id": "1",
