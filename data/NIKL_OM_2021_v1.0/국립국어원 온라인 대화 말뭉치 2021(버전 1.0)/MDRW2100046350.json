        },
        {
          "id": "MDRW2100046350.1.2",
          "form": "안녕하세요! 점심 드셨나요!?",
          "original_form": "안녕하세요! 점심 드셨나요!?",
          "speaker_id": "2",
          "time": "20211019 13:19"
        },
        {
          "id": "MDRW2100046350.1.3",
          "form": "네 맛있게 먹었어요 ㅋ",
          "original_form": "네 맛있게 먹었어요 ㅋ",
          "speaker_id": "1",
          "time": "20211019 13:19"
        },
        {
          "id": "MDRW2100046350.1.4",
          "form": "식사하셨어요??",
          "original_form": "식사하셨어요??",
          "speaker_id": "1",
          "time": "20211019 13:19"
        },
        {
          "id": "MDRW2100046350.1.5",
          "form": "네 저는 햄버거 먹었어요!",
          "original_form": "네 저는 햄버거 먹었어요!",
          "speaker_id": "2",
          "time": "20211019 13:20"
        },
        {
          "id": "MDRW2100046350.1.6",
          "form": "젊은 분이신가봐요 ㅋ",
          "original_form": "젊은 분이신가봐요 ㅋ",
          "speaker_id": "1",
          "time": "20211019 13:20"
        },
        {
          "id": "MDRW2100046350.1.7",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20211019 13:20"
        },
        {
          "id": "MDRW2100046350.1.8",
          "form": "전 짬뽕이요 ㅋ",
          "original_form": "전 짬뽕이요 ㅋ",
          "speaker_id": "1",
          "time": "20211019 13:20"
        },
        {
          "id": "MDRW2100046350.1.9",
          "form": "헉 오늘 짬뽕이랑 햄버거 중에 고민했는데",
          "original_form": "헉 오늘 짬뽕이랑 햄버거 중에 고민했는데",
          "speaker_id": "2",
          "time": "20211019 13:26"
        },
        {
          "id": "MDRW2100046350.1.10",
          "form": "점심 시간은 끝나셨나요?",
          "original_form": "점심 시간은 끝나셨나요?",
          "speaker_id": "2",
          "time": "20211019 13:26"
        },
        {
          "id": "MDRW2100046350.1.11",
          "form": "짬뽕은 언제나 사랑입니다.",
          "original_form": "짬뽕은 언제나 사랑입니다.",
          "speaker_id": "1",
          "time": "20211019 13:26"
        },
        {
          "id": "MDRW2100046350.1.12",
          "form": "저희 1시 30분까지에요",
          "original_form": "저희 1시 30분까지에요",
          "speaker_id": "1",
          "time": "20211019 13:26"
        },
        {
          "id": "MDRW2100046350.1.13",
          "form": "우와 좋은 회사네요..!!",
          "original_form": "우와 좋은 회사네요..!!",
          "speaker_id": "2",
          "time": "20211019 13:27"
        },
        {
          "id": "MDRW2100046350.1.14",
          "form": "회사 다니신지는 얼마나 되셨어요?",
          "original_form": "회사 다니신지는 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20211019 13:27"
        },
        {
          "id": "MDRW2100046350.1.15",
          "form": "09년 하반기에 입사했어요",
          "original_form": "09년 하반기에 입사했어요",
          "speaker_id": "1",
          "time": "20211019 13:28"
        },
        {
          "id": "MDRW2100046350.1.16",
          "form": "헉 완전 오래되셨네요.",
          "original_form": "헉 완전 오래되셨네요.",
          "speaker_id": "2",
          "time": "20211019 13:29"
        },
        {
          "id": "MDRW2100046350.1.17",
          "form": "신입 사원에게 조언 한마디 해주세요 ㅎㅎ",
          "original_form": "신입 사원에게 조언 한마디 해주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 13:29"
        },
        {
          "id": "MDRW2100046350.1.18",
          "form": "제가 늙어서 그래요",
          "original_form": "제가 늙어서 그래요",
          "speaker_id": "1",
          "time": "20211019 13:29"
        },
        {
          "id": "MDRW2100046350.1.19",
          "form": "그냥 무조건 버티는 자가 승리자입이다.",
          "original_form": "그냥 무조건 버티는 자가 승리자입이다.",
