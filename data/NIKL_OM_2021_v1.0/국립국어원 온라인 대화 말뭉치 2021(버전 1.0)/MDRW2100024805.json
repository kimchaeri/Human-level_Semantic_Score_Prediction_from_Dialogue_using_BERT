      "utterance": [
        {
          "id": "MDRW2100024805.1.1",
          "form": "넹 해봤어요",
          "original_form": "넹 해봤어요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024805.1.2",
          "form": "망겜이라던데 할만해요?",
          "original_form": "망겜이라던데 할만해요?",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024805.1.3",
          "form": "하던 사람들만 하니까 정들어서 해요",
          "original_form": "하던 사람들만 하니까 정들어서 해요",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024805.1.4",
          "form": "네? 친구들이랑 같이 하시나봐요",
          "original_form": "네? 친구들이랑 같이 하시나봐요",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024805.1.5",
          "form": "저는 블소는 안해요",
          "original_form": "저는 블소는 안해요",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024805.1.6",
          "form": "저는 테트리스 해요",
          "original_form": "저는 테트리스 해요",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024805.1.7",
          "form": "테트리스랑 퍼즐게임이요",
          "original_form": "테트리스랑 퍼즐게임이요",
          "speaker_id": "2",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100024805.1.8",
          "form": "저는퍼즐겜",
          "original_form": "저는퍼즐겜",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100024805.1.9",
          "form": "프로게이머로는 무릎 알아요",
          "original_form": "프로게이머로는 무릎 알아요",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024805.1.10",
          "form": "무릎 알죠철권",
          "original_form": "무릎 알죠철권",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100024805.1.11",
          "form": "근데 철권도 안해봐서리",
          "original_form": "근데 철권도 안해봐서리",
          "speaker_id": "1",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100024805.1.12",
          "form": "네 저도 티비서 그 게임대결하는거 보았어요",
          "original_form": "네 저도 티비서 그 게임대결하는거 보았어요",
          "speaker_id": "2",
          "time": "20210902 08:12"
        },
        {
          "id": "MDRW2100024805.1.13",
          "form": "네 꽤 매니아 있죠",
          "original_form": "네 꽤 매니아 있죠",
          "speaker_id": "1",
          "time": "20210902 08:17"
        },
        {
          "id": "MDRW2100024805.1.14",
          "form": "그리고 배틀그라운드 정도만 알고있어요",
          "original_form": "그리고 배틀그라운드 정도만 알고있어요",
          "speaker_id": "2",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100024805.1.15",
          "form": "치킨먹자ㅋ 배그 이기면 나오는 말이에요",
          "original_form": "치킨먹자ㅋ 배그 이기면 나오는 말이에요",
          "speaker_id": "1",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100024805.1.16",
          "form": "스타크래프트랑요",
          "original_form": "스타크래프트랑요",
          "speaker_id": "2",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100024805.1.17",
          "form": "스타랑 배그는 달라요",
          "original_form": "스타랑 배그는 달라요",
          "speaker_id": "1",
