      "utterance": [
        {
          "id": "MDRW2100032921.1.1",
          "form": "좋아하는 소설 뭐야?",
          "original_form": "좋아하는 소설 뭐야?",
          "speaker_id": "1",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100032921.1.2",
          "form": "아님 굳이 소설 아니더라도",
          "original_form": "아님 굳이 소설 아니더라도",
          "speaker_id": "1",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100032921.1.3",
          "form": "좋아하는 책",
          "original_form": "좋아하는 책",
          "speaker_id": "1",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100032921.1.4",
          "form": "오오오오오오오옹",
          "original_form": "오오오오오오오옹",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032921.1.5",
          "form": "일단 가장 감명깊게읽은책들이있는데",
          "original_form": "일단 가장 감명깊게읽은책들이있는데",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032921.1.6",
          "form": "하나는 미비포유고",
          "original_form": "하나는 미비포유고",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032921.1.7",
          "form": "하나는 파피용...!!!!!!",
          "original_form": "하나는 파피용...!!!!!!",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032921.1.8",
          "form": "베르나르베르베르는......천재일까...",
          "original_form": "베르나르베르베르는......천재일까...",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100032921.1.9",
          "form": "파피용은 나도 읽어봄",
          "original_form": "파피용은 나도 읽어봄",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100032921.1.10",
          "form": "사실 베르나르 소설 많이 안 봤는데",
          "original_form": "사실 베르나르 소설 많이 안 봤는데",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100032921.1.11",
          "form": "넘 오지지않냐",
          "original_form": "넘 오지지않냐",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100032921.1.12",
          "form": "파피용이 거의 유일할걸",
          "original_form": "파피용이 거의 유일할걸",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100032921.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100032921.1.14",
          "form": "어땠어",
          "original_form": "어땠어",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100032921.1.15",
          "form": "책은",
          "original_form": "책은",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100032921.1.16",
          "form": "재밌었엉?!",
          "original_form": "재밌었엉?!",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100032921.1.17",
          "form": "재밌었던 거 같아",
          "original_form": "재밌었던 거 같아",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100032921.1.18",
          "form": "굉장히 빨리 읽었던듯..",
          "original_form": "굉장히 빨리 읽었던듯..",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100032921.1.19",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100032921.1.20",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100032921.1.21",
          "form": "두꺼운데",
          "original_form": "두꺼운데",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100032921.1.22",
          "form": "술술읽혀진다니깐",
          "original_form": "술술읽혀진다니깐",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100032921.1.23",
          "form": "그런 글이 진짜 잘쓴거야...",
          "original_form": "그런 글이 진짜 잘쓴거야...",
          "speaker_id": "1",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100032921.1.24",
          "form": "살인자의 기억법 읽어봤어??",
          "original_form": "살인자의 기억법 읽어봤어??",
          "speaker_id": "1",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100032921.1.25",
          "form": "허거거걱 나 아직 영화만봤어",
          "original_form": "허거거걱 나 아직 영화만봤어",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100032921.1.26",
          "form": "우허는 영화봤어?",
          "original_form": "우허는 영화봤어?",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100032921.1.27",
          "form": "책이랑 비슷하려나",
          "original_form": "책이랑 비슷하려나",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100032921.1.28",
          "form": "아니 나는 책만 ㅋㅋㅋ",
          "original_form": "아니 나는 책만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032921.1.29",
          "form": "꽤 긴 책인데 앉은자리에서 다 읽었어",
          "original_form": "꽤 긴 책인데 앉은자리에서 다 읽었어",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032921.1.30",
          "form": "그... 정확히 말하면 누운 자리",
          "original_form": "그... 정확히 말하면 누운 자리",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032921.1.31",
          "form": "누워서 읽으면안댜",
          "original_form": "누워서 읽으면안댜",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100032921.1.32",
          "form": "눈나빠져",
          "original_form": "눈나빠져",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100032921.1.33",
          "form": "오옹오옹",
          "original_form": "오옹오옹",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100032921.1.34",
          "form": "내용이 그래도비슷하려나",
          "original_form": "내용이 그래도비슷하려나",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100032921.1.35",
          "form": "마지막에",
          "original_form": "마지막에",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100032921.1.36",
          "form": "설경구가 할아버지로나오고 김남길이 살인자로나오자나",
          "original_form": "설경구가 할아버지로나오고 김남길이 살인자로나오자나",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100032921.1.37",
          "form": "죽었던 남기르가 살아돌아오는듯하면서 보여줬는데",
          "original_form": "죽었던 남기르가 살아돌아오는듯하면서 보여줬는데",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100032921.1.38",
          "form": "책도 그렇게끝나??",
          "original_form": "책도 그렇게끝나??",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032921.1.39",
          "form": "어...",
          "original_form": "어...",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032921.1.40",
          "form": "그 여자 아빠가 범인이고",
          "original_form": "그 여자 아빠가 범인이고",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032921.1.41",
          "form": "으앙",
          "original_form": "으앙",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032921.1.42",
          "form": "딸이 사실 자기 딸이 아니었던 거 같은데",
          "original_form": "딸이 사실 자기 딸이 아니었던 거 같은데",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032921.1.43",
          "form": "책을 읽어야게ㅛ다",
          "original_form": "책을 읽어야게ㅛ다",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100032921.1.44",
          "form": "내용이다르구먼",
          "original_form": "내용이다르구먼",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100032921.1.45",
          "form": "헉 내가 너무 스포한 거 아니야??",
          "original_form": "헉 내가 너무 스포한 거 아니야??",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100032921.1.46",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100032921.1.47",
          "form": "아니야...",
          "original_form": "아니야...",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100032921.1.48",
          "form": "(훌쩍",
          "original_form": "(훌쩍",
          "speaker_id": "2",
          "time": "20210906 15:44"
        }
      ]
    }
  ]
