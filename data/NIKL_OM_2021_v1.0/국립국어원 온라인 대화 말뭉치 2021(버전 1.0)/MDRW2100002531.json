      "utterance": [
        {
          "id": "MDRW2100002531.1.1",
          "form": "요즘 이게 여름인가 싶을 정도로 날씨가 선선해지던데",
          "original_form": "요즘 이게 여름인가 싶을 정도로 날씨가 선선해지던데",
          "speaker_id": "1",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002531.1.2",
          "form": "어제 그쪽에 비 왔어?",
          "original_form": "어제 그쪽에 비 왔어?",
          "speaker_id": "1",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002531.1.3",
          "form": "비는 안왔고",
          "original_form": "비는 안왔고",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.4",
          "form": "흐리기만 했어",
          "original_form": "흐리기만 했어",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.5",
          "form": "근데 확실히 슬슬 여름 끝나갈려고 하는거같아",
          "original_form": "근데 확실히 슬슬 여름 끝나갈려고 하는거같아",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.6",
          "form": "진짜 더운거 거의다 풀린거같음",
          "original_form": "진짜 더운거 거의다 풀린거같음",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.7",
          "form": "그니까 창문 열고 선풍기 틀고 자다가 추워서 깼다니까",
          "original_form": "그니까 창문 열고 선풍기 틀고 자다가 추워서 깼다니까",
          "speaker_id": "1",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.8",
          "form": "근데 비바람 들어오고 있더라",
          "original_form": "근데 비바람 들어오고 있더라",
          "speaker_id": "1",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002531.1.9",
          "form": "습기 풍만",
          "original_form": "습기 풍만",
          "speaker_id": "1",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002531.1.10",
          "form": "거기는 비왔구나",
          "original_form": "거기는 비왔구나",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002531.1.11",
          "form": "여기도 오늘 비올거같기도 하고",
          "original_form": "여기도 오늘 비올거같기도 하고",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002531.1.12",
          "form": "2일 연속으로 흐려 날씨거",
          "original_form": "2일 연속으로 흐려 날씨거",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002531.1.13",
          "form": "날씨가",
          "original_form": "날씨가",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002531.1.14",
          "form": "여기 갑자기 비가 우르르 오다가 그치더니 마른하늘에 날벼락도 아니고 막 천둥이 치더라",
          "original_form": "여기 갑자기 비가 우르르 오다가 그치더니 마른하늘에 날벼락도 아니고 막 천둥이 치더라",
          "speaker_id": "1",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002531.1.15",
          "form": "시끄러워서 중간에 몇번 깼어",
          "original_form": "시끄러워서 중간에 몇번 깼어",
          "speaker_id": "1",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002531.1.16",
          "form": "너희쪽이 바닷가 근처라서 날씨가",
          "original_form": "너희쪽이 바닷가 근처라서 날씨가",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002531.1.17",
          "form": "좀 많이 변덕스러운거 같기도 하다",
          "original_form": "좀 많이 변덕스러운거 같기도 하다",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002531.1.18",
          "form": "근데 확실히 요즘날씨가 좀 이상하긴해",
          "original_form": "근데 확실히 요즘날씨가 좀 이상하긴해",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002531.1.19",
          "form": "그러고보니 아직 태풍도 안왔네",
          "original_form": "그러고보니 아직 태풍도 안왔네",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002531.1.20",
          "form": "다 일본쪽으로 갔잖아",
          "original_form": "다 일본쪽으로 갔잖아",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002531.1.21",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002531.1.22",
          "form": "우리쪽이 기압이라해야되나 그게 워낙 높아서 옆으로 휜걸수도 있겠다",
          "original_form": "우리쪽이 기압이라해야되나 그게 워낙 높아서 옆으로 휜걸수도 있겠다",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002531.1.23",
          "form": "맨날 옆에 미세먼지 불어오고 반대쪽은 지진이랑 방사능이라서 안좋앗는데",
          "original_form": "맨날 옆에 미세먼지 불어오고 반대쪽은 지진이랑 방사능이라서 안좋앗는데",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002531.1.24",
          "form": "이런거보면 은근 좋음",
          "original_form": "이런거보면 은근 좋음",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002531.1.25",
          "form": "그치 물론",
          "original_form": "그치 물론",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002531.1.26",
          "form": "너무 덥긴했어",
          "original_form": "너무 덥긴했어",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002531.1.27",
          "form": "그러니까 태풍 다 옆나라한테 가라~",
          "original_form": "그러니까 태풍 다 옆나라한테 가라~",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002531.1.28",
          "form": "맨날 여름마다 강력 태풍하면서 우리집 창문 깨질까 홍수 날까 걱정하는것도 실증나",
          "original_form": "맨날 여름마다 강력 태풍하면서 우리집 창문 깨질까 홍수 날까 걱정하는것도 실증나",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002531.1.29",
          "form": "너네 지역이 태풍이 직격타긴하니까",
          "original_form": "너네 지역이 태풍이 직격타긴하니까",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002531.1.30",
          "form": "안오는게 훨 좋긴하지",
          "original_form": "안오는게 훨 좋긴하지",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002531.1.31",
          "form": "그래서 작년에 우리 뒷베란다에 물난리 났잖아",
          "original_form": "그래서 작년에 우리 뒷베란다에 물난리 났잖아",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.32",
          "form": "아파트가 오래되서 그렇다지만",
          "original_form": "아파트가 오래되서 그렇다지만",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.33",
          "form": "식겁했어",
          "original_form": "식겁했어",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.34",
          "form": "맞아 그거 사진봤는데",
          "original_form": "맞아 그거 사진봤는데",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.35",
          "form": "엄청 물난리 난거보고 놀랬지",
          "original_form": "엄청 물난리 난거보고 놀랬지",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.36",
          "form": "그때 넌 놀란게 아니라 웃겨 죽을려고 하던데",
          "original_form": "그때 넌 놀란게 아니라 웃겨 죽을려고 하던데",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002531.1.37",
          "form": "허허 그럴수있지",
          "original_form": "허허 그럴수있지",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002531.1.38",
          "form": "놀랬긴했어",
          "original_form": "놀랬긴했어",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002531.1.39",
          "form": "물론 웃음도 나오기도했고",
          "original_form": "물론 웃음도 나오기도했고",
          "speaker_id": "2",
          "time": "20210820 13:27"
        }
      ]
    }
