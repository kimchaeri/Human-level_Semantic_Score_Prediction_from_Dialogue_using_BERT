        },
        {
          "id": "MDRW2100042698.1.3",
          "form": "우리 어제 본 영화 정말 재밌었지",
          "original_form": "우리 어제 본 영화 정말 재밌었지",
          "speaker_id": "2",
          "time": "20210907 01:47"
        },
        {
          "id": "MDRW2100042698.1.4",
          "form": "맞아 코엑스에서 오랜만에 영화 봤네",
          "original_form": "맞아 코엑스에서 오랜만에 영화 봤네",
          "speaker_id": "1",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042698.1.5",
          "form": "나는 사실 제목 기대를 하나도 안 했거든",
          "original_form": "나는 사실 제목 기대를 하나도 안 했거든",
          "speaker_id": "2",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042698.1.6",
          "form": "제목이 어렵지 샹치와 탤링 지였나",
          "original_form": "제목이 어렵지 샹치와 탤링 지였나",
          "speaker_id": "1",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042698.1.7",
          "form": "남자 주인공이 처음에는 안 잘생겨 보였어",
          "original_form": "남자 주인공이 처음에는 안 잘생겨 보였어",
          "speaker_id": "2",
          "time": "20210907 01:48"
        },
        {
          "id": "MDRW2100042698.1.8",
          "form": "사실 양조위 보러 간 것",
          "original_form": "사실 양조위 보러 간 것",
          "speaker_id": "1",
          "time": "20210907 01:49"
        },
        {
          "id": "MDRW2100042698.1.9",
          "form": "내 생각보다 멋있더라",
          "original_form": "내 생각보다 멋있더라",
          "speaker_id": "2",
          "time": "20210907 01:49"
        },
        {
          "id": "MDRW2100042698.1.10",
          "form": "다른 양조위 영화도 봐 보려고",
          "original_form": "다른 양조위 영화도 봐 보려고",
          "speaker_id": "1",
          "time": "20210907 01:49"
        },
        {
          "id": "MDRW2100042698.1.11",
          "form": "눈빛이 좋으시더라",
          "original_form": "눈빛이 좋으시더라",
          "speaker_id": "2",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042698.1.12",
          "form": "영화도 재밌었어",
          "original_form": "영화도 재밌었어",
          "speaker_id": "1",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042698.1.13",
          "form": "역시 마블",
          "original_form": "역시 마블",
          "speaker_id": "2",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042698.1.14",
          "form": "후속편도 기대되더라",
          "original_form": "후속편도 기대되더라",
          "speaker_id": "1",
          "time": "20210907 01:51"
        },
        {
          "id": "MDRW2100042698.1.15",
