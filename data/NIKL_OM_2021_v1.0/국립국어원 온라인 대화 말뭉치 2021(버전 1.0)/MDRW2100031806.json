      "utterance": [
        {
          "id": "MDRW2100031806.1.1",
          "form": "여행 많이 가보셨나요?",
          "original_form": "여행 많이 가보셨나요?",
          "speaker_id": "2",
          "time": "20210905 01:00"
        },
        {
          "id": "MDRW2100031806.1.2",
          "form": "저는 국내여행을 주로 많이갔었어요",
          "original_form": "저는 국내여행을 주로 많이갔었어요",
          "speaker_id": "1",
          "time": "20210905 01:04"
        },
        {
          "id": "MDRW2100031806.1.3",
          "form": "오 어디가 가장 기억에 남나요?",
          "original_form": "오 어디가 가장 기억에 남나요?",
          "speaker_id": "2",
          "time": "20210905 01:06"
        },
        {
          "id": "MDRW2100031806.1.4",
          "form": "저는 김해에 캠핑간게 코로나 전 마지막 여행여섴ㅋㅋ",
          "original_form": "저는 김해에 캠핑간게 코로나 전 마지막 여행여섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:07"
        },
        {
          "id": "MDRW2100031806.1.5",
          "form": "그게 제일 기억에 남네요",
          "original_form": "그게 제일 기억에 남네요",
          "speaker_id": "1",
          "time": "20210905 01:08"
        },
        {
          "id": "MDRW2100031806.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 01:08"
        },
        {
          "id": "MDRW2100031806.1.7",
          "form": "오오 김해는 한번도 안가본 것 같아요",
          "original_form": "오오 김해는 한번도 안가본 것 같아요",
          "speaker_id": "2",
          "time": "20210905 01:20"
        },
        {
          "id": "MDRW2100031806.1.8",
          "form": "김해는 어디가 명소인가요??",
          "original_form": "김해는 어디가 명소인가요??",
          "speaker_id": "2",
          "time": "20210905 01:20"
        },
        {
          "id": "MDRW2100031806.1.9",
          "form": "저는 아무래도 제주도가 기억에 남는 것 같아요",
          "original_form": "저는 아무래도 제주도가 기억에 남는 것 같아요",
          "speaker_id": "2",
          "time": "20210905 01:20"
        },
        {
          "id": "MDRW2100031806.1.10",
          "form": "저는 김해태마파크 캠핑장에 갔었는데 좋았어요",
          "original_form": "저는 김해태마파크 캠핑장에 갔었는데 좋았어요",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031806.1.11",
          "form": "제주도도 엄청 좋죠",
          "original_form": "제주도도 엄청 좋죠",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031806.1.12",
          "form": "기억에 남는 곳 있으세요?",
          "original_form": "기억에 남는 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031806.1.13",
          "form": "오 테마파크면 진짜 재밌었을 것 같아요!",
          "original_form": "오 테마파크면 진짜 재밌었을 것 같아요!",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100031806.1.14",
          "form": "전 에코랜드 좋았어요 사진찍기도 좋고 넓어서 볼 것도 많더라구요",
          "original_form": "전 에코랜드 좋았어요 사진찍기도 좋고 넓어서 볼 것도 많더라구요",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031806.1.15",
          "form": "저는 제주도에 갔을 때 드라이빙 하는 것도 좋더라고요",
          "original_form": "저는 제주도에 갔을 때 드라이빙 하는 것도 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031806.1.16",
          "form": "바깥 풍경이 새로운 느낌이여서 그런지",
          "original_form": "바깥 풍경이 새로운 느낌이여서 그런지",
          "speaker_id": "1",
          "time": "20210905 01:31"
        },
        {
          "id": "MDRW2100031806.1.17",
          "form": "맞아요 해안도로 달리는 것 자체가 좋아요",
          "original_form": "맞아요 해안도로 달리는 것 자체가 좋아요",
          "speaker_id": "2",
          "time": "20210905 01:39"
        },
        {
          "id": "MDRW2100031806.1.18",
          "form": "그냥 뻥뚫리는 기분ㅎㅎ",
          "original_form": "그냥 뻥뚫리는 기분ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 01:39"
        },
        {
          "id": "MDRW2100031806.1.19",
          "form": "그 동해안 쪽 국도도 바다보면서 드라이빙하기 좋다고 하더라고요",
          "original_form": "그 동해안 쪽 국도도 바다보면서 드라이빙하기 좋다고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 01:43"
        },
        {
          "id": "MDRW2100031806.1.20",
          "form": "오 역시 바다 근처에서 뭘 하든 좋은 것 같아요",
          "original_form": "오 역시 바다 근처에서 뭘 하든 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210905 01:50"
        },
        {
          "id": "MDRW2100031806.1.21",
          "form": "맛있는 것도 많구요",
          "original_form": "맛있는 것도 많구요",
          "speaker_id": "2",
          "time": "20210905 01:50"
        },
        {
          "id": "MDRW2100031806.1.22",
          "form": "맞아요 해산물요리도 맛있는게 엄청 많으니까요",
          "original_form": "맞아요 해산물요리도 맛있는게 엄청 많으니까요",
          "speaker_id": "1",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100031806.1.23",
          "form": "해외에 가보신적 있으세요?",
          "original_form": "해외에 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100031806.1.24",
          "form": "아니요 해외는 아직 가본적이 없어요ㅠ",
          "original_form": "아니요 해외는 아직 가본적이 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:35"
        }
      ]
    }
