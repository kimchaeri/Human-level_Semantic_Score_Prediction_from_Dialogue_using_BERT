      "utterance": [
        {
          "id": "MDRW2100018746.1.1",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018746.1.2",
          "form": "지방이라 아직...!",
          "original_form": "지방이라 아직...!",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018746.1.3",
          "form": "중국도 안 가봤지만",
          "original_form": "중국도 안 가봤지만",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018746.1.4",
          "form": "중국인들 아침에 가볍게 콩물에 빵 담가 먹거든요",
          "original_form": "중국인들 아침에 가볍게 콩물에 빵 담가 먹거든요",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018746.1.5",
          "form": "교수님들이 그쪽으로 많이 가셔서 이야기는 많이 들어봤어요",
          "original_form": "교수님들이 그쪽으로 많이 가셔서 이야기는 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018746.1.6",
          "form": "설탕 탄 콩물이요! 그 느낌이려나요",
          "original_form": "설탕 탄 콩물이요! 그 느낌이려나요",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.7",
          "form": "헉 세상에 저희보다 콩물에 진심이네요",
          "original_form": "헉 세상에 저희보다 콩물에 진심이네요",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.8",
          "form": "근데 많이 묽어요",
          "original_form": "근데 많이 묽어요",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.9",
          "form": "생각보다 괜찮더라고요",
          "original_form": "생각보다 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.10",
          "form": "아아 궁금하네요 ㅎㅎ",
          "original_form": "아아 궁금하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.11",
          "form": "그거 생각하면 제 입맛에도 맞을듯ㅎ",
          "original_form": "그거 생각하면 제 입맛에도 맞을듯ㅎ",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.12",
          "form": "나중에 코로나 끝나면 함 가보고 싶어요",
          "original_form": "나중에 코로나 끝나면 함 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018746.1.13",
          "form": "차이나타운만 가도 아마 팔 거예요",
          "original_form": "차이나타운만 가도 아마 팔 거예요",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.14",
          "form": "아마 비슷할 것 같아요",
          "original_form": "아마 비슷할 것 같아요",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.15",
          "form": "호불호가 갈리긴 합니다ㅎ",
          "original_form": "호불호가 갈리긴 합니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.16",
          "form": "나중에 올라가면 먹어봐야겠어요",
          "original_form": "나중에 올라가면 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.17",
          "form": "빵은 설탕 안친 꽈배기? 처럼 담백했어요",
          "original_form": "빵은 설탕 안친 꽈배기? 처럼 담백했어요",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.18",
          "form": "그땐 설탕을 와르르...",
          "original_form": "그땐 설탕을 와르르...",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018746.1.20",
          "form": "ㅎㅎ 국물이 달려면 확 달아야죠",
          "original_form": "ㅎㅎ 국물이 달려면 확 달아야죠",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018746.1.21",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:44"
        }
      ]
