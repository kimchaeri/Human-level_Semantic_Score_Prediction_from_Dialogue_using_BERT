      "utterance": [
        {
          "id": "MDRW2100030580.1.1",
          "form": "근데 어제 누가 갈궜어요",
          "original_form": "근데 어제 누가 갈궜어요",
          "speaker_id": "1",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030580.1.2",
          "form": "아니 누가 그랬나요!",
          "original_form": "아니 누가 그랬나요!",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030580.1.3",
          "form": "근데 아마 name1님이 잘못 하셨을 거예요ㅇ.ㅇ",
          "original_form": "근데 아마 &name1&님이 잘못 하셨을 거예요ㅇ.ㅇ",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030580.1.4",
          "form": "ㅋㅋ술쟁이 취객이 괴롭힌거 같은데요??",
          "original_form": "ㅋㅋ술쟁이 취객이 괴롭힌거 같은데요??",
          "speaker_id": "1",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030580.1.5",
          "form": "저런..취객한테 시비 거셨나요",
          "original_form": "저런..취객한테 시비 거셨나요",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030580.1.6",
          "form": "육포취객이요?",
          "original_form": "육포취객이요?",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030580.1.7",
          "form": "좀 그런거 같기도",
          "original_form": "좀 그런거 같기도",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030580.1.8",
          "form": "ㅉㅉ 자업자득이죠!",
          "original_form": "ㅉㅉ 자업자득이죠!",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030580.1.9",
          "form": "지금도 업무중인가요ㅜ",
          "original_form": "지금도 업무중인가요ㅜ",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030580.1.10",
          "form": "네? 9시에 집왔는데요",
          "original_form": "네? 9시에 집왔는데요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100030580.1.11",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030580.1.12",
          "form": "아 5시에 기상한다는게",
          "original_form": "아 5시에 기상한다는게",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100030580.1.13",
          "form": "기상해서 인수인계하고 집에 온단 거였나요?",
          "original_form": "기상해서 인수인계하고 집에 온단 거였나요?",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100030580.1.14",
          "form": "제가 거기 시스템을 몰라서리ㅜ",
          "original_form": "제가 거기 시스템을 몰라서리ㅜ",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100030580.1.15",
          "form": "음 주야비휴인데",
          "original_form": "음 주야비휴인데",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100030580.1.16",
          "form": "주는 9시부터 6시\r\n야비는 6시부터 다음날 9시",
          "original_form": "주는 9시부터 6시\r\n야비는 6시부터 다음날 9시",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030580.1.17",
          "form": "근데 지하철 운행 끝나면 휴게시간 있어서 그때 자는거에요 ㅎㅎ",
          "original_form": "근데 지하철 운행 끝나면 휴게시간 있어서 그때 자는거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030580.1.18",
          "form": "아 그래서ㅎㅎ",
          "original_form": "아 그래서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030580.1.19",
          "form": "금 코로나로 막차 시간 앞당겨진게 그나마 다행이네요!",
          "original_form": "금 코로나로 막차 시간 앞당겨진게 그나마 다행이네요!",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100030580.1.20",
          "form": "ㅋㅋㅋ이게 우리는 열받는게",
          "original_form": "ㅋㅋㅋ이게 우리는 열받는게",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030580.1.21",
          "form": "원래 열두시였는데 명박이가 1시로 연장한겁니다......",
          "original_form": "원래 열두시였는데 명박이가 1시로 연장한겁니다......",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100030580.1.22",
          "form": "단축이 아니라 연장 폐지에요 ㅎㅎㅎㅎ",
          "original_form": "단축이 아니라 연장 폐지에요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100030580.1.23",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100030580.1.24",
          "form": "그럼 연장 중지냐 연장 폐지냐가 포인트인데요",
          "original_form": "그럼 연장 중지냐 연장 폐지냐가 포인트인데요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100030580.1.25",
          "form": "아 그치 지금은 연장 중단이지 ㅋㅋ",
          "original_form": "아 그치 지금은 연장 중단이지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100030580.1.26",
          "form": "1년 넘어서 폐지된줄요ㅋㅋ",
          "original_form": "1년 넘어서 폐지된줄요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100030580.1.27",
          "form": "ㅎㅎ 코로나가 하나라도 긍정적 효과가 있다니 다행입니당",
          "original_form": "ㅎㅎ 코로나가 하나라도 긍정적 효과가 있다니 다행입니당",
          "speaker_id": "2",
