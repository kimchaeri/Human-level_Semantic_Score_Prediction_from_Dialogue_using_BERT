      "utterance": [
        {
          "id": "MDRW2100043536.1.1",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043536.1.2",
          "form": "자주 하진 않는데 최근엔 컴퓨터랑 옷 등 여러 개 샀어요",
          "original_form": "자주 하진 않는데 최근엔 컴퓨터랑 옷 등 여러 개 샀어요",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043536.1.3",
          "form": "컴퓨터면 돈 많이 나가셨겠어요",
          "original_form": "컴퓨터면 돈 많이 나가셨겠어요",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043536.1.4",
          "form": "듀얼모니터에 본체까지 80만원 정도요 ㅎㅎ",
          "original_form": "듀얼모니터에 본체까지 80만원 정도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043536.1.5",
          "form": "우와 듀얼모니터 듀얼로 쓰면 훨씬 좋은가요?",
          "original_form": "우와 듀얼모니터 듀얼로 쓰면 훨씬 좋은가요?",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043536.1.6",
          "form": "아무래도 화면이 넓어서 문서작업할 때 편하더라구요",
          "original_form": "아무래도 화면이 넓어서 문서작업할 때 편하더라구요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043536.1.7",
          "form": "오 그렇겠어요. 집에서도 문서작업 많이 하시나봐요",
          "original_form": "오 그렇겠어요. 집에서도 문서작업 많이 하시나봐요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043536.1.8",
          "form": "저 프리랜서라 재택이거든요ㅎㅎ",
          "original_form": "저 프리랜서라 재택이거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043536.1.9",
          "form": "앗 그러시군요!",
          "original_form": "앗 그러시군요!",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043536.1.10",
          "form": "옷은 인터넷 쇼핑하셨나요?",
          "original_form": "옷은 인터넷 쇼핑하셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043536.1.11",
          "form": "아니요, 탑텐 오프라인 매장이었어요",
          "original_form": "아니요, 탑텐 오프라인 매장이었어요",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043536.1.12",
          "form": "카카오 콜라보 티셔츠ㅋㅋㅋ",
          "original_form": "카카오 콜라보 티셔츠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043536.1.13",
          "form": "카카오 콜라보도 했어요? 와 몰랐어요",
          "original_form": "카카오 콜라보도 했어요? 와 몰랐어요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043536.1.14",
          "form": "그렇더라구요ㅋㅋㅋ 라이언 티 두장 질렀어요 짱귀",
          "original_form": "그렇더라구요ㅋㅋㅋ 라이언 티 두장 질렀어요 짱귀",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043536.1.15",
          "form": "라이언 귀여웠겠다! 요즘 밖에 잘 안 돌아다녀서 소식이 늦네요",
          "original_form": "라이언 귀여웠겠다! 요즘 밖에 잘 안 돌아다녀서 소식이 늦네요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043536.1.16",
          "form": "ㅎㅎㅎ\r\nname1님은 지르신 거 있으세요??",
          "original_form": "ㅎㅎㅎ\r\n&name1&님은 지르신 거 있으세요??",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043536.1.17",
          "form": "저는 곧 지를 거 있어요",
          "original_form": "저는 곧 지를 거 있어요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043536.1.18",
          "form": "옷 사려고요",
          "original_form": "옷 사려고요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043536.1.19",
          "form": "오, 가을맞이인가요??",
          "original_form": "오, 가을맞이인가요??",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043536.1.20",
          "form": "곧 친구 결혼식이 있어서요ㅋㅋㅋㅋ",
          "original_form": "곧 친구 결혼식이 있어서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043536.1.21",
          "form": "오, 예쁘게 입고 가야겠네요!!",
          "original_form": "오, 예쁘게 입고 가야겠네요!!",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043536.1.22",
          "form": "네 그래서 인터넷 쇼핑몰 뒤적거리고 있어요",
          "original_form": "네 그래서 인터넷 쇼핑몰 뒤적거리고 있어요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043536.1.23",
          "form": "친구들 중에 첫 결혼식이라 잘 가늠이 안되여",
          "original_form": "친구들 중에 첫 결혼식이라 잘 가늠이 안되여",
          "speaker_id": "1",
          "time": "20210907 19:51"
        }
      ]
    }
