      "utterance": [
        {
          "id": "MDRW2100008759.1.1",
          "form": "아이가 몇인가요",
          "original_form": "아이가 몇인가요",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008759.1.2",
          "form": "2명이요",
          "original_form": "2명이요",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008759.1.3",
          "form": "저도 애둘맘 입니다",
          "original_form": "저도 애둘맘 입니다",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008759.1.4",
          "form": "저는 5 6살 연년생이예요 ㅎ",
          "original_form": "저는 5 6살 연년생이예요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008759.1.5",
          "form": "연년생 힘들겠어요",
          "original_form": "연년생 힘들겠어요",
          "speaker_id": "1",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100008759.1.6",
          "form": "힘들기도한데 좋은점도 있는거같아요",
          "original_form": "힘들기도한데 좋은점도 있는거같아요",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100008759.1.7",
          "form": "아이들은 모두 장단점이 있어요",
          "original_form": "아이들은 모두 장단점이 있어요",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100008759.1.8",
          "form": "둘이 같이 크니 어딜가도 같이 보내고 \r\n집에서도 둘이 잘놀아요",
          "original_form": "둘이 같이 크니 어딜가도 같이 보내고 \r\n집에서도 둘이 잘놀아요",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100008759.1.9",
          "form": "장점이 더많죠",
          "original_form": "장점이 더많죠",
          "speaker_id": "1",
          "time": "20210823 06:25"
        },
        {
          "id": "MDRW2100008759.1.10",
          "form": "네 첨엔 후회했는데",
          "original_form": "네 첨엔 후회했는데",
          "speaker_id": "2",
          "time": "20210823 06:40"
        },
        {
          "id": "MDRW2100008759.1.11",
          "form": "지금은 괜찮은거같기도하더라구요",
          "original_form": "지금은 괜찮은거같기도하더라구요",
          "speaker_id": "2",
          "time": "20210823 06:41"
        },
        {
          "id": "MDRW2100008759.1.12",
          "form": "하나보단 둘이 낫죠",
          "original_form": "하나보단 둘이 낫죠",
          "speaker_id": "1",
          "time": "20210823 06:50"
        },
        {
          "id": "MDRW2100008759.1.13",
          "form": "그렇더라구요 아이들이 몇살이세요?",
          "original_form": "그렇더라구요 아이들이 몇살이세요?",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100008759.1.14",
          "form": "저는 중 고생 이예요",
          "original_form": "저는 중 고생 이예요",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100008759.1.15",
          "form": "다 키우셨네요 ㅎ",
          "original_form": "다 키우셨네요 ㅎ",
          "speaker_id": "2",
