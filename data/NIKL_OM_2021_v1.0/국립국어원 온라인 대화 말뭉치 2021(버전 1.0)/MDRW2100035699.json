      "utterance": [
        {
          "id": "MDRW2100035699.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035699.1.2",
          "form": "운동 밤에 공원 뛰고옵니다!",
          "original_form": "운동 밤에 공원 뛰고옵니다!",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035699.1.3",
          "form": "name2님 하시나요?",
          "original_form": "&name2&님 하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035699.1.4",
          "form": "저는 아침에 걷기해요!",
          "original_form": "저는 아침에 걷기해요!",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100035699.1.5",
          "form": "밤에 달리기라니.. 전 무서워서 밤엔 못나가겠더라구요",
          "original_form": "밤에 달리기라니.. 전 무서워서 밤엔 못나가겠더라구요",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100035699.1.6",
          "form": "아 저도무서워서 6시에 가요ㅎㅎ",
          "original_form": "아 저도무서워서 6시에 가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035699.1.7",
          "form": "여름엔7시에갔는데  해가짧아져서",
          "original_form": "여름엔7시에갔는데  해가짧아져서",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035699.1.8",
          "form": "일찍어두워지더라고요",
          "original_form": "일찍어두워지더라고요",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035699.1.9",
          "form": "백신맞으셨어요??",
          "original_form": "백신맞으셨어요??",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035699.1.10",
          "form": "아항! 맞아요ㅜ 일찍 어두워져요",
          "original_form": "아항! 맞아요ㅜ 일찍 어두워져요",
          "speaker_id": "2",
          "time": "20210830 00:16"
        },
        {
          "id": "MDRW2100035699.1.11",
          "form": "아직이요 맞으셨어요?",
          "original_form": "아직이요 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210830 00:16"
        },
        {
          "id": "MDRW2100035699.1.12",
          "form": "네 저는 저번주에! 맞았어요",
          "original_form": "네 저는 저번주에! 맞았어요",
          "speaker_id": "1",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100035699.1.13",
          "form": "오! 어떠셨어요?",
          "original_form": "오! 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035699.1.14",
          "form": "친구는 팔이 부었다더라구요",
          "original_form": "친구는 팔이 부었다더라구요",
          "speaker_id": "2",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035699.1.15",
          "form": "저도 근육통 장난아니엇어요ㅠ3일내내",
          "original_form": "저도 근육통 장난아니엇어요ㅠ3일내내",
          "speaker_id": "1",
          "time": "20210830 09:48"
        },
        {
          "id": "MDRW2100035699.1.16",
          "form": "헉..ㄷㄷ 장난아니네요ㅠ",
          "original_form": "헉..ㄷㄷ 장난아니네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 10:00"
        },
        {
          "id": "MDRW2100035699.1.17",
          "form": "타이레놀먹어도 아프더라고요 저는",
          "original_form": "타이레놀먹어도 아프더라고요 저는",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035699.1.18",
          "form": "엄청 힘드셨겠어요ㅜㅜㅠ 어떤거 맞으셨어요?",
          "original_form": "엄청 힘드셨겠어요ㅜㅜㅠ 어떤거 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210830 10:17"
        },
        {
          "id": "MDRW2100035699.1.19",
          "form": "저 화이자요!",
          "original_form": "저 화이자요!",
          "speaker_id": "1",
          "time": "20210830 10:17"
        },
        {
          "id": "MDRW2100035699.1.20",
          "form": "2차는 더아프다던데ㅠ",
          "original_form": "2차는 더아프다던데ㅠ",
          "speaker_id": "1",
          "time": "20210830 10:17"
        },
        {
          "id": "MDRW2100035699.1.21",
          "form": "세상에 2차는 더 아프대요..?",
          "original_form": "세상에 2차는 더 아프대요..?",
          "speaker_id": "2",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100035699.1.22",
          "form": "무섭네요 증말",
          "original_form": "무섭네요 증말",
          "speaker_id": "2",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100035699.1.23",
          "form": "2차는 더 아프다고 하더라구요!",
          "original_form": "2차는 더 아프다고 하더라구요!",
          "speaker_id": "1",
          "time": "20210830 10:21"
        }
      ]
