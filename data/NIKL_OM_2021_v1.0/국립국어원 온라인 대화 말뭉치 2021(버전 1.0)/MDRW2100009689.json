      "utterance": [
        {
          "id": "MDRW2100009689.1.1",
          "form": "어제 날씨는 어땠나요",
          "original_form": "어제 날씨는 어땠나요",
          "speaker_id": "1",
          "time": "20210823 03:57"
        },
        {
          "id": "MDRW2100009689.1.2",
          "form": "어제는 맑아서 좋았어요",
          "original_form": "어제는 맑아서 좋았어요",
          "speaker_id": "2",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009689.1.3",
          "form": "맑음을 넘어 더웠지만요",
          "original_form": "맑음을 넘어 더웠지만요",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009689.1.4",
          "form": "오늘은 비가오나요?",
          "original_form": "오늘은 비가오나요?",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009689.1.5",
          "form": "제가 사는곳은 쏟아져요",
          "original_form": "제가 사는곳은 쏟아져요",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009689.1.6",
          "form": "지금은 흐려요",
          "original_form": "지금은 흐려요",
          "speaker_id": "2",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009689.1.7",
          "form": "비가 곧 올 거 같네요",
          "original_form": "비가 곧 올 거 같네요",
          "speaker_id": "2",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009689.1.8",
          "form": "태풍이 온다는 말도 있고 그래서",
          "original_form": "태풍이 온다는 말도 있고 그래서",
          "speaker_id": "1",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009689.1.9",
          "form": "비가 아마 많이 올거에요ㅠ",
          "original_form": "비가 아마 많이 올거에요ㅠ",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009689.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009689.1.11",
          "form": "비 오는 거 좋아하세요?",
          "original_form": "비 오는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009689.1.12",
          "form": "저는.. 비를 좋아하지않아요",
          "original_form": "저는.. 비를 좋아하지않아요",
          "speaker_id": "1",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009689.1.13",
          "form": "약간 우산 써도 젖어서 뭔지 아시죠",
          "original_form": "약간 우산 써도 젖어서 뭔지 아시죠",
          "speaker_id": "1",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009689.1.14",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009689.1.15",
          "form": "저는 날씨를 좀 타서",
          "original_form": "저는 날씨를 좀 타서",
          "speaker_id": "2",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.16",
          "form": "비 오면 좀 힘이 없어져서",
          "original_form": "비 오면 좀 힘이 없어져서",
          "speaker_id": "2",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.17",
          "form": "좋을 때도 있고 싫을 때도 있으신가요",
          "original_form": "좋을 때도 있고 싫을 때도 있으신가요",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.18",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "2",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.19",
          "form": "아 뭔지알아요",
          "original_form": "아 뭔지알아요",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.20",
          "form": "네 가끔은 좋기도 해요!",
          "original_form": "네 가끔은 좋기도 해요!",
          "speaker_id": "2",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.21",
          "form": "약간 비오면 힘도빠지고",
          "original_form": "약간 비오면 힘도빠지고",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009689.1.22",
          "form": "쳐지고",
          "original_form": "쳐지고",
          "speaker_id": "1",
          "time": "20210823 12:40"
        }
      ]
    }
