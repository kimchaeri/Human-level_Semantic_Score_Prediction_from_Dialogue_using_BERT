      "utterance": [
        {
          "id": "MDRW2100030721.1.1",
          "form": "네이버에서 검색하면 최저가로 사기 편해요",
          "original_form": "네이버에서 검색하면 최저가로 사기 편해요",
          "speaker_id": "2",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.2",
          "form": "저는 진주로 이사 하면요",
          "original_form": "저는 진주로 이사 하면요",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.3",
          "form": "모든 걸 다 사야 해요",
          "original_form": "모든 걸 다 사야 해요",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.5",
          "form": "VIP 되겠죠",
          "original_form": "VIP 되겠죠",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.6",
          "form": "할인 이벤트 하는 거를 잘 활용하세요",
          "original_form": "할인 이벤트 하는 거를 잘 활용하세요",
          "speaker_id": "2",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.7",
          "form": "이불부터 숟가락 가지",
          "original_form": "이불부터 숟가락 가지",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.8",
          "form": "지금 지마켓 이벤트 하는데",
          "original_form": "지금 지마켓 이벤트 하는데",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.9",
          "form": "모든 걸 다 사야 하나요",
          "original_form": "모든 걸 다 사야 하나요",
          "speaker_id": "2",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.10",
          "form": "집이 없어서 못 사요",
          "original_form": "집이 없어서 못 사요",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.11",
          "form": "가진게 캐리어 두 개 뿐이에요",
          "original_form": "가진게 캐리어 두 개 뿐이에요",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030721.1.12",
          "form": "11번가에서 아마존 직구 관련해서 연계 한다던데",
          "original_form": "11번가에서 아마존 직구 관련해서 연계 한다던데",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030721.1.13",
          "form": "당근마켓에서 노려보려고요",
          "original_form": "당근마켓에서 노려보려고요",
          "speaker_id": "1",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030721.1.14",
          "form": "무료배송 이벤트 할 것 같더라고요",
          "original_form": "무료배송 이벤트 할 것 같더라고요",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030721.1.15",
          "form": "가전은 중고도 괜찮아요",
          "original_form": "가전은 중고도 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030721.1.16",
          "form": "당근마켓 또 굉장히 좋지요",
          "original_form": "당근마켓 또 굉장히 좋지요",
