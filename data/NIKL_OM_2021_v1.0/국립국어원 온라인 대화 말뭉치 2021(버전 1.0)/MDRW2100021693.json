      "utterance": [
        {
          "id": "MDRW2100021693.1.1",
          "form": "사고싶은거 있어요?",
          "original_form": "사고싶은거 있어요?",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021693.1.2",
          "form": "요즘 쇼피 어마무시하게 많이 해서 딱히 없어요...",
          "original_form": "요즘 쇼피 어마무시하게 많이 해서 딱히 없어요...",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021693.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021693.1.4",
          "form": "저도 최근 물욕이 사라진 때라ㅋㅋㅋㅋ",
          "original_form": "저도 최근 물욕이 사라진 때라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021693.1.5",
          "form": "이번에 소개팅 망하면 금융치료로",
          "original_form": "이번에 소개팅 망하면 금융치료로",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021693.1.6",
          "form": "갤럭시와치 살려고햇어요",
          "original_form": "갤럭시와치 살려고햇어요",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021693.1.7",
          "form": "아 진짜 웃긴다 ㅋㅋㅋㅋ 금융치료 ㅋㅋㅋ",
          "original_form": "아 진짜 웃긴다 ㅋㅋㅋㅋ 금융치료 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021693.1.8",
          "form": "갤럭시워치4 말씀이신거죠?",
          "original_form": "갤럭시워치4 말씀이신거죠?",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021693.1.9",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021693.1.10",
          "form": "그거 저도 사고 싶은데 기능과 가격을 잘 모르겠어요",
          "original_form": "그거 저도 사고 싶은데 기능과 가격을 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021693.1.11",
          "form": "얼마 정도 해요 ?",
          "original_form": "얼마 정도 해요 ?",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021693.1.12",
          "form": "저도 망하면 찾아보려고 했던거라 아직 안 찾아봤는데",
          "original_form": "저도 망하면 찾아보려고 했던거라 아직 안 찾아봤는데",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021693.1.13",
          "form": "다행히 안 사도 될거같아서요?",
          "original_form": "다행히 안 사도 될거같아서요?",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021693.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021693.1.15",
          "form": "어머어머 잘 되신거에요?",
          "original_form": "어머어머 잘 되신거에요?",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021693.1.16",
          "form": "네넵 그러니 금융치료가 필요없어졌어요",
          "original_form": "네넵 그러니 금융치료가 필요없어졌어요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021693.1.17",
          "form": "제가 그동안 금융치료 자주 했거든요 그래서 가난해졌어요..",
          "original_form": "제가 그동안 금융치료 자주 했거든요 그래서 가난해졌어요..",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021693.1.18",
          "form": "어머 축하드려요~ ㅜㅜ",
          "original_form": "어머 축하드려요~ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021693.1.19",
          "form": "금융치료로 어떤 것들 해보셨어요?ㅋㅋㅋ",
          "original_form": "금융치료로 어떤 것들 해보셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021693.1.20",
          "form": "저 전남친하고 헤어지던날에는 항공권을 끊었고..",
          "original_form": "저 전남친하고 헤어지던날에는 항공권을 끊었고..",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021693.1.21",
          "form": "소개팅하고나선 필라테스 수강권 끊고..",
          "original_form": "소개팅하고나선 필라테스 수강권 끊고..",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021693.1.22",
          "form": "또 다른 소개팅하고는 폰을 신형으로 바꿨고..",
          "original_form": "또 다른 소개팅하고는 폰을 신형으로 바꿨고..",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021693.1.23",
          "form": "또또 다른 소개팅에는 가방을 샀어요",
          "original_form": "또또 다른 소개팅에는 가방을 샀어요",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021693.1.24",
          "form": "어머 ㅜㅜ",
          "original_form": "어머 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021693.1.25",
          "form": "그래도 막 쓸모 없는거 쓸데 없는거 과잉충동구매는 안하셨네요",
          "original_form": "그래도 막 쓸모 없는거 쓸데 없는거 과잉충동구매는 안하셨네요",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021693.1.26",
          "form": "키워드로 금융치료 제출했어요 ㅋㅋㅋ",
          "original_form": "키워드로 금융치료 제출했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021693.1.27",
          "form": "혹시 그럼 지금까지 산 물건 중에 쓸모 없던 물건 있어요?",
          "original_form": "혹시 그럼 지금까지 산 물건 중에 쓸모 없던 물건 있어요?",
          "speaker_id": "1",
          "time": "20210828 22:01"
        }
      ]
