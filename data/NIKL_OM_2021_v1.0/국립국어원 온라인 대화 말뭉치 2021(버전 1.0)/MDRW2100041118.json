      "utterance": [
        {
          "id": "MDRW2100041118.1.1",
          "form": "안녕하세요 자주 하는 게임 있으세요?",
          "original_form": "안녕하세요 자주 하는 게임 있으세요?",
          "speaker_id": "1",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100041118.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041118.1.3",
          "form": "쿠킹덤하다가 지웠어요",
          "original_form": "쿠킹덤하다가 지웠어요",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041118.1.4",
          "form": "게임좋아하시나요?",
          "original_form": "게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041118.1.5",
          "form": "내 게임 좋아해요 캔디크러쉬사가 좋아해요",
          "original_form": "내 게임 좋아해요 캔디크러쉬사가 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041118.1.6",
          "form": "오 ㅋㅋ저도그거 예전에 좋아했었어요 ㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋ저도그거 예전에 좋아했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041118.1.7",
          "form": "그거가 또 각종 퍼즐게임의 원조였죠 ㅋㅋㅋㅋ",
          "original_form": "그거가 또 각종 퍼즐게임의 원조였죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041118.1.8",
          "form": "네 중독성있고 귀엽고 재밌어요",
          "original_form": "네 중독성있고 귀엽고 재밌어요",
          "speaker_id": "1",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041118.1.9",
          "form": "위베베 퍼즐도 그거랑 비슷한데 혹시 해보셨나요",
          "original_form": "위베베 퍼즐도 그거랑 비슷한데 혹시 해보셨나요",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041118.1.10",
          "form": "아니요 그런게임은 오늘 처음 들어 봐요",
          "original_form": "아니요 그런게임은 오늘 처음 들어 봐요",
          "speaker_id": "1",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041118.1.11",
          "form": "카카오톡에 있어요 ㅋㅋㅋ 위베어 베어스 캐릭터나와요 ㅋㅋㅋ",
          "original_form": "카카오톡에 있어요 ㅋㅋㅋ 위베어 베어스 캐릭터나와요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041118.1.12",
          "form": "앗 그렇군요...",
          "original_form": "앗 그렇군요...",
          "speaker_id": "1",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041118.1.13",
          "form": "집순이 생활을 오래해서 몰랐나봐요 ㅋㅋ",
          "original_form": "집순이 생활을 오래해서 몰랐나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100041118.1.14",
          "form": "ㅋㅋ저도 게임 잘안해서 모르는데 친구가 초대메세지 보내서 알았어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋ저도 게임 잘안해서 모르는데 친구가 초대메세지 보내서 알았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100041118.1.15",
          "form": "아 그러시군요 소개해 주셔서 감사해요",
          "original_form": "아 그러시군요 소개해 주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
          "id": "MDRW2100041118.1.16",
          "form": "귀여운거 좋아하시면 쿠키런킹덤도 한번 구경해보세요 ㅋㅋㅋㅋ",
          "original_form": "귀여운거 좋아하시면 쿠키런킹덤도 한번 구경해보세요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100041118.1.17",
          "form": "넵 감사합니다 ㅎㅎ ",
          "original_form": "넵 감사합니다 ㅎㅎ ",
          "speaker_id": "1",
          "time": "20210905 17:58"
        }
      ]
