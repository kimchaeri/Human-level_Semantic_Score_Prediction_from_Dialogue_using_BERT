      "utterance": [
        {
          "id": "MDRW2100017378.1.1",
          "form": "건강비빔밥이네요ㅋㅋㅋ",
          "original_form": "건강비빔밥이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017378.1.2",
          "form": "저도고추장파라",
          "original_form": "저도고추장파라",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017378.1.3",
          "form": "저도 고추장파라서 처음엔 좀 그랬는데",
          "original_form": "저도 고추장파라서 처음엔 좀 그랬는데",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017378.1.4",
          "form": "노른자랑 참기름 간장 섞이면 은근 어울릴것도 같아요",
          "original_form": "노른자랑 참기름 간장 섞이면 은근 어울릴것도 같아요",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017378.1.5",
          "form": "얘기하니까 배고프네요 저녁은 뭐드세요?",
          "original_form": "얘기하니까 배고프네요 저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017378.1.6",
          "form": "저 아직고민중이랍니다ㅋㅋㅋ",
          "original_form": "저 아직고민중이랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017378.1.7",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017378.1.8",
          "form": "전 비빔밥 먹고싶어져서 비빔밥 대충 먹어요",
          "original_form": "전 비빔밥 먹고싶어져서 비빔밥 대충 먹어요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017378.1.9",
          "form": "ㅋㅋㅋ 저도비빔밥먹었는데",
          "original_form": "ㅋㅋㅋ 저도비빔밥먹었는데",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100017378.1.10",
          "form": "점심 저녁 비빔밥 드신건가요?",
          "original_form": "점심 저녁 비빔밥 드신건가요?",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100017378.1.11",
          "form": "비빔밥이 안물리니까 계속 먹을수있는것같아요",
          "original_form": "비빔밥이 안물리니까 계속 먹을수있는것같아요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100017378.1.12",
          "form": "ㅋㅋㅋㅋ점심에요!",
          "original_form": "ㅋㅋㅋㅋ점심에요!",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100017378.1.13",
          "form": "저녁은 가볍게",
          "original_form": "저녁은 가볍게",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100017378.1.14",
          "form": "햄버거먹었답니다",
          "original_form": "햄버거먹었답니다",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100017378.1.15",
          "form": "햄버거 맛있겠어요!",
          "original_form": "햄버거 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100017378.1.16",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.17",
          "form": "무슨 버거 드셨나용",
          "original_form": "무슨 버거 드셨나용",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.18",
          "form": "저 버거킹이요ㅋㅋㅋ",
          "original_form": "저 버거킹이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.19",
          "form": "무슨버거를 먹었을까요?",
          "original_form": "무슨버거를 먹었을까요?",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.20",
          "form": "아 버거킹 맛있죠",
          "original_form": "아 버거킹 맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100017378.1.22",
          "form": "으음?!",
          "original_form": "으음?!",
