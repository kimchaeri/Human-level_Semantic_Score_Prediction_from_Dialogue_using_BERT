      "utterance": [
        {
          "id": "MDRW2100030040.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100030040.1.2",
          "form": "어제 축구봣나요",
          "original_form": "어제 축구봣나요",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100030040.1.3",
          "form": "보다 안보다했네요",
          "original_form": "보다 안보다했네요",
          "speaker_id": "1",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100030040.1.4",
          "form": "많이재미있지는않아서요",
          "original_form": "많이재미있지는않아서요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030040.1.5",
          "form": "무승뷰라 안보길 잘했다는요",
          "original_form": "무승뷰라 안보길 잘했다는요",
          "speaker_id": "2",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030040.1.6",
          "form": "잘하셨네요",
          "original_form": "잘하셨네요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030040.1.7",
          "form": "스포츠 좋아하시나요",
          "original_form": "스포츠 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030040.1.8",
          "form": "그냥그냥 봐요..스포츠뉴스가 제일재밌고",
          "original_form": "그냥그냥 봐요..스포츠뉴스가 제일재밌고",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030040.1.9",
          "form": "전 올림픽때만 열심히 보는거같아요",
          "original_form": "전 올림픽때만 열심히 보는거같아요",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100030040.1.10",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100030040.1.11",
          "form": "높이뛰기가 아쉬웠어요",
          "original_form": "높이뛰기가 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100030040.1.12",
          "form": "긍정에너지 멋졌어요",
          "original_form": "긍정에너지 멋졌어요",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100030040.1.13",
          "form": "맞아요 박수 같이 쳤어요  포상금? 을 줘서 다행이라 생각",
          "original_form": "맞아요 박수 같이 쳤어요  포상금? 을 줘서 다행이라 생각",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030040.1.14",
          "form": "기록두 깻는뎅...",
          "original_form": "기록두 깻는뎅...",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030040.1.15",
          "form": "대전분이더군요..",
          "original_form": "대전분이더군요..",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100030040.1.16",
          "form": "군인이시던데 다시 군대로 가셨겠죠?",
          "original_form": "군인이시던데 다시 군대로 가셨겠죠?",
