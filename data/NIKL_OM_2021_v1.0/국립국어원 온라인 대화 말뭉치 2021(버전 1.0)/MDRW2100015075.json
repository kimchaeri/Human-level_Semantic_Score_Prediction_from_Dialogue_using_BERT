      "utterance": [
        {
          "id": "MDRW2100015075.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015075.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015075.1.3",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015075.1.4",
          "form": "못가서 몸에 곰팡이 필거같아요",
          "original_form": "못가서 몸에 곰팡이 필거같아요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015075.1.5",
          "form": "여행가고싶어요",
          "original_form": "여행가고싶어요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015075.1.6",
          "form": "저도 여행가고싶어요...",
          "original_form": "저도 여행가고싶어요...",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015075.1.7",
          "form": "최근에 어디 다녀오신데 있으신가요",
          "original_form": "최근에 어디 다녀오신데 있으신가요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015075.1.8",
          "form": "친구랑",
          "original_form": "친구랑",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015075.1.9",
          "form": "차빌려서",
          "original_form": "차빌려서",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015075.1.10",
          "form": "대천해수욕장 구경만하고왔어요ㅠ",
          "original_form": "대천해수욕장 구경만하고왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015075.1.11",
          "form": "구경이라도 바다 보면 기분 좋죠",
          "original_form": "구경이라도 바다 보면 기분 좋죠",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015075.1.12",
          "form": "해변 드라이브도 좋구요",
          "original_form": "해변 드라이브도 좋구요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015075.1.13",
          "form": "보기만해서 감질났어요ㅠ",
          "original_form": "보기만해서 감질났어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015075.1.14",
          "form": "물좋아하거든요",
          "original_form": "물좋아하거든요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015075.1.15",
          "form": "코로나가 끝나야 물놀이도 맘껏 할텐데",
          "original_form": "코로나가 끝나야 물놀이도 맘껏 할텐데",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015075.1.16",
          "form": "좀 나아지면 저도 바다나 보러가야겠네요",
          "original_form": "좀 나아지면 저도 바다나 보러가야겠네요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015075.1.17",
          "form": "겨울에라도...ㅠㅠ",
          "original_form": "겨울에라도...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015075.1.18",
          "form": "크 겨울바다",
          "original_form": "크 겨울바다",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015075.1.19",
          "form": "뭘좀아시는군요",
          "original_form": "뭘좀아시는군요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015075.1.20",
          "form": "겨울바다는 해운대가 좋습니다",
          "original_form": "겨울바다는 해운대가 좋습니다",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015075.1.21",
          "form": "정동진도 좋고...",
          "original_form": "정동진도 좋고...",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015075.1.22",
          "form": "사실 바다는 다 좋아요",
          "original_form": "사실 바다는 다 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015075.1.23",
          "form": "사람없는게 특히 ㅋㅋ",
          "original_form": "사람없는게 특히 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015075.1.24",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015075.1.25",
          "form": "사람없는바다 최고",
          "original_form": "사람없는바다 최고",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015075.1.26",
          "form": "그래서 저는 여름 바다는 잘 안갑니다 ...",
          "original_form": "그래서 저는 여름 바다는 잘 안갑니다 ...",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015075.1.27",
          "form": "해운대 해수욕장 한번 겪어봤는데",
          "original_form": "해운대 해수욕장 한번 겪어봤는데",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015075.1.28",
          "form": "물보다 사람이 많았어요 ㅋㅋ",
          "original_form": "물보다 사람이 많았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015075.1.29",
          "form": "저도 경포대갔었는데",
          "original_form": "저도 경포대갔었는데",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015075.1.30",
          "form": "바다가 안보여ㅋㅋㅋㄱㅋ",
          "original_form": "바다가 안보여ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015075.1.31",
          "form": "경포대는 일출명소 아닌가요",
          "original_form": "경포대는 일출명소 아닌가요",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015075.1.32",
          "form": "혹시 일출도 보셨나요",
          "original_form": "혹시 일출도 보셨나요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015075.1.33",
          "form": "아뇨 저는 일출에는 크게 의의를 두지않는편이라",
          "original_form": "아뇨 저는 일출에는 크게 의의를 두지않는편이라",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015075.1.34",
          "form": "집 창문으로도 충분히 볼수있는걸요!",
          "original_form": "집 창문으로도 충분히 볼수있는걸요!",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015075.1.35",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015075.1.36",
          "form": "저희집 창문에선 일출은 안보이고 일몰만 보여요",
          "original_form": "저희집 창문에선 일출은 안보이고 일몰만 보여요",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015075.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015075.1.38",
          "form": "전 해운대에서 일출 본 경험이 있습니다",
          "original_form": "전 해운대에서 일출 본 경험이 있습니다",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015075.1.39",
          "form": "역시나 물보다 사람이 많았던...",
          "original_form": "역시나 물보다 사람이 많았던...",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015075.1.40",
          "form": "ㅋㅋㄲㅋㅋㅋㅋㅋ사람사이에서 뜨는해라",
          "original_form": "ㅋㅋㄲㅋㅋㅋㅋㅋ사람사이에서 뜨는해라",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015075.1.41",
          "form": "장관이겠군요",
          "original_form": "장관이겠군요",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015075.1.42",
          "form": "졸다가도 해뜨면 사람들이 소리 질러서 일출을 볼 수 있습니다",
          "original_form": "졸다가도 해뜨면 사람들이 소리 질러서 일출을 볼 수 있습니다",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015075.1.43",
          "form": "여러모로 장관이죠",
          "original_form": "여러모로 장관이죠",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015075.1.44",
          "form": "한번 가보고 다시는 안가는 ㅋㅋㅋ",
          "original_form": "한번 가보고 다시는 안가는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:09"
        }
      ]
    }
  ]
