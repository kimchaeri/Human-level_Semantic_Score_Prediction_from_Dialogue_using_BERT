      "utterance": [
        {
          "id": "MDRW2100017860.1.1",
          "form": "ㅣ요즘 아프신 곳은 없으신가요",
          "original_form": "ㅣ요즘 아프신 곳은 없으신가요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017860.1.2",
          "form": "허리가 좀 아파",
          "original_form": "허리가 좀 아파",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017860.1.3",
          "form": "맨날 파스 붙이시는거 같더니",
          "original_form": "맨날 파스 붙이시는거 같더니",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017860.1.4",
          "form": "똑같은 곳이 아파",
          "original_form": "똑같은 곳이 아파",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017860.1.5",
          "form": "요즘에는 한의원을 못 가시네요",
          "original_form": "요즘에는 한의원을 못 가시네요",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017860.1.6",
          "form": "회사 다니느라 그렇지",
          "original_form": "회사 다니느라 그렇지",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017860.1.7",
          "form": "토요일에는 쉬어야 한의원을 가는데",
          "original_form": "토요일에는 쉬어야 한의원을 가는데",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017860.1.8",
          "form": "괜찮아 그정도 아니야",
          "original_form": "괜찮아 그정도 아니야",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017860.1.9",
          "form": "쉬는 날에는 밭일 하느라 또 아프실만도 해요 일을 줄여야해",
          "original_form": "쉬는 날에는 밭일 하느라 또 아프실만도 해요 일을 줄여야해",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017860.1.10",
          "form": "내걱정 하지말고 너나 잘챙기고 살아",
          "original_form": "내걱정 하지말고 너나 잘챙기고 살아",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017860.1.11",
          "form": "맨날 그 소리하시네요 이게 무슨 걱정이야 걱정될만 하니까 걱정하는거지",
          "original_form": "맨날 그 소리하시네요 이게 무슨 걱정이야 걱정될만 하니까 걱정하는거지",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017860.1.12",
          "form": "시끄러워 나는 괜찮아 니가 맨날 아침도 안먹고 비실대서 걱정이지",
          "original_form": "시끄러워 나는 괜찮아 니가 맨날 아침도 안먹고 비실대서 걱정이지",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017860.1.13",
          "form": "이제 노동말고 운동을 하셔야해요",
          "original_form": "이제 노동말고 운동을 하셔야해요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        }
      ]
