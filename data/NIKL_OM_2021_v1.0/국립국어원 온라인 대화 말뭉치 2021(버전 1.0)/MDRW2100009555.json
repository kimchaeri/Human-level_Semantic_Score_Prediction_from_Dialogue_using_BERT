      "utterance": [
        {
          "id": "MDRW2100009555.1.1",
          "form": "음 난 얼마전 도서관카드 발급받음",
          "original_form": "음 난 얼마전 도서관카드 발급받음",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.2",
          "form": "이번에 극한견주 작가님 새 연재시작하셨더랑",
          "original_form": "이번에 극한견주 작가님 새 연재시작하셨더랑",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.3",
          "form": "도서관  좋음",
          "original_form": "도서관  좋음",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.4",
          "form": "극한가드너? 식물키우는거임",
          "original_form": "극한가드너? 식물키우는거임",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.5",
          "form": "국가의서비스는 무조건 최대한 누려야함 ㅋㄱ",
          "original_form": "국가의서비스는 무조건 최대한 누려야함 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.6",
          "form": "도서관 문여나",
          "original_form": "도서관 문여나",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.7",
          "form": "오 뭐 키우심?",
          "original_form": "오 뭐 키우심?",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.8",
          "form": "거야 당연하제!",
          "original_form": "거야 당연하제!",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.9",
          "form": "ㅇㅇ50프로 개방함 좌석",
          "original_form": "ㅇㅇ50프로 개방함 좌석",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009555.1.10",
          "form": "난 벤자민하고 나비란하고 민트허브 등등 키움 극한가드너 넘 기대된다 1화나왔지만",
          "original_form": "난 벤자민하고 나비란하고 민트허브 등등 키움 극한가드너 넘 기대된다 1화나왔지만",
          "speaker_id": "1",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009555.1.11",
          "form": "서울은 문닫음 아직ㅜㅜ",
          "original_form": "서울은 문닫음 아직ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009555.1.12",
          "form": "희망도서 신청하고 원하는 책빌려본답",
          "original_form": "희망도서 신청하고 원하는 책빌려본답",
          "speaker_id": "1",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009555.1.13",
          "form": "아하 서울은 그렇군요",
          "original_form": "아하 서울은 그렇군요",
          "speaker_id": "2",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009555.1.14",
          "form": "우린 지방이라",
          "original_form": "우린 지방이라",
          "speaker_id": "2",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009555.1.15",
          "form": "극한 가드너 공짜임?",
          "original_form": "극한 가드너 공짜임?",
          "speaker_id": "2",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009555.1.16",
          "form": "요샌 웹툰도 유료가 많아서",
          "original_form": "요샌 웹툰도 유료가 많아서",
          "speaker_id": "2",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009555.1.17",
          "form": "카카오페이지에",
          "original_form": "카카오페이지에",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009555.1.18",
          "form": "크레이지가드너",
          "original_form": "크레이지가드너",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009555.1.19",
          "form": "막시작했오",
          "original_form": "막시작했오",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009555.1.20",
          "form": "오호 그분 뭔가 키우시는거 잘하시구나",
          "original_form": "오호 그분 뭔가 키우시는거 잘하시구나",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009555.1.21",
          "form": "name3이는 아직 같이있으심?",
          "original_form": "&name3&이는 아직 같이있으심?",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009555.1.22",
          "form": "웅 name3이 잘크고잇는거같아",
          "original_form": "웅 &name3&이 잘크고잇는거같아",
