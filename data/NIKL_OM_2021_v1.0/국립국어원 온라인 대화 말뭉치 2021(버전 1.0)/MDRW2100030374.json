      "utterance": [
        {
          "id": "MDRW2100030374.1.1",
          "form": "우아 대화주제,,,",
          "original_form": "우아 대화주제,,,",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030374.1.2",
          "form": "와.. 주제 바꿔도 괜찮을까요?",
          "original_form": "와.. 주제 바꿔도 괜찮을까요?",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030374.1.3",
          "form": "우아",
          "original_form": "우아",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100030374.1.4",
          "form": "오.. 상당히 익숙치 않은 주제인데",
          "original_form": "오.. 상당히 익숙치 않은 주제인데",
          "speaker_id": "2",
          "time": "20210905 18:15"
        },
        {
          "id": "MDRW2100030374.1.5",
          "form": "지인중에 군대 간 사람이 있나요?",
          "original_form": "지인중에 군대 간 사람이 있나요?",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100030374.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100030374.1.7",
          "form": "거의다 전역했죠!",
          "original_form": "거의다 전역했죠!",
          "speaker_id": "1",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100030374.1.8",
          "form": "오 대부분 전역했군요!",
          "original_form": "오 대부분 전역했군요!",
          "speaker_id": "2",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100030374.1.9",
          "form": "군생활이랑은 사실 거의 연이 없어서",
          "original_form": "군생활이랑은 사실 거의 연이 없어서",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100030374.1.10",
          "form": "동기들 군대갔어도 별로 연락 안했던 것 같아요 ㅋㅋ",
          "original_form": "동기들 군대갔어도 별로 연락 안했던 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100030374.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100030374.1.12",
          "form": "아아아 전 19년에전역해서,,,",
          "original_form": "아아아 전 19년에전역해서,,,",
          "speaker_id": "1",
          "time": "20210905 18:26"
        },
        {
          "id": "MDRW2100030374.1.13",
          "form": "이번에 코로나때문에 다들 휴가때도 많이 못나오고",
          "original_form": "이번에 코로나때문에 다들 휴가때도 많이 못나오고",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100030374.1.14",
          "form": "그래서 전역 날짜가 빨라졌다고 들었어요",
          "original_form": "그래서 전역 날짜가 빨라졌다고 들었어요",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100030374.1.15",
          "form": "그렇더라구요!",
          "original_form": "그렇더라구요!",
          "speaker_id": "1",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100030374.1.16",
          "form": "앗 name1님께서는 그러면 어디로 가셨었나요? 육군? 해군? 공군?",
          "original_form": "앗 &name1&님께서는 그러면 어디로 가셨었나요? 육군? 해군? 공군?",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100030374.1.17",
          "form": "육군 15사단 수색대대 전역햌ㅅ어욤!",
          "original_form": "육군 15사단 수색대대 전역햌ㅅ어욤!",
          "speaker_id": "1",
          "time": "20210905 18:29"
        },
        {
          "id": "MDRW2100030374.1.18",
          "form": "아핳 ㅋㅋㅋ 그렇군요!",
          "original_form": "아핳 ㅋㅋㅋ 그렇군요!",
          "speaker_id": "2",
          "time": "20210905 18:30"
        },
        {
          "id": "MDRW2100030374.1.19",
          "form": "군생활 하시는 동안 많이 힘드셨죠?",
          "original_form": "군생활 하시는 동안 많이 힘드셨죠?",
          "speaker_id": "2",
          "time": "20210905 18:30"
        },
        {
          "id": "MDRW2100030374.1.20",
          "form": "그래도 뭔가 재밌었어요!!",
          "original_form": "그래도 뭔가 재밌었어요!!",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100030374.1.21",
          "form": "19년도쯤 제대 하신거면 비슷한 시기에 같이 학교 다녔을 것 같아요 ㅋㅋ 아마 가시기 전쯤에",
          "original_form": "19년도쯤 제대 하신거면 비슷한 시기에 같이 학교 다녔을 것 같아요 ㅋㅋ 아마 가시기 전쯤에",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100030374.1.22",
          "form": "오 뭔가 재미있었다니 새로운 사실인데 어떤점이요?",
          "original_form": "오 뭔가 재미있었다니 새로운 사실인데 어떤점이요?",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100030374.1.23",
          "form": "그냥 뭐랄까군생활이",
          "original_form": "그냥 뭐랄까군생활이",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100030374.1.24",
          "form": "특수부대다보니까",
          "original_form": "특수부대다보니까",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100030374.1.25",
          "form": "힘들면서도 재밌었어요!",
          "original_form": "힘들면서도 재밌었어요!",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100030374.1.26",
          "form": "군대가 재미있다고 느끼기 힘들었을텐데",
          "original_form": "군대가 재미있다고 느끼기 힘들었을텐데",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100030374.1.27",
          "form": "체질이신거 아니에요? ㅋㅋㅋㅋ",
          "original_form": "체질이신거 아니에요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100030374.1.28",
          "form": "농담이구요",
          "original_form": "농담이구요",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100030374.1.29",
          "form": "특수부대 출신이신거면 엄청난 엘리트셨던거 아니에요?",
          "original_form": "특수부대 출신이신거면 엄청난 엘리트셨던거 아니에요?",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100030374.1.30",
          "form": "에이 그정도는아니에요!!!",
          "original_form": "에이 그정도는아니에요!!!",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100030374.1.31",
          "form": "굉장히 긍정적이고 멋진 분일 것 같아요 ㅋㅋ",
          "original_form": "굉장히 긍정적이고 멋진 분일 것 같아요 ㅋㅋ",
          "speaker_id": "2",
