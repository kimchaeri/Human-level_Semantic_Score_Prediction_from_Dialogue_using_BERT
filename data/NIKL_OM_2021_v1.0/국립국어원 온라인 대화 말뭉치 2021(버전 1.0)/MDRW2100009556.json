      "utterance": [
        {
          "id": "MDRW2100009556.1.1",
          "form": "봉사활동은 하고 계신가요?",
          "original_form": "봉사활동은 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009556.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009556.1.3",
          "form": "최대한 많이 하려고",
          "original_form": "최대한 많이 하려고",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009556.1.4",
          "form": "하는 편이에요",
          "original_form": "하는 편이에요",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009556.1.5",
          "form": "봉사활동 하시나요?",
          "original_form": "봉사활동 하시나요?",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009556.1.6",
          "form": "저는 하다가 최근에 쉬고 있어요",
          "original_form": "저는 하다가 최근에 쉬고 있어요",
          "speaker_id": "1",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009556.1.7",
          "form": "무슨 봉사를 주로 하시나요?",
          "original_form": "무슨 봉사를 주로 하시나요?",
          "speaker_id": "1",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009556.1.8",
          "form": "장기적인 봉사를 주로 하는 거 같아요",
          "original_form": "장기적인 봉사를 주로 하는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 00:38"
        },
        {
          "id": "MDRW2100009556.1.9",
          "form": "어르신과 약속한 날짜에 어르신이 하고 싶으신거",
          "original_form": "어르신과 약속한 날짜에 어르신이 하고 싶으신거",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009556.1.10",
          "form": "병원방문, 산책, 장보기 등등을 같이하거나",
          "original_form": "병원방문, 산책, 장보기 등등을 같이하거나",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009556.1.11",
          "form": "완전 보람찬 봉사활동이네요!",
          "original_form": "완전 보람찬 봉사활동이네요!",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009556.1.12",
          "form": "아동을 대상으로 수업을 하는 봉사등등!",
          "original_form": "아동을 대상으로 수업을 하는 봉사등등!",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009556.1.13",
          "form": "다양하게 하는 편이에요",
          "original_form": "다양하게 하는 편이에요",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009556.1.14",
          "form": "기억에 남는 봉사가 있으세요?",
          "original_form": "기억에 남는 봉사가 있으세요?",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009556.1.15",
          "form": "저는 과학관 특별전 봉사활동이 제일 기억에 남네요",
          "original_form": "저는 과학관 특별전 봉사활동이 제일 기억에 남네요",
          "speaker_id": "1",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009556.1.16",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "2",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009556.1.17",
          "form": "과학관 특별전 봉사에는 어떤 거 하셨어요?",
          "original_form": "과학관 특별전 봉사에는 어떤 거 하셨어요?",
          "speaker_id": "2",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009556.1.18",
          "form": "제 학과랑 맞춰서 과학 이론에 대해서 설명하는 봉사활동이었어요!",
          "original_form": "제 학과랑 맞춰서 과학 이론에 대해서 설명하는 봉사활동이었어요!",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009556.1.19",
          "form": "우와!! 멋있으세요",
          "original_form": "우와!! 멋있으세요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009556.1.20",
          "form": "과학 이론 설명이라니!!",
          "original_form": "과학 이론 설명이라니!!",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009556.1.21",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009556.1.22",
          "form": "근데 주로 물리가 많아서 꽤 애먹었던 기억이 있네요ㅋㅋㅋ",
          "original_form": "근데 주로 물리가 많아서 꽤 애먹었던 기억이 있네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009556.1.23",
          "form": "물리.. 듣기만해도 어려울거 같아요",
          "original_form": "물리.. 듣기만해도 어려울거 같아요",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009556.1.24",
          "form": "맞아요... 제 전공과는 다른 내용이었지만 보람찼던 거 같아요",
          "original_form": "맞아요... 제 전공과는 다른 내용이었지만 보람찼던 거 같아요",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009556.1.25",
          "form": "혹시 그런 느낌 아세요..",
          "original_form": "혹시 그런 느낌 아세요..",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009556.1.26",
          "form": "나와 다른 분야하시는 분이 멋져보이는 느끼이요!!",
          "original_form": "나와 다른 분야하시는 분이 멋져보이는 느끼이요!!",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009556.1.27",
          "form": "느낌이요!!",
          "original_form": "느낌이요!!",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009556.1.28",
