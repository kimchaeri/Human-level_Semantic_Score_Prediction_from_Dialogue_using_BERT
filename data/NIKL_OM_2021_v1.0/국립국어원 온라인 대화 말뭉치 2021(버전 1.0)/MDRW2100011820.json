      "utterance": [
        {
          "id": "MDRW2100011820.1.1",
          "form": "계속 전화를 해보는수밖에",
          "original_form": "계속 전화를 해보는수밖에",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011820.1.2",
          "form": "입고는 언제 되는지 알아?",
          "original_form": "입고는 언제 되는지 알아?",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011820.1.3",
          "form": "아ㅜ그렇군",
          "original_form": "아ㅜ그렇군",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011820.1.4",
          "form": "그러면 언제입고됩니다 한다",
          "original_form": "그러면 언제입고됩니다 한다",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011820.1.5",
          "form": "일단 찾아봐",
          "original_form": "일단 찾아봐",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.6",
          "form": "샤넬...가방 종류",
          "original_form": "샤넬...가방 종류",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.7",
          "form": "우선 돈을 마련해야",
          "original_form": "우선 돈을 마련해야",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.8",
          "form": "맘에 드는게있어야하니",
          "original_form": "맘에 드는게있어야하니",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.9",
          "form": "난 크로스백 사고싶은데",
          "original_form": "난 크로스백 사고싶은데",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.10",
          "form": "입주하고 사던지",
          "original_form": "입주하고 사던지",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.11",
          "form": "자라다가니 어떤엄마했던데 부티나더라",
          "original_form": "자라다가니 어떤엄마했던데 부티나더라",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.12",
          "form": "그래라 심심할때 찾아봐 가방종류",
          "original_form": "그래라 심심할때 찾아봐 가방종류",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011820.1.13",
          "form": "심심할때가 없네ㅜ",
          "original_form": "심심할때가 없네ㅜ",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.14",
          "form": "샤넬은 부티난다\r\n내 woc도 다이쁘다 하드라",
          "original_form": "샤넬은 부티난다\r\n내 woc도 다이쁘다 하드라",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.15",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.16",
          "form": "틈날때 봐바라",
          "original_form": "틈날때 봐바라",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.17",
          "form": "클러치도 맘에든다 편하게 들고다니고",
          "original_form": "클러치도 맘에든다 편하게 들고다니고",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.18",
          "form": "알았어~실물이 이쁘더라",
          "original_form": "알았어~실물이 이쁘더라",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.19",
          "form": "구찌가방이 안들리더라",
          "original_form": "구찌가방이 안들리더라",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011820.1.20",
          "form": "구찌는 왜 안들어?",
          "original_form": "구찌는 왜 안들어?",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011820.1.21",
          "form": "구찌 크로스도 이쁘던데",
          "original_form": "구찌 크로스도 이쁘던데",
          "speaker_id": "1",
          "time": "20210824 16:19"
        }
      ]
