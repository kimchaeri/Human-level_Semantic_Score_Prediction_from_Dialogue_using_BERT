      "utterance": [
        {
          "id": "MDRW2100005406.1.1",
          "form": "버거왕 알바하면서 스트레스 받았던 썰 풀어주세요",
          "original_form": "버거왕 알바하면서 스트레스 받았던 썰 풀어주세요",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005406.1.2",
          "form": "허하",
          "original_form": "허하",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005406.1.3",
          "form": "제가 주방일을 하는데요",
          "original_form": "제가 주방일을 하는데요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005406.1.4",
          "form": "분명 버거가 만드는데 시간이 걸리고 카운터 쪽으로 패스해야하는데",
          "original_form": "분명 버거가 만드는데 시간이 걸리고 카운터 쪽으로 패스해야하는데",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005406.1.5",
          "form": "카운터에서 계속 버거 안주냐고 뭐라해서 스트레스 좀 받구요",
          "original_form": "카운터에서 계속 버거 안주냐고 뭐라해서 스트레스 좀 받구요",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005406.1.6",
          "form": "매니저가 자기는 한가할 때 놀면서",
          "original_form": "매니저가 자기는 한가할 때 놀면서",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005406.1.7",
          "form": "저는 한가해보이면 일 시키고 못 쉬게 하구요",
          "original_form": "저는 한가해보이면 일 시키고 못 쉬게 하구요",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005406.1.8",
          "form": "카운터도 버거 만드는 데 시간 걸리는 거 알 거 아냐...",
          "original_form": "카운터도 버거 만드는 데 시간 걸리는 거 알 거 아냐...",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.9",
          "form": "카운터는 주방 일 아예 모르나요",
          "original_form": "카운터는 주방 일 아예 모르나요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.10",
          "form": "그래도 요즘은 덜하는데",
          "original_form": "그래도 요즘은 덜하는데",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.11",
          "form": "한두달 전에는 계속 안주냐고 뭐라해서",
          "original_form": "한두달 전에는 계속 안주냐고 뭐라해서",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.12",
          "form": "조금 화가 많이 났네요",
          "original_form": "조금 화가 많이 났네요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.13",
          "form": "자기들이 주방일 하라고 해요...",
          "original_form": "자기들이 주방일 하라고 해요...",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005406.1.14",
          "form": "버거왕 말고 다른 알바 할 생각은?",
          "original_form": "버거왕 말고 다른 알바 할 생각은?",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005406.1.15",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005406.1.16",
          "form": "이번달까지만 버거왕 알바해요",
          "original_form": "이번달까지만 버거왕 알바해요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005406.1.17",
          "form": "위염도져서 일 못하겠어여",
          "original_form": "위염도져서 일 못하겠어여",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005406.1.18",
          "form": "무슨 알바 할 건지",
          "original_form": "무슨 알바 할 건지",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005406.1.19",
          "form": "거기까지는 생각 안해봤지만",
          "original_form": "거기까지는 생각 안해봤지만",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005406.1.20",
          "form": "쿠팡 아니면 다른 보건증 쓸 수 있는 알바?",
          "original_form": "쿠팡 아니면 다른 보건증 쓸 수 있는 알바?",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005406.1.21",
          "form": "오 쿠팡 물류센터 알바?",
          "original_form": "오 쿠팡 물류센터 알바?",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005406.1.22",
          "form": "쿠팡이 단기로 땡기기에는 좀 짭짤한가바",
          "original_form": "쿠팡이 단기로 땡기기에는 좀 짭짤한가바",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005406.1.23",
          "form": "주위에 쿠팡 하는 애들 꽤 있더라",
          "original_form": "주위에 쿠팡 하는 애들 꽤 있더라",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.24",
          "form": "근데 몸이 버텨줄지 의문",
          "original_form": "근데 몸이 버텨줄지 의문",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.25",
          "form": "버거킹도 몸이 안돼서 나가떨어지는데",
          "original_form": "버거킹도 몸이 안돼서 나가떨어지는데",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.26",
          "form": "맞아 쿠팡은 진짜... 몸이 힘들다더라",
          "original_form": "맞아 쿠팡은 진짜... 몸이 힘들다더라",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.27",
          "form": "미래의 내가 알아서 하겠죠",
          "original_form": "미래의 내가 알아서 하겠죠",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.28",
          "form": "카페 같은 데는?",
          "original_form": "카페 같은 데는?",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.29",
          "form": "카페..",
          "original_form": "카페..",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.30",
          "form": "나랑 너무 안어울리는데",
          "original_form": "나랑 너무 안어울리는데",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005406.1.31",
          "form": "그렇게 안 어울리지는 않는 거 같은데",
          "original_form": "그렇게 안 어울리지는 않는 거 같은데",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005406.1.32",
          "form": "하면 잘 할 듯",
          "original_form": "하면 잘 할 듯",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005406.1.33",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005406.1.34",
          "form": "난 하면 뭐 다 잘하지",
          "original_form": "난 하면 뭐 다 잘하지",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005406.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래...",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005406.1.36",
          "form": "다른 알바 구하면 서울에서 구하남",
          "original_form": "다른 알바 구하면 서울에서 구하남",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005406.1.37",
          "form": "일단 올해까진 김해에서 하고..",
          "original_form": "일단 올해까진 김해에서 하고..",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005406.1.38",
          "form": "내년에 복학을 하니",
          "original_form": "내년에 복학을 하니",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005406.1.39",
          "form": "근로를 뛸까 생각중",
          "original_form": "근로를 뛸까 생각중",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005406.1.40",
          "form": "나도 졸업 전까지 알바 한 번은 해볼까 했는데",
          "original_form": "나도 졸업 전까지 알바 한 번은 해볼까 했는데",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005406.1.41",
          "form": "늘 마음만 먹어보고 접음",
          "original_form": "늘 마음만 먹어보고 접음",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005406.1.42",
          "form": "나도 한번은 하겠지 했는데",
          "original_form": "나도 한번은 하겠지 했는데",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005406.1.43",
          "form": "남의돈 버는게 이렇게 힘들줄은",
          "original_form": "남의돈 버는게 이렇게 힘들줄은",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005406.1.44",
          "form": "맞아 남의 돈 버는 거 힘들더라",
          "original_form": "맞아 남의 돈 버는 거 힘들더라",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005406.1.45",
          "form": "내 돈은 그렇게 퍼주는데",
          "original_form": "내 돈은 그렇게 퍼주는데",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005406.1.46",
          "form": "학원 보조강사도 개꿀이긴 했는데 학생들 때문에 스트레스 받았었지...",
          "original_form": "학원 보조강사도 개꿀이긴 했는데 학생들 때문에 스트레스 받았었지...",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005406.1.47",
          "form": "보조강사...",
          "original_form": "보조강사...",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005406.1.48",
          "form": "학생들..",
          "original_form": "학생들..",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005406.1.49",
          "form": "생각만해도 서터레스",
          "original_form": "생각만해도 서터레스",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005406.1.50",
          "form": "애들이 딱 말 안 들을 나이었음",
          "original_form": "애들이 딱 말 안 들을 나이었음",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005406.1.51",
          "form": "초3 중2",
          "original_form": "초3 중2",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005406.1.52",
          "form": "나이만 봐도",
          "original_form": "나이만 봐도",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005406.1.53",
          "form": "머리가 지끈함",
          "original_form": "머리가 지끈함",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005406.1.54",
          "form": "중2라니",
          "original_form": "중2라니",
          "speaker_id": "2",
