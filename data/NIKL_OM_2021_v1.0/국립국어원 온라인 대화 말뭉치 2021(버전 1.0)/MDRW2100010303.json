      "utterance": [
        {
          "id": "MDRW2100010303.1.1",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010303.1.2",
          "form": "지금은 동물 안키우는데 키우고 싶은 생각은 있어요",
          "original_form": "지금은 동물 안키우는데 키우고 싶은 생각은 있어요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010303.1.3",
          "form": "강아지 중에 말티즈를 좋아해요",
          "original_form": "강아지 중에 말티즈를 좋아해요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010303.1.4",
          "form": "그쪽은 반려동물 좋아하시나요?",
          "original_form": "그쪽은 반려동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010303.1.5",
          "form": "너무 좋아하죠ㅎㅎㅎ",
          "original_form": "너무 좋아하죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010303.1.6",
          "form": "저는 포메 좋아한답니다",
          "original_form": "저는 포메 좋아한답니다",
          "speaker_id": "2",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010303.1.7",
          "form": "포메보면 심장 아프죠 행동하는게 귀엽고 게속 보게되여",
          "original_form": "포메보면 심장 아프죠 행동하는게 귀엽고 게속 보게되여",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100010303.1.8",
          "form": "포메 키우는 친구집에 놀려가서 포메를 알게되었어요",
          "original_form": "포메 키우는 친구집에 놀려가서 포메를 알게되었어요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100010303.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100010303.1.10",
          "form": "너무부럽네요",
          "original_form": "너무부럽네요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100010303.1.11",
          "form": "저는 나중에",
          "original_form": "저는 나중에",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100010303.1.12",
          "form": "꼭 키우려구요",
          "original_form": "꼭 키우려구요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100010303.1.13",
          "form": "동물을 키우는데 있어서 어려움이 있지 않을까요?",
          "original_form": "동물을 키우는데 있어서 어려움이 있지 않을까요?",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100010303.1.14",
          "form": "그래서지금",
          "original_form": "그래서지금",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100010303.1.15",
          "form": "너무키우고싶은데",
          "original_form": "너무키우고싶은데",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100010303.1.16",
          "form": "못키우고있답니다",
          "original_form": "못키우고있답니다",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100010303.1.17",
          "form": "강아지 키우는 친구들은 없나요?",
          "original_form": "강아지 키우는 친구들은 없나요?",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100010303.1.18",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100010303.1.19",
          "form": "많답니다",
          "original_form": "많답니다",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100010303.1.20",
          "form": "어려움이나 주의사항 같은거 들으면 될거같는데..",
          "original_form": "어려움이나 주의사항 같은거 들으면 될거같는데..",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100010303.1.21",
          "form": "친구들이 귀찮아서 잘 안알러주나봐요",
          "original_form": "친구들이 귀찮아서 잘 안알러주나봐요",
          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100010303.1.22",
          "form": "ㅋㅋㅋ 주의사항은 다알죠",
          "original_form": "ㅋㅋㅋ 주의사항은 다알죠",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100010303.1.23",
          "form": "못키우는이유는 강아지 혼자두는시간이 많아서",
          "original_form": "못키우는이유는 강아지 혼자두는시간이 많아서",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100010303.1.24",
          "form": "강아지에게도 집중해야하고 돌아다니는걸 좋아하니",
          "original_form": "강아지에게도 집중해야하고 돌아다니는걸 좋아하니",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100010303.1.25",
          "form": "방도 지저분해지더라고요 ㅋㅋ",
          "original_form": "방도 지저분해지더라고요 ㅋㅋ",
