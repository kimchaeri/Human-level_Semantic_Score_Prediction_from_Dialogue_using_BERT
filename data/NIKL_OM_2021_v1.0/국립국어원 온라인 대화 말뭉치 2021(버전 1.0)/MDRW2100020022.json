      "utterance": [
        {
          "id": "MDRW2100020022.1.1",
          "form": "안녕하세여...",
          "original_form": "안녕하세여...",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020022.1.2",
          "form": "안냐세옄ㄱㄱㄱㄱㄱ",
          "original_form": "안냐세옄ㄱㄱㄱㄱㄱ",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020022.1.3",
          "form": "연애 근황 어떻게 되나요",
          "original_form": "연애 근황 어떻게 되나요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020022.1.4",
          "form": "남자친구 있으세여?",
          "original_form": "남자친구 있으세여?",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020022.1.5",
          "form": "아니 전 없습니다만",
          "original_form": "아니 전 없습니다만",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.6",
          "form": "순조롭죠~",
          "original_form": "순조롭죠~",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.7",
          "form": "왜 없어여?",
          "original_form": "왜 없어여?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.8",
          "form": "그쪽은 왜 남친 있어요?ㅠㅠ",
          "original_form": "그쪽은 왜 남친 있어요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.9",
          "form": "이뻐서요ㅎㅎ",
          "original_form": "이뻐서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.10",
          "form": "좋겠네요ㅡㅡ",
          "original_form": "좋겠네요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020022.1.11",
          "form": "데이트 후기 알려주세요",
          "original_form": "데이트 후기 알려주세요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.12",
          "form": "당빠 좋져><",
          "original_form": "당빠 좋져><",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.13",
          "form": "모쏠은 데이트가 궁금해요 ㅎㄹ",
          "original_form": "모쏠은 데이트가 궁금해요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.14",
          "form": "짜릿하져",
          "original_form": "짜릿하져",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.15",
          "form": "저는 밥먹는게 젤 좋답니다 ㅎㅎ",
          "original_form": "저는 밥먹는게 젤 좋답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.16",
          "form": "그게 다 예요?",
          "original_form": "그게 다 예요?",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.17",
          "form": "이상형이 어떻게 되시져",
          "original_form": "이상형이 어떻게 되시져",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.18",
          "form": "전 강아지상 좋아해요",
          "original_form": "전 강아지상 좋아해요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.19",
          "form": "귀엽잖아요><",
          "original_form": "귀엽잖아요><",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.20",
          "form": "밥,  카페,  영화가 기본이져",
          "original_form": "밥,  카페,  영화가 기본이져",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020022.1.21",
          "form": "연예인을 예로들자면?",
          "original_form": "연예인을 예로들자면?",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020022.1.22",
          "form": "I LOVE 박보검..",
          "original_form": "I LOVE 박보검..",
          "speaker_id": "2",
          "time": "20210827 23:21"
        }
      ]
