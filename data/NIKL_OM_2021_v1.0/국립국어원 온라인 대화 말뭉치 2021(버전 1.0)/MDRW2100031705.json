      "utterance": [
        {
          "id": "MDRW2100031705.1.1",
          "form": "백신을 맞기 전 후로 꽤나 술을 안 마시고 있었는데 탄산이 들어가는 음료를 빼 먹고 싶었거든요",
          "original_form": "백신을 맞기 전 후로 꽤나 술을 안 마시고 있었는데 탄산이 들어가는 음료를 빼 먹고 싶었거든요",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031705.1.2",
          "form": "밥반찬으로 무엇이 맛있나요",
          "original_form": "밥반찬으로 무엇이 맛있나요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.3",
          "form": "탄산수에 타서 먹었더니 천국이었습니다",
          "original_form": "탄산수에 타서 먹었더니 천국이었습니다",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.4",
          "form": "진짜 시원하고 맛있겠다",
          "original_form": "진짜 시원하고 맛있겠다",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.5",
          "form": "아무래도 고기가 짱인 것 같아요",
          "original_form": "아무래도 고기가 짱인 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.6",
          "form": "혼자 다 먹었나 봐요",
          "original_form": "혼자 다 먹었나 봐요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.7",
          "form": "장조림 이요",
          "original_form": "장조림 이요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.8",
          "form": "풀떼기 만 먹으면 맛은 있지만 빨리 배가 고파 지더라고요",
          "original_form": "풀떼기 만 먹으면 맛은 있지만 빨리 배가 고파 지더라고요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.9",
          "form": "그냥 돼지갈비 두 괜찮네요",
          "original_form": "그냥 돼지갈비 두 괜찮네요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.10",
          "form": "고모랑 먹었습니다",
          "original_form": "고모랑 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.11",
          "form": "탑 마트에서 파는 돼지고기 맛있는 것 같아요",
          "original_form": "탑 마트에서 파는 돼지고기 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.12",
          "form": "혹시 포크밸리 드셔 보셨나요",
          "original_form": "혹시 포크밸리 드셔 보셨나요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.13",
          "form": "탑마트 식품이 은근히 괜찮아요",
          "original_form": "탑마트 식품이 은근히 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.14",
          "form": "저는 최근에 처음 먹어 봤거든요",
          "original_form": "저는 최근에 처음 먹어 봤거든요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.15",
          "form": "거기 빵도 맛있더라고요",
          "original_form": "거기 빵도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031705.1.16",
          "form": "탑마트 닭발도 굉장히 맛있습니다",
          "original_form": "탑마트 닭발도 굉장히 맛있습니다",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031705.1.17",
          "form": "브랜드 이름인가요",
          "original_form": "브랜드 이름인가요",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031705.1.18",
          "form": "포크밸리 구워서 저녁에 먹고 남은 걸 다음날 먹었거든요",
          "original_form": "포크밸리 구워서 저녁에 먹고 남은 걸 다음날 먹었거든요",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031705.1.19",
          "form": "닭발 세일할때 먹으러 가야죠",
          "original_form": "닭발 세일할때 먹으러 가야죠",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031705.1.20",
          "form": "대부분 다 팔리고 없겠죠",
          "original_form": "대부분 다 팔리고 없겠죠",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031705.1.21",
          "form": "고기 파는 곳 브랜드라고 할까요 굉장히 맛있고 비싼 브랜드입니다",
          "original_form": "고기 파는 곳 브랜드라고 할까요 굉장히 맛있고 비싼 브랜드입니다",
          "speaker_id": "2",
          "time": "20210904 23:41"
        }
      ]
