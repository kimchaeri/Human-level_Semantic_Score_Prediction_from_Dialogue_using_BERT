      "utterance": [
        {
          "id": "MDRW2100005525.1.1",
          "form": "재밌어 ㅋㅋㅋ 나는 드라마 보다는 애니메이션을 더 많이 봐",
          "original_form": "재밌어 ㅋㅋㅋ 나는 드라마 보다는 애니메이션을 더 많이 봐",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005525.1.2",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005525.1.3",
          "form": "오랜만에 나도 애니메이션 볼까",
          "original_form": "오랜만에 나도 애니메이션 볼까",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005525.1.4",
          "form": "응 시간 날 때 봐봐",
          "original_form": "응 시간 날 때 봐봐",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005525.1.5",
          "form": "킬링타임으로 좋아",
          "original_form": "킬링타임으로 좋아",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005525.1.6",
          "form": "뭐 추천해줄래?",
          "original_form": "뭐 추천해줄래?",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005525.1.7",
          "form": "어디 나라 애니메이션이 좋아?",
          "original_form": "어디 나라 애니메이션이 좋아?",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005525.1.8",
          "form": "안가리는데",
          "original_form": "안가리는데",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005525.1.9",
          "form": "그럼 킹오브더힐 봐봐",
          "original_form": "그럼 킹오브더힐 봐봐",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005525.1.10",
          "form": "아 그거 알아 ㅋㅋㅋ 짤로 돌아다니는 거",
          "original_form": "아 그거 알아 ㅋㅋㅋ 짤로 돌아다니는 거",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005525.1.11",
          "form": "본 거야?",
          "original_form": "본 거야?",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005525.1.12",
          "form": "아니 본 적은 없어",
          "original_form": "아니 본 적은 없어",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005525.1.13",
          "form": "ㅋㅋㅋ 주인공 바비 이야기인데 웃겨",
          "original_form": "ㅋㅋㅋ 주인공 바비 이야기인데 웃겨",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005525.1.14",
          "form": "미국 애니메이션 좋지",
          "original_form": "미국 애니메이션 좋지",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005525.1.15",
          "form": "맞아 영어공부도 되고 ㅋㅋ",
          "original_form": "맞아 영어공부도 되고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005525.1.16",
          "form": "어 나도 스펀지밥이랑 심슨으로 회화 공부 했어 ㅋㅋ",
          "original_form": "어 나도 스펀지밥이랑 심슨으로 회화 공부 했어 ㅋㅋ",
          "speaker_id": "1",
