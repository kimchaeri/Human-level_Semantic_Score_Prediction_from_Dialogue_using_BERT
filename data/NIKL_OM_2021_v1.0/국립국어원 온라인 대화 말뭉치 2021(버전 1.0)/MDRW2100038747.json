      "utterance": [
        {
          "id": "MDRW2100038747.1.1",
          "form": "혹시 키워본 동물 있나요?",
          "original_form": "혹시 키워본 동물 있나요?",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038747.1.2",
          "form": "하나도 안 키워봤어요ㅠ",
          "original_form": "하나도 안 키워봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038747.1.3",
          "form": "그래요? 전 예전에 말티즈 강아지 키웠었어요",
          "original_form": "그래요? 전 예전에 말티즈 강아지 키웠었어요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038747.1.4",
          "form": "동물을 좋아하나요?",
          "original_form": "동물을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038747.1.5",
          "form": "동물은 엄청 좋아해요",
          "original_form": "동물은 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038747.1.6",
          "form": "그러면 환경때문에 못 키운 건가요?",
          "original_form": "그러면 환경때문에 못 키운 건가요?",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100038747.1.7",
          "form": "부모님이 막으셨어요",
          "original_form": "부모님이 막으셨어요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038747.1.8",
          "form": "관리 잘할 자신이 없어서 빨리 포기했죠",
          "original_form": "관리 잘할 자신이 없어서 빨리 포기했죠",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038747.1.9",
          "form": "맞아요.. 그거 다 부모님 특히 엄마 몫이에요",
          "original_form": "맞아요.. 그거 다 부모님 특히 엄마 몫이에요",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038747.1.10",
          "form": "저도 강아지 키운게 제 의지가 아니라 막내딸이 원해서 키운거였죠",
          "original_form": "저도 강아지 키운게 제 의지가 아니라 막내딸이 원해서 키운거였죠",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038747.1.11",
          "form": "아하 막내따님이 좋아하셨군요",
          "original_form": "아하 막내따님이 좋아하셨군요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038747.1.12",
          "form": "아직도 좋아하시나요",
          "original_form": "아직도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038747.1.13",
          "form": "그쵸.. 근데 이 아이가 데리고 오면 본인이 다 돌보겠다고 약속해놓고 결국은 제가 뒤치닥거리 다 했거든요",
          "original_form": "그쵸.. 근데 이 아이가 데리고 오면 본인이 다 돌보겠다고 약속해놓고 결국은 제가 뒤치닥거리 다 했거든요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038747.1.14",
          "form": "결국 그렇게 되더라구요",
          "original_form": "결국 그렇게 되더라구요",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038747.1.15",
          "form": "어릴 때는 다들 그렇죠",
          "original_form": "어릴 때는 다들 그렇죠",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038747.1.16",
          "form": "맞아요.. 그래서 대부분의 부모들이 반대하는거죠..ㅎㅎ",
          "original_form": "맞아요.. 그래서 대부분의 부모들이 반대하는거죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 05:58"
        },
        {
          "id": "MDRW2100038747.1.17",
          "form": "근데 딸이 워낙 키우고 싶어하니까 허락하게 되더라구요",
          "original_form": "근데 딸이 워낙 키우고 싶어하니까 허락하게 되더라구요",
