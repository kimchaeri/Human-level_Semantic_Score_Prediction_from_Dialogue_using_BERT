        },
        {
          "id": "MDRW2100010335.1.2",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "2",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010335.1.3",
          "form": "네네 거긴 비가오나요?",
          "original_form": "네네 거긴 비가오나요?",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010335.1.4",
          "form": "부산인데 아침부터 흐리고 비가 왔다가 안왔다가하네요..",
          "original_form": "부산인데 아침부터 흐리고 비가 왔다가 안왔다가하네요..",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010335.1.5",
          "form": "날씨도 저번주부터 갑자기 선선해진거같아요",
          "original_form": "날씨도 저번주부터 갑자기 선선해진거같아요",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010335.1.6",
          "form": "여기도 지금은 그쳤네요ㅋㅋㅋ",
          "original_form": "여기도 지금은 그쳤네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010335.1.7",
          "form": "부산은 더웠나요?",
          "original_form": "부산은 더웠나요?",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010335.1.8",
          "form": "조금 습하긴했는데 덥진않았어요",
          "original_form": "조금 습하긴했는데 덥진않았어요",
          "speaker_id": "2",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100010335.1.9",
          "form": "처서매직이라더니 저번주부터 날씨가 확 바뀌는게",
          "original_form": "처서매직이라더니 저번주부터 날씨가 확 바뀌는게",
          "speaker_id": "2",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100010335.1.10",
          "form": "신기하네요...ㅋㅋ",
          "original_form": "신기하네요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:54"
        },
        {
          "id": "MDRW2100010335.1.11",
          "form": "맞아요ㅋㅋ 기온이 확내려갔어요",
          "original_form": "맞아요ㅋㅋ 기온이 확내려갔어요",
          "speaker_id": "1",
          "time": "20210824 10:38"
        },
        {
          "id": "MDRW2100010335.1.12",
          "form": "여기는 또 비가오네요",
          "original_form": "여기는 또 비가오네요",
          "speaker_id": "1",
          "time": "20210824 10:38"
        },
        {
          "id": "MDRW2100010335.1.13",
          "form": "새벽에..바람소리 장난 아니던데요",
          "original_form": "새벽에..바람소리 장난 아니던데요",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100010335.1.14",
          "form": "태풍 지나갈때 피해는 없으셨어요?",
          "original_form": "태풍 지나갈때 피해는 없으셨어요?",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100010335.1.15",
          "form": "태풍이 지나갔나요??????...!",
          "original_form": "태풍이 지나갔나요??????...!",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100010335.1.16",
          "form": "아 아직 비 많이 오시나요?",
          "original_form": "아 아직 비 많이 오시나요?",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100010335.1.17",
          "form": "저희지역은  이제 비는 안오고 바람만 많이 불고있네요",
          "original_form": "저희지역은  이제 비는 안오고 바람만 많이 불고있네요",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100010335.1.18",
          "form": "지금은 그쳤어요ㅎㅎ",
          "original_form": "지금은 그쳤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100010335.1.19",
          "form": "이젠 추운것같아요ㅠㅠ",
          "original_form": "이젠 추운것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100010335.1.20",
          "form": "그러게요..태풍 다 지나가고나면 눈에띄게 쌀쌀해질거같아요",
          "original_form": "그러게요..태풍 다 지나가고나면 눈에띄게 쌀쌀해질거같아요",
          "speaker_id": "2",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100010335.1.21",
          "form": "올해 여름은 생각보다 일찍 지나가는거같아요ㅎㅎ",
          "original_form": "올해 여름은 생각보다 일찍 지나가는거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100010335.1.22",
          "form": "맞아요 뭔가",
          "original_form": "맞아요 뭔가",
          "speaker_id": "1",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100010335.1.23",
          "form": "덥긴더웠는데 휙가네요ㅋㅋㅋ",
          "original_form": "덥긴더웠는데 휙가네요ㅋㅋㅋ",
