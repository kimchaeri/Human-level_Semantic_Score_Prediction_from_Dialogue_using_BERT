      "utterance": [
        {
          "id": "MDRW2100029297.1.1",
          "form": "햄버거도 좋아하세요??",
          "original_form": "햄버거도 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029297.1.2",
          "form": "좋아해요 어제점심 햄버거였어요",
          "original_form": "좋아해요 어제점심 햄버거였어요",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.3",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.4",
          "form": "어느 브랜드 햄버거 좋아하세요?",
          "original_form": "어느 브랜드 햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.5",
          "form": "맥날이요!",
          "original_form": "맥날이요!",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.6",
          "form": "하지만 새우버거는 롯데리아만 취급합니다",
          "original_form": "하지만 새우버거는 롯데리아만 취급합니다",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.7",
          "form": "저는 버거킹이 맛있더라고요",
          "original_form": "저는 버거킹이 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029297.1.8",
          "form": "맞아요 새우버거는 롯데리아뿐 ㅋㅋ",
          "original_form": "맞아요 새우버거는 롯데리아뿐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.9",
          "form": "버거킹 양파가 너무 생양파지않나요?!",
          "original_form": "버거킹 양파가 너무 생양파지않나요?!",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.10",
          "form": "버거킹도 새우버거 있더라고요 그건 안먹어봤어요",
          "original_form": "버거킹도 새우버거 있더라고요 그건 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.11",
          "form": "전 그 생양파가 너무 좋아요",
          "original_form": "전 그 생양파가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.12",
          "form": "버거킹 새우버거 그냥그래요 새우는 롯리!",
          "original_form": "버거킹 새우버거 그냥그래요 새우는 롯리!",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.13",
          "form": "아 저 생양파를 안먹어요,,,매워요,,,",
          "original_form": "아 저 생양파를 안먹어요,,,매워요,,,",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.14",
          "form": "토마토랑 생양파 맛있더라고요",
          "original_form": "토마토랑 생양파 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029297.1.15",
          "form": "전 생고추는 별론데 양파는 좋아해요",
          "original_form": "전 생고추는 별론데 양파는 좋아해요",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029297.1.16",
          "form": "아토마토 맛있죠ㅠ",
          "original_form": "아토마토 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029297.1.17",
          "form": "베토디도 맛있는뎅",
          "original_form": "베토디도 맛있는뎅",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029297.1.18",
          "form": "요즘은 수제버거 먹고싶어요,..",
          "original_form": "요즘은 수제버거 먹고싶어요,..",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029297.1.19",
          "form": "맞아요 맛있죠",
          "original_form": "맞아요 맛있죠",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029297.1.20",
          "form": "저 이삭버거 궁금해요",
          "original_form": "저 이삭버거 궁금해요",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.21",
          "form": "허 저도요!!",
          "original_form": "허 저도요!!",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.22",
          "form": "그거 이삭토스트가면 파나요?!",
          "original_form": "그거 이삭토스트가면 파나요?!",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.23",
          "form": "이삭버거가게가 따로있는건가요?!",
          "original_form": "이삭버거가게가 따로있는건가요?!",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.24",
          "form": "토스트랑 같이 팔았으면 좋겠는데 아닌거 같더라고요",
          "original_form": "토스트랑 같이 팔았으면 좋겠는데 아닌거 같더라고요",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.25",
          "form": "장사못하네요,..",
          "original_form": "장사못하네요,..",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029297.1.26",
          "form": "근데 따로여야 될거 같아요",
          "original_form": "근데 따로여야 될거 같아요",
          "speaker_id": "2",
          "time": "20210903 01:15"
        }
      ]
