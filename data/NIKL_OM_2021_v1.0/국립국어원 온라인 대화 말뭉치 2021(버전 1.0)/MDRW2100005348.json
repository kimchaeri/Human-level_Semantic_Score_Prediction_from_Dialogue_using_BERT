      "utterance": [
        {
          "id": "MDRW2100005348.1.1",
          "form": "아는형님 보시나요?",
          "original_form": "아는형님 보시나요?",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005348.1.2",
          "form": "네 가끔 봐요!",
          "original_form": "네 가끔 봐요!",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100005348.1.3",
          "form": "name1님은 아는형님 좋아하시나요?",
          "original_form": "&name1&님은 아는형님 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100005348.1.4",
          "form": "자주챙겨보진 않고 올림픽선수 나오길래 얘기했었어요ㅎ",
          "original_form": "자주챙겨보진 않고 올림픽선수 나오길래 얘기했었어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100005348.1.5",
          "form": "아 펜싱선수 나왔었죠",
          "original_form": "아 펜싱선수 나왔었죠",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100005348.1.6",
          "form": "네네ㅎ 그때보고있던 중이였어서",
          "original_form": "네네ㅎ 그때보고있던 중이였어서",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100005348.1.7",
          "form": "아!그때가 아형하는 시건이였군요",
          "original_form": "아!그때가 아형하는 시건이였군요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100005348.1.8",
          "form": "시간",
          "original_form": "시간",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100005348.1.9",
          "form": "유퀴즈에도 선수분들 나오셨더라구요",
          "original_form": "유퀴즈에도 선수분들 나오셨더라구요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100005348.1.10",
          "form": "잘생겨서 보게되더라구요ㅎ",
          "original_form": "잘생겨서 보게되더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100005348.1.11",
          "form": "맞아요 코에 점 있으신분!",
          "original_form": "맞아요 코에 점 있으신분!",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100005348.1.12",
          "form": "김준호 선수 ㅎ",
          "original_form": "김준호 선수 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100005348.1.13",
          "form": "약간 영웅재중 닯았어요",
          "original_form": "약간 영웅재중 닯았어요",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100005348.1.14",
          "form": "오오오 그렇네요!",
          "original_form": "오오오 그렇네요!",
          "speaker_id": "2",
          "time": "20210824 00:59"
        },
        {
          "id": "MDRW2100005348.1.15",
          "form": "계속 보면 닮은거같죠?ㅎ",
          "original_form": "계속 보면 닮은거같죠?ㅎ",
          "speaker_id": "1",
          "time": "20210824 00:59"
        },
        {
          "id": "MDRW2100005348.1.16",
          "form": "연예계관심 있는지 소속사 계약했더라구요",
          "original_form": "연예계관심 있는지 소속사 계약했더라구요",
          "speaker_id": "1",
          "time": "20210824 01:00"
        },
        {
          "id": "MDRW2100005348.1.17",
          "form": "얼굴이 잘생기셨으니 관심가지실만하네요",
          "original_form": "얼굴이 잘생기셨으니 관심가지실만하네요",
