      "utterance": [
        {
          "id": "MDRW2100041195.1.1",
          "form": "저 오늘 올리브영 할인이라..",
          "original_form": "저 오늘 올리브영 할인이라..",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041195.1.2",
          "form": "6만원 털리고 왔슴다..",
          "original_form": "6만원 털리고 왔슴다..",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041195.1.3",
          "form": "ㅋㅋㅋ 어떤 거 사셨어요?",
          "original_form": "ㅋㅋㅋ 어떤 거 사셨어요?",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100041195.1.4",
          "form": "저 팩만 이만원어치 샀어요..",
          "original_form": "저 팩만 이만원어치 샀어요..",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100041195.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ휴..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ휴..",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041195.1.6",
          "form": "그리고 스킨.. 뜬금업ㄱ이 전동칫솔..",
          "original_form": "그리고 스킨.. 뜬금업ㄱ이 전동칫솔..",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041195.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041195.1.8",
          "form": "환절기니까 팩은 잘 쓰시겠네요ㅋㅋㅋ",
          "original_form": "환절기니까 팩은 잘 쓰시겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041195.1.9",
          "form": "그러게요.. 열심히써야겠어요",
          "original_form": "그러게요.. 열심히써야겠어요",
          "speaker_id": "1",
          "time": "20210906 09:43"
        },
        {
          "id": "MDRW2100041195.1.10",
          "form": "집에 안그래도 쟁여둔게 있는데 또 이렇게 사버리다니..!",
          "original_form": "집에 안그래도 쟁여둔게 있는데 또 이렇게 사버리다니..!",
          "speaker_id": "1",
          "time": "20210906 09:43"
        },
        {
          "id": "MDRW2100041195.1.11",
          "form": "사고싶은 물건이나 최근에 사신 물건 있으신가요?",
          "original_form": "사고싶은 물건이나 최근에 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 09:43"
        },
        {
          "id": "MDRW2100041195.1.12",
          "form": "전 빵 샀어요ㅋㅋ",
          "original_form": "전 빵 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041195.1.13",
          "form": "어제 밤만쥬랑 치즈파이 같은 거 하나씩 사먹었어요",
          "original_form": "어제 밤만쥬랑 치즈파이 같은 거 하나씩 사먹었어요",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041195.1.14",
          "form": "헐 빵 너무 맛있겎어요!",
          "original_form": "헐 빵 너무 맛있겎어요!",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041195.1.15",
          "form": "저 갑자기 에센스 너무 사고힢은데",
          "original_form": "저 갑자기 에센스 너무 사고힢은데",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041195.1.16",
          "form": "아껴야겠죠..",
          "original_form": "아껴야겠죠..",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041195.1.17",
          "form": "ㅋㅋㅋㅋ갑자기 소비욕구가 들 때는 일단 좀 참아봐야해요",
          "original_form": "ㅋㅋㅋㅋ갑자기 소비욕구가 들 때는 일단 좀 참아봐야해요",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100041195.1.18",
          "form": "비가와서 나갈수없으므로",
          "original_form": "비가와서 나갈수없으므로",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100041195.1.19",
          "form": "자동으로 참아지네요!",
          "original_form": "자동으로 참아지네요!",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100041195.1.20",
          "form": "ㅋㅋㅋㅋㅋ 저도 비 오면 집 밖으로 못 나가요",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 비 오면 집 밖으로 못 나가요",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041195.1.21",
          "form": "비오명 정말 나가기싫어요..",
          "original_form": "비오명 정말 나가기싫어요..",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100041195.1.22",
          "form": "결국 운동도 가지못했습니다..!",
          "original_form": "결국 운동도 가지못했습니다..!",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100041195.1.23",
          "form": "올리브영 할인은 이번주까지갰죠..?",
          "original_form": "올리브영 할인은 이번주까지갰죠..?",
          "speaker_id": "1",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100041195.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100041195.1.25",
          "form": "전 올리브영 거의 안 가서 모르겠어요",
          "original_form": "전 올리브영 거의 안 가서 모르겠어요",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100041195.1.26",
          "form": "저도 자주 안갔었는데",
          "original_form": "저도 자주 안갔었는데",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041195.1.27",
          "form": "갑자기",
          "original_form": "갑자기",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041195.1.28",
          "form": "한번가니까",
          "original_form": "한번가니까",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041195.1.29",
          "form": "약간 입터지듯이 계속 가고힟어요..",
          "original_form": "약간 입터지듯이 계속 가고힟어요..",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041195.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 과소비 참아야해요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 과소비 참아야해요!!",
