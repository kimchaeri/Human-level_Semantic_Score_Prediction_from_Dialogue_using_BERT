      "utterance": [
        {
          "id": "MDRW2100019481.1.1",
          "form": "아니면 도마에 놓고 납작한 접시로 으깨는 방법도 있어요",
          "original_form": "아니면 도마에 놓고 납작한 접시로 으깨는 방법도 있어요",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019481.1.2",
          "form": "저도 한번 조져봐야겠어요",
          "original_form": "저도 한번 조져봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019481.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019481.1.4",
          "form": "후추 다 날아갈까봐 귀찮아서 안해봤거든요",
          "original_form": "후추 다 날아갈까봐 귀찮아서 안해봤거든요",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019481.1.5",
          "form": "요리 유투버중에 해피쿠킹 아시나요",
          "original_form": "요리 유투버중에 해피쿠킹 아시나요",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019481.1.6",
          "form": "약간 탔나? 싶을 정도로 굴려주고 빻으면 무게감이 생겨서 잘 안 날아가요.",
          "original_form": "약간 탔나? 싶을 정도로 굴려주고 빻으면 무게감이 생겨서 잘 안 날아가요.",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019481.1.7",
          "form": "오 들어봤어요",
          "original_form": "오 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019481.1.8",
          "form": "보진 못 한 것 같아요",
          "original_form": "보진 못 한 것 같아요",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019481.1.9",
          "form": "저는 엄청 열심히 봤어요 ㅋㅋㅋㅋ",
          "original_form": "저는 엄청 열심히 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019481.1.10",
          "form": "여기서 요리를 많이 배웠어요.. 후추 굽는것도 여기서 보여줬었엇는데 혹시 아시나 ㅎㅎ해서",
          "original_form": "여기서 요리를 많이 배웠어요.. 후추 굽는것도 여기서 보여줬었엇는데 혹시 아시나 ㅎㅎ해서",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019481.1.11",
          "form": "오 약간 태우기.. 메모하겠슴다",
          "original_form": "오 약간 태우기.. 메모하겠슴다",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019481.1.12",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019481.1.13",
          "form": "고급 스킬을 녹인 생활요리네요",
          "original_form": "고급 스킬을 녹인 생활요리네요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019481.1.14",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019481.1.15",
          "form": "스탠팬을 엄청 강조합니다",
          "original_form": "스탠팬을 엄청 강조합니다",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019481.1.16",
          "form": "저는 김밀란이라는 채널에서 봤는데",
          "original_form": "저는 김밀란이라는 채널에서 봤는데",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.17",
          "form": "아 그사람",
          "original_form": "아 그사람",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.18",
          "form": "여자분이 아니라서 좀 그렇지만",
          "original_form": "여자분이 아니라서 좀 그렇지만",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.19",
          "form": "양식유ㅜ투버죠?",
          "original_form": "양식유ㅜ투버죠?",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.20",
          "form": "저도 몇번 봤어요",
          "original_form": "저도 몇번 봤어요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.21",
          "form": "이탈리아 요리예요",
          "original_form": "이탈리아 요리예요",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019481.1.22",
          "form": "저는 아주 초기에 보기 시작해서",
          "original_form": "저는 아주 초기에 보기 시작해서",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.23",
          "form": "카치오에페페도 그사람 영상으로 본거같아요 아닌가..",
          "original_form": "카치오에페페도 그사람 영상으로 본거같아요 아닌가..",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.24",
          "form": "전문적이게 잘 하는거같더라구요",
          "original_form": "전문적이게 잘 하는거같더라구요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.25",
          "form": "영상은 서툰데 전달하는 지식들이 쏠쏠하더라고요",
          "original_form": "영상은 서툰데 전달하는 지식들이 쏠쏠하더라고요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.27",
          "form": "요즘은 이것저것 많이 하던데 오히려 안 보게 되는 ㅎㅎ",
          "original_form": "요즘은 이것저것 많이 하던데 오히려 안 보게 되는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.28",
          "form": "양식 배우기 좋은거같아요",
          "original_form": "양식 배우기 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019481.1.29",
          "form": "맞아요 ㅋㅋㅋㅋ 귀찮아서 요리 안하고..",
          "original_form": "맞아요 ㅋㅋㅋㅋ 귀찮아서 요리 안하고..",
          "speaker_id": "1",
          "time": "20210827 19:45"
        }
      ]
