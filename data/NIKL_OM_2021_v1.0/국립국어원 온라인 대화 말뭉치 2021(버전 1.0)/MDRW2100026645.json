        {
          "id": "MDRW2100026645.1.1",
          "form": "여항가도 제출할게많죠",
          "original_form": "여항가도 제출할게많죠",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026645.1.2",
          "form": "서류들 여권",
          "original_form": "서류들 여권",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026645.1.3",
          "form": "자동으로 나오네요",
          "original_form": "자동으로 나오네요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026645.1.4",
          "form": "여행도 자동으로 갈 수 있죠",
          "original_form": "여행도 자동으로 갈 수 있죠",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026645.1.5",
          "form": "제가 제출하는거죠",
          "original_form": "제가 제출하는거죠",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026645.1.6",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026645.1.7",
          "form": "이번에 비자 제출 해보세요",
          "original_form": "이번에 비자 제출 해보세요",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026645.1.8",
          "form": "기념품사세요",
          "original_form": "기념품사세요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026645.1.9",
          "form": "여권부터 만들어야겠네요",
          "original_form": "여권부터 만들어야겠네요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026645.1.10",
          "form": "여권 비자",
          "original_form": "여권 비자",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026645.1.11",
          "form": "기념품은 가이드만 좋음",
          "original_form": "기념품은 가이드만 좋음",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026645.1.12",
          "form": "여권만들고 계획잡고 여행사 끼고",
          "original_form": "여권만들고 계획잡고 여행사 끼고",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026645.1.13",
          "form": "관광명소 검색하고",
          "original_form": "관광명소 검색하고",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026645.1.14",
          "form": "지도어플 필수",
          "original_form": "지도어플 필수",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026645.1.15",
          "form": "지인 추천코스도 필수",
          "original_form": "지인 추천코스도 필수",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026645.1.16",
          "form": "음식도 필수 밥먹고 영수증 제출 해보세요",
          "original_form": "음식도 필수 밥먹고 영수증 제출 해보세요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026645.1.17",
          "form": "네이버로 여행지 검색하고\r\n네이버에 제출하고",
          "original_form": "네이버로 여행지 검색하고\r\n네이버에 제출하고",
