      "utterance": [
        {
          "id": "MDRW2100037725.1.1",
          "form": "ㅎㅎ먹고 붓겟어요",
          "original_form": "ㅎㅎ먹고 붓겟어요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037725.1.2",
          "form": "ㅋㅋㅋ현명하십니다",
          "original_form": "ㅋㅋㅋ현명하십니다",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037725.1.3",
          "form": "점심엔 머드셧나요",
          "original_form": "점심엔 머드셧나요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037725.1.4",
          "form": "스파게티요 배고프네요ㅜ",
          "original_form": "스파게티요 배고프네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037725.1.5",
          "form": "저는 면중독자라서",
          "original_form": "저는 면중독자라서",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037725.1.6",
          "form": "라면먹고도 스파게티 혹하네욤",
          "original_form": "라면먹고도 스파게티 혹하네욤",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037725.1.7",
          "form": "ㅋㅋㅋname1님 한결같으시군요ㅋㅋㅋ 전밥묵는데 그럴땐",
          "original_form": "ㅋㅋㅋ&name1&님 한결같으시군요ㅋㅋㅋ 전밥묵는데 그럴땐",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037725.1.8",
          "form": "ㅠㅠ미련하고 행복한돼지에요",
          "original_form": "ㅠㅠ미련하고 행복한돼지에요",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037725.1.9",
          "form": "저녁일찍드세요",
          "original_form": "저녁일찍드세요",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037725.1.10",
          "form": "곧 먹으려구요ㅎㅎ밥ㅎㅎ  일찍먹어야 잘때 소화 다시켜서 편하게자요ㅜㅜ",
          "original_form": "곧 먹으려구요ㅎㅎ밥ㅎㅎ  일찍먹어야 잘때 소화 다시켜서 편하게자요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037725.1.11",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037725.1.12",
          "form": "저는 특기가 밥먹고 바로자는건데",
          "original_form": "저는 특기가 밥먹고 바로자는건데",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037725.1.13",
          "form": "에궁",
          "original_form": "에궁",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037725.1.14",
          "form": "위장약하신분들은 그렇다더라구요",
          "original_form": "위장약하신분들은 그렇다더라구요",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037725.1.15",
          "form": "그게 몸에는 훨씬좋아요",
          "original_form": "그게 몸에는 훨씬좋아요",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037725.1.16",
          "form": "저는 건강망치는 지름길이에요 ㅋㅋㅋㅋ",
          "original_form": "저는 건강망치는 지름길이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037725.1.17",
          "form": "역류성 식도염 조심하셔야해용ㅜ 열시미 돈벌어서 의사 다줌ㅜㅜㅜ",
          "original_form": "역류성 식도염 조심하셔야해용ㅜ 열시미 돈벌어서 의사 다줌ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037725.1.18",
          "form": "얼마전에 위내시경햇는데",
          "original_form": "얼마전에 위내시경햇는데",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037725.1.19",
          "form": "깨끗하데서 신기햇어요",
          "original_form": "깨끗하데서 신기햇어요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037725.1.20",
          "form": "저 식습관 진짜 안좋거든요",
          "original_form": "저 식습관 진짜 안좋거든요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037725.1.21",
          "form": "ㅋㅋ그래도 이제 고쳐나갈려구요ㅠㅠ",
          "original_form": "ㅋㅋ그래도 이제 고쳐나갈려구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037725.1.22",
          "form": "ㅋㅋ나중에 뭉치로 옵니다",
          "original_form": "ㅋㅋ나중에 뭉치로 옵니다",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037725.1.23",
          "form": "조심하셔야해요ㅋㅋ습관만들기",
          "original_form": "조심하셔야해요ㅋㅋ습관만들기",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037725.1.24",
          "form": "네네ㅠㅠ 나이더먹고 훅갈듯ㅠㅠ",
