      "utterance": [
        {
          "id": "MDRW2100033117.1.1",
          "form": "오늘 저녁은 뭐야?",
          "original_form": "오늘 저녁은 뭐야?",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033117.1.2",
          "form": "닭갈비 먹을까 하는데 어때?",
          "original_form": "닭갈비 먹을까 하는데 어때?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033117.1.3",
          "form": "ㅇㅇ 좋아 ㅎㅎ 근데 난 밥은 주지마",
          "original_form": "ㅇㅇ 좋아 ㅎㅎ 근데 난 밥은 주지마",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033117.1.4",
          "form": "왜? 밥 안먹게?",
          "original_form": "왜? 밥 안먹게?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033117.1.5",
          "form": "ㅇㅇ 다이어트 하니까 닭갈비만 먹으려고",
          "original_form": "ㅇㅇ 다이어트 하니까 닭갈비만 먹으려고",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033117.1.6",
          "form": "그럼 그냥 닭갈비랑 사리 위주로 먹어",
          "original_form": "그럼 그냥 닭갈비랑 사리 위주로 먹어",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033117.1.7",
          "form": "ㅇㅇ 고구마랑 떡 면도 넣을거 아냐?",
          "original_form": "ㅇㅇ 고구마랑 떡 면도 넣을거 아냐?",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033117.1.8",
          "form": "글치 ㅎㅎ",
          "original_form": "글치 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033117.1.9",
          "form": "그거면 충분해",
          "original_form": "그거면 충분해",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033117.1.10",
          "form": "고럼 오늘 저녁은 구구닭갈비 ㅋ",
          "original_form": "고럼 오늘 저녁은 구구닭갈비 ㅋ",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033117.1.11",
          "form": "나 7시쯤 도착하니까 살짝 미리 구워주시면 감사 ㅎㅎ",
          "original_form": "나 7시쯤 도착하니까 살짝 미리 구워주시면 감사 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033117.1.12",
          "form": "알았어 사리 넣기 직전까지 볶아놓을께",
          "original_form": "알았어 사리 넣기 직전까지 볶아놓을께",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033117.1.13",
          "form": "맛있겠다 배고파 ㅎ",
          "original_form": "맛있겠다 배고파 ㅎ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033117.1.14",
          "form": "사리는 우동사리?",
          "original_form": "사리는 우동사리?",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033117.1.15",
