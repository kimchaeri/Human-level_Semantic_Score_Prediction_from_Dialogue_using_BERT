      "utterance": [
        {
          "id": "MDRW2100012510.1.1",
          "form": "요즘 신작 도서에 대해 읽어보신 것이 있나요?",
          "original_form": "요즘 신작 도서에 대해 읽어보신 것이 있나요?",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012510.1.2",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012510.1.3",
          "form": "저는 요즘 미드나잇 라이브러리라는 책을 읽고 있어요",
          "original_form": "저는 요즘 미드나잇 라이브러리라는 책을 읽고 있어요",
          "speaker_id": "1",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012510.1.4",
          "form": "어떤 내용인가요?",
          "original_form": "어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012510.1.5",
          "form": "죽기 전에만 열리는 도서관에서 제 2의 인생을 찾아 떠나는 내용이에요",
          "original_form": "죽기 전에만 열리는 도서관에서 제 2의 인생을 찾아 떠나는 내용이에요",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012510.1.6",
          "form": "최근에 인상 깊으셨던 책이 있나요?",
          "original_form": "최근에 인상 깊으셨던 책이 있나요?",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012510.1.7",
          "form": "오 내용이 흥미진진하네요",
          "original_form": "오 내용이 흥미진진하네요",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012510.1.8",
          "form": "요즘 엘리어트 파동이론이라는 책 읽고있어요",
          "original_form": "요즘 엘리어트 파동이론이라는 책 읽고있어요",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012510.1.9",
          "form": "재미있어요 한번쯤 읽어보시는걸 추천드려요",
          "original_form": "재미있어요 한번쯤 읽어보시는걸 추천드려요",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012510.1.10",
          "form": "어려워 보이는 책이네요 읽게 된 계기가 있으신가요?",
          "original_form": "어려워 보이는 책이네요 읽게 된 계기가 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012510.1.11",
          "form": "네 읽어볼게요~",
          "original_form": "네 읽어볼게요~",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012510.1.12",
          "form": "주식 관련 책이에요",
          "original_form": "주식 관련 책이에요",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012510.1.13",
          "form": "부자되고 싶어서 읽고 있습니다",
          "original_form": "부자되고 싶어서 읽고 있습니다",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012510.1.14",
          "form": "그렇군요 저도 한번 읽어봐야겠어요",
          "original_form": "그렇군요 저도 한번 읽어봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012510.1.15",
          "form": "저도 부자가 되고 싶네요",
          "original_form": "저도 부자가 되고 싶네요",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012510.1.16",
          "form": "다른 추천해주실 책이 있을까요?",
          "original_form": "다른 추천해주실 책이 있을까요?",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012510.1.17",
          "form": "차트의 기술 이라는 책도 읽었고요",
          "original_form": "차트의 기술 이라는 책도 읽었고요",
          "speaker_id": "2",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012510.1.18",
          "form": "알려주셔서 감사합니다! 저도 도서를 많이 읽어봐야겠어요",
          "original_form": "알려주셔서 감사합니다! 저도 도서를 많이 읽어봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012510.1.19",
          "form": "좋아하는 작가있으세요?",
          "original_form": "좋아하는 작가있으세요?",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012510.1.20",
          "form": "저는 보건교사 안은영을 쓴 정세랑 작가를 좋아해요",
          "original_form": "저는 보건교사 안은영을 쓴 정세랑 작가를 좋아해요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        }
      ]
