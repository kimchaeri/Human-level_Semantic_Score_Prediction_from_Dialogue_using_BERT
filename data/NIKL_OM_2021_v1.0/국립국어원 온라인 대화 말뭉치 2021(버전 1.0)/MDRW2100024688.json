      "utterance": [
        {
          "id": "MDRW2100024688.1.1",
          "form": "곧 저녁인데 메뉴는 정하셨나요?",
          "original_form": "곧 저녁인데 메뉴는 정하셨나요?",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024688.1.2",
          "form": "전 보통 도시락 싸가지고 다녀요",
          "original_form": "전 보통 도시락 싸가지고 다녀요",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024688.1.3",
          "form": "아.. 그럼 점심 저녁 다 도시락 먹나요?",
          "original_form": "아.. 그럼 점심 저녁 다 도시락 먹나요?",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024688.1.4",
          "form": "네... 같은 밥, 같은 반찬이죠",
          "original_form": "네... 같은 밥, 같은 반찬이죠",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024688.1.5",
          "form": "도시락 주문인가요? 집에서 싸는 건가요?",
          "original_form": "도시락 주문인가요? 집에서 싸는 건가요?",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024688.1.6",
          "form": "걍 밑반찬이랑 적당히 그렇게",
          "original_form": "걍 밑반찬이랑 적당히 그렇게",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024688.1.7",
          "form": "집에서 제가 싸요",
          "original_form": "집에서 제가 싸요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024688.1.8",
          "form": "부지런하네요..",
          "original_form": "부지런하네요..",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024688.1.9",
          "form": "도시락 싸는게 쉽지 않은데",
          "original_form": "도시락 싸는게 쉽지 않은데",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024688.1.10",
          "form": "매일이 반찬고민이긴 한데요",
          "original_form": "매일이 반찬고민이긴 한데요",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024688.1.11",
          "form": "나가서 사먹는게 더 귀찮아요",
          "original_form": "나가서 사먹는게 더 귀찮아요",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024688.1.12",
          "form": "요즘같은 떄는 나가서 먹는 건 위험하기도 하죠",
          "original_form": "요즘같은 떄는 나가서 먹는 건 위험하기도 하죠",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024688.1.13",
          "form": "그럼 집밥 먹는 거랑 똑같네요.. 직접 싸는 도시락이니까",
          "original_form": "그럼 집밥 먹는 거랑 똑같네요.. 직접 싸는 도시락이니까",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024688.1.14",
          "form": "그렇긴 한데요",
          "original_form": "그렇긴 한데요",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024688.1.15",
          "form": "반쯤 레토르트예요",
          "original_form": "반쯤 레토르트예요",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024688.1.16",
          "form": "백종원의 은총을 받고 있죠",
          "original_form": "백종원의 은총을 받고 있죠",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024688.1.17",
          "form": "만능 양념장 최고ㅠㅠ",
          "original_form": "만능 양념장 최고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024688.1.18",
          "form": "반조리라도 그게 어디에요",
          "original_form": "반조리라도 그게 어디에요",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024688.1.19",
          "form": "백종원요리 즐겨 보는군요",
          "original_form": "백종원요리 즐겨 보는군요",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024688.1.20",
          "form": "저도 봅니다 티비로",
          "original_form": "저도 봅니다 티비로",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024688.1.21",
          "form": "name2님은 유투브로 보나요?",
          "original_form": "&name2&님은 유투브로 보나요?",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024688.1.22",
          "form": "전 보통 걍 검색해요",
          "original_form": "전 보통 걍 검색해요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024688.1.23",
          "form": "그럼 누군가가 글로 정리해놨기때문에",
          "original_form": "그럼 누군가가 글로 정리해놨기때문에",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024688.1.24",
          "form": "유튜브는 완전 초보일땐 유용하지만  중수쯤 되면 너무 시간이 많이 걸린달까",
          "original_form": "유튜브는 완전 초보일땐 유용하지만  중수쯤 되면 너무 시간이 많이 걸린달까",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024688.1.25",
          "form": "아.. 블로그 보는군요",
          "original_form": "아.. 블로그 보는군요",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024688.1.26",
          "form": "네, 유튜브도 보긴 해요",
          "original_form": "네, 유튜브도 보긴 해요",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024688.1.27",
          "form": "재밌어서",
          "original_form": "재밌어서",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024688.1.28",
          "form": "유투브 몇번 봤지만 아직은 티비가 익숙해서 티비프로를 봐요",
          "original_form": "유투브 몇번 봤지만 아직은 티비가 익숙해서 티비프로를 봐요",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024688.1.29",
          "form": "티비 많이 나오시죠",
          "original_form": "티비 많이 나오시죠",
          "speaker_id": "2",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024688.1.30",
          "form": "전 근데 요리하시는것보단 먹방하실때가 좋아요",
          "original_form": "전 근데 요리하시는것보단 먹방하실때가 좋아요",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024688.1.31",
          "form": "티비 프로도 여러개 하잖아요.. 그래서 그거보고 참고 해요",
          "original_form": "티비 프로도 여러개 하잖아요.. 그래서 그거보고 참고 해요",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024688.1.32",
          "form": "저번에 눈꽃만두였나 그거 따라해봤어요",
          "original_form": "저번에 눈꽃만두였나 그거 따라해봤어요",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024688.1.33",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024688.1.34",
          "form": "요리를 좀 하다보니까 티비에서 하는 거 보면 어떤 건 따라하기 쉽겠더라구요",
          "original_form": "요리를 좀 하다보니까 티비에서 하는 거 보면 어떤 건 따라하기 쉽겠더라구요",
          "speaker_id": "1",
          "time": "20210831 18:09"
        }
      ]
