      "utterance": [
        {
          "id": "MDRW2100008835.1.1",
          "form": "오늘 날씨 어떤가요?",
          "original_form": "오늘 날씨 어떤가요?",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008835.1.2",
          "form": "날씨 선선하고 좋네요. 그 동네는 어때요?",
          "original_form": "날씨 선선하고 좋네요. 그 동네는 어때요?",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008835.1.3",
          "form": "여긴 흐리고 습하고",
          "original_form": "여긴 흐리고 습하고",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008835.1.4",
          "form": "별루네요ㅠ",
          "original_form": "별루네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008835.1.5",
          "form": "그래도 여름처럼 덥고 습한것 보단 나은 것 같아요.^^",
          "original_form": "그래도 여름처럼 덥고 습한것 보단 나은 것 같아요.^^",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008835.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008835.1.7",
          "form": "이제 가을이 온듯해요",
          "original_form": "이제 가을이 온듯해요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008835.1.8",
          "form": "금새 겨울되겠지요?",
          "original_form": "금새 겨울되겠지요?",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008835.1.9",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008835.1.10",
          "form": "이제 간절기 옷은 꺼내셨어요?",
          "original_form": "이제 간절기 옷은 꺼내셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100008835.1.11",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008835.1.12",
          "form": "넘나 귀찮은ㅠ",
          "original_form": "넘나 귀찮은ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008835.1.13",
          "form": "전 봄 옷 정리도 아직 못해서 그냥 그거 입으려고요ㅋ",
          "original_form": "전 봄 옷 정리도 아직 못해서 그냥 그거 입으려고요ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100008835.1.14",
          "form": "하하 봄이나 가을이나 비슷",
          "original_form": "하하 봄이나 가을이나 비슷",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100008835.1.15",
          "form": "전 색깔만 좀 고려하고 거의 비슷한거 같아요",
          "original_form": "전 색깔만 좀 고려하고 거의 비슷한거 같아요",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100008835.1.16",
          "form": "그냥 들어가면 주서입고ㅠ",
          "original_form": "그냥 들어가면 주서입고ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        }
