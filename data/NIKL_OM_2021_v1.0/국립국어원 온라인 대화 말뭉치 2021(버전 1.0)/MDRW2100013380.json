      "utterance": [
        {
          "id": "MDRW2100013380.1.1",
          "form": "최근에 극장 영화본게  있으신가요?",
          "original_form": "최근에 극장 영화본게  있으신가요?",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013380.1.2",
          "form": "랑종을 봤습니다",
          "original_form": "랑종을 봤습니다",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013380.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013380.1.4",
          "form": "랑종 많이 무섭나요?",
          "original_form": "랑종 많이 무섭나요?",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013380.1.5",
          "form": "무서운 것보다 찜찜함이 컸어요",
          "original_form": "무서운 것보다 찜찜함이 컸어요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.6",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.7",
          "form": "아 물론 안 무서운 건 아녜요",
          "original_form": "아 물론 안 무서운 건 아녜요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.8",
          "form": "잔인하다고 들었어요",
          "original_form": "잔인하다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.9",
          "form": "연출같은게",
          "original_form": "연출같은게",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.10",
          "form": "네. 음 일반 공포물과 스타일이 많이 달라요",
          "original_form": "네. 음 일반 공포물과 스타일이 많이 달라요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.11",
          "form": "몰입하기는 좋았던 것 같아요",
          "original_form": "몰입하기는 좋았던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.12",
          "form": "오 재미는 있어요??",
          "original_form": "오 재미는 있어요??",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013380.1.13",
          "form": "두번 볼 생각은 없지만 돈이 아깝지는 않았어요",
          "original_form": "두번 볼 생각은 없지만 돈이 아깝지는 않았어요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013380.1.14",
          "form": "name1은 최근에 뭘 보셨나요",
          "original_form": "&name1&은 최근에 뭘 보셨나요",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013380.1.15",
          "form": "저는 루카 아시나요? 애니메이션영화 ㅋㅋ",
          "original_form": "저는 루카 아시나요? 애니메이션영화 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013380.1.16",
          "form": "봤고요 콰이어트플레이스2도 봤어요",
          "original_form": "봤고요 콰이어트플레이스2도 봤어요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013380.1.17",
          "form": "아 루카 볼까말까 고민했었어요",
          "original_form": "아 루카 볼까말까 고민했었어요",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013380.1.18",
          "form": "루카 재밌어요?",
          "original_form": "루카 재밌어요?",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013380.1.19",
          "form": "네 재밌어요! 힐링되는 영화에요 ㅋㅋ",
          "original_form": "네 재밌어요! 힐링되는 영화에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013380.1.20",
          "form": "약간 유치하고 말이 안되긴하는데",
          "original_form": "약간 유치하고 말이 안되긴하는데",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013380.1.21",
          "form": "그런거 좋아해서 ㅋㅋ",
          "original_form": "그런거 좋아해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013380.1.22",
          "form": "애니영화는 그런 맛이 있어야죠ㅎ",
          "original_form": "애니영화는 그런 맛이 있어야죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013380.1.23",
          "form": "듣고보니 더 보고 싶어지네요",
          "original_form": "듣고보니 더 보고 싶어지네요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013380.1.24",
          "form": "근데 감동적이에요",
          "original_form": "근데 감동적이에요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013380.1.25",
          "form": "추천해요 ㅋㅋ시간 훅가요",
          "original_form": "추천해요 ㅋㅋ시간 훅가요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013380.1.26",
          "form": "네네 지금은 영화관서는 내려갔겠지요?",
          "original_form": "네네 지금은 영화관서는 내려갔겠지요?",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013380.1.27",
          "form": "안간지 너무 오래되어서 그렇겠죠? ㅠ",
          "original_form": "안간지 너무 오래되어서 그렇겠죠? ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013380.1.28",
          "form": "동영상 플랫폼에서라도 봐야겠어요",
          "original_form": "동영상 플랫폼에서라도 봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013380.1.29",
          "form": "크루엘라는 보셨나요?",
          "original_form": "크루엘라는 보셨나요?",
