      "utterance": [
        {
          "id": "MDRW2100042172.1.1",
          "form": "아 공차 너무 좋죠",
          "original_form": "아 공차 너무 좋죠",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042172.1.2",
          "form": "저 타로진짜 좋아해요",
          "original_form": "저 타로진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042172.1.3",
          "form": "저도요~저 우롱티도 좋아해요",
          "original_form": "저도요~저 우롱티도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042172.1.4",
          "form": "당도몇으로드세용ㅎㅎ",
          "original_form": "당도몇으로드세용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042172.1.5",
          "form": "저 예전에는 70 100으로 먹었는데",
          "original_form": "저 예전에는 70 100으로 먹었는데",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042172.1.6",
          "form": "요즘엔 달아서 30으로 먹어요....",
          "original_form": "요즘엔 달아서 30으로 먹어요....",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042172.1.7",
          "form": "입맛이 바뀌더라구요...",
          "original_form": "입맛이 바뀌더라구요...",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042172.1.8",
          "form": "엇 저도 50으로먹다가 30으로먹어요",
          "original_form": "엇 저도 50으로먹다가 30으로먹어요",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042172.1.9",
          "form": "ㅎㅎㅅ오설록도좋아하세요?",
          "original_form": "ㅎㅎㅅ오설록도좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042172.1.10",
          "form": "음 ㅠ 아뇨 ㅠㅠ 차 좋아한다구하긴햇는데 ㅋㅋㅋㅋ 녹차는 그냥 녹차 말고는 별루 안좋아해요 ㅎㅎ",
          "original_form": "음 ㅠ 아뇨 ㅠㅠ 차 좋아한다구하긴햇는데 ㅋㅋㅋㅋ 녹차는 그냥 녹차 말고는 별루 안좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042172.1.11",
          "form": "녹차맛이나 그런거는 별루 안좋아해서 ㅠㅠ",
          "original_form": "녹차맛이나 그런거는 별루 안좋아해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042172.1.12",
          "form": "그럼어떤차좋아하세요?",
          "original_form": "그럼어떤차좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042172.1.13",
          "form": "ㅎㅅㅎ",
          "original_form": "ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042172.1.14",
          "form": "홍차종류? 좋아해요!!",
          "original_form": "홍차종류? 좋아해요!!",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042172.1.15",
          "form": "앗ㅎㅎ그래서밀크티",
          "original_form": "앗ㅎㅎ그래서밀크티",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042172.1.16",
          "form": "저도잉글리시블랙퍼스트좋아해요",
          "original_form": "저도잉글리시블랙퍼스트좋아해요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042172.1.17",
          "form": "네 ㅋㅋㅋㅋ 잉글리시블랙퍼스트 좋죠ㅠㅠ",
          "original_form": "네 ㅋㅋㅋㅋ 잉글리시블랙퍼스트 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042172.1.18",
          "form": "저는 레이디그레이!도 좋아하구",
          "original_form": "저는 레이디그레이!도 좋아하구",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042172.1.19",
          "form": "최애는 웨딩임페리얼인데 ㅠㅠ",
          "original_form": "최애는 웨딩임페리얼인데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042172.1.20",
          "form": "요즘 살까말까 고민중이에요 ㅋㅋㅋ",
          "original_form": "요즘 살까말까 고민중이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042172.1.21",
          "form": "가을엔차따끈하게먹음좋죠",
          "original_form": "가을엔차따끈하게먹음좋죠",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042172.1.22",
          "form": "카페도자주가세요?",
          "original_form": "카페도자주가세요?",
