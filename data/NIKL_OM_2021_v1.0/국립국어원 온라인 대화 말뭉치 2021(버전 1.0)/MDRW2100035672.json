      "utterance": [
        {
          "id": "MDRW2100035672.1.1",
          "form": "맞아요 롱패딩 만든사람 상줘야해요ㅋㅋㅋ",
          "original_form": "맞아요 롱패딩 만든사람 상줘야해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.2",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.3",
          "form": "봄은 좋아하세요?",
          "original_form": "봄은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.4",
          "form": "봄좋죠",
          "original_form": "봄좋죠",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.5",
          "form": "저는 제일 싫어하는 계절이 봄이에요ㅜㅠㅋㅋㅋㅋ",
          "original_form": "저는 제일 싫어하는 계절이 봄이에요ㅜㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.6",
          "form": "저는 다행히 알러지가 없어서",
          "original_form": "저는 다행히 알러지가 없어서",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.7",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035672.1.8",
          "form": "꽃도좋고 소나무도 좋아요",
          "original_form": "꽃도좋고 소나무도 좋아요",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.9",
          "form": "꽃은 너무 예쁜데",
          "original_form": "꽃은 너무 예쁜데",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.10",
          "form": "미세먼지에",
          "original_form": "미세먼지에",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.11",
          "form": "황사에",
          "original_form": "황사에",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.12",
          "form": "꽃가루 흩날려서",
          "original_form": "꽃가루 흩날려서",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.13",
          "form": "ㅠㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "ㅠㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.14",
          "form": "아핯그건 좀 글쵸ㅜ",
          "original_form": "아핯그건 좀 글쵸ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.15",
          "form": "그래서 봄이 제일 싫어요ㅠㅠ 피부 뒤집어지기",
          "original_form": "그래서 봄이 제일 싫어요ㅠㅠ 피부 뒤집어지기",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.16",
          "form": "쉬운 계절이라서ㅠㅠ",
          "original_form": "쉬운 계절이라서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.17",
          "form": "저는 name2님이 겨울감성 좋아하시는것처럼 봄에서 여름 넘어가는 감성을 좋아해요ㅎㅎ",
          "original_form": "저는 &name2&님이 겨울감성 좋아하시는것처럼 봄에서 여름 넘어가는 감성을 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035672.1.18",
          "form": "그 따뜻하고 초록초록한 분위기요",
          "original_form": "그 따뜻하고 초록초록한 분위기요",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035672.1.19",
          "form": "아하 막 5~6월 그쯤을 좋아하시나봐용?",
          "original_form": "아하 막 5~6월 그쯤을 좋아하시나봐용?",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035672.1.20",
          "form": "네네 슬슬 더워질때요",
          "original_form": "네네 슬슬 더워질때요",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035672.1.21",
          "form": "5~6월쯤은 황사도없고 꽃가루로 덜 날리고",
          "original_form": "5~6월쯤은 황사도없고 꽃가루로 덜 날리고",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035672.1.22",
          "form": "저도 좋아해요ㅎㅎ",
          "original_form": "저도 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:15"
        }
      ]
