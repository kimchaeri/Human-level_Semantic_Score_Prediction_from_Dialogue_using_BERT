      "utterance": [
        {
          "id": "MDRW2100025314.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아마 안예은식 귀신얘기는 아직 능소화랑 창귀가 다일테지만..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아마 안예은식 귀신얘기는 아직 능소화랑 창귀가 다일테지만..",
          "speaker_id": "2",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.2",
          "form": "능소화는.... 응......",
          "original_form": "능소화는.... 응......",
          "speaker_id": "2",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.3",
          "form": "15세 버전만 보세요",
          "original_form": "15세 버전만 보세요",
          "speaker_id": "2",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.5",
          "form": "보시더라도..",
          "original_form": "보시더라도..",
          "speaker_id": "2",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.6",
          "form": "19세 버전은 아예 유튜브에서 볼때도",
          "original_form": "19세 버전은 아예 유튜브에서 볼때도",
          "speaker_id": "2",
          "time": "20210901 09:09"
        },
        {
          "id": "MDRW2100025314.1.7",
          "form": "더내줬으면좋겠다",
          "original_form": "더내줬으면좋겠다",
          "speaker_id": "1",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.8",
          "form": "성인 인증 해야 볼 수 있더라",
          "original_form": "성인 인증 해야 볼 수 있더라",
          "speaker_id": "2",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.9",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.10",
          "form": "근데 잔인한것도 나오나여",
          "original_form": "근데 잔인한것도 나오나여",
          "speaker_id": "1",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.11",
          "form": "뮤비에",
          "original_form": "뮤비에",
          "speaker_id": "1",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.12",
          "form": "귀로 듣는 납량특집인가? 그런 시리즈로",
          "original_form": "귀로 듣는 납량특집인가? 그런 시리즈로",
          "speaker_id": "2",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.13",
          "form": "내려는듯",
          "original_form": "내려는듯",
          "speaker_id": "2",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.14",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.15",
          "form": "주인공한테 잔인한 짓을 하진 않는데",
          "original_form": "주인공한테 잔인한 짓을 하진 않는데",
          "speaker_id": "2",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.16",
          "form": "오...기획괜찮다",
          "original_form": "오...기획괜찮다",
          "speaker_id": "1",
          "time": "20210901 09:10"
        },
        {
          "id": "MDRW2100025314.1.17",
          "form": "잔인한 몰골을 한 귀신이.. 나옵니다",
          "original_form": "잔인한 몰골을 한 귀신이.. 나옵니다",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.18",
          "form": "아아......?!",
          "original_form": "아아......?!",
          "speaker_id": "1",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.19",
          "form": "디용 무서워",
          "original_form": "디용 무서워",
          "speaker_id": "1",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.20",
          "form": "그래서 19세...",
          "original_form": "그래서 19세...",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.21",
          "form": "아니 막 공포영화급 그런건 아닌데",
          "original_form": "아니 막 공포영화급 그런건 아닌데",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.22",
          "form": "일단 15세버잔을보고",
          "original_form": "일단 15세버잔을보고",
          "speaker_id": "1",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.23",
          "form": "나머지를볼지말지",
          "original_form": "나머지를볼지말지",
          "speaker_id": "1",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.24",
          "form": "무서운거 싫어하면 좋아할만한 분장은 아닌...",
          "original_form": "무서운거 싫어하면 좋아할만한 분장은 아닌...",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.25",
          "form": "결정해야겠군여",
          "original_form": "결정해야겠군여",
          "speaker_id": "1",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.26",
          "form": "ㅇㅇㅇㅇ 그렇게 하소서",
          "original_form": "ㅇㅇㅇㅇ 그렇게 하소서",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025314.1.27",
          "form": "아 저는 깜놀하는부분만 없으면 돼",
          "original_form": "아 저는 깜놀하는부분만 없으면 돼",
          "speaker_id": "1",
          "time": "20210901 09:12"
        },
        {
          "id": "MDRW2100025314.1.28",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210901 09:12"
        },
        {
          "id": "MDRW2100025314.1.29",
          "form": "잠시만여",
          "original_form": "잠시만여",
          "speaker_id": "2",
          "time": "20210901 09:12"
        },
        {
          "id": "MDRW2100025314.1.30",
          "form": "다시 한번 보고 와야겠다",
          "original_form": "다시 한번 보고 와야겠다",
          "speaker_id": "2",
          "time": "20210901 09:13"
        },
        {
          "id": "MDRW2100025314.1.31",
          "form": "그 왕!!하고 놀래키는거만 없으면 ㅇㅋ",
          "original_form": "그 왕!!하고 놀래키는거만 없으면 ㅇㅋ",
          "speaker_id": "1",
          "time": "20210901 09:13"
        }
      ]
