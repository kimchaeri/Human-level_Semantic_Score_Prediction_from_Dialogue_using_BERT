      "utterance": [
        {
          "id": "MDRW2100011721.1.1",
          "form": "뮤지컬본적있음?",
          "original_form": "뮤지컬본적있음?",
          "speaker_id": "1",
          "time": "20210825 09:58"
        },
        {
          "id": "MDRW2100011721.1.2",
          "form": "연극이나",
          "original_form": "연극이나",
          "speaker_id": "1",
          "time": "20210825 09:58"
        },
        {
          "id": "MDRW2100011721.1.3",
          "form": "응 학창시절에 현장 학습으로 가서 본 적이 있어",
          "original_form": "응 학창시절에 현장 학습으로 가서 본 적이 있어",
          "speaker_id": "2",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100011721.1.4",
          "form": "그거말고",
          "original_form": "그거말고",
          "speaker_id": "1",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100011721.1.5",
          "form": "그냥개인으로가서본적있음?",
          "original_form": "그냥개인으로가서본적있음?",
          "speaker_id": "1",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100011721.1.6",
          "form": "아니 초등학교 고학년때 아동용 피터팬 본 거 이후로는 없는 것 같아 아마",
          "original_form": "아니 초등학교 고학년때 아동용 피터팬 본 거 이후로는 없는 것 같아 아마",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100011721.1.7",
          "form": "볼려고 했는데 바로 코로나가 터져버려가지고",
          "original_form": "볼려고 했는데 바로 코로나가 터져버려가지고",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.8",
          "form": "너는 뭐 본 거 있어?",
          "original_form": "너는 뭐 본 거 있어?",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.9",
          "form": "ㄴㄴㄴ없어",
          "original_form": "ㄴㄴㄴ없어",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.10",
          "form": "나도그때",
          "original_form": "나도그때",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.11",
          "form": "학교에서간거밖에없음",
          "original_form": "학교에서간거밖에없음",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.12",
          "form": "그냥넌덕질해서",
          "original_form": "그냥넌덕질해서",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.13",
          "form": "뮤지컬봤을중알았음",
          "original_form": "뮤지컬봤을중알았음",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100011721.1.14",
          "form": "조승우 티켓은 구하기 힘들어...",
          "original_form": "조승우 티켓은 구하기 힘들어...",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100011721.1.15",
          "form": "나는 꼭 조승우가 자기 작품 티켓팅 해봤으면 좋겠어",
          "original_form": "나는 꼭 조승우가 자기 작품 티켓팅 해봤으면 좋겠어",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.16",
          "form": "근데여러번하잖아",
          "original_form": "근데여러번하잖아",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래야알긴함",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래야알긴함",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.18",
          "form": "여러번 하지 대신 나는 여러번 떨어지고",
          "original_form": "여러번 하지 대신 나는 여러번 떨어지고",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.19",
          "form": "이 양반 맨날 전석 매진이면서",
          "original_form": "이 양반 맨날 전석 매진이면서",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.20",
          "form": "ㅋㅋㅋㅋ아 조승우 아직도",
          "original_form": "ㅋㅋㅋㅋ아 조승우 아직도",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.21",
          "form": "뮤지컬해?",
          "original_form": "뮤지컬해?",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.22",
          "form": "드라마만하는줄",
          "original_form": "드라마만하는줄",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.23",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011721.1.24",
          "form": "헤드윅 했고",
          "original_form": "헤드윅 했고",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.25",
          "form": "이제 지앤하 할 걸?",
          "original_form": "이제 지앤하 할 걸?",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.26",
          "form": "매회차마다매진인거임?",
          "original_form": "매회차마다매진인거임?",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.27",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.28",
          "form": "뷥석부터 알석까지 다",
          "original_form": "뷥석부터 알석까지 다",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.29",
          "form": "전회전석매진",
          "original_form": "전회전석매진",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.30",
          "form": "오 뮤지컬티켓비싸지않음?",
          "original_form": "오 뮤지컬티켓비싸지않음?",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011721.1.31",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 11:42"
        }
      ]
    }
