      "utterance": [
        {
          "id": "MDRW2100022621.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022621.1.2",
          "form": "집안일이요?",
          "original_form": "집안일이요?",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022621.1.3",
          "form": "뭐 기본적으로 빨래나 설거지는 하긴하죠",
          "original_form": "뭐 기본적으로 빨래나 설거지는 하긴하죠",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022621.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022621.1.5",
          "form": "아! 저는 주로 빨래를 담당했어요",
          "original_form": "아! 저는 주로 빨래를 담당했어요",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022621.1.6",
          "form": "화장실이대박이죠",
          "original_form": "화장실이대박이죠",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022621.1.7",
          "form": "아 화장실 진짜 대박이죠...",
          "original_form": "아 화장실 진짜 대박이죠...",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022621.1.8",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022621.1.9",
          "form": "락스냄새 엄청 심하죠..",
          "original_form": "락스냄새 엄청 심하죠..",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022621.1.10",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022621.1.11",
          "form": "그리고 잘못 쓰면 피부도 상하고요",
          "original_form": "그리고 잘못 쓰면 피부도 상하고요",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022621.1.12",
          "form": "넘 안 좋아여",
          "original_form": "넘 안 좋아여",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022621.1.13",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022621.1.14",
          "form": "그래도 요즘에는 락스 대신 화장실 대체용품이 많이 나오더라고요",
          "original_form": "그래도 요즘에는 락스 대신 화장실 대체용품이 많이 나오더라고요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022621.1.15",
          "form": "아 그리고 락스 대신에",
          "original_form": "아 그리고 락스 대신에",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022621.1.16",
          "form": "쓰다가 애매하게 남은 세수비누를 활용하는 방법도 있고요.",
          "original_form": "쓰다가 애매하게 남은 세수비누를 활용하는 방법도 있고요.",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022621.1.17",
          "form": "곰팡이걱정은요",
          "original_form": "곰팡이걱정은요",
          "speaker_id": "1",
          "time": "20210829 16:56"
        }
      ]
