      "utterance": [
        {
          "id": "MDRW2100000846.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000846.1.2",
          "form": "여행좋아해?",
          "original_form": "여행좋아해?",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000846.1.3",
          "form": "좋아하지 나쁜코로나",
          "original_form": "좋아하지 나쁜코로나",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000846.1.4",
          "form": "맞아 코로나때문에 여행이고 뭐고 다 불가능해졌어 ㅠ",
          "original_form": "맞아 코로나때문에 여행이고 뭐고 다 불가능해졌어 ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000846.1.5",
          "form": "코로나 종식되면 어디가고싶어",
          "original_form": "코로나 종식되면 어디가고싶어",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000846.1.6",
          "form": "부산 전주 서울 어린이대공원 ㅎ",
          "original_form": "부산 전주 서울 어린이대공원 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000846.1.7",
          "form": "가고싶은 곳 너무 많지 애들도 답답해하고",
          "original_form": "가고싶은 곳 너무 많지 애들도 답답해하고",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000846.1.8",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000846.1.9",
          "form": "누구랑 가고싶엉",
          "original_form": "누구랑 가고싶엉",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000846.1.10",
          "form": "가족이랑도 가고 싶고 절친이랑도 몇년째 못간 여행 가고파",
          "original_form": "가족이랑도 가고 싶고 절친이랑도 몇년째 못간 여행 가고파",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000846.1.11",
          "form": "여행 많이 다닌 편이야?",
          "original_form": "여행 많이 다닌 편이야?",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000846.1.12",
          "form": "그럼 좋아하지~",
          "original_form": "그럼 좋아하지~",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000846.1.13",
          "form": "요즘 혼자 내차타고 다니고있어",
          "original_form": "요즘 혼자 내차타고 다니고있어",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000846.1.14",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000846.1.15",
          "form": "부산 강원도 천안 전주 창원",
          "original_form": "부산 강원도 천안 전주 창원",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000846.1.16",
          "form": "우와 집이 어딘지 물어도 되나요?",
          "original_form": "우와 집이 어딘지 물어도 되나요?",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000846.1.17",
          "form": "여행지가 너무 전국구라 ㅎ",
          "original_form": "여행지가 너무 전국구라 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000846.1.18",
          "form": "난 수원에 살아",
          "original_form": "난 수원에 살아",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000846.1.19",
          "form": "수원도 여행지로 괜찮아",
          "original_form": "수원도 여행지로 괜찮아",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000846.1.20",
          "form": "수원화성? 그거밖에 모르겠어....",
          "original_form": "수원화성? 그거밖에 모르겠어....",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000846.1.21",
          "form": "추천해주고픈 관광지 있어?",
          "original_form": "추천해주고픈 관광지 있어?",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000846.1.22",
          "form": "광명동굴",
          "original_form": "광명동굴",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000846.1.23",
          "form": "가봤어?",
          "original_form": "가봤어?",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000846.1.24",
          "form": "인스타에서 보기만 ㅎ",
          "original_form": "인스타에서 보기만 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000846.1.25",
          "form": "가족들과 한번 가봐 좋앙",
          "original_form": "가족들과 한번 가봐 좋앙",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000846.1.26",
          "form": "어린 애들데리고 괜찮아?",
          "original_form": "어린 애들데리고 괜찮아?",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000846.1.27",
          "form": "응 괜찮아 좀 길긴하지만",
          "original_form": "응 괜찮아 좀 길긴하지만",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000846.1.28",
          "form": "기억해놨다가 코로나 잠잠해지면 꼭 가볼게",
          "original_form": "기억해놨다가 코로나 잠잠해지면 꼭 가볼게",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000846.1.29",
          "form": "구랭 꼭 가봐!ㅎ",
          "original_form": "구랭 꼭 가봐!ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000846.1.30",
          "form": "다녔던 곳중 너무 좋았던 곳 있음 추천 좀 해줘",
          "original_form": "다녔던 곳중 너무 좋았던 곳 있음 추천 좀 해줘",
          "speaker_id": "1",
          "time": "20210819 20:42"
        }
      ]
    }
  ]
