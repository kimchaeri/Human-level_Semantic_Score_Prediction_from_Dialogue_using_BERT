      "utterance": [
        {
          "id": "MDRW2100018332.1.1",
          "form": "운동복 많이사기나요?",
          "original_form": "운동복 많이사기나요?",
          "speaker_id": "1",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100018332.1.2",
          "form": "ㅋㅋㅋㅋ 요즘은 안사요 ..자제해야죠",
          "original_form": "ㅋㅋㅋㅋ 요즘은 안사요 ..자제해야죠",
          "speaker_id": "2",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100018332.1.3",
          "form": "근데이쁜거넘많아요",
          "original_form": "근데이쁜거넘많아요",
          "speaker_id": "2",
          "time": "20210826 23:54"
        },
        {
          "id": "MDRW2100018332.1.4",
          "form": "ㅋㅋㅋ 저도 운동족 좋아해서..",
          "original_form": "ㅋㅋㅋ 저도 운동족 좋아해서..",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018332.1.5",
          "form": "어디거 조아하세용 ㅋㅋ",
          "original_form": "어디거 조아하세용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018332.1.6",
          "form": "안사야지 하면서도.. ㅋㅋㅋ\n기능성이라면 그냥..",
          "original_form": "안사야지 하면서도.. ㅋㅋㅋ\n기능성이라면 그냥..",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018332.1.7",
          "form": "최애는 아디다스요",
          "original_form": "최애는 아디다스요",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018332.1.8",
          "form": "아웃도어는 노페",
          "original_form": "아웃도어는 노페",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018332.1.9",
          "form": "오 마자요 아디 이뻐요 ㅠㅠ",
          "original_form": "오 마자요 아디 이뻐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018332.1.10",
          "form": "세일만 기다림",
          "original_form": "세일만 기다림",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018332.1.11",
          "form": "신발도 조아하구 ㅋㅋㅋㅋ",
          "original_form": "신발도 조아하구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018332.1.12",
          "form": "어디꺼 좋아하 세요?",
          "original_form": "어디꺼 좋아하 세요?",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018332.1.13",
          "form": "신발 아디다스 주로 ㅋㅋㅋ 나이키도 이쁜거잇음 좋아하고용...",
          "original_form": "신발 아디다스 주로 ㅋㅋㅋ 나이키도 이쁜거잇음 좋아하고용...",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018332.1.14",
          "form": "올마네 뉴발 530도 삿엇는데 이쁘던데용",
          "original_form": "올마네 뉴발 530도 삿엇는데 이쁘던데용",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018332.1.15",
          "form": "신발 나이키꺼가 요즘 넘 이뻐요...",
          "original_form": "신발 나이키꺼가 요즘 넘 이뻐요...",
          "speaker_id": "1",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100018332.1.16",
          "form": "전 바람막이 재질 좋아해서..",
          "original_form": "전 바람막이 재질 좋아해서..",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100018332.1.17",
          "form": "사고서 맨날 후회해요..",
          "original_form": "사고서 맨날 후회해요..",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100018332.1.18",
          "form": "ㅋㅋㅋㅋ 왜후회하세용",
          "original_form": "ㅋㅋㅋㅋ 왜후회하세용",
          "speaker_id": "2",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100018332.1.19",
          "form": "사고 보면 비슷한게 있어요.. ㅋㅋㅋ",
          "original_form": "사고 보면 비슷한게 있어요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100018332.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 바막은...글쵸... 그래도이뻐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 바막은...글쵸... 그래도이뻐요",
          "speaker_id": "2",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100018332.1.21",
          "form": "맨날 이뻐서 사는데.. 집에와서 보면 있는거고",
          "original_form": "맨날 이뻐서 사는데.. 집에와서 보면 있는거고",
