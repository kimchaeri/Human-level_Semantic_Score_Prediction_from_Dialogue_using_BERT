      "utterance": [
        {
          "id": "MDRW2100045752.1.1",
          "form": "배달 자주 드시나요?",
          "original_form": "배달 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045752.1.2",
          "form": "네 요리 귀찮을 때 시켜요",
          "original_form": "네 요리 귀찮을 때 시켜요",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045752.1.3",
          "form": "어플은 어떤거 쓰세요?",
          "original_form": "어플은 어떤거 쓰세요?",
          "speaker_id": "2",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045752.1.4",
          "form": "요기요 씁니다!",
          "original_form": "요기요 씁니다!",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045752.1.5",
          "form": "저는 경기도 살아서 경기도 공공앱 가끔 써요",
          "original_form": "저는 경기도 살아서 경기도 공공앱 가끔 써요",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045752.1.6",
          "form": "오 그거 할인되나요?",
          "original_form": "오 그거 할인되나요?",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045752.1.7",
          "form": "지역화폐를 쓸수있어서 무조건 10퍼할인이에요 ㅎㅎ",
          "original_form": "지역화폐를 쓸수있어서 무조건 10퍼할인이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045752.1.8",
          "form": "헐 저도 이제 써야겠어요",
          "original_form": "헐 저도 이제 써야겠어요",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045752.1.9",
          "form": "개꿀이네요",
          "original_form": "개꿀이네요",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045752.1.10",
          "form": "경기도세요?",
          "original_form": "경기도세요?",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045752.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045752.1.12",
          "form": "그럼써보세요 ㅎㅎ",
          "original_form": "그럼써보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045752.1.13",
          "form": "저는 없는 음식점도 있긴 하지미ㅏㄴ",
          "original_form": "저는 없는 음식점도 있긴 하지미ㅏㄴ",
          "speaker_id": "2",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045752.1.14",
          "form": "대부분 있더라구요",
          "original_form": "대부분 있더라구요",
          "speaker_id": "2",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045752.1.15",
          "form": "글고 이번에 지원금 주는거 혹시 받으시면 지역화폐로 넣어서 배달도 가능하다고.하네요",
          "original_form": "글고 이번에 지원금 주는거 혹시 받으시면 지역화폐로 넣어서 배달도 가능하다고.하네요",
          "speaker_id": "2",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045752.1.16",
          "form": "오 완전 꿀팁이네요!",
          "original_form": "오 완전 꿀팁이네요!",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045752.1.17",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045752.1.18",
          "form": "네 저는 지원금 받아서 배달시켜먹을 생각만 하고있네요 ㅋㅋ",
          "original_form": "네 저는 지원금 받아서 배달시켜먹을 생각만 하고있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045752.1.19",
          "form": "저도 그래야겠어요ㅋㅋㅋㅋ",
          "original_form": "저도 그래야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045752.1.20",
          "form": "횡재네요",
          "original_form": "횡재네요",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045752.1.21",
          "form": "네 한번 어플 받아보세요 ㅎㅎ",
          "original_form": "네 한번 어플 받아보세요 ㅎㅎ",
          "speaker_id": "2",
