      "utterance": [
        {
          "id": "MDRW2100029379.1.1",
          "form": "고양이 vs 개",
          "original_form": "고양이 vs 개",
          "speaker_id": "1",
          "time": "20210903 03:42"
        },
        {
          "id": "MDRW2100029379.1.2",
          "form": "음, 난 둘다 동등하게 귀엽다고 생각해 ㅋㅋㅋㅋㅋ",
          "original_form": "음, 난 둘다 동등하게 귀엽다고 생각해 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:52"
        },
        {
          "id": "MDRW2100029379.1.3",
          "form": "햄스터는 왼볼잡이와 오른볼잡이가 있대",
          "original_form": "햄스터는 왼볼잡이와 오른볼잡이가 있대",
          "speaker_id": "1",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100029379.1.4",
          "form": "먹이를 저장하는거지",
          "original_form": "먹이를 저장하는거지",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029379.1.5",
          "form": "내 친구 반려햄은 양볼잡이래",
          "original_form": "내 친구 반려햄은 양볼잡이래",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029379.1.6",
          "form": "그러자 다른 친구가 넣자마자 판단하지 말라면서",
          "original_form": "그러자 다른 친구가 넣자마자 판단하지 말라면서",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029379.1.7",
          "form": "욕했어",
          "original_form": "욕했어",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029379.1.8",
          "form": "와 그냥 양쪽에 넣는 게 아니라",
          "original_form": "와 그냥 양쪽에 넣는 게 아니라",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029379.1.9",
          "form": "각자 방식이있구나",
          "original_form": "각자 방식이있구나",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029379.1.10",
          "form": "우수납 좌수납",
          "original_form": "우수납 좌수납",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029379.1.11",
          "form": "ㅇㅇ 그렇대",
          "original_form": "ㅇㅇ 그렇대",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029379.1.12",
          "form": "나는 햄이 없지만 내친구 햄 이름은",
          "original_form": "나는 햄이 없지만 내친구 햄 이름은",
          "speaker_id": "1",
