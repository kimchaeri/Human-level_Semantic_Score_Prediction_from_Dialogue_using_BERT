        {
          "id": "MDRW2100029379.1.19",
          "form": "6마리야",
          "original_form": "6마리야",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029379.1.20",
          "form": "7마리였던 거 같은데 이름 잘못기억하고 있나?",
          "original_form": "7마리였던 거 같은데 이름 잘못기억하고 있나?",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029379.1.21",
          "form": "아니다 name3 name5 name6 name9 다섯 마리였나",
          "original_form": "아니다 &name3& &name5& &name6& &name9& 다섯 마리였나",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029379.1.22",
          "form": "암튼 그래",
          "original_form": "암튼 그래",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029379.1.23",
          "form": "헐 ㅋㅋㅋㅋㅋ 왜 그렇게 지은거야",
          "original_form": "헐 ㅋㅋㅋㅋㅋ 왜 그렇게 지은거야",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029379.1.24",
          "form": "근데 햄찌는 1찌 1케이지 아닌가",
          "original_form": "근데 햄찌는 1찌 1케이지 아닌가",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029379.1.25",
          "form": "그만큼 키우는 거 넘\r\n\r\n....대단",
          "original_form": "그만큼 키우는 거 넘\r\n\r\n....대단",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029379.1.26",
          "form": "마저ㅋㅋㅋㅋ 예전에보니까 벽 한면이 햄이더라",
          "original_form": "마저ㅋㅋㅋㅋ 예전에보니까 벽 한면이 햄이더라",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029379.1.27",
          "form": "와.... 그거 청소도 힘들겠다",
          "original_form": "와.... 그거 청소도 힘들겠다",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029379.1.28",
          "form": "반려동물키우는 사람은 돈많고 부지런해야함",
          "original_form": "반려동물키우는 사람은 돈많고 부지런해야함",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029379.1.29",
          "form": "맞아 내 다른 친구는 생선 키우는데",
          "original_form": "맞아 내 다른 친구는 생선 키우는데",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029379.1.30",
          "form": "물고기",
          "original_form": "물고기",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029379.1.31",
          "form": "일어나서 제일 먼저 하는게",
          "original_form": "일어나서 제일 먼저 하는게",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029379.1.32",
          "form": "수조 뒤집기래",
          "original_form": "수조 뒤집기래",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029379.1.33",
          "form": "생선ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "생선ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029379.1.34",
          "form": "다들 많이 키우네 수조도 물떼껴서",
          "original_form": "다들 많이 키우네 수조도 물떼껴서",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029379.1.35",
          "form": "관리어려울듯 ㅠㅠ",
          "original_form": "관리어려울듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029379.1.36",
          "form": "친구 하나는 구피 키우는데 쪼만한 생성",
          "original_form": "친구 하나는 구피 키우는데 쪼만한 생성",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029379.1.37",
          "form": "한명은 관상용 키우는데",
          "original_form": "한명은 관상용 키우는데",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029379.1.38",
          "form": "매주 수조정리하는데 얘는 수조정리를 사랑해",
          "original_form": "매주 수조정리하는데 얘는 수조정리를 사랑해",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029379.1.39",
          "form": "헐 보람있겠네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 보람있겠네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029379.1.40",
          "form": "좋아하는 일이면 ㄱㅊㄱㅊ",
          "original_form": "좋아하는 일이면 ㄱㅊㄱㅊ",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029379.1.41",
          "form": "그래서 뭔가 내눈엔 달라진게 없는데",
          "original_form": "그래서 뭔가 내눈엔 달라진게 없는데",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029379.1.42",
          "form": "뭔가 꾸몄대",
          "original_form": "뭔가 꾸몄대",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029379.1.43",
          "form": "허엉... 하는 중",
          "original_form": "허엉... 하는 중",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029379.1.44",
          "form": "ㅋㅋㅋㅋㅋ이끼가 달라졋다구!",
          "original_form": "ㅋㅋㅋㅋㅋ이끼가 달라졋다구!",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029379.1.45",
          "form": "돌맹이가 달라졌다구!",
          "original_form": "돌맹이가 달라졌다구!",
