        {
          "id": "MDRW2100033785.1.1",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033785.1.2",
          "form": "짜장면이 좋아요",
          "original_form": "짜장면이 좋아요",
          "speaker_id": "2",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033785.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033785.1.4",
          "form": "ㅋㅋㅋ저는",
          "original_form": "ㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033785.1.5",
          "form": "짬뽕파입니다",
          "original_form": "짬뽕파입니다",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033785.1.6",
          "form": "찍먹vs부먹은요?",
          "original_form": "찍먹vs부먹은요?",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033785.1.7",
          "form": "배고파서 맘대로먹어요",
          "original_form": "배고파서 맘대로먹어요",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033785.1.8",
          "form": "전엔 찍먹선호했지만 지금은 아무렇게나먹어요",
          "original_form": "전엔 찍먹선호했지만 지금은 아무렇게나먹어요",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100033785.1.9",
          "form": "전 찍먹이요",
          "original_form": "전 찍먹이요",
          "speaker_id": "1",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100033785.1.10",
          "form": "부먹은 정말 이해할수없어요",
          "original_form": "부먹은 정말 이해할수없어요",
          "speaker_id": "1",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100033785.1.11",
          "form": "오 그러시군요 확고하시다",
          "original_form": "오 그러시군요 확고하시다",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033785.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033785.1.13",
          "form": "크림스파게티vs토마토스파게티 어떠세요?",
          "original_form": "크림스파게티vs토마토스파게티 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033785.1.14",
          "form": "배고프연 다 잘먹는병이 있나봐요 저",
          "original_form": "배고프연 다 잘먹는병이 있나봐요 저",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033785.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033785.1.16",
          "form": "하나를",
          "original_form": "하나를",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033785.1.17",
          "form": "못고르시는군요ㅋㅋㅋ",
          "original_form": "못고르시는군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033785.1.18",
          "form": "앞에 식사메뉴가 중요해요",
          "original_form": "앞에 식사메뉴가 중요해요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033785.1.19",
          "form": "우유마셨으면 크림피해요",
          "original_form": "우유마셨으면 크림피해요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033785.1.20",
          "form": "ㅋㅋㅋㅋㅋ 앞에식사가중요하군요",
          "original_form": "ㅋㅋㅋㅋㅋ 앞에식사가중요하군요",
