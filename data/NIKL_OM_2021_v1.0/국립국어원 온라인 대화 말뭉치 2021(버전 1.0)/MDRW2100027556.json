      "utterance": [
        {
          "id": "MDRW2100027556.1.1",
          "form": "맞아요 저도",
          "original_form": "맞아요 저도",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027556.1.2",
          "form": "관리는못하겠더르규욬ㅋㅋㅋ",
          "original_form": "관리는못하겠더르규욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027556.1.3",
          "form": "연애랑 결혼... 사실은 너무 먼 이야기같아요 지금은ㅋㅋㅋ",
          "original_form": "연애랑 결혼... 사실은 너무 먼 이야기같아요 지금은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027556.1.4",
          "form": "솔로라서 그렇기도 하고 결혼은 특히 환상만 있어요 ㅋㅋ",
          "original_form": "솔로라서 그렇기도 하고 결혼은 특히 환상만 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027556.1.5",
          "form": "저도 결혼은 먼가 환상이 많아요",
          "original_form": "저도 결혼은 먼가 환상이 많아요",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027556.1.6",
          "form": "연애도 나름 환상이있는데",
          "original_form": "연애도 나름 환상이있는데",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027556.1.7",
          "form": "실천이안되더라구욬ㅋㅋㅋ",
          "original_form": "실천이안되더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027556.1.8",
          "form": "앜ㅋㅋㅋ 연애 환상은 어떤거 있으세요?",
          "original_form": "앜ㅋㅋㅋ 연애 환상은 어떤거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027556.1.9",
          "form": "맞아오 막",
          "original_form": "맞아오 막",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027556.1.10",
          "form": "음 안싸우고",
          "original_form": "음 안싸우고",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027556.1.11",
          "form": "오순도순 이야기하면서",
          "original_form": "오순도순 이야기하면서",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027556.1.12",
          "form": "서로 사진도찍어주고",
          "original_form": "서로 사진도찍어주고",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027556.1.13",
          "form": "머그런거 ㅋㅋㅋㅋ",
          "original_form": "머그런거 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027556.1.14",
          "form": "우와 딱 로망이네요",
          "original_form": "우와 딱 로망이네요",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027556.1.15",
          "form": "저는 그거 커플템 싹 맞추기 ㅋㅋㅋ",
          "original_form": "저는 그거 커플템 싹 맞추기 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027556.1.16",
          "form": "그런게 로망이에요 악세사리 옷 신발 핸드폰까지 ㅋㅋㅋ",
          "original_form": "그런게 로망이에요 악세사리 옷 신발 핸드폰까지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027556.1.17",
          "form": "홀 저도요...",
          "original_form": "홀 저도요...",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027556.1.18",
          "form": "그런거좋아하는데",
          "original_form": "그런거좋아하는데",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027556.1.19",
          "form": "말하기도 무섭더라구요",
          "original_form": "말하기도 무섭더라구요",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027556.1.20",
          "form": "아앗ㅋㅋ 왜 무서워요ㅠㅠ 말해보면 되징ㅠㅜ",
          "original_form": "아앗ㅋㅋ 왜 무서워요ㅠㅠ 말해보면 되징ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100027556.1.21",
          "form": "걔는싫은데저홐자 티내는걸까바영ㅋㅋㅋ",
          "original_form": "걔는싫은데저홐자 티내는걸까바영ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100027556.1.22",
          "form": "아하ㅜㅠ 그럴 수도 있네요ㅠㅠ",
          "original_form": "아하ㅜㅠ 그럴 수도 있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100027556.1.23",
          "form": "말하면 먼가 싫어할깜시 ㅠ",
          "original_form": "말하면 먼가 싫어할깜시 ㅠ",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100027556.1.24",
          "form": "외국애들은 특히 이해를 못한대용",
          "original_form": "외국애들은 특히 이해를 못한대용",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100027556.1.25",
          "form": "아 코로나 끝나면 소개팅하고 싶어요ㅜㅜ",
          "original_form": "아 코로나 끝나면 소개팅하고 싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100027556.1.26",
          "form": "소개팅좋쵸",
          "original_form": "소개팅좋쵸",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100027556.1.27",
          "form": "지금안어플로 많던뎅",
          "original_form": "지금안어플로 많던뎅",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100027556.1.28",
          "form": "그런거햐보시는건어떠세요",
          "original_form": "그런거햐보시는건어떠세요",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100027556.1.29",
          "form": "안어플이 뭐예요???",
          "original_form": "안어플이 뭐예요???",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100027556.1.30",
