      "utterance": [
        {
          "id": "MDRW2100017138.1.1",
          "form": "나중에 자취하면 동물 키우고 싶어?",
          "original_form": "나중에 자취하면 동물 키우고 싶어?",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.2",
          "form": "당근",
          "original_form": "당근",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.3",
          "form": "말해뭐해",
          "original_form": "말해뭐해",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.4",
          "form": "애옹이",
          "original_form": "애옹이",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.5",
          "form": "데려와...",
          "original_form": "데려와...",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.6",
          "form": "고양이?",
          "original_form": "고양이?",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.7",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.8",
          "form": "귀여운",
          "original_form": "귀여운",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.9",
          "form": "털 복실복실",
          "original_form": "털 복실복실",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.10",
          "form": "애옹!!",
          "original_form": "애옹!!",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.11",
          "form": "돈 많이 벌어야겠당",
          "original_form": "돈 많이 벌어야겠당",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017138.1.13",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017138.1.14",
          "form": "나아는야 그지...",
          "original_form": "나아는야 그지...",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017138.1.15",
          "form": "슬프네",
          "original_form": "슬프네",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017138.1.16",
          "form": "고양이",
          "original_form": "고양이",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017138.1.17",
          "form": "치즈냥 키우고싶어",
          "original_form": "치즈냥 키우고싶어",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017138.1.18",
          "form": "아 너무 귀엽겠다",
          "original_form": "아 너무 귀엽겠다",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017138.1.19",
          "form": "친구들 중에 고양이 키우는 사람 있어?",
          "original_form": "친구들 중에 고양이 키우는 사람 있어?",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017138.1.20",
          "form": "내 친구",
          "original_form": "내 친구",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017138.1.21",
          "form": "두명",
          "original_form": "두명",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017138.1.22",
          "form": "한명은 본가에거",
          "original_form": "한명은 본가에거",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017138.1.23",
          "form": "한명은 자취방에서",
          "original_form": "한명은 자취방에서",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017138.1.24",
          "form": "부럽다 난 아무도 없어",
          "original_form": "부럽다 난 아무도 없어",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017138.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017138.1.26",
          "form": "동물 키워본 적 있어?",
          "original_form": "동물 키워본 적 있어?",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017138.1.27",
          "form": "금붕어 name3?",
          "original_form": "금붕어 &name3&?",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.28",
          "form": "하루만에 자유를 찾아",
          "original_form": "하루만에 자유를 찾아",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.29",
          "form": "저세상간 금붕어찡...",
          "original_form": "저세상간 금붕어찡...",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.30",
          "form": "오노ㅠㅠ",
          "original_form": "오노ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.31",
          "form": "하루만에 싸늘한 시체가...",
          "original_form": "하루만에 싸늘한 시체가...",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.32",
          "form": "나도 구피 키웠던 거 같아",
          "original_form": "나도 구피 키웠던 거 같아",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.33",
          "form": "오오오!",
          "original_form": "오오오!",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017138.1.34",
          "form": "사촌한테 준 거 같긴한데",
          "original_form": "사촌한테 준 거 같긴한데",
          "speaker_id": "1",
          "time": "20210826 17:39"
        }
      ]
