      "utterance": [
        {
          "id": "MDRW2100011521.1.1",
          "form": "그래서졸업하자마자대학교로가려고",
          "original_form": "그래서졸업하자마자대학교로가려고",
          "speaker_id": "1",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.2",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.3",
          "form": "그게 필수 코스야?",
          "original_form": "그게 필수 코스야?",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.4",
          "form": "ㅇㅇㅇㅇ1년안다니면",
          "original_form": "ㅇㅇㅇㅇ1년안다니면",
          "speaker_id": "1",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.5",
          "form": "실기시험보는방법도있긴한데",
          "original_form": "실기시험보는방법도있긴한데",
          "speaker_id": "1",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.6",
          "form": "어렵다해서",
          "original_form": "어렵다해서",
          "speaker_id": "1",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011521.1.7",
          "form": "그1년까지만지원받고",
          "original_form": "그1년까지만지원받고",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.8",
          "form": "연끊으려고",
          "original_form": "연끊으려고",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.9",
          "form": "우와 신기하다 진짜",
          "original_form": "우와 신기하다 진짜",
          "speaker_id": "2",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.10",
          "form": "오메",
          "original_form": "오메",
          "speaker_id": "2",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.11",
          "form": "강단있구먼",
          "original_form": "강단있구먼",
          "speaker_id": "2",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.12",
          "form": "보통문과는대학원으로",
          "original_form": "보통문과는대학원으로",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.13",
          "form": "먼저나가지않음?",
          "original_form": "먼저나가지않음?",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011521.1.14",
          "form": "근데 나는 예대라서 보통 졸업하면 전공 관련 진로로 나가는 경우가 많아",
          "original_form": "근데 나는 예대라서 보통 졸업하면 전공 관련 진로로 나가는 경우가 많아",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011521.1.15",
          "form": "너네 학교랑 미국 대학이랑 연계되어 있어?",
          "original_form": "너네 학교랑 미국 대학이랑 연계되어 있어?",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011521.1.16",
          "form": "연계되어있긴한데",
          "original_form": "연계되어있긴한데",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011521.1.17",
          "form": "그1년과정을",
          "original_form": "그1년과정을",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011521.1.18",
          "form": "pave라하는데",
          "original_form": "pave라하는데",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.19",
          "form": "이걸받아주는대학교로지원해야됨",
          "original_form": "이걸받아주는대학교로지원해야됨",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.20",
          "form": "바로직장으로나가는거임?",
          "original_form": "바로직장으로나가는거임?",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.21",
          "form": "오오오오옹 신기하구먼 정말",
          "original_form": "오오오오옹 신기하구먼 정말",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.22",
          "form": "아니면대학원관련학과로?",
          "original_form": "아니면대학원관련학과로?",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.23",
          "form": "아직 잘 모르겠어 관련 학과로 갈지 일할지",
          "original_form": "아직 잘 모르겠어 관련 학과로 갈지 일할지",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011521.1.24",
          "form": "미국대학원은그래도",
          "original_form": "미국대학원은그래도",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.25",
          "form": "펀딩받아서",
          "original_form": "펀딩받아서",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.26",
          "form": "돈안내고가는",
          "original_form": "돈안내고가는",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.27",
          "form": "사람들도있더라",
          "original_form": "사람들도있더라",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.28",
          "form": "우와 그건 또 뭐람",
          "original_form": "우와 그건 또 뭐람",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.29",
          "form": "다양하구나",
          "original_form": "다양하구나",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011521.1.30",
          "form": "ㅇㅇㅇ석박사통합일때",
          "original_form": "ㅇㅇㅇ석박사통합일때",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011521.1.31",
          "form": "펀딩을더많이받을수있는데",
          "original_form": "펀딩을더많이받을수있는데",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011521.1.32",
          "form": "석사만일때도찾아보면",
          "original_form": "석사만일때도찾아보면",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011521.1.33",
          "form": "있을걸",
          "original_form": "있을걸",
          "speaker_id": "1",
          "time": "20210824 13:38"
        }
      ]
    }
