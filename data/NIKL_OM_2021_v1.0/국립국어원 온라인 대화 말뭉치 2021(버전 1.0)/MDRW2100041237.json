      "utterance": [
        {
          "id": "MDRW2100041237.1.1",
          "form": "외모에 신경쓰시는 편이신가요?",
          "original_form": "외모에 신경쓰시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041237.1.2",
          "form": "아이가 생기고는 손 놨어요 ㅎㅎㅎ",
          "original_form": "아이가 생기고는 손 놨어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041237.1.3",
          "form": "신경많이 쓰세요?",
          "original_form": "신경많이 쓰세요?",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041237.1.4",
          "form": "ㅋㅋㅋㅋ저도 지금 아이들을 키우고 있는 주부라 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저도 지금 아이들을 키우고 있는 주부라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041237.1.5",
          "form": "신경을 거의 못써요 ㅋㅋ",
          "original_form": "신경을 거의 못써요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041237.1.6",
          "form": "근데 딸이 신경을 좀 쓰라고 하네요 ㅋㅋㅋㅋ",
          "original_form": "근데 딸이 신경을 좀 쓰라고 하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041237.1.7",
          "form": "그런 말을 할정도로 큰 아이인가봐요 ㅎㅎ",
          "original_form": "그런 말을 할정도로 큰 아이인가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.8",
          "form": "저는 아직 5 6살이라 제가 최고로 예쁘데요 ㅋ",
          "original_form": "저는 아직 5 6살이라 제가 최고로 예쁘데요 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.9",
          "form": "아 첫째가 초등학생이라 ㅋㅋㅋㅋ",
          "original_form": "아 첫째가 초등학생이라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.11",
          "form": "아 그러시구나 ㅋㅋㅋㅋ",
          "original_form": "아 그러시구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.12",
          "form": "그런말 들으면 좋죠 ㅋㅋ",
          "original_form": "그런말 들으면 좋죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.13",
          "form": "저는 ㅋㅋㅋ 얼굴에 점도 좀 빼라구 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 ㅋㅋㅋ 얼굴에 점도 좀 빼라구 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041237.1.14",
          "form": "ㅎㅎㅎ 애 키우면 머 애한테 신경쓰느라",
          "original_form": "ㅎㅎㅎ 애 키우면 머 애한테 신경쓰느라",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041237.1.15",
          "form": "나한테 신경을 아무래도 못쓰죠",
          "original_form": "나한테 신경을 아무래도 못쓰죠",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041237.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041237.1.17",
          "form": "앞머리도 없다가 ㅋㅋ 제발 앞머리좀 내봐라고 하도 그래서 ㅋㅋ",
          "original_form": "앞머리도 없다가 ㅋㅋ 제발 앞머리좀 내봐라고 하도 그래서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041237.1.18",
          "form": "앞머리 잘랐더니 이쁘다고 하더라구요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앞머리 잘랐더니 이쁘다고 하더라구요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041237.1.19",
          "form": "좋으시겠어요 친구같이",
          "original_form": "좋으시겠어요 친구같이",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041237.1.20",
          "form": "이래저래 이야기 해주는 딸이라 ㅎㅎ",
          "original_form": "이래저래 이야기 해주는 딸이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041237.1.21",
          "form": "네네 좋을때도 있고 .....흠...그래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 좋을때도 있고 .....흠...그래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041237.1.22",
          "form": "행복하죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ....",
          "original_form": "행복하죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ....",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041237.1.23",
          "form": "최근에 다이어트를 했는데 ㅋㅋ 3키로만 빠지고 더이상 안빠지더라구요 ㅠㅠ ㅋㅋㅋㅋㅋㅋ살빼기 힘드네요 ㅋㅋ",
          "original_form": "최근에 다이어트를 했는데 ㅋㅋ 3키로만 빠지고 더이상 안빠지더라구요 ㅠㅠ ㅋㅋㅋㅋㅋㅋ살빼기 힘드네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041237.1.24",
          "form": "나이들수록 더 안빠지는거 같아요 ㅜㅡ",
          "original_form": "나이들수록 더 안빠지는거 같아요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041237.1.25",
          "form": "맞아요 ㅠㅠ 빼기도 힘들구요 ㅠ",
          "original_form": "맞아요 ㅠㅠ 빼기도 힘들구요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041237.1.26",
          "form": "안먹으면 손떨리고 ㅋ",
          "original_form": "안먹으면 손떨리고 ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041237.1.27",
          "form": "그래도 3키로정도도 성공하셨네요 축하드려요",
