        },
        {
          "id": "MDRW2100040339.1.4",
          "form": "먹을거는진짜아낌업ㅈ이주셨지만",
          "original_form": "먹을거는진짜아낌업ㅈ이주셨지만",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040339.1.5",
          "form": "너무정신적으로힘이들어서..ㅋㅋㅋㅋㅋ",
          "original_form": "너무정신적으로힘이들어서..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040339.1.6",
          "form": "할말이진짜진짜많은데",
          "original_form": "할말이진짜진짜많은데",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040339.1.7",
          "form": "jnj흐윽흐윽",
          "original_form": "jnj흐윽흐윽",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040339.1.8",
          "form": "ㅜㅇㅜ 아 먹는걸로 극복이 안되는 직장이라니",
          "original_form": "ㅜㅇㅜ 아 먹는걸로 극복이 안되는 직장이라니",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040339.1.9",
          "form": "그러게요 먹을거잘주는게최곤데",
          "original_form": "그러게요 먹을거잘주는게최곤데",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100040339.1.10",
          "form": "ㅋㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100040339.1.11",
          "form": "간식도마구사도뭐라안하셨는데 흑흑",
          "original_form": "간식도마구사도뭐라안하셨는데 흑흑",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100040339.1.12",
          "form": "저랑 직장을 바꾸셨어야ㅜㅇㅜ",
          "original_form": "저랑 직장을 바꾸셨어야ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100040339.1.13",
          "form": "아니 근데 제 직장은 더 시궁창이네요ㅜ",
          "original_form": "아니 근데 제 직장은 더 시궁창이네요ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100040339.1.14",
          "form": "이거끝나면직장얘기한판돌려야겠어여",
          "original_form": "이거끝나면직장얘기한판돌려야겠어여",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040339.1.15",
          "form": "할말이많아요",
          "original_form": "할말이많아요",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040339.1.16",
          "form": "ㅋㅋㅋㄱㅈㅋㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㅋㅋㅋㄱㅈㅋㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040339.1.17",
          "form": "간식이고뭐고!!!",
          "original_form": "간식이고뭐고!!!",
          "speaker_id": "2",
