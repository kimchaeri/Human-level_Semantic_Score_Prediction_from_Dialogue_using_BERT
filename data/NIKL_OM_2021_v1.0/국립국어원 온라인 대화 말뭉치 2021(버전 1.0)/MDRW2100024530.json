      "utterance": [
        {
          "id": "MDRW2100024530.1.1",
          "form": "맞습니다 제발 강요 노우..",
          "original_form": "맞습니다 제발 강요 노우..",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024530.1.2",
          "form": "우리 취업할 때 쯤엔 세대교체 되어있길 바라는 중입니다",
          "original_form": "우리 취업할 때 쯤엔 세대교체 되어있길 바라는 중입니다",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024530.1.3",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "1",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100024530.1.4",
          "form": "제발.. 꼰대 아웃!",
          "original_form": "제발.. 꼰대 아웃!",
          "speaker_id": "1",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100024530.1.5",
          "form": "ㅋㅋㅋ저는 전문직 공부 중인게",
          "original_form": "ㅋㅋㅋ저는 전문직 공부 중인게",
          "speaker_id": "2",
          "time": "20210831 13:20"
        },
        {
          "id": "MDRW2100024530.1.6",
          "form": "여차하면 이직할라고 하는거도 있네요 ㅎ",
          "original_form": "여차하면 이직할라고 하는거도 있네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:20"
        },
        {
          "id": "MDRW2100024530.1.7",
          "form": "이직이고 나발시고 취준생 입장이라^^ ㅎㅎ",
          "original_form": "이직이고 나발시고 취준생 입장이라^^ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100024530.1.8",
          "form": "전 휴학 취준 중인데 빨리 졸업도 하고싶답니다",
          "original_form": "전 휴학 취준 중인데 빨리 졸업도 하고싶답니다",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100024530.1.9",
          "form": "복학도 하실 예정이세요?",
          "original_form": "복학도 하실 예정이세요?",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024530.1.10",
          "form": "복학해야지요",
          "original_form": "복학해야지요",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024530.1.11",
          "form": "졸업할라면 한학기 남았어요ㅋㅋ",
          "original_form": "졸업할라면 한학기 남았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024530.1.12",
          "form": "한학기 남으셨그ㅡ나ㅋㅋ",
          "original_form": "한학기 남으셨그ㅡ나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024530.1.13",
          "form": "내년복학이시겠네요",
          "original_form": "내년복학이시겠네요",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024530.1.14",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024530.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024530.1.16",
          "form": "저는 백수..ㅋㅋㅋㅋ",
          "original_form": "저는 백수..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024530.1.17",
          "form": "좋게 취준생 임닷",
          "original_form": "좋게 취준생 임닷",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024530.1.18",
          "form": "졸업하신거예요??",
          "original_form": "졸업하신거예요??",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100024530.1.19",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100024530.1.20",
          "form": "최근에는 휴학 좀 할껄 싶어요",
          "original_form": "최근에는 휴학 좀 할껄 싶어요",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100024530.1.21",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "2",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100024530.1.22",
          "form": "졸업전에 진로 생각해봤음 좋았을걸 싶더라구요",
          "original_form": "졸업전에 진로 생각해봤음 좋았을걸 싶더라구요",
