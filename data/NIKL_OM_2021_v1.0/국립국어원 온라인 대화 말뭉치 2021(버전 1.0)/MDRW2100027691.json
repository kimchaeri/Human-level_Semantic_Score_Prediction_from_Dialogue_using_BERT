      "utterance": [
        {
          "id": "MDRW2100027691.1.1",
          "form": "요즘 어떤게임을 하시나요?",
          "original_form": "요즘 어떤게임을 하시나요?",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100027691.1.2",
          "form": "요즘 단순게임을 하고있어요",
          "original_form": "요즘 단순게임을 하고있어요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100027691.1.3",
          "form": "프렌즈팝 재밌더라구요",
          "original_form": "프렌즈팝 재밌더라구요",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100027691.1.4",
          "form": "아 그거 저도해봤어요",
          "original_form": "아 그거 저도해봤어요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100027691.1.5",
          "form": "전 어려웠어요",
          "original_form": "전 어려웠어요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100027691.1.6",
          "form": "해보면 재미있어요",
          "original_form": "해보면 재미있어요",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100027691.1.7",
          "form": "그렇긴하죠 갈수력 어렵더라구요",
          "original_form": "그렇긴하죠 갈수력 어렵더라구요",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100027691.1.8",
          "form": "템이 좀 많이 필요하죠",
          "original_form": "템이 좀 많이 필요하죠",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100027691.1.9",
          "form": "네 캐쉬를 너무써야해서 포기했던",
          "original_form": "네 캐쉬를 너무써야해서 포기했던",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100027691.1.10",
          "form": "저도 현질좀 했어요",
          "original_form": "저도 현질좀 했어요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100027691.1.11",
          "form": "없이 하긴힘들어서",
          "original_form": "없이 하긴힘들어서",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100027691.1.12",
          "form": "전 요즘 브롤스타즈 자주해요",
          "original_form": "전 요즘 브롤스타즈 자주해요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100027691.1.13",
          "form": "아이와함께",
          "original_form": "아이와함께",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100027691.1.14",
          "form": "그거 아이들사이서 유명한 게임이죠~?",
          "original_form": "그거 아이들사이서 유명한 게임이죠~?",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100027691.1.15",
          "form": "네 아이가 좋아해서 자주해요",
          "original_form": "네 아이가 좋아해서 자주해요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100027691.1.16",
          "form": "레온,제시 알고있어요 굿즈를봐서",
          "original_form": "레온,제시 알고있어요 굿즈를봐서",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100027691.1.17",
          "form": "맞아요 콜라보 자주하죠",
          "original_form": "맞아요 콜라보 자주하죠",
          "speaker_id": "2",
          "time": "20210905 22:33"
        }
      ]
