      "utterance": [
        {
          "id": "MDRW2100004959.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004959.1.2",
          "form": "오늘 비가 계속 오네요 ㅜㅜ",
          "original_form": "오늘 비가 계속 오네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004959.1.3",
          "form": "여기는 아까 아침에 비가 엄청 내리다가 햇빛나다가 다시 비가오네요ㅜ",
          "original_form": "여기는 아까 아침에 비가 엄청 내리다가 햇빛나다가 다시 비가오네요ㅜ",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004959.1.4",
          "form": "여기도요ㅜ",
          "original_form": "여기도요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004959.1.5",
          "form": "다시 해나는것같아서 창문열었더니",
          "original_form": "다시 해나는것같아서 창문열었더니",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004959.1.6",
          "form": "비가 또와요ㅠ",
          "original_form": "비가 또와요ㅠ",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004959.1.7",
          "form": "담주까지 비가 내렸다가 말았다가 한대요",
          "original_form": "담주까지 비가 내렸다가 말았다가 한대요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.8",
          "form": "장마도 아니고...",
          "original_form": "장마도 아니고...",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.9",
          "form": "그래도 이번에 비 내리고 나면 시원해지겠어요",
          "original_form": "그래도 이번에 비 내리고 나면 시원해지겠어요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.10",
          "form": "가을비가 참",
          "original_form": "가을비가 참",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.11",
          "form": "그러게요 정말 비그치면",
          "original_form": "그러게요 정말 비그치면",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.12",
          "form": "기온이 확 떨어질것같아요",
          "original_form": "기온이 확 떨어질것같아요",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004959.1.13",
          "form": "여름옷은 그렇게 많이 못입고...가을옷도 사봤자 못입을 거같구...",
          "original_form": "여름옷은 그렇게 많이 못입고...가을옷도 사봤자 못입을 거같구...",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100004959.1.14",
          "form": "겨울옷이나 꺼내놔야겠어요ㅜㅋㅋㅋ",
          "original_form": "겨울옷이나 꺼내놔야겠어요ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100004959.1.15",
          "form": "ㅠㅠ 정말 한국 사계절이라지만",
          "original_form": "ㅠㅠ 정말 한국 사계절이라지만",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004959.1.16",
          "form": "여름 겨울 초여름 초겨울 이렇게인거같아요",
          "original_form": "여름 겨울 초여름 초겨울 이렇게인거같아요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004959.1.17",
          "form": "ㅋㅋㅋ봄과 가을이 너무 빨리 지나가죠?",
          "original_form": "ㅋㅋㅋ봄과 가을이 너무 빨리 지나가죠?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004959.1.18",
          "form": "맞어요 너무 아쉬워요ㅜ",
          "original_form": "맞어요 너무 아쉬워요ㅜ",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100004959.1.19",
          "form": "name1님은 어떤 계절이 제일 좋으세요?",
          "original_form": "&name1&님은 어떤 계절이 제일 좋으세요?",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004959.1.20",
          "form": "가을이요!!",
          "original_form": "가을이요!!",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004959.1.21",
          "form": "앗 저도 가을!!!",
          "original_form": "앗 저도 가을!!!",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004959.1.22",
          "form": "봄에는 미세먼지만 아니면 참 좋을텐데ㅜ",
          "original_form": "봄에는 미세먼지만 아니면 참 좋을텐데ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004959.1.23",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100004959.1.24",
          "form": "미세먼지랑 금방 꽃샘추위지나면 금방더워져서",
          "original_form": "미세먼지랑 금방 꽃샘추위지나면 금방더워져서",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100004959.1.25",
          "form": "좋은 날씨가 진짜 짧아요ㅜ",
          "original_form": "좋은 날씨가 진짜 짧아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100004959.1.26",
          "form": "ㅋㅋㅋ그러게요ㅜ 꽃도 볼 수 있어서 좋은데ㅜ 그 미세먼지 때문에!!!",
          "original_form": "ㅋㅋㅋ그러게요ㅜ 꽃도 볼 수 있어서 좋은데ㅜ 그 미세먼지 때문에!!!",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004959.1.27",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004959.1.28",
          "form": "으 미세먼지ㅜ",
          "original_form": "으 미세먼지ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004959.1.29",
          "form": "올해는 그래도 좀 덜했는데 내년에도 그랬으면 좋겠네요ㅎ",
          "original_form": "올해는 그래도 좀 덜했는데 내년에도 그랬으면 좋겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004959.1.30",
          "form": "그러면 정말 좋겠어요ㅠ",
          "original_form": "그러면 정말 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 21:15"
        }
      ]
