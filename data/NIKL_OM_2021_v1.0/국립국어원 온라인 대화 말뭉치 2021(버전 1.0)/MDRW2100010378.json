      "utterance": [
        {
          "id": "MDRW2100010378.1.1",
          "form": "아니 내 저녁이 뭔지 맞춰볼래?",
          "original_form": "아니 내 저녁이 뭔지 맞춰볼래?",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.2",
          "form": "나는 아직 안했어.",
          "original_form": "나는 아직 안했어.",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.3",
          "form": "하하 저녁을 먹기엔 좀 이릊",
          "original_form": "하하 저녁을 먹기엔 좀 이릊",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.4",
          "form": "흠.. 흰쌀밥이 들어가니?",
          "original_form": "흠.. 흰쌀밥이 들어가니?",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.5",
          "form": "국밥을 먹니?",
          "original_form": "국밥을 먹니?",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.6",
          "form": "응 ㅎㅎ 와 너는 천재구나",
          "original_form": "응 ㅎㅎ 와 너는 천재구나",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.7",
          "form": "오늘같이 흐린날은 국밥이 최고지",
          "original_form": "오늘같이 흐린날은 국밥이 최고지",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.8",
          "form": "음 나도 잘 모르겠어",
          "original_form": "음 나도 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.9",
          "form": "어? 정말 국밥을 먹는거야?",
          "original_form": "어? 정말 국밥을 먹는거야?",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.10",
          "form": "내가 일찍 들어간다면",
          "original_form": "내가 일찍 들어간다면",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.11",
          "form": "공짜밥을 먹을텐데",
          "original_form": "공짜밥을 먹을텐데",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.12",
          "form": "아하. 너도 잘 모르겠다는 뜻이구나.",
          "original_form": "아하. 너도 잘 모르겠다는 뜻이구나.",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.13",
          "form": "늦어버리면 국밥을 먹을지도",
          "original_form": "늦어버리면 국밥을 먹을지도",
          "speaker_id": "2",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010378.1.14",
          "form": "국밥이 졓다고 생각해.",
          "original_form": "국밥이 졓다고 생각해.",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.15",
          "form": "아니면 해장국도 쫌 땡겨",
          "original_form": "아니면 해장국도 쫌 땡겨",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.16",
          "form": "래장국이 더 좋다",
          "original_form": "래장국이 더 좋다",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.17",
          "form": "나랑 감자탕 먹으러가자꾸나",
          "original_form": "나랑 감자탕 먹으러가자꾸나",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.18",
          "form": "해장국을 추천할게",
          "original_form": "해장국을 추천할게",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.19",
          "form": "좋아.",
          "original_form": "좋아.",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.20",
          "form": "좋은 추천 고마워!",
          "original_form": "좋은 추천 고마워!",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010378.1.21",
          "form": "감자탕은 많이 뜨거워 근데",
          "original_form": "감자탕은 많이 뜨거워 근데",
          "speaker_id": "1",
          "time": "20210823 17:20"
        }
      ]
