      "utterance": [
        {
          "id": "MDRW2100016086.1.1",
          "form": "가보고싶은 해외여행지 있으신가요",
          "original_form": "가보고싶은 해외여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016086.1.2",
          "form": "유럽이요 영국!",
          "original_form": "유럽이요 영국!",
          "speaker_id": "1",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016086.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016086.1.4",
          "form": "영국 볼거리 너무 많아서 좋을거같아요",
          "original_form": "영국 볼거리 너무 많아서 좋을거같아요",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016086.1.5",
          "form": "저는 스페인이요!",
          "original_form": "저는 스페인이요!",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.6",
          "form": "영국 가고싶은 이유 물어봐도될까요",
          "original_form": "영국 가고싶은 이유 물어봐도될까요",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.7",
          "form": "저도 스페인도 가고싶어요ㅠㅠ",
          "original_form": "저도 스페인도 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.8",
          "form": "축구 좋아해서요 ㅋㅋㅋ 가서 보고싶어요 직관",
          "original_form": "축구 좋아해서요 ㅋㅋㅋ 가서 보고싶어요 직관",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.9",
          "form": "축구팬이시구나",
          "original_form": "축구팬이시구나",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.10",
          "form": "축구경기 보는거 너무 좋죠!",
          "original_form": "축구경기 보는거 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016086.1.11",
          "form": "넵넵 name2은 스페인가고싶은 이유가 있나요?ㅎㅎ",
          "original_form": "넵넵 &name2&은 스페인가고싶은 이유가 있나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016086.1.12",
          "form": "저는 순례길 가고싶어서 코로나 전에 알아봤는데 코로나가 터져버리더라구요ㅎ",
          "original_form": "저는 순례길 가고싶어서 코로나 전에 알아봤는데 코로나가 터져버리더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016086.1.13",
          "form": "참고로 종교없습니다",
          "original_form": "참고로 종교없습니다",
          "speaker_id": "2",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016086.1.14",
          "form": "ㅋㅋㅋㅋㅋ네네 많이들 가시잖아요 산티아고 맞나..",
          "original_form": "ㅋㅋㅋㅋㅋ네네 많이들 가시잖아요 산티아고 맞나..",
          "speaker_id": "1",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100016086.1.15",
          "form": "맞아요 산티아고",
          "original_form": "맞아요 산티아고",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016086.1.16",
          "form": "좀 생각없이 걷는 게 좋아보이더라고요",
          "original_form": "좀 생각없이 걷는 게 좋아보이더라고요",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016086.1.17",
          "form": "생각정리하기 좋을거같아요 ㅎㅎ",
          "original_form": "생각정리하기 좋을거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016086.1.18",
          "form": "스페인은 음식도 맛있을거같고..ㅎㅎ",
          "original_form": "스페인은 음식도 맛있을거같고..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016086.1.19",
          "form": "맞아요 빠예야나 마리스코스 이런거 츄러스도요",
          "original_form": "맞아요 빠예야나 마리스코스 이런거 츄러스도요",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016086.1.20",
          "form": "너무 맛있을거같아요",
          "original_form": "너무 맛있을거같아요",
