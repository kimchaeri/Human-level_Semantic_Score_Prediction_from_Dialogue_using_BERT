      "utterance": [
        {
          "id": "MDRW2100023555.1.1",
          "form": "점심은 뭐 드세요?",
          "original_form": "점심은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100023555.1.2",
          "form": "저는 코로나로 회사에서 사람들이 많이 걸린 다음부터는 간단한 샐러드나 두유를 싸와요",
          "original_form": "저는 코로나로 회사에서 사람들이 많이 걸린 다음부터는 간단한 샐러드나 두유를 싸와요",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.3",
          "form": "오늘은 두유 셰이크요",
          "original_form": "오늘은 두유 셰이크요",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.4",
          "form": "아 간편식으로 드시는군요",
          "original_form": "아 간편식으로 드시는군요",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.5",
          "form": "name1은 뭐드셍ㆍ",
          "original_form": "&name1&은 뭐드셍ㆍ",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.6",
          "form": "네 회사식당이 위험한거같아서요",
          "original_form": "네 회사식당이 위험한거같아서요",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.7",
          "form": "저는 짬뽕 먹으러 갈까 합니다",
          "original_form": "저는 짬뽕 먹으러 갈까 합니다",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023555.1.8",
          "form": "날씨가 흐리니 뜨거운 국물이 먹고 싶네요",
          "original_form": "날씨가 흐리니 뜨거운 국물이 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023555.1.9",
          "form": "맛있겠네요 그쵸 날씨 참 우중충하네요",
          "original_form": "맛있겠네요 그쵸 날씨 참 우중충하네요",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023555.1.10",
          "form": "코로나라 점심 먹으러 가기도 꺼려지네요.. 그래도 일하려면 먹어야하니",
          "original_form": "코로나라 점심 먹으러 가기도 꺼려지네요.. 그래도 일하려면 먹어야하니",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100023555.1.11",
          "form": "그쵸 저는 그래서 점심 간단히로 바뀠어요\r\n습관되니 괜찮아요",
          "original_form": "그쵸 저는 그래서 점심 간단히로 바뀠어요\r\n습관되니 괜찮아요",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023555.1.12",
          "form": "그런데 두유셰이크만 드시면 오후에 배안고프세요?",
          "original_form": "그런데 두유셰이크만 드시면 오후에 배안고프세요?",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023555.1.13",
          "form": "익숙해지니 안고프더라고요ㅋㅋ\r\n대신 아침든든먹고 출근하고요ㅋㅋ",
          "original_form": "익숙해지니 안고프더라고요ㅋㅋ\r\n대신 아침든든먹고 출근하고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023555.1.14",
          "form": "아 네.. 아침 많이 먹고 출근하는 것도 하나의 방법이네요 ㅎㅎ",
          "original_form": "아 네.. 아침 많이 먹고 출근하는 것도 하나의 방법이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023555.1.15",
          "form": "가끔 일하다 간식도 먹곤 했는데 요즘엔 마스크 벗기가 참 눈치보입니다",
          "original_form": "가끔 일하다 간식도 먹곤 했는데 요즘엔 마스크 벗기가 참 눈치보입니다",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023555.1.16",
          "form": "그쵸 저도 사무실에 혼자 있을때만 마스크 벗고 항상 쓰고 있어요 ㅋㅋ",
          "original_form": "그쵸 저도 사무실에 혼자 있을때만 마스크 벗고 항상 쓰고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023555.1.17",
          "form": "저도 간식 중간 중간 몰래 먹으면 배안고프고 괜찮아요",
          "original_form": "저도 간식 중간 중간 몰래 먹으면 배안고프고 괜찮아요",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023555.1.18",
          "form": "두유가 은근 든든하고 좋아요",
          "original_form": "두유가 은근 든든하고 좋아요",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023555.1.19",
          "form": "저도 한 번 먹어봐야겠네요 ㅎㅎ 점심 먹으러 가기 귀찮은날",
          "original_form": "저도 한 번 먹어봐야겠네요 ㅎㅎ 점심 먹으러 가기 귀찮은날",
          "speaker_id": "1",
          "time": "20210830 11:38"
        }
      ]
    }
