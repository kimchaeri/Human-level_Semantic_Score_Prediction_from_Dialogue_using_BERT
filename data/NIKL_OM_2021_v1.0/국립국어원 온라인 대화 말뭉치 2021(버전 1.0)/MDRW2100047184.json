      "utterance": [
        {
          "id": "MDRW2100047184.1.1",
          "form": "맥주vs소주는 어떠세요?",
          "original_form": "맥주vs소주는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100047184.1.2",
          "form": "저.... 맥주 좋아합니다.",
          "original_form": "저.... 맥주 좋아합니다.",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100047184.1.3",
          "form": "소주는 잘 안먹게 되더라고요",
          "original_form": "소주는 잘 안먹게 되더라고요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100047184.1.4",
          "form": "주량이 하찮아서ㅜㅜㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "주량이 하찮아서ㅜㅜㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100047184.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100047184.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047184.1.7",
          "form": "저도요 알쓰예요",
          "original_form": "저도요 알쓰예요",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047184.1.8",
          "form": "전 하이볼좋아한답니다",
          "original_form": "전 하이볼좋아한답니다",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047184.1.9",
          "form": "전 카스를 먹어욧 다른 맥주는 또 손이 안가더라고요!",
          "original_form": "전 카스를 먹어욧 다른 맥주는 또 손이 안가더라고요!",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047184.1.10",
          "form": "그럼 안주는\r\n과자 VS 견과류",
          "original_form": "그럼 안주는\r\n과자 VS 견과류",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047184.1.11",
          "form": "어떤걸 선호하시나요?",
          "original_form": "어떤걸 선호하시나요?",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047184.1.12",
          "form": "안주는 음 둘중고르자면",
          "original_form": "안주는 음 둘중고르자면",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047184.1.13",
          "form": "과저네요ㅋㅋㅋㅋ",
          "original_form": "과저네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047184.1.14",
          "form": "사실은 국물류지만요",
          "original_form": "사실은 국물류지만요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047184.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ헐 헐 헐 저도요~~~~!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ헐 헐 헐 저도요~~~~!",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100047184.1.16",
          "form": "그건 누구나 선호하는거니까 뺏다구욧!",
          "original_form": "그건 누구나 선호하는거니까 뺏다구욧!",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100047184.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100047184.1.18",
          "form": "저는 견과류안주이해못해요!",
          "original_form": "저는 견과류안주이해못해요!",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100047184.1.19",
          "form": "으아아~~~! 생각보다 맛있던데요 견과류안주?!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "으아아~~~! 생각보다 맛있던데요 견과류안주?!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100047184.1.20",
          "form": "그래도...국물은 못 이기지만요...하 최고",
          "original_form": "그래도...국물은 못 이기지만요...하 최고",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047184.1.21",
          "form": "ㅋㅋㅋㅋ 맞아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100047184.1.22",
          "form": "저는 얼큰한거좋아해요 맑은국보다",
          "original_form": "저는 얼큰한거좋아해요 맑은국보다",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100047184.1.23",
          "form": "바지락탕같은거보다 짬뽕탕 나가사키탕 크으",
          "original_form": "바지락탕같은거보다 짬뽕탕 나가사키탕 크으",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100047184.1.24",
          "form": "하 헐 저도요 저도요",
          "original_form": "하 헐 저도요 저도요",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047184.1.25",
          "form": "짬뽕탕 진국. 부대찌개 짱",
          "original_form": "짬뽕탕 진국. 부대찌개 짱",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047184.1.26",
          "form": "크으으느으으으 술당기네요",
          "original_form": "크으으느으으으 술당기네요",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047184.1.27",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100047184.1.28",
          "form": "역시 탕이짱이죠",
          "original_form": "역시 탕이짱이죠",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100047184.1.29",
          "form": "아 맞습니다 맞아요",
          "original_form": "아 맞습니다 맞아요",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047184.1.30",
          "form": "진짜 탕이 최고예요......",
          "original_form": "진짜 탕이 최고예요......",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
