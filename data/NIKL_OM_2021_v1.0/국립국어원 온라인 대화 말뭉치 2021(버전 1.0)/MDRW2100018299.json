      "utterance": [
        {
          "id": "MDRW2100018299.1.1",
          "form": "양념게장은 거기선 안 먹어봤어요",
          "original_form": "양념게장은 거기선 안 먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.2",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.3",
          "form": "그냥그냥",
          "original_form": "그냥그냥",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.4",
          "form": "간장게장이 더 좋네요",
          "original_form": "간장게장이 더 좋네요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.5",
          "form": "전 둘다",
          "original_form": "전 둘다",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.6",
          "form": "넘 좋아요!!!!",
          "original_form": "넘 좋아요!!!!",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018299.1.7",
          "form": "양념게장은 너무 매운 데가 많아서..ㅎ",
          "original_form": "양념게장은 너무 매운 데가 많아서..ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.8",
          "form": "저는 약간",
          "original_form": "저는 약간",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.9",
          "form": "옹기꽃게장 같은",
          "original_form": "옹기꽃게장 같은",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.10",
          "form": "무한리필집이나 가야겠어요..",
          "original_form": "무한리필집이나 가야겠어요..",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.11",
          "form": "그런데도 맛있죠",
          "original_form": "그런데도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.12",
          "form": "간장게장 is 뭔들..",
          "original_form": "간장게장 is 뭔들..",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.13",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.14",
          "form": "다 맛있죠",
          "original_form": "다 맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018299.1.15",
          "form": "아파트 장에 오는 게장도 맛있어요 ㅋㅋㅋ",
          "original_form": "아파트 장에 오는 게장도 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.16",
          "form": "싸구 ㅎㅎ",
          "original_form": "싸구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.17",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.18",
          "form": "저희동네는 안파네요",
          "original_form": "저희동네는 안파네요",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.19",
          "form": "네 일주일에 한번 와요",
          "original_form": "네 일주일에 한번 와요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.20",
          "form": "얼마정도에요?",
          "original_form": "얼마정도에요?",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.21",
          "form": "보통 2만원어치 사면",
          "original_form": "보통 2만원어치 사면",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.22",
          "form": "몇 끼 먹어요",
          "original_form": "몇 끼 먹어요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018299.1.23",
