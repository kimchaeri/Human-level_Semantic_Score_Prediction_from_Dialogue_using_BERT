      "utterance": [
        {
          "id": "MDRW2100028797.1.1",
          "form": "요즘 학교는 어떠신가요?",
          "original_form": "요즘 학교는 어떠신가요?",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028797.1.2",
          "form": "그냥.. 학교에요",
          "original_form": "그냥.. 학교에요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028797.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028797.1.4",
          "form": "저는 고등학교때가 더 즐거웠던 것 같아요",
          "original_form": "저는 고등학교때가 더 즐거웠던 것 같아요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028797.1.5",
          "form": "앗 저두요",
          "original_form": "앗 저두요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028797.1.6",
          "form": "코로나때문에 그런건지 뭔지 모르겠지만ㅋㅋㅋㅋ",
          "original_form": "코로나때문에 그런건지 뭔지 모르겠지만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028797.1.7",
          "form": "고딩때는 뭔가",
          "original_form": "고딩때는 뭔가",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028797.1.8",
          "form": "아무것도 안해도 웃겼던ㅋㅋㅋㅋ",
          "original_form": "아무것도 안해도 웃겼던ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028797.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028797.1.10",
          "form": "그때는 그냥 뭐든 재밌었어요",
          "original_form": "그때는 그냥 뭐든 재밌었어요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028797.1.11",
          "form": "근데 졸업하고 나니까",
          "original_form": "근데 졸업하고 나니까",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028797.1.12",
          "form": "친했던 친구들도 연락을 잘 안하게 되더라구요",
          "original_form": "친했던 친구들도 연락을 잘 안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028797.1.13",
          "form": "앗 뭔지 알 것 같아요",
          "original_form": "앗 뭔지 알 것 같아요",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028797.1.14",
          "form": "고딩때는 거의 맨날 붙어있었는데",
          "original_form": "고딩때는 거의 맨날 붙어있었는데",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028797.1.15",
          "form": "이제 서로 멀어지니까",
          "original_form": "이제 서로 멀어지니까",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028797.1.16",
          "form": "자주 만나지도 못하고..",
          "original_form": "자주 만나지도 못하고..",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028797.1.17",
          "form": "이제는 시간이 꽤 지나서 조금",
          "original_form": "이제는 시간이 꽤 지나서 조금",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028797.1.18",
          "form": "익숙해졌지만",
          "original_form": "익숙해졌지만",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028797.1.19",
          "form": "처음에는 좀 슬펐어요",
          "original_form": "처음에는 좀 슬펐어요",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028797.1.20",
          "form": "저는 제가 원래 연락을 잘 하는 편이 아니라서",
          "original_form": "저는 제가 원래 연락을 잘 하는 편이 아니라서",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028797.1.21",
          "form": "막 큰 느낌은 없었던 것 같아요",
          "original_form": "막 큰 느낌은 없었던 것 같아요",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028797.1.22",
          "form": "그렇군요..",
          "original_form": "그렇군요..",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028797.1.23",
          "form": "그래도 연락하는 애들은 잘 연락하더라구요~",
          "original_form": "그래도 연락하는 애들은 잘 연락하더라구요~",
          "speaker_id": "1",
          "time": "20210902 22:06"
        }
      ]
    }
