      "utterance": [
        {
          "id": "MDRW2100043949.1.1",
          "form": "음악듣는거 좋아하시나요?",
          "original_form": "음악듣는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043949.1.2",
          "form": "내 좋아해료",
          "original_form": "내 좋아해료",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043949.1.3",
          "form": "어떤 노래 들으시나여?",
          "original_form": "어떤 노래 들으시나여?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043949.1.4",
          "form": "드림캐쳐나 이달소 자주들어요",
          "original_form": "드림캐쳐나 이달소 자주들어요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043949.1.5",
          "form": "우와 ㅋㅋ 둘다 약간 벅차오르는? 애니 주제곡같은? 그런 느낌있는것같아요!! 맞나요?",
          "original_form": "우와 ㅋㅋ 둘다 약간 벅차오르는? 애니 주제곡같은? 그런 느낌있는것같아요!! 맞나요?",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043949.1.6",
          "form": "아 저 막 오덕후 그런 타입은 아니고..",
          "original_form": "아 저 막 오덕후 그런 타입은 아니고..",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043949.1.7",
          "form": "ㅎ..",
          "original_form": "ㅎ..",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043949.1.8",
          "form": "여돌은 그냥 거의 다 좋아하고 잘들어욬ㅋㅋㅋㅋ",
          "original_form": "여돌은 그냥 거의 다 좋아하고 잘들어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043949.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋㅋㅋ오덕후같은 타입은 뭐에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋㅋㅋㅋ오덕후같은 타입은 뭐에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043949.1.10",
          "form": "레벨 신곡도 자주듣고..ㅋㅋㅋㅋ저는 유튜브뮤직으로 듣거든요",
          "original_form": "레벨 신곡도 자주듣고..ㅋㅋㅋㅋ저는 유튜브뮤직으로 듣거든요",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043949.1.11",
          "form": "걍 지맘대로 랜덤으로 나와요 ㅋㅋㅋ",
          "original_form": "걍 지맘대로 랜덤으로 나와요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043949.1.12",
          "form": "ㅋㅋㅋㅋㅋ오 유투브 뮤직 좋네여",
          "original_form": "ㅋㅋㅋㅋㅋ오 유투브 뮤직 좋네여",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043949.1.13",
          "form": "저도 여돌노래 마니들어요",
          "original_form": "저도 여돌노래 마니들어요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043949.1.14",
          "form": "근데 저는 남돌노래도 마니들어요",
          "original_form": "근데 저는 남돌노래도 마니들어요",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043949.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 막 오덕후.. 그런타입은 아니지만요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 막 오덕후.. 그런타입은 아니지만요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043949.1.16",
          "form": "누구노래 들어요???",
          "original_form": "누구노래 들어요???",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043949.1.17",
          "form": "음 그냥 누구라고 꼽을것없이 걍 다들어요 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "음 그냥 누구라고 꼽을것없이 걍 다들어요 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043949.1.18",
          "form": "완전 듣보아니면 ㅋㅋㅋㅋㅋ",
          "original_form": "완전 듣보아니면 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043949.1.19",
          "form": "ㅋㅋㅋㅋ오저랑 비슷하시네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ오저랑 비슷하시네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043949.1.20",
          "form": "요즘에는 저도 레벨앨범 듣구 ㅋㅋㅋㅋㅋ",
          "original_form": "요즘에는 저도 레벨앨범 듣구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043949.1.21",
          "form": "요즘 자주듣는 노래 잇으세요ㅡ?",
          "original_form": "요즘 자주듣는 노래 잇으세요ㅡ?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043949.1.22",
          "form": "날이쌀쌀해지니까 데이식스노래도 또 다시들어주고ㅠㅋㅋㅋㅋ",
          "original_form": "날이쌀쌀해지니까 데이식스노래도 또 다시들어주고ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043949.1.23",
          "form": "오 데식이들 인기 많네요",
          "original_form": "오 데식이들 인기 많네요",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043949.1.24",
          "form": "다른분도 데식 좋아하시던뎈ㅋㅋㅋ",
          "original_form": "다른분도 데식 좋아하시던뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043949.1.25",
          "form": "ㅋㅋㅋㅋ혹시 아이돌 자체컨텐츠는 잘 안보시나요?",
          "original_form": "ㅋㅋㅋㅋ혹시 아이돌 자체컨텐츠는 잘 안보시나요?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043949.1.26",
          "form": "ㅋㅋㅋㅋㅋ맞아요 데식노래넘좋죠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 데식노래넘좋죠 ㅋㅋㅋ",
