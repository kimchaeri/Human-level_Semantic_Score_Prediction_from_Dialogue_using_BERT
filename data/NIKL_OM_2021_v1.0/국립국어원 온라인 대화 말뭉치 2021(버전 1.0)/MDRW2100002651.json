        },
        {
          "id": "MDRW2100002651.1.2",
          "form": "안녕하세요.\r\n여행 좋아하세요?",
          "original_form": "안녕하세요.\r\n여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100002651.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100002651.1.4",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100002651.1.5",
          "form": "좋아하지만 못가서 요즘 아쉬워요ㅜ",
          "original_form": "좋아하지만 못가서 요즘 아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100002651.1.6",
          "form": "어디가고싶으세요?",
          "original_form": "어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100002651.1.7",
          "form": "제주도나 여건되면 해외도 가고싶어요",
          "original_form": "제주도나 여건되면 해외도 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002651.1.8",
          "form": "제주도 넘좋죠ㅠㅠ",
          "original_form": "제주도 넘좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100002651.1.9",
          "form": "저도 진짜가고싶어요",
          "original_form": "저도 진짜가고싶어요",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100002651.1.10",
          "form": "가서 맛있는것도 먹고 쉬고오고싶어요ㅜ",
          "original_form": "가서 맛있는것도 먹고 쉬고오고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100002651.1.11",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100002651.1.12",
          "form": "맛집탐방 하고싶어요",
          "original_form": "맛집탐방 하고싶어요",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100002651.1.13",
          "form": "제주도에서 가장 먹고싶은게 뭐예요?~",
          "original_form": "제주도에서 가장 먹고싶은게 뭐예요?~",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100002651.1.14",
          "form": "저 해산물이요",
          "original_form": "저 해산물이요",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100002651.1.15",
          "form": "ㅋㅋㅋ 딱새우랑 회먹고싶네요",
          "original_form": "ㅋㅋㅋ 딱새우랑 회먹고싶네요",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100002651.1.16",
          "form": "딱새우 맛있겠어요~~~~우와ㅋㅋ",
          "original_form": "딱새우 맛있겠어요~~~~우와ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100002651.1.17",
          "form": "또",
          "original_form": "또",
          "speaker_id": "1",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100002651.1.18",
