      "utterance": [
        {
          "id": "MDRW2100045485.1.1",
          "form": "가사일 좋아하세요?",
          "original_form": "가사일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045485.1.2",
          "form": "아뇨 싫어해요",
          "original_form": "아뇨 싫어해요",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045485.1.3",
          "form": "그런데 해야하니까 하는거지",
          "original_form": "그런데 해야하니까 하는거지",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045485.1.4",
          "form": "남이 해주면 안하죠",
          "original_form": "남이 해주면 안하죠",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045485.1.5",
          "form": "가정부 없잖아요",
          "original_form": "가정부 없잖아요",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.6",
          "form": "그런데 남이 하는게",
          "original_form": "그런데 남이 하는게",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.7",
          "form": "저는 요리는 좋아서 해요",
          "original_form": "저는 요리는 좋아서 해요",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.8",
          "form": "억지로 하는거면 싫어요",
          "original_form": "억지로 하는거면 싫어요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.9",
          "form": "맛있고",
          "original_form": "맛있고",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.10",
          "form": "너무 짜요",
          "original_form": "너무 짜요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.11",
          "form": "내취향으로 만들수 있잖아요",
          "original_form": "내취향으로 만들수 있잖아요",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.12",
          "form": "음 ~ 국이 좀 짜다",
          "original_form": "음 ~ 국이 좀 짜다",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.13",
          "form": "고기뿐인 카레같은거요",
          "original_form": "고기뿐인 카레같은거요",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.14",
          "form": "나트륨 좀 줄여요",
          "original_form": "나트륨 좀 줄여요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.15",
          "form": "고기많은 잡채나",
          "original_form": "고기많은 잡채나",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.16",
          "form": "저는 감자 좋아해서",
          "original_form": "저는 감자 좋아해서",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.17",
          "form": "직접 요리해보세요그럼",
          "original_form": "직접 요리해보세요그럼",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.18",
          "form": "고기반 감자반 카레해주세요",
          "original_form": "고기반 감자반 카레해주세요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045485.1.19",
          "form": "감자 맛있죠",
          "original_form": "감자 맛있죠",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.20",
          "form": "당금도요",
          "original_form": "당금도요",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.21",
          "form": "정리만 잘해주신다면",
          "original_form": "정리만 잘해주신다면",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.22",
          "form": "설겆이 정도는 제가 할 수 있어요",
          "original_form": "설겆이 정도는 제가 할 수 있어요",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.23",
          "form": "이3개는 꼭 넣는듯",
          "original_form": "이3개는 꼭 넣는듯",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.24",
          "form": "당근 별로",
          "original_form": "당근 별로",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.25",
          "form": "아 양파",
          "original_form": "아 양파",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045485.1.26",
          "form": "양파최고맛있음",
          "original_form": "양파최고맛있음",
