        {
          "id": "MDRW2100040839.1.1",
          "form": "여행 추천지 있어?",
          "original_form": "여행 추천지 있어?",
          "speaker_id": "1",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040839.1.2",
          "form": "음 제주??",
          "original_form": "음 제주??",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100040839.1.3",
          "form": "너무 흔한가 ㅋㅋㅋ",
          "original_form": "너무 흔한가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100040839.1.4",
          "form": "아그래?",
          "original_form": "아그래?",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100040839.1.5",
          "form": "제주가 최고지",
          "original_form": "제주가 최고지",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100040839.1.6",
          "form": "난 후쿠오카 또 가고파",
          "original_form": "난 후쿠오카 또 가고파",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100040839.1.7",
          "form": "오ㅜㅜ 나도 일본가고싶어",
          "original_form": "오ㅜㅜ 나도 일본가고싶어",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100040839.1.8",
          "form": "코시국 대체 언제까지갈까ㅎㅎ..",
          "original_form": "코시국 대체 언제까지갈까ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100040839.1.9",
          "form": "그러게ㅠㅠ",
          "original_form": "그러게ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100040839.1.10",
          "form": "2022년엔 왠지 끝날거같아",
          "original_form": "2022년엔 왠지 끝날거같아",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100040839.1.11",
          "form": "오",
          "original_form": "오",
