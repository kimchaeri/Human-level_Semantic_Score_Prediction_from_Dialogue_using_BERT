      "utterance": [
        {
          "id": "MDRW2100030125.1.1",
          "form": "아들 목욕이면 힘 뺐겠네요",
          "original_form": "아들 목욕이면 힘 뺐겠네요",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100030125.1.2",
          "form": "야식 드셔야겠음",
          "original_form": "야식 드셔야겠음",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100030125.1.3",
          "form": "설치니까 ㅋㅋ빡십니다",
          "original_form": "설치니까 ㅋㅋ빡십니다",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100030125.1.4",
          "form": "이제 운동하려구요",
          "original_form": "이제 운동하려구요",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100030125.1.5",
          "form": "아ㄷㄷㄷ",
          "original_form": "아ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100030125.1.6",
          "form": "운동광인이시군요ㄷㄷㄷ",
          "original_form": "운동광인이시군요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100030125.1.7",
          "form": "계란 드시구요?",
          "original_form": "계란 드시구요?",
          "speaker_id": "1",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100030125.1.8",
          "form": "아 사실 운동광은 아니궄ㅋㄱㅅ",
          "original_form": "아 사실 운동광은 아니궄ㅋㄱㅅ",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100030125.1.9",
          "form": "와이프랑 함께하는 홈트 2일차가",
          "original_form": "와이프랑 함께하는 홈트 2일차가",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.10",
          "form": "차라서",
          "original_form": "차라서",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.11",
          "form": "같이 해볼려구요 ㅋㅋ",
          "original_form": "같이 해볼려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.12",
          "form": "와이프가 살이 좀 쪄서 건강해지려고 꼬셨습니다",
          "original_form": "와이프가 살이 좀 쪄서 건강해지려고 꼬셨습니다",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.13",
          "form": "절대 운동안하거든요",
          "original_form": "절대 운동안하거든요",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100030125.1.15",
          "form": "근데 한다고 하던가요!!",
          "original_form": "근데 한다고 하던가요!!",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100030125.1.16",
          "form": "역시 에인절",
          "original_form": "역시 에인절",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100030125.1.17",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030125.1.18",
          "form": "답이늦었습니다",
          "original_form": "답이늦었습니다",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030125.1.19",
          "form": "유희열 스케치북보자고해서",
          "original_form": "유희열 스케치북보자고해서",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030125.1.20",
          "form": "혹시 보시나요?",
          "original_form": "혹시 보시나요?",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030125.1.21",
          "form": "추억돋네요",
          "original_form": "추억돋네요",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030125.1.22",
          "form": "아 야식은 안했습니다",
          "original_form": "아 야식은 안했습니다",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030125.1.23",
          "form": "가아끔 봅니다",
          "original_form": "가아끔 봅니다",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030125.1.24",
          "form": "홈트하면서 보신 건가요?",
          "original_form": "홈트하면서 보신 건가요?",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030125.1.25",
          "form": "아녀 홈트 끝내고 보는데",
          "original_form": "아녀 홈트 끝내고 보는데",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030125.1.26",
          "form": "2000년대 레전드 가수들",
          "original_form": "2000년대 레전드 가수들",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.27",
          "form": "나왓서요",
          "original_form": "나왓서요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.28",
          "form": "헉! 누구요?",
          "original_form": "헉! 누구요?",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.29",
          "form": "고유진 테이 채연 이기찬 왁스",
          "original_form": "고유진 테이 채연 이기찬 왁스",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.30",
          "form": "BMK",
          "original_form": "BMK",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.31",
          "form": "그시절로 돌아왓습니다",
          "original_form": "그시절로 돌아왓습니다",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.32",
          "form": "맨앞만 모르고 다 아는 얼굴이네요!",
          "original_form": "맨앞만 모르고 다 아는 얼굴이네요!",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030125.1.33",
          "form": "나도 볼걸ㅜㅜ",
          "original_form": "나도 볼걸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030125.1.34",
          "form": "가슴이 저리네요",
          "original_form": "가슴이 저리네요",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030125.1.35",
          "form": "그시절이 그립습니다",
          "original_form": "그시절이 그립습니다",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030125.1.36",
          "form": "유튭 뜰듯 ㅎㅎ",
          "original_form": "유튭 뜰듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030125.1.37",
          "form": "ㅎㅎ 간만에 유튜브 들어가야겠네요",
          "original_form": "ㅎㅎ 간만에 유튜브 들어가야겠네요",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030125.1.38",
          "form": "그때 진짜 재밌었는데",
          "original_form": "그때 진짜 재밌었는데",
          "speaker_id": "1",
          "time": "20210904 00:39"
        }
      ]
