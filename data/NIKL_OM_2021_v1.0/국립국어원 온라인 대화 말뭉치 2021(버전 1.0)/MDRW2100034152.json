      "utterance": [
        {
          "id": "MDRW2100034152.1.1",
          "form": "스킨바르고 크림발라요",
          "original_form": "스킨바르고 크림발라요",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034152.1.2",
          "form": "name1님은 어떻게하시나요",
          "original_form": "&name1&님은 어떻게하시나요",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034152.1.3",
          "form": "저도요~^^ 가끔 생각나면 팩도 하고요",
          "original_form": "저도요~^^ 가끔 생각나면 팩도 하고요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100034152.1.4",
          "form": "팩은 어떤거 사용하세요?",
          "original_form": "팩은 어떤거 사용하세요?",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034152.1.5",
          "form": "제이준꺼 해요",
          "original_form": "제이준꺼 해요",
          "speaker_id": "1",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034152.1.6",
          "form": "제이준꺼 좋아요? 흡수잘되는편인가요",
          "original_form": "제이준꺼 좋아요? 흡수잘되는편인가요",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034152.1.7",
          "form": "그냥 싸서 샀는데 매일 하면 정말 다르긴 하더라고요",
          "original_form": "그냥 싸서 샀는데 매일 하면 정말 다르긴 하더라고요",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034152.1.8",
          "form": "오 확실히 수분감이 다를거같아요",
          "original_form": "오 확실히 수분감이 다를거같아요",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100034152.1.9",
          "form": "어디서 구매하시나요",
          "original_form": "어디서 구매하시나요",
          "speaker_id": "2",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100034152.1.10",
          "form": "인터넷에서 샀어요 여기가 전 몰랐는데 나름 유명하대요",
          "original_form": "인터넷에서 샀어요 여기가 전 몰랐는데 나름 유명하대요",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034152.1.11",
          "form": "아 인터넷에 검색하면 그 사이트가 바로나오나요? 마스크팩",
          "original_form": "아 인터넷에 검색하면 그 사이트가 바로나오나요? 마스크팩",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034152.1.12",
          "form": "네~ 그게 예전에 가입하면 거의 무료로 10팩 이상의.마스크팩을 줬거든요 써보고 좋아서 전 야이 기초도 샀엉ㅅ",
          "original_form": "네~ 그게 예전에 가입하면 거의 무료로 10팩 이상의.마스크팩을 줬거든요 써보고 좋아서 전 야이 기초도 샀엉ㅅ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034152.1.13",
          "form": "오 이벤트를했었군요",
          "original_form": "오 이벤트를했었군요",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034152.1.14",
          "form": "기초는 그럼 원래 어떤제품사용하세요",
          "original_form": "기초는 그럼 원래 어떤제품사용하세요",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034152.1.15",
          "form": "지금은 한율꺼 쓰는데 이것보단 애터미랑 제이준이 나은것 같아요",
          "original_form": "지금은 한율꺼 쓰는데 이것보단 애터미랑 제이준이 나은것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034152.1.16",
          "form": "애터미 스킨로션 좋은가요?",
          "original_form": "애터미 스킨로션 좋은가요?",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034152.1.17",
          "form": "애터미는 치약만써본거같아요",
          "original_form": "애터미는 치약만써본거같아요",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034152.1.18",
          "form": "쓸만해요. 향도 좋고 가격도 싸고요.  치약도 나오는군요..",
          "original_form": "쓸만해요. 향도 좋고 가격도 싸고요.  치약도 나오는군요..",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034152.1.19",
          "form": "네 별거 다팔긴하더라구여",
          "original_form": "네 별거 다팔긴하더라구여",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034152.1.20",
          "form": "한율은 어때요",
          "original_form": "한율은 어때요",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034152.1.21",
          "form": "한율은 좀 약해요.",
          "original_form": "한율은 좀 약해요.",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100034152.1.22",
          "form": "보습이 좀약할까요?",
          "original_form": "보습이 좀약할까요?",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034152.1.23",
          "form": "맞아요 전 건성인이 이건 좀 약해서 다신 안쓰려고요",
          "original_form": "맞아요 전 건성인이 이건 좀 약해서 다신 안쓰려고요",
          "speaker_id": "1",
