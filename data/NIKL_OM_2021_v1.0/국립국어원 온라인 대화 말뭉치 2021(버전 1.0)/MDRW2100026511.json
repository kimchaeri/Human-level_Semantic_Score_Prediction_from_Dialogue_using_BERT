      "utterance": [
        {
          "id": "MDRW2100026511.1.1",
          "form": "혹시 요리는 많이 하시는 편이세요?",
          "original_form": "혹시 요리는 많이 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026511.1.2",
          "form": "아뇨 가끔 해먹어보고싶은거 하는편이에여 ㅋㅋ",
          "original_form": "아뇨 가끔 해먹어보고싶은거 하는편이에여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026511.1.3",
          "form": "백종원 유튜브도 자주보고 ㅎㅎ",
          "original_form": "백종원 유튜브도 자주보고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026511.1.4",
          "form": "백종원 유튜브 유용하죠!",
          "original_form": "백종원 유튜브 유용하죠!",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026511.1.5",
          "form": "요즘은 진짜 유튜브만 봐도 방법이 자세하게 잘 나와서 좋아요",
          "original_form": "요즘은 진짜 유튜브만 봐도 방법이 자세하게 잘 나와서 좋아요",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026511.1.6",
          "form": "따라만 해도 요리가 맛있게 되니까 거의 오뚜기 3분 카레 마냥 뚝딱 만들 수 있어요",
          "original_form": "따라만 해도 요리가 맛있게 되니까 거의 오뚜기 3분 카레 마냥 뚝딱 만들 수 있어요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026511.1.7",
          "form": "맞아욬ㅋㅋㅋ 근데 그와중에 잘못하면.. 이게뭔가 를 만들긴해욬ㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ 근데 그와중에 잘못하면.. 이게뭔가 를 만들긴해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026511.1.8",
          "form": "육개장 좋아해서 만들고싶었는데",
          "original_form": "육개장 좋아해서 만들고싶었는데",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026511.1.9",
          "form": "진짜 대실퍀ㅋㅋㅋ했어요",
          "original_form": "진짜 대실퍀ㅋㅋㅋ했어요",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026511.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026511.1.11",
          "form": "얼마만큼 실패하샸길래ㅠㅠ",
          "original_form": "얼마만큼 실패하샸길래ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026511.1.12",
          "form": "전 실패 느낌이어도 걍 눈막고 먹어요",
          "original_form": "전 실패 느낌이어도 걍 눈막고 먹어요",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026511.1.13",
          "form": "이맛이겠거니... 하면서",
          "original_form": "이맛이겠거니... 하면서",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026511.1.14",
          "form": "음... 육개장이아니라",
          "original_form": "음... 육개장이아니라",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.15",
          "form": "숙주탕..?",
          "original_form": "숙주탕..?",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.16",
          "form": "숙주담근탕..?",
          "original_form": "숙주담근탕..?",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.18",
          "form": "도저히 못먹어서",
          "original_form": "도저히 못먹어서",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.19",
          "form": "전부 안녕했어요 ㅠ",
          "original_form": "전부 안녕했어요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닠ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.21",
          "form": "숙주담근탕ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "숙주담근탕ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.22",
          "form": "떠나보내길 잘하셨네요",
          "original_form": "떠나보내길 잘하셨네요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026511.1.23",
          "form": "숙주가 위에서 만나서 융털이랑 하이파이브 할 뻔 했잖아요..",
          "original_form": "숙주가 위에서 만나서 융털이랑 하이파이브 할 뻔 했잖아요..",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026511.1.24",
          "form": "어우... 숙주 좋아하지만",
          "original_form": "어우... 숙주 좋아하지만",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026511.1.25",
          "form": "진짜 저건 아니었습니다..",
          "original_form": "진짜 저건 아니었습니다..",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026511.1.26",
          "form": "레시피도 백종원이었지만.....하하하하하",
          "original_form": "레시피도 백종원이었지만.....하하하하하",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026511.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026511.1.28",
          "form": "백종원씨가 조금 더 분발하셔야 겠네요",
          "original_form": "백종원씨가 조금 더 분발하셔야 겠네요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026511.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026511.1.30",
          "form": "댓글에 그렇게 쓸걸 그랬나요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "댓글에 그렇게 쓸걸 그랬나요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026511.1.31",
          "form": "저같은 초보들도 잘 따라할 수 있게 좀 더 쉽게 해주셔야 겠어요",
          "original_form": "저같은 초보들도 잘 따라할 수 있게 좀 더 쉽게 해주셔야 겠어요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026511.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026511.1.33",
          "form": "그런거 있잖아요 그 진짜 나이 조금 있으신 주부님들 유튜브가 가정식 레시피로는 쩡이에요",
