      "utterance": [
        {
          "id": "MDRW2100014299.1.1",
          "form": "좋아하는음식이 어떻게되시나요",
          "original_form": "좋아하는음식이 어떻게되시나요",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014299.1.2",
          "form": "전 고기요! 육식동물이에요",
          "original_form": "전 고기요! 육식동물이에요",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014299.1.3",
          "form": "뭘 좋아하시죠?",
          "original_form": "뭘 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014299.1.4",
          "form": "전 카페인중독이에요! 커피쟁이죠",
          "original_form": "전 카페인중독이에요! 커피쟁이죠",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014299.1.5",
          "form": "ㅋㅋㅋ인정 오늘은 몇 잔 드셨죠?",
          "original_form": "ㅋㅋㅋ인정 오늘은 몇 잔 드셨죠?",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014299.1.6",
          "form": "한잔마셨어요",
          "original_form": "한잔마셨어요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014299.1.7",
          "form": "엄청큰한잔이요!",
          "original_form": "엄청큰한잔이요!",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014299.1.8",
          "form": "빽다방? 디저트39? 컴포즈?",
          "original_form": "빽다방? 디저트39? 컴포즈?",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014299.1.9",
          "form": "오 ㅋㅋㅋ 다땡이에요 어제 이마트24에서 네이버페이커피 100원한거",
          "original_form": "오 ㅋㅋㅋ 다땡이에요 어제 이마트24에서 네이버페이커피 100원한거",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014299.1.10",
          "form": "퇴근하면서 4잔사서 ㅋㅋㅋ 어제 두잔 오늘 2잔마셔요",
          "original_form": "퇴근하면서 4잔사서 ㅋㅋㅋ 어제 두잔 오늘 2잔마셔요",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014299.1.11",
          "form": "떼잉...ㅋㅋㅋ다 틀리다니 아쉽군요",
          "original_form": "떼잉...ㅋㅋㅋ다 틀리다니 아쉽군요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014299.1.12",
          "form": "얼음이 아까보니 녹앗지만 아직떠있더라군요",
          "original_form": "얼음이 아까보니 녹앗지만 아직떠있더라군요",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014299.1.13",
          "form": "커피 이제 아아말고 뜨아 시대가 다가오능거같군요",
          "original_form": "커피 이제 아아말고 뜨아 시대가 다가오능거같군요",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014299.1.14",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014299.1.15",
          "form": "내가 뜨아 2 아아2산거는",
          "original_form": "내가 뜨아 2 아아2산거는",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014299.1.16",
          "form": "어찌알고",
          "original_form": "어찌알고",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014299.1.17",
          "form": "하하하! 맛있는거 먹고싶은데 뭐 없나유 흑흑",
          "original_form": "하하하! 맛있는거 먹고싶은데 뭐 없나유 흑흑",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014299.1.18",
          "form": "새콤한 비빔국수",
          "original_form": "새콤한 비빔국수",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014299.1.19",
          "form": "ㅋㅋㅋ오 비빔국수하니까 뜬금없이 비빔만두 생각나네",
          "original_form": "ㅋㅋㅋ오 비빔국수하니까 뜬금없이 비빔만두 생각나네",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014299.1.20",
          "form": "납짝만두에",
          "original_form": "납짝만두에",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014299.1.21",
          "form": "양배추랑 깻잎이랑 얇게 채쳐서",
          "original_form": "양배추랑 깻잎이랑 얇게 채쳐서",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014299.1.22",
          "form": "싸묵자",
          "original_form": "싸묵자",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014299.1.23",
          "form": "헐.. 놀랍게도 재료가 전부 다 없어...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐.. 놀랍게도 재료가 전부 다 없어...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014299.1.24",
