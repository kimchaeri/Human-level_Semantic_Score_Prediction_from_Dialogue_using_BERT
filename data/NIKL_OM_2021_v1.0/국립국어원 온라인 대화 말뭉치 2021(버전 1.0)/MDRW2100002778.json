        {
          "id": "MDRW2100002778.1.1",
          "form": "중고 거래해요?",
          "original_form": "중고 거래해요?",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002778.1.2",
          "form": "좋아해요ㅎ 자주하는편",
          "original_form": "좋아해요ㅎ 자주하는편",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002778.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002778.1.4",
          "form": "새것도 많고 괜찮더라구요",
          "original_form": "새것도 많고 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002778.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002778.1.6",
          "form": "근데 신랑은별로 안좋아하더라구요",
          "original_form": "근데 신랑은별로 안좋아하더라구요",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002778.1.7",
          "form": "남꺼라서 그런가?",
          "original_form": "남꺼라서 그런가?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002778.1.8",
          "form": "그런게 잇는거같아요ㅋ",
          "original_form": "그런게 잇는거같아요ㅋ",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002778.1.9",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100002778.1.10",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
