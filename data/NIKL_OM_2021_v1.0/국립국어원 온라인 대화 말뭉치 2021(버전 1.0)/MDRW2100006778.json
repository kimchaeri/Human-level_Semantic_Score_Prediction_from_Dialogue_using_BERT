      "utterance": [
        {
          "id": "MDRW2100006778.1.1",
          "form": "저는 안해요ㅜ주말에 산책만 간간히 해요",
          "original_form": "저는 안해요ㅜ주말에 산책만 간간히 해요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006778.1.2",
          "form": "저도 아픈 뒤로는 산책만 합니다",
          "original_form": "저도 아픈 뒤로는 산책만 합니다",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100006778.1.3",
          "form": "심한 운동은 지금 할 수가 없어요",
          "original_form": "심한 운동은 지금 할 수가 없어요",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100006778.1.4",
          "form": "대신 매일 산책 합니다",
          "original_form": "대신 매일 산책 합니다",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100006778.1.5",
          "form": "아 많이 아프셨군요\n대상포진..",
          "original_form": "아 많이 아프셨군요\n대상포진..",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006778.1.6",
          "form": "몇달간 제대로 걷지도 못했어요",
          "original_form": "몇달간 제대로 걷지도 못했어요",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100006778.1.7",
          "form": "다리쪽 신경이 망가진거라 걸을때마다 아팠지요",
          "original_form": "다리쪽 신경이 망가진거라 걸을때마다 아팠지요",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100006778.1.8",
          "form": "이런 몇달씩이나... 지금은 쾌차하셨나여",
          "original_form": "이런 몇달씩이나... 지금은 쾌차하셨나여",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006778.1.9",
          "form": "지금도 한달에 한번 일주일씩 입원합니다",
          "original_form": "지금도 한달에 한번 일주일씩 입원합니다",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006778.1.10",
          "form": "그래도 처음에 비해 아주 좋아졌답니다",
          "original_form": "그래도 처음에 비해 아주 좋아졌답니다",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100006778.1.11",
          "form": "저런... 저는 지금도 건강관리를 안 하는데... 걱정입니다ㅜ",
          "original_form": "저런... 저는 지금도 건강관리를 안 하는데... 걱정입니다ㅜ",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006778.1.12",
          "form": "이번에 처음으로 입원이란 걸 해보고 느꼈습니다",
          "original_form": "이번에 처음으로 입원이란 걸 해보고 느꼈습니다",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006778.1.13",
          "form": "건강은 건강할 때 지켜야 한다는 것을요",
          "original_form": "건강은 건강할 때 지켜야 한다는 것을요",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006778.1.14",
          "form": "아 은퇴 전엔 입원도 안하셨었나보네요",
          "original_form": "아 은퇴 전엔 입원도 안하셨었나보네요",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006778.1.15",
          "form": "네 작년 가을에 대상포진 때문에",
          "original_form": "네 작년 가을에 대상포진 때문에",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006778.1.16",
          "form": "처음으로 입원을 했습니다",
          "original_form": "처음으로 입원을 했습니다",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006778.1.17",
          "form": "아 저런... 입원을 얼마나 하신 거에여",
          "original_form": "아 저런... 입원을 얼마나 하신 거에여",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006778.1.18",
          "form": "한달에 한번씩 이제 일년 되어가네요",
          "original_form": "한달에 한번씩 이제 일년 되어가네요",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006778.1.19",
          "form": "아 지금도 한달 한번 입원하시는거에요? 이런",
          "original_form": "아 지금도 한달 한번 입원하시는거에요? 이런",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006778.1.20",
          "form": "지난 주에 퇴원했었지요",
          "original_form": "지난 주에 퇴원했었지요",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006778.1.21",
          "form": "다음달에 한번 더 갈까 생각중입니다",
          "original_form": "다음달에 한번 더 갈까 생각중입니다",
          "speaker_id": "1",
          "time": "20210908 09:49"
        }
