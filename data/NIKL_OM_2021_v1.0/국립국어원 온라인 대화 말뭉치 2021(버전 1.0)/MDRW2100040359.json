      "utterance": [
        {
          "id": "MDRW2100040359.1.1",
          "form": "미뤄졌어??!",
          "original_form": "미뤄졌어??!",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040359.1.2",
          "form": "응응 쌤 백신 내백신 친구백신",
          "original_form": "응응 쌤 백신 내백신 친구백신",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040359.1.3",
          "form": "일정",
          "original_form": "일정",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040359.1.4",
          "form": "아..세상에 ㅋㅋㄱ백신파티네ㅋㄱㅈㅋ",
          "original_form": "아..세상에 ㅋㅋㄱ백신파티네ㅋㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040359.1.5",
          "form": "그거랑 학원 공사랑 겹쳐서 ㅋㅋㅋ",
          "original_form": "그거랑 학원 공사랑 겹쳐서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040359.1.6",
          "form": "아 세상에 한참걸리것네",
          "original_form": "아 세상에 한참걸리것네",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040359.1.7",
          "form": "어어 이제 애프터 미드나잇 찍고",
          "original_form": "어어 이제 애프터 미드나잇 찍고",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040359.1.8",
          "form": "24모 영상 넘어가먼 될것같아",
          "original_form": "24모 영상 넘어가먼 될것같아",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040359.1.9",
          "form": "진짜 하나씩 완성해가네",
          "original_form": "진짜 하나씩 완성해가네",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040359.1.10",
          "form": "이거끝니면 수지 예스올노 하면 이제 댄스 장정 마무리",
          "original_form": "이거끝니면 수지 예스올노 하면 이제 댄스 장정 마무리",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040359.1.11",
          "form": "더배우고싶은건없구?",
          "original_form": "더배우고싶은건없구?",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040359.1.12",
          "form": "응응 아직은 없어",
          "original_form": "응응 아직은 없어",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040359.1.13",
          "form": "또생기면 이어가겟네댄스열정쓰",
          "original_form": "또생기면 이어가겟네댄스열정쓰",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040359.1.14",
          "form": "어어 그럴것같아 ㅋㅋㅋ",
          "original_form": "어어 그럴것같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040359.1.15",
          "form": "오늘 유튜브 회사에서 볼랬는데 소리가 안나오더라",
          "original_form": "오늘 유튜브 회사에서 볼랬는데 소리가 안나오더라",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040359.1.16",
          "form": "지겨워",
          "original_form": "지겨워",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040359.1.17",
          "form": "아직도 다음 사람 안왔어",
          "original_form": "아직도 다음 사람 안왔어",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040359.1.18",
          "form": "소리가안나왓다니이런?",
