      "utterance": [
        {
          "id": "MDRW2100042736.1.1",
          "form": "아 다행이네",
          "original_form": "아 다행이네",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100042736.1.2",
          "form": "애플제품은 뭔가 예쁜데 허약하다는 느낌이 있어서",
          "original_form": "애플제품은 뭔가 예쁜데 허약하다는 느낌이 있어서",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100042736.1.3",
          "form": "그래도 충전하고 해결되어 다행이지",
          "original_form": "그래도 충전하고 해결되어 다행이지",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042736.1.4",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042736.1.5",
          "form": "생각해보면 내 아이패드도 쌩으로 쓰고있는데 용케 버티구 있구만",
          "original_form": "생각해보면 내 아이패드도 쌩으로 쓰고있는데 용케 버티구 있구만",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042736.1.6",
          "form": "ㅋㅋㅋ보호필름 없이?",
          "original_form": "ㅋㅋㅋ보호필름 없이?",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042736.1.7",
          "form": "근데 그거 집에서 주로 쓰니까",
          "original_form": "근데 그거 집에서 주로 쓰니까",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042736.1.8",
          "form": "엉 밖에 안들고나가",
          "original_form": "엉 밖에 안들고나가",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100042736.1.9",
          "form": "침대로도 잘 안들고가고",
          "original_form": "침대로도 잘 안들고가고",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100042736.1.10",
          "form": "그치 조심히 다루니까",
          "original_form": "그치 조심히 다루니까",
          "speaker_id": "1",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100042736.1.11",
          "form": "상품이 훼손이 덜되지",
          "original_form": "상품이 훼손이 덜되지",
          "speaker_id": "1",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100042736.1.12",
          "form": "싸서 부서져도 덜 타격이 없거나",
          "original_form": "싸서 부서져도 덜 타격이 없거나",
          "speaker_id": "2",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100042736.1.13",
          "form": "비싸도 튼튼해야 쓸만해",
          "original_form": "비싸도 튼튼해야 쓸만해",
          "speaker_id": "2",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042736.1.14",
          "form": "비싼돈주고 설탕유리사면 열받지",
          "original_form": "비싼돈주고 설탕유리사면 열받지",
          "speaker_id": "2",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100042736.1.15",
          "form": "응 그치",
          "original_form": "응 그치",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042736.1.16",
          "form": "액정이 튼튼해야지",
          "original_form": "액정이 튼튼해야지",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042736.1.17",
          "form": "엉 꼭 액정 아니어도 다른 제품도",
          "original_form": "엉 꼭 액정 아니어도 다른 제품도",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042736.1.18",
          "form": "가격이 싸든지 가격만큼 내구도가 좋던지",
          "original_form": "가격이 싸든지 가격만큼 내구도가 좋던지",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042736.1.19",
          "form": "응응 그치",
          "original_form": "응응 그치",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042736.1.20",
          "form": "비싸면 내구도가 좋든지",
          "original_form": "비싸면 내구도가 좋든지",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042736.1.21",
          "form": "아님 저렴하든지",
          "original_form": "아님 저렴하든지",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042736.1.22",
          "form": "엉 여튼 아이팟 정상으로 돌아왔다니 다행이야",
          "original_form": "엉 여튼 아이팟 정상으로 돌아왔다니 다행이야",
          "speaker_id": "2",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042736.1.23",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042736.1.24",
          "form": "as 센터 가야하나 생각했다구ㅠ",
          "original_form": "as 센터 가야하나 생각했다구ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042736.1.25",
          "form": "또르르",
          "original_form": "또르르",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042736.1.26",
          "form": "애플제품 에이에스 짱귀찮아",
          "original_form": "애플제품 에이에스 짱귀찮아",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042736.1.27",
          "form": "아이패드 말고 또 사고싶거나 봐둔건 없수?",
          "original_form": "아이패드 말고 또 사고싶거나 봐둔건 없수?",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100042736.1.28",
