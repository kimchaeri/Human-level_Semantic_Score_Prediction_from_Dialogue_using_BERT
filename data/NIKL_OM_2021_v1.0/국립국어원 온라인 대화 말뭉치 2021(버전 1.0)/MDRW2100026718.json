      "utterance": [
        {
          "id": "MDRW2100026718.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026718.1.2",
          "form": "동호회 활동하는거 있어요?",
          "original_form": "동호회 활동하는거 있어요?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026718.1.3",
          "form": "아뇨. 하시는거 있나요?",
          "original_form": "아뇨. 하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026718.1.4",
          "form": "저도 지금은 안해요!",
          "original_form": "저도 지금은 안해요!",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026718.1.5",
          "form": "활동하고 싶은 분야는 있어요?",
          "original_form": "활동하고 싶은 분야는 있어요?",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026718.1.6",
          "form": "막연해서 잘은ㅋㅋ 여유있음 그림은 한번 배워보고 싶긴 하네요.",
          "original_form": "막연해서 잘은ㅋㅋ 여유있음 그림은 한번 배워보고 싶긴 하네요.",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026718.1.7",
          "form": "예전에 하시던거 있나요?",
          "original_form": "예전에 하시던거 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026718.1.8",
          "form": "운동 모임 들려고 하다가 코로나가 터지는 바람에ㅠㅠ",
          "original_form": "운동 모임 들려고 하다가 코로나가 터지는 바람에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026718.1.9",
          "form": "아... 요새 코로나땜에 거의 막히죠",
          "original_form": "아... 요새 코로나땜에 거의 막히죠",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026718.1.10",
          "form": "운동 좋아하시나봐요",
          "original_form": "운동 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026718.1.11",
          "form": "혼자 할수 있는 운동이면 혼자 할 수 있는데",
          "original_form": "혼자 할수 있는 운동이면 혼자 할 수 있는데",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026718.1.12",
          "form": "여러명이서 하는거는 동호회 가입해야 할수 있겠더라고요ㅠ",
          "original_form": "여러명이서 하는거는 동호회 가입해야 할수 있겠더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026718.1.13",
          "form": "그쵸. 같이 하면 잘 가르쳐주기도 하구요",
          "original_form": "그쵸. 같이 하면 잘 가르쳐주기도 하구요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026718.1.14",
          "form": "그리고 친구 사귀기에도 동호회 활동도 괜찮은거 같더라고요",
          "original_form": "그리고 친구 사귀기에도 동호회 활동도 괜찮은거 같더라고요",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026718.1.15",
          "form": "활동하는거 아니면 온라인모임으로도 많더라구요",
          "original_form": "활동하는거 아니면 온라인모임으로도 많더라구요",
          "speaker_id": "2",
          "time": "20210902 08:17"
        },
        {
          "id": "MDRW2100026718.1.16",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100026718.1.17",
          "form": "게임 같은건 활발한거 같구요ㅋㅋ",
          "original_form": "게임 같은건 활발한거 같구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100026718.1.18",
          "form": "아 그렇네요! 게임동호회면 온라인으로도 충분히 가능하겠어요",
          "original_form": "아 그렇네요! 게임동호회면 온라인으로도 충분히 가능하겠어요",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100026718.1.19",
          "form": "요샌 화상채팅도 많아서 스터디 모임도 그런식으로 한대요",
          "original_form": "요샌 화상채팅도 많아서 스터디 모임도 그런식으로 한대요",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100026718.1.20",
          "form": "오 그러게요 이시국 전에도 전화로 공부하는 것들도 있었으니까요",
          "original_form": "오 그러게요 이시국 전에도 전화로 공부하는 것들도 있었으니까요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        }
      ]
