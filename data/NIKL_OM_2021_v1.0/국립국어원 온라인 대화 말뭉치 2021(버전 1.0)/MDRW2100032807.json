      "utterance": [
        {
          "id": "MDRW2100032807.1.1",
          "form": "잘 참으셨네요... 액정으로 뛰어드시지만 않았어도 반은 가신 거예요",
          "original_form": "잘 참으셨네요... 액정으로 뛰어드시지만 않았어도 반은 가신 거예요",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.2",
          "form": "그쵸 제가 생각해도",
          "original_form": "그쵸 제가 생각해도",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.3",
          "form": "전 참을성이 많아요",
          "original_form": "전 참을성이 많아요",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.4",
          "form": "너무 젠틀하셨어요",
          "original_form": "너무 젠틀하셨어요",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.5",
          "form": "은우가 화면에서 절 먼저 꼬셧는데",
          "original_form": "은우가 화면에서 절 먼저 꼬셧는데",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.6",
          "form": "저 젠틀하게 안방에서 기다리잔아요",
          "original_form": "저 젠틀하게 안방에서 기다리잔아요",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.7",
          "form": "은우야 기다릴게",
          "original_form": "은우야 기다릴게",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.8",
          "form": "님은 그저... 참을성 있게..",
          "original_form": "님은 그저... 참을성 있게..",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.9",
          "form": "진짜 이쯤되면 와주라 좀(?)",
          "original_form": "진짜 이쯤되면 와주라 좀(?)",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032807.1.10",
          "form": "근데 진짜 유사 안먹으려해도",
          "original_form": "근데 진짜 유사 안먹으려해도",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.11",
          "form": "먹게되는 애들 따로 있지 않아요?",
          "original_form": "먹게되는 애들 따로 있지 않아요?",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.12",
          "form": "어쩔수가없어요,,,,,",
          "original_form": "어쩔수가없어요,,,,,",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.13",
          "form": "저..회사는 버블도 해서",
          "original_form": "저..회사는 버블도 해서",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.14",
          "form": "걍...",
          "original_form": "걍...",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.15",
          "form": "육아멤 유사멤 나뉘는데",
          "original_form": "육아멤 유사멤 나뉘는데",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.16",
          "form": "먼저 꼬셔요 이녀석들이,,",
          "original_form": "먼저 꼬셔요 이녀석들이,,",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.17",
          "form": "유사멤은 근데 그냥 지들이",
          "original_form": "유사멤은 근데 그냥 지들이",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.18",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.19",
          "form": "먼저 꼬셔버려가지고",
          "original_form": "먼저 꼬셔버려가지고",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.20",
          "form": "아니 전 가만히 있는데 왜",
          "original_form": "아니 전 가만히 있는데 왜",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.21",
          "form": "저 벌써 웨딩드레스 다 입고",
          "original_form": "저 벌써 웨딩드레스 다 입고",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.22",
          "form": "왜 그러냐고요 왜..",
          "original_form": "왜 그러냐고요 왜..",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.23",
          "form": "그런 상상까지 해야되냐구요 제가",
          "original_form": "그런 상상까지 해야되냐구요 제가",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032807.1.24",
          "form": "진짜 먼저꼬셔놓지말어라,,,",
          "original_form": "진짜 먼저꼬셔놓지말어라,,,",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.25",
          "form": "그니까요 그렇게 꼬시는거 유죄",
          "original_form": "그니까요 그렇게 꼬시는거 유죄",
          "speaker_id": "1",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.26",
          "form": "약간 그런거죠..",
          "original_form": "약간 그런거죠..",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.27",
          "form": "웃어주지마!!!!",
          "original_form": "웃어주지마!!!!",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.28",
          "form": "아 아냐... 한번만 웃어즈ㅓㅇ....아악 웃지마!!!",
          "original_form": "아 아냐... 한번만 웃어즈ㅓㅇ....아악 웃지마!!!",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.29",
          "form": "이런 느낌이랄까..",
          "original_form": "이런 느낌이랄까..",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.30",
          "form": "아니 진짜 유사멤은 누가봐도 유사멤인게 넘 웃겨요",
          "original_form": "아니 진짜 유사멤은 누가봐도 유사멤인게 넘 웃겨요",
          "speaker_id": "1",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.31",
          "form": "최애는 이제 머 어쩌란..이거죠",
          "original_form": "최애는 이제 머 어쩌란..이거죠",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.32",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.33",
          "form": "누구의 반발도 없이",
          "original_form": "누구의 반발도 없이",
          "speaker_id": "1",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.34",
          "form": "유사멤이에요",
          "original_form": "유사멤이에요",
          "speaker_id": "1",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032807.1.35",
          "form": "그냥 그래요",
          "original_form": "그냥 그래요",
          "speaker_id": "2",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100032807.1.36",
          "form": "생긴거자체가..꼬시는거야",
          "original_form": "생긴거자체가..꼬시는거야",
          "speaker_id": "2",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100032807.1.37",
          "form": "저희 연예인 얘기로 싹 쓸어가기 가능할 듯여",
          "original_form": "저희 연예인 얘기로 싹 쓸어가기 가능할 듯여",
          "speaker_id": "1",
          "time": "20210906 00:02"
        }
