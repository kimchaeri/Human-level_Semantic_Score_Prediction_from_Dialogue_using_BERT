      "utterance": [
        {
          "id": "MDRW2100041308.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041308.1.2",
          "form": "네~ 치킨 먹었어요ㅋㅋㅋㅋ",
          "original_form": "네~ 치킨 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041308.1.3",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041308.1.4",
          "form": "네 콩나물밥이랑 먹었습니다ㅎㅎ",
          "original_form": "네 콩나물밥이랑 먹었습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041308.1.5",
          "form": "어디 치킨시키셨나요?",
          "original_form": "어디 치킨시키셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041308.1.6",
          "form": "네네치킨이요ㅋㅋㅋㅋ",
          "original_form": "네네치킨이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041308.1.7",
          "form": "크리미언이라고 양파가 올라간 치킨이랑 매운양념치킨 ㅋㅋ",
          "original_form": "크리미언이라고 양파가 올라간 치킨이랑 매운양념치킨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041308.1.8",
          "form": "헐 양파 올라간 치킨 먹고싶네요ㅠㅠ",
          "original_form": "헐 양파 올라간 치킨 먹고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041308.1.9",
          "form": "매운양념은 많이 매운가요?",
          "original_form": "매운양념은 많이 매운가요?",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041308.1.10",
          "form": "네 많이 매워요.. 불닭이랑 신라면 그아딘가인데ㅋㅋㅋㅋ",
          "original_form": "네 많이 매워요.. 불닭이랑 신라면 그아딘가인데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100041308.1.11",
          "form": "크리민언치킨으로 중화ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "크리민언치킨으로 중화ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041308.1.12",
          "form": "헉 저는 매운걸 못먹어서...",
          "original_form": "헉 저는 매운걸 못먹어서...",
          "speaker_id": "1",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100041308.1.13",
          "form": "매운치킨은 주문하면 안되겠네요ㅋㅋㅋㅋ",
          "original_form": "매운치킨은 주문하면 안되겠네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100041308.1.14",
          "form": "매운거 잘드시나요?",
          "original_form": "매운거 잘드시나요?",
          "speaker_id": "1",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100041308.1.15",
          "form": "딱 불닭볶음면까지어요",
          "original_form": "딱 불닭볶음면까지어요",
          "speaker_id": "2",
          "time": "20210906 12:56"
        },
        {
          "id": "MDRW2100041308.1.16",
          "form": "그 이상은 못먹어요ㅋㅋㅋ",
          "original_form": "그 이상은 못먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:57"
        },
        {
          "id": "MDRW2100041308.1.17",
          "form": "오 그래도 잘드시는편이네요!!",
          "original_form": "오 그래도 잘드시는편이네요!!",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041308.1.18",
          "form": "저는 불닭볶음면 완전 맵다고 생각해서ㅠㅠ",
          "original_form": "저는 불닭볶음면 완전 맵다고 생각해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041308.1.19",
          "form": "고통스러운 맛....",
          "original_form": "고통스러운 맛....",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100041308.1.20",
          "form": "매운걸 꾸준히 먹어서  그렇지.. 요즘 캡사이신 넣고 안남고추넣고 하는건 음식이 아닙니다ㅡㅡ",
          "original_form": "매운걸 꾸준히 먹어서  그렇지.. 요즘 캡사이신 넣고 안남고추넣고 하는건 음식이 아닙니다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041308.1.21",
          "form": "사실 너무 매운건 건강에도 안 좋아요..",
          "original_form": "사실 너무 매운건 건강에도 안 좋아요..",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041308.1.22",
          "form": "그래도 혹시나 불닭이 먹어보고 싶다면 최근에 나온 4가지맛치즈불닭은 드실 수 도 있을거 같아용",
          "original_form": "그래도 혹시나 불닭이 먹어보고 싶다면 최근에 나온 4가지맛치즈불닭은 드실 수 도 있을거 같아용",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041308.1.23",
          "form": "신라면도 못 먹는 친구가 맵다했지만 먹을만 하다고 했거든요",
          "original_form": "신라면도 못 먹는 친구가 맵다했지만 먹을만 하다고 했거든요",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041308.1.24",
          "form": "아 까르보나라불닭 먹어봤는데 그건 괜찮더라구요@@",
          "original_form": "아 까르보나라불닭 먹어봤는데 그건 괜찮더라구요@@",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041308.1.25",
          "form": "다른맛도 먹어봐야겠어요ㅎㅎ",
          "original_form": "다른맛도 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041308.1.26",
          "form": "저녁은 뭘 드실 예정이신가요?",
          "original_form": "저녁은 뭘 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041308.1.27",
          "form": "저는 동료가 좀 큰 고구마를 줘서 라떼와 먹었어요ㅋㅋㅋㅋ",
          "original_form": "저는 동료가 좀 큰 고구마를 줘서 라떼와 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100041308.1.28",
          "form": "뭐드실예정이신가요?",
          "original_form": "뭐드실예정이신가요?",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100041308.1.29",
          "form": "저는 피자 먹으려구요ㅎㅎ",
          "original_form": "저는 피자 먹으려구요ㅎㅎ",
