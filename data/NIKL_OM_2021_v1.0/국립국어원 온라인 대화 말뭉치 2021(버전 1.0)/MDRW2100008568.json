      "utterance": [
        {
          "id": "MDRW2100008568.1.1",
          "form": "해운대 가보셨나요?",
          "original_form": "해운대 가보셨나요?",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.2",
          "form": "저 본가가 해운대입니다 ㅎㅎㅎ",
          "original_form": "저 본가가 해운대입니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.3",
          "form": "해운대 좋아하세요?",
          "original_form": "해운대 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.4",
          "form": "와우 아니요 가본적은 없어요ㅎㅎㅎ",
          "original_form": "와우 아니요 가본적은 없어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.5",
          "form": "해운대에 정말 돌고래가 사나요?",
          "original_form": "해운대에 정말 돌고래가 사나요?",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.6",
          "form": "아쿠아리움 가시면 볼 수 있어요",
          "original_form": "아쿠아리움 가시면 볼 수 있어요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008568.1.7",
          "form": "해운대 바닷가에는 해파리가 많아요",
          "original_form": "해운대 바닷가에는 해파리가 많아요",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008568.1.8",
          "form": "아하 부산아쿠아리움",
          "original_form": "아하 부산아쿠아리움",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008568.1.9",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008568.1.10",
          "form": "저 여행가는 거 정말 좋아하는데 최근엔 못 가서",
          "original_form": "저 여행가는 거 정말 좋아하는데 최근엔 못 가서",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008568.1.11",
          "form": "좀 많이 아쉬워요",
          "original_form": "좀 많이 아쉬워요",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008568.1.12",
          "form": "코로나 때문에 아쉽다 ㅠㅠ",
          "original_form": "코로나 때문에 아쉽다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008568.1.13",
          "form": "가장 기억에 남는 여행지 있나요?",
          "original_form": "가장 기억에 남는 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008568.1.14",
          "form": "음 국내에서는 공주요!!",
          "original_form": "음 국내에서는 공주요!!",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.15",
          "form": "공주?",
          "original_form": "공주?",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.16",
          "form": "거기서 김피탕이라고 처음 먹어봤는데",
          "original_form": "거기서 김피탕이라고 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.17",
          "form": "넘 맛있더라고요 ㅋㅋㅋㅋ",
          "original_form": "넘 맛있더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.18",
          "form": "오 맛있었겠다",
          "original_form": "오 맛있었겠다",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.19",
          "form": "공주 가본적이 없어서 가면 저도 김피탕 먹어봐야겠어요",
          "original_form": "공주 가본적이 없어서 가면 저도 김피탕 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008568.1.20",
          "form": "잘하는 집 있던가요?",
          "original_form": "잘하는 집 있던가요?",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.21",
          "form": "잘하는 집으로 찾아갔었어요!!",
          "original_form": "잘하는 집으로 찾아갔었어요!!",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.22",
          "form": "사실 그거 먹으러 간 것이나 다름 없었어요 ㅋㅋㅋㅋ",
          "original_form": "사실 그거 먹으러 간 것이나 다름 없었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.23",
          "form": "저에게만 알려주세요",
          "original_form": "저에게만 알려주세요",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.24",
          "form": "어딘가요",
          "original_form": "어딘가요",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.25",
          "form": "3-4년 전이라 기억이 잘 안 나요ㅠ 죄송해요",
          "original_form": "3-4년 전이라 기억이 잘 안 나요ㅠ 죄송해요",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.26",
          "form": "괜찮습니다 검색해보겠습니다",
          "original_form": "괜찮습니다 검색해보겠습니다",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.27",
          "form": "아마 네이버에 검색하면 몇 군데 안 나왔던 걸로 기억해요",
          "original_form": "아마 네이버에 검색하면 몇 군데 안 나왔던 걸로 기억해요",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008568.1.28",
          "form": "세종이 김피탕의 본고장이라고도 하더라구요",
          "original_form": "세종이 김피탕의 본고장이라고도 하더라구요",
          "speaker_id": "2",
          "time": "20210822 16:49"
        }
      ]
