      "utterance": [
        {
          "id": "MDRW2100023685.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023685.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100023685.1.3",
          "form": "요즘 뭐 하고 지내시나요?",
          "original_form": "요즘 뭐 하고 지내시나요?",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100023685.1.4",
          "form": "저는 그냥 회사 다니고 있어요!\n회사 말고는 따로 하는게 없네요ㅋㅋ",
          "original_form": "저는 그냥 회사 다니고 있어요!\n회사 말고는 따로 하는게 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023685.1.5",
          "form": "어떻게 지내시나요?",
          "original_form": "어떻게 지내시나요?",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023685.1.6",
          "form": "아하 저는 아직 학생이라 이제 곧 개강이어서 마음의 준비만 하고 있네요ㅜㅜ",
          "original_form": "아하 저는 아직 학생이라 이제 곧 개강이어서 마음의 준비만 하고 있네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023685.1.7",
          "form": "회사 다니신지 오래되셨나요?",
          "original_form": "회사 다니신지 오래되셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023685.1.8",
          "form": "아 학생이시군요!",
          "original_form": "아 학생이시군요!",
          "speaker_id": "2",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100023685.1.9",
          "form": "지금 회사는 일년 조금 넘었어요!",
          "original_form": "지금 회사는 일년 조금 넘었어요!",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100023685.1.10",
          "form": "아하 다니실만한가요?",
          "original_form": "아하 다니실만한가요?",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100023685.1.11",
          "form": "회사는 어떤가요?",
          "original_form": "회사는 어떤가요?",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100023685.1.12",
          "form": "네 아직까지는요ㅋㅋㅋㅋ",
          "original_form": "네 아직까지는요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100023685.1.13",
          "form": "저는 학교보다 회사가 차라리 낫더라구요",
          "original_form": "저는 학교보다 회사가 차라리 낫더라구요",
          "speaker_id": "2",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023685.1.14",
          "form": "아 ㅋㅋㅋㅋㅋㅋ 어떤점에서 그런가요?",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ 어떤점에서 그런가요?",
          "speaker_id": "1",
          "time": "20210830 15:10"
        },
        {
          "id": "MDRW2100023685.1.15",
          "form": "학교는 그만두고싶어도 못그만두는데",
          "original_form": "학교는 그만두고싶어도 못그만두는데",
          "speaker_id": "2",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100023685.1.16",
          "form": "회사는 그만둘수 있어서요",
          "original_form": "회사는 그만둘수 있어서요",
          "speaker_id": "2",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100023685.1.17",
          "form": "직장은 새로 구하면되니까요ㅠ",
          "original_form": "직장은 새로 구하면되니까요ㅠ",
          "speaker_id": "2",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100023685.1.18",
          "form": "아하 그죠그죠..ㅜㅜ",
          "original_form": "아하 그죠그죠..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100023685.1.19",
          "form": "회사는 집에서 가까우신가요?",
          "original_form": "회사는 집에서 가까우신가요?",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100023685.1.20",
          "form": "집에서 차로 30분 정도 걸려요!",
          "original_form": "집에서 차로 30분 정도 걸려요!",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100023685.1.21",
          "form": "학생이시면 전공이 어떻게 되세요?",
          "original_form": "학생이시면 전공이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100023685.1.22",
          "form": "아하 저는 디자인 전공이에요~",
          "original_form": "아하 저는 디자인 전공이에요~",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100023685.1.23",
          "form": "몇학년이세요?",
          "original_form": "몇학년이세요?",
          "speaker_id": "2",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100023685.1.24",
          "form": "저는 올해 졸업해요! 이제 곧 취업준비해야죠 ㅎㅎ..",
          "original_form": "저는 올해 졸업해요! 이제 곧 취업준비해야죠 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100023685.1.25",
          "form": "아하! 이제 졸업하는군요\n취업 준비하시기 힘드시겠네요ㅠ",
          "original_form": "아하! 이제 졸업하는군요\n취업 준비하시기 힘드시겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:45"
        }
      ]
    }
