      "utterance": [
        {
          "id": "MDRW2100035714.1.1",
          "form": "브라운신부라는 영드에요",
          "original_form": "브라운신부라는 영드에요",
          "speaker_id": "2",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035714.1.2",
          "form": "영국 드라마",
          "original_form": "영국 드라마",
          "speaker_id": "2",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035714.1.3",
          "form": "오 지적인걸보는구만 name2님",
          "original_form": "오 지적인걸보는구만 &name2&님",
          "speaker_id": "1",
          "time": "20210830 12:10"
        },
        {
          "id": "MDRW2100035714.1.4",
          "form": "재미있어요",
          "original_form": "재미있어요",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035714.1.5",
          "form": "가벼운 수사물임",
          "original_form": "가벼운 수사물임",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035714.1.6",
          "form": "추리물좋아하남?",
          "original_form": "추리물좋아하남?",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035714.1.7",
          "form": "이런걸 코믹 수사물이라고한대요",
          "original_form": "이런걸 코믹 수사물이라고한대요",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035714.1.8",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035714.1.9",
          "form": "나는 가벼운추리물 미스피셔의 사건일진가 그거 재밌게봄",
          "original_form": "나는 가벼운추리물 미스피셔의 사건일진가 그거 재밌게봄",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035714.1.10",
          "form": "미스피셔가 아주 우아하고 뷰티하게 수사함",
          "original_form": "미스피셔가 아주 우아하고 뷰티하게 수사함",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035714.1.11",
          "form": "그건 어느나라 꺼에요?",
          "original_form": "그건 어느나라 꺼에요?",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035714.1.12",
          "form": "깐깐한 형사님과 케미도 좋음",
          "original_form": "깐깐한 형사님과 케미도 좋음",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035714.1.13",
          "form": "기억안나네 눈이 즐거움",
          "original_form": "기억안나네 눈이 즐거움",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035714.1.14",
          "form": "브라운신부는 말그대로 신부님이 수사하심 ㅋㅋㅋ",
          "original_form": "브라운신부는 말그대로 신부님이 수사하심 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100035714.1.15",
          "form": "오 신부님이 고귀하게 그것도 재밌겠군",
          "original_form": "오 신부님이 고귀하게 그것도 재밌겠군",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100035714.1.16",
          "form": "아 해리포터 영화 위즐리아빠가 신부님임!",
          "original_form": "아 해리포터 영화 위즐리아빠가 신부님임!",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100035714.1.17",
          "form": "와우!@",
          "original_form": "와우!@",
          "speaker_id": "1",
          "time": "20210830 16:22"
        },
        {
          "id": "MDRW2100035714.1.18",
          "form": "좋당",
          "original_form": "좋당",
          "speaker_id": "1",
          "time": "20210830 16:22"
        },
        {
          "id": "MDRW2100035714.1.19",
          "form": "그리고 음 50년대 영국 시골마을이 배경임 ㅋㅋ",
          "original_form": "그리고 음 50년대 영국 시골마을이 배경임 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100035714.1.20",
          "form": "오 정겨워",
          "original_form": "오 정겨워",
          "speaker_id": "1",
          "time": "20210830 17:15"
        },
