      "utterance": [
        {
          "id": "MDRW2100021645.1.1",
          "form": "재택근무 해본 적 있어요?",
          "original_form": "재택근무 해본 적 있어요?",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021645.1.2",
          "form": "네 해보셨어요?",
          "original_form": "네 해보셨어요?",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021645.1.3",
          "form": "네네 재택근무 해보니까 좋았어요",
          "original_form": "네네 재택근무 해보니까 좋았어요",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021645.1.4",
          "form": "근데 마냥 좋기만 하진 않더라구요 ㅜ",
          "original_form": "근데 마냥 좋기만 하진 않더라구요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021645.1.5",
          "form": "집중이 안되죠?",
          "original_form": "집중이 안되죠?",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021645.1.6",
          "form": "전 격일로 해요",
          "original_form": "전 격일로 해요",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021645.1.7",
          "form": "네 ㅋㅋ 뭔가 회사보다 일하기 불편한 느낌이에요",
          "original_form": "네 ㅋㅋ 뭔가 회사보다 일하기 불편한 느낌이에요",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021645.1.8",
          "form": "분위기는 편안한데 업무하기가 불편하네요 ㅜㅜ",
          "original_form": "분위기는 편안한데 업무하기가 불편하네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021645.1.9",
          "form": "의자도 딱딱하고 너무 딴짓 많이해요",
          "original_form": "의자도 딱딱하고 너무 딴짓 많이해요",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021645.1.10",
          "form": "감시시스템 구축이 되어있나요",
          "original_form": "감시시스템 구축이 되어있나요",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021645.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021645.1.12",
          "form": "감시시스템이요? ㄷㄷ",
          "original_form": "감시시스템이요? ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021645.1.13",
          "form": "따로 그런건 없는데 필요한 프로그램이나 파일을 다 회사 컴퓨터에 저장해두다보니 옮겨오기 귀찮아요",
          "original_form": "따로 그런건 없는데 필요한 프로그램이나 파일을 다 회사 컴퓨터에 저장해두다보니 옮겨오기 귀찮아요",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021645.1.14",
          "form": "무슨 파일 필요한데 지금 없고 회사에 있고 하면 하....",
          "original_form": "무슨 파일 필요한데 지금 없고 회사에 있고 하면 하....",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021645.1.15",
          "form": "저흰 원격써요",
          "original_form": "저흰 원격써요",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021645.1.16",
          "form": "친구네는 십분이상 마우스 멈춰있으면 경고누적ㅋㅋ",
          "original_form": "친구네는 십분이상 마우스 멈춰있으면 경고누적ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021645.1.17",
          "form": "아 대박 ㅋㅋ 진짜 무섭네요",
          "original_form": "아 대박 ㅋㅋ 진짜 무섭네요",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021645.1.18",
          "form": "똥 싸고 오려면 변비인사람 어떡함..바로 경고 각인데",
          "original_form": "똥 싸고 오려면 변비인사람 어떡함..바로 경고 각인데",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021645.1.19",
          "form": "누워서 티비보다 발로 몇번 친다고 완전 월급도둑이라고ㅋㅋ",
          "original_form": "누워서 티비보다 발로 몇번 친다고 완전 월급도둑이라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021645.1.20",
          "form": "앗 ㅋㅋㅋ 제대로 월급루팡이네요",
          "original_form": "앗 ㅋㅋㅋ 제대로 월급루팡이네요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021645.1.21",
          "form": "무선마우스로 하면 더 꿀이겠어요",
          "original_form": "무선마우스로 하면 더 꿀이겠어요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021645.1.22",
          "form": "ㅋㅋ사실 재택하면 잡일도 안하니까 좋은데 너무 루즈해져서ㅜ 죄책감 살짝 ㅎㅎ",
          "original_form": "ㅋㅋ사실 재택하면 잡일도 안하니까 좋은데 너무 루즈해져서ㅜ 죄책감 살짝 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021645.1.23",
          "form": "그럼 재택근무 vs 출근 뭐가 더 좋아요?",
