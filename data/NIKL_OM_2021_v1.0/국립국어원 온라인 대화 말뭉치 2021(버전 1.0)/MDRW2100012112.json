      "utterance": [
        {
          "id": "MDRW2100012112.1.1",
          "form": "싸이월드 추억 많으시나요",
          "original_form": "싸이월드 추억 많으시나요",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012112.1.2",
          "form": "싸이월드 즐기셨나요",
          "original_form": "싸이월드 즐기셨나요",
          "speaker_id": "1",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012112.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012112.1.4",
          "form": "전 좀하다 말았어요",
          "original_form": "전 좀하다 말았어요",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012112.1.5",
          "form": "도토리 랑 자료 많으세요?",
          "original_form": "도토리 랑 자료 많으세요?",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012112.1.6",
          "form": "조그마한 이모티콘 같은거 생각이 나긴해요",
          "original_form": "조그마한 이모티콘 같은거 생각이 나긴해요",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012112.1.7",
          "form": "이뿌게 꾸미고 그러셨어요?",
          "original_form": "이뿌게 꾸미고 그러셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012112.1.8",
          "form": "ㅋㅋㅋ 글씨체도 샀나요? 글씨체 바꾸고 그랬던 기억이..",
          "original_form": "ㅋㅋㅋ 글씨체도 샀나요? 글씨체 바꾸고 그랬던 기억이..",
          "speaker_id": "1",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100012112.1.9",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012112.1.10",
          "form": "전 정말 안꾸몄어요",
          "original_form": "전 정말 안꾸몄어요",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012112.1.11",
          "form": "친구는 사진이며 자료가 어마어마 했는데",
          "original_form": "친구는 사진이며 자료가 어마어마 했는데",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012112.1.12",
          "form": "전 가난했어요 ㅋㅋ",
          "original_form": "전 가난했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012112.1.13",
          "form": "근데 언제 오픈해요?",
          "original_form": "근데 언제 오픈해요?",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012112.1.14",
          "form": "자꾸 밀어지던데",
          "original_form": "자꾸 밀어지던데",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012112.1.15",
          "form": "폰이 바껴서 찾을수 있나 모르겠어요",
          "original_form": "폰이 바껴서 찾을수 있나 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012112.1.16",
          "form": "아 그렇군요 저는 언제나 들어갈 수 있는줄 알았어야",
          "original_form": "아 그렇군요 저는 언제나 들어갈 수 있는줄 알았어야",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100012112.1.17",
          "form": "아이디 알ㄹ면 언제나 갈슈있을걸요",
          "original_form": "아이디 알ㄹ면 언제나 갈슈있을걸요",
          "speaker_id": "2",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100012112.1.18",
          "form": "전 기억을 못해서요",
          "original_form": "전 기억을 못해서요",
          "speaker_id": "2",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100012112.1.19",
          "form": "그럼 확인해야하는데 번호가 바껴서",
          "original_form": "그럼 확인해야하는데 번호가 바껴서",
          "speaker_id": "2",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100012112.1.20",
          "form": "메일도 달라지고요",
          "original_form": "메일도 달라지고요",
          "speaker_id": "2",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100012112.1.21",
          "form": "안된다는것 같던데 모르겠어요",
          "original_form": "안된다는것 같던데 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012112.1.22",
          "form": "아이디 아세요 ?정보?",
          "original_form": "아이디 아세요 ?정보?",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012112.1.23",
          "form": "그럼 되겠죠 ㅎㅎ",
          "original_form": "그럼 되겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012112.1.24",
          "form": "아 그렇군요 네이트닷컴 계정으로 들어갔던 것 같은데 ㅋ",
          "original_form": "아 그렇군요 네이트닷컴 계정으로 들어갔던 것 같은데 ㅋ",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012112.1.25",
          "form": "아 네이트 추억돋네요 ㅋㅋ",
          "original_form": "아 네이트 추억돋네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012112.1.26",
          "form": "싸이월드가 그 시절 감성은 가득한것같아요",
          "original_form": "싸이월드가 그 시절 감성은 가득한것같아요",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100012112.1.27",
          "form": "맞아요 ㅎㅎ전 그때 쫌 촌스러웠던것 같아요 ㅋㅋ",
          "original_form": "맞아요 ㅎㅎ전 그때 쫌 촌스러웠던것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012112.1.28",
          "form": "돌아보면 다그럴죠",
          "original_form": "돌아보면 다그럴죠",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
