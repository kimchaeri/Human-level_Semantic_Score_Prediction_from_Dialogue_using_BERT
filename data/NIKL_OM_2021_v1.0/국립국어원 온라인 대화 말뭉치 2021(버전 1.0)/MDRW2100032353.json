        },
        {
          "id": "MDRW2100032353.1.10",
          "form": "유럽요",
          "original_form": "유럽요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032353.1.11",
          "form": "좋으셨겠어요 몇개국이나 들르셨나요",
          "original_form": "좋으셨겠어요 몇개국이나 들르셨나요",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100032353.1.12",
          "form": "7개국정도요",
