      "utterance": [
        {
          "id": "MDRW2100042461.1.1",
          "form": "뭐 진짜? 난 보쌈 생각했는데",
          "original_form": "뭐 진짜? 난 보쌈 생각했는데",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042461.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042461.1.3",
          "form": "수육을 삼겹살로 해..?",
          "original_form": "수육을 삼겹살로 해..?",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042461.1.4",
          "form": "ㅋㅋㅋㄱㄱㄱㅋㄱㄱ어 삼겹살진짜 존맛",
          "original_form": "ㅋㅋㅋㄱㄱㄱㅋㄱㄱ어 삼겹살진짜 존맛",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042461.1.5",
          "form": "적당히 기름지고",
          "original_form": "적당히 기름지고",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.6",
          "form": "부드럽고",
          "original_form": "부드럽고",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.7",
          "form": "삼겹살로 만든 수육을 보쌈이라고 하나",
          "original_form": "삼겹살로 만든 수육을 보쌈이라고 하나",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.8",
          "form": "안질겨서 너무 좋아",
          "original_form": "안질겨서 너무 좋아",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.9",
          "form": "비슷할걸",
          "original_form": "비슷할걸",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ모든 고기를 물에 삶아내면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ모든 고기를 물에 삶아내면",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.11",
          "form": "수육이고",
          "original_form": "수육이고",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.12",
          "form": "수육 중 삼겹살이나 목살로 한 걸",
          "original_form": "수육 중 삼겹살이나 목살로 한 걸",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.13",
          "form": "보쌈이라고 부르는 건가",
          "original_form": "보쌈이라고 부르는 건가",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.14",
          "form": "내나 그렇긴해",
          "original_form": "내나 그렇긴해",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.15",
          "form": "거참",
          "original_form": "거참",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.16",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042461.1.17",
          "form": "넌 저녁 많이 먹었니",
          "original_form": "넌 저녁 많이 먹었니",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.18",
          "form": "아니 고등어가 비려서 조금 먹었어",
          "original_form": "아니 고등어가 비려서 조금 먹었어",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.20",
          "form": "넌 많이 먹었니?",
          "original_form": "넌 많이 먹었니?",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.21",
          "form": "어 엄청 먹었어",
          "original_form": "어 엄청 먹었어",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.22",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042461.1.23",
          "form": "진심 맛있었겠다",
          "original_form": "진심 맛있었겠다",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042461.1.24",
          "form": "보쌈집 무생채랑 김치 너무 맛있어",
          "original_form": "보쌈집 무생채랑 김치 너무 맛있어",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042461.1.25",
          "form": "너랑 고기집가서",
          "original_form": "너랑 고기집가서",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042461.1.26",
          "form": "엄청 먹고싶어",
          "original_form": "엄청 먹고싶어",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042461.1.27",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042461.1.28",
          "form": "너랑 같이 돼지파티하고파",
