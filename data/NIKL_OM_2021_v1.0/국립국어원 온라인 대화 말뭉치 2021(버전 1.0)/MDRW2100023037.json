        },
        {
          "id": "MDRW2100023037.1.2",
          "form": "베이글은 살짝 맹맹한 맛이있잖요",
          "original_form": "베이글은 살짝 맹맹한 맛이있잖요",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023037.1.3",
          "form": "하도 안드시다보니 밀가루 본연의 맛을 느끼셨나봐요ㅋㅋ",
          "original_form": "하도 안드시다보니 밀가루 본연의 맛을 느끼셨나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023037.1.4",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023037.1.5",
          "form": "전 빵순이가 아니라",
          "original_form": "전 빵순이가 아니라",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023037.1.6",
          "form": "빵순이아니세요?",
          "original_form": "빵순이아니세요?",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023037.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023037.1.8",
          "form": "저도 아닌데 넘 맛있었어요",
          "original_form": "저도 아닌데 넘 맛있었어요",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023037.1.9",
          "form": "ㅋㅋㅋ 다행이네요",
          "original_form": "ㅋㅋㅋ 다행이네요",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023037.1.10",
          "form": "또 먹고싶어요",
          "original_form": "또 먹고싶어요",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023037.1.11",
          "form": "진짜 달달한거",
          "original_form": "진짜 달달한거",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
