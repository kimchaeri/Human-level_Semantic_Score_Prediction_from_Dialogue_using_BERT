      "utterance": [
        {
          "id": "MDRW2100027713.1.1",
          "form": "요즘 꽂힌 음식 있어?",
          "original_form": "요즘 꽂힌 음식 있어?",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027713.1.2",
          "form": "음 딱히 없는거가테",
          "original_form": "음 딱히 없는거가테",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100027713.1.3",
          "form": "꽂힌음싣잇서?",
          "original_form": "꽂힌음싣잇서?",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100027713.1.4",
          "form": "난 있어",
          "original_form": "난 있어",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100027713.1.5",
          "form": "윤옥연떡볶이....",
          "original_form": "윤옥연떡볶이....",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100027713.1.6",
          "form": "먹고싶다...ㅋㅋㅋㅋㅋ",
          "original_form": "먹고싶다...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100027713.1.7",
          "form": "ㅋㅋㅋㅋㅋ와진짜오랜만이다",
          "original_form": "ㅋㅋㅋㅋㅋ와진짜오랜만이다",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100027713.1.8",
          "form": "존맛탱 ㅠㅠㅠ",
          "original_form": "존맛탱 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100027713.1.9",
          "form": "그 특유의 매콤함 맛잇는겤ㅋㅋㅋㅋ",
          "original_form": "그 특유의 매콤함 맛잇는겤ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100027713.1.10",
          "form": "ㅋㅋㅋ카니까 후추때려넣은 느낌 맛보고싶다...",
          "original_form": "ㅋㅋㅋ카니까 후추때려넣은 느낌 맛보고싶다...",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027713.1.11",
          "form": "거기 오뎅 만두도 완전옛날서타일",
          "original_form": "거기 오뎅 만두도 완전옛날서타일",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100027713.1.12",
          "form": "ㅠㅠㅠ지금엄청땡기네",
          "original_form": "ㅠㅠㅠ지금엄청땡기네",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100027713.1.13",
          "form": "저녁 뭐먹음~~~~",
          "original_form": "저녁 뭐먹음~~~~",
          "speaker_id": "1",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100027713.1.14",
          "form": "아직 안뭄...",
          "original_form": "아직 안뭄...",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100027713.1.15",
          "form": "무파마 먹을라고 샀으여",
          "original_form": "무파마 먹을라고 샀으여",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100027713.1.16",
          "form": "무파마 라면?? 진짜올만에듣느ㄴ다",
          "original_form": "무파마 라면?? 진짜올만에듣느ㄴ다",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100027713.1.17",
          "form": "맞재 ㅋㅋㅋㅋ",
          "original_form": "맞재 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100027713.1.18",
          "form": "뭔가 얼큰한 국물 땡겨서",
          "original_form": "뭔가 얼큰한 국물 땡겨서",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100027713.1.19",
          "form": "ㅋㅋㅋㅋㅋ비와서 땡기나여",
          "original_form": "ㅋㅋㅋㅋㅋ비와서 땡기나여",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100027713.1.20",
          "form": "당연ㅋㅋㅋ",
          "original_form": "당연ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100027713.1.21",
