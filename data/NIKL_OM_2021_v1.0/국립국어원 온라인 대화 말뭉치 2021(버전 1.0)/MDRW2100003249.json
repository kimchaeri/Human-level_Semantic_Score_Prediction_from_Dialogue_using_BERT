      "utterance": [
        {
          "id": "MDRW2100003249.1.1",
          "form": "지금 뭐 마시고 있어?",
          "original_form": "지금 뭐 마시고 있어?",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003249.1.2",
          "form": "카페라떼 마일드. 아빠가 사다줬어.",
          "original_form": "카페라떼 마일드. 아빠가 사다줬어.",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003249.1.3",
          "form": "아빠가? 아빠 커피도 안 마시면서 웬일이래",
          "original_form": "아빠가? 아빠 커피도 안 마시면서 웬일이래",
          "speaker_id": "2",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003249.1.4",
          "form": "몰라. 편의점에서 샀대.",
          "original_form": "몰라. 편의점에서 샀대.",
          "speaker_id": "1",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003249.1.5",
          "form": "근데 너무 달아.",
          "original_form": "근데 너무 달아.",
          "speaker_id": "1",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003249.1.6",
          "form": "아 편의점 커피는 원래 다 설탕 들어가잖아ㅋㅋㅋ",
          "original_form": "아 편의점 커피는 원래 다 설탕 들어가잖아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003249.1.7",
          "form": "음료로 설탕 섭취하는 게 제일 안 좋대~",
          "original_form": "음료로 설탕 섭취하는 게 제일 안 좋대~",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003249.1.8",
          "form": "단 커피 너무 많이 마시지 마",
          "original_form": "단 커피 너무 많이 마시지 마",
          "speaker_id": "2",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003249.1.9",
          "form": "어. 평소에는 잘 안 마셔.",
          "original_form": "어. 평소에는 잘 안 마셔.",
          "speaker_id": "1",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003249.1.10",
          "form": "카페 가서는 항상 아메리카노만 마시지.",
          "original_form": "카페 가서는 항상 아메리카노만 마시지.",
          "speaker_id": "1",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003249.1.11",
          "form": "나도 지금 아아메 마시고 있어",
          "original_form": "나도 지금 아아메 마시고 있어",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003249.1.12",
          "form": "요새 매일 1일 1잔 하는 중",
          "original_form": "요새 매일 1일 1잔 하는 중",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003249.1.13",
          "form": "너도 찬 거 많이 먹지마.",
          "original_form": "너도 찬 거 많이 먹지마.",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003249.1.14",
          "form": "ㅋㅋㅋㅋ알겠습니다",
          "original_form": "ㅋㅋㅋㅋ알겠습니다",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003249.1.15",
          "form": "내일 집 가기 전에 스타벅스에서 만날까?",
          "original_form": "내일 집 가기 전에 스타벅스에서 만날까?",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003249.1.16",
          "form": "신메뉴 뭐 나온 거 있나",
          "original_form": "신메뉴 뭐 나온 거 있나",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003249.1.17",
          "form": "알마 전에 신메뉴 나왔지.",
          "original_form": "알마 전에 신메뉴 나왔지.",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003249.1.18",
          "form": "아 진짜?? 먹어 봤어?",
          "original_form": "아 진짜?? 먹어 봤어?",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003249.1.19",
          "form": "응 컵도 준대서 한번 마셔 봤어",
          "original_form": "응 컵도 준대서 한번 마셔 봤어",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003249.1.20",
          "form": "음료 색이 초로색이라 청포도인줄 알고 시켰는데 아니더라고.",
          "original_form": "음료 색이 초로색이라 청포도인줄 알고 시켰는데 아니더라고.",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003249.1.21",
          "form": "잘 보고 주문하지ㅋㅋㅋㅋ 무슨 맛이었는데?",
          "original_form": "잘 보고 주문하지ㅋㅋㅋㅋ 무슨 맛이었는데?",
          "speaker_id": "2",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003249.1.22",
          "form": "그냥 사과맛 느낌.. 기대했던 맛은 아니었어",
          "original_form": "그냥 사과맛 느낌.. 기대했던 맛은 아니었어",
          "speaker_id": "1",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003249.1.23",
          "form": "ㅎㅎㅎ스타벅스는 라떼가 제일 맛있다구~~",
          "original_form": "ㅎㅎㅎ스타벅스는 라떼가 제일 맛있다구~~",
          "speaker_id": "2",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003249.1.24",
          "form": "엄마는 딸이 사주는 거면 다 맛있어.",
          "original_form": "엄마는 딸이 사주는 거면 다 맛있어.",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003249.1.25",
          "form": "그래놓고 베이글까지 시켜달라고 할거지",
          "original_form": "그래놓고 베이글까지 시켜달라고 할거지",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003249.1.26",
          "form": "어침으로 먹기 좋잖아.",
          "original_form": "어침으로 먹기 좋잖아.",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003249.1.27",
          "form": "아침으로",
          "original_form": "아침으로",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003249.1.28",
          "form": "근데 맨날 똑같은 빵 먹으면 안 질려??",
          "original_form": "근데 맨날 똑같은 빵 먹으면 안 질려??",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003249.1.29",
          "form": "거기서 엄마가 먹을 빵은 그것밖에 없잖아.",
          "original_form": "거기서 엄마가 먹을 빵은 그것밖에 없잖아.",
          "speaker_id": "1",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003249.1.30",
          "form": "그렇긴 하지~",
          "original_form": "그렇긴 하지~",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003249.1.31",
          "form": "엄마가 치주도 안 먹고 크림도 안 먹으니까",
          "original_form": "엄마가 치주도 안 먹고 크림도 안 먹으니까",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003249.1.32",
          "form": "치즈",
          "original_form": "치즈",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003249.1.33",
          "form": "샌드위치에는 거의 다 치즈 들어가는데",
          "original_form": "샌드위치에는 거의 다 치즈 들어가는데",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003249.1.34",
          "form": "커피만 마시고 나와도 돼.",
          "original_form": "커피만 마시고 나와도 돼.",
          "speaker_id": "1",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100003249.1.35",
          "form": "아니면 조각 케익이라도 살까?",
          "original_form": "아니면 조각 케익이라도 살까?",
          "speaker_id": "2",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100003249.1.36",
          "form": "무슨 케익 먹고 싶어? 사다줅0",
          "original_form": "무슨 케익 먹고 싶어? 사다줅0",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003249.1.37",
          "form": "사다줄게",
          "original_form": "사다줄게",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003249.1.38",
          "form": "몰라. 너 초코케익 좋아하니까 그거 사.",
          "original_form": "몰라. 너 초코케익 좋아하니까 그거 사.",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003249.1.39",
          "form": "나 말고 엄마가 먹고 싶은 거 사다줄려고 그러지~",
          "original_form": "나 말고 엄마가 먹고 싶은 거 사다줄려고 그러지~",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003249.1.40",
          "form": "치즈 빼고 맛있어 보이는 걸로 사갈게!",
          "original_form": "치즈 빼고 맛있어 보이는 걸로 사갈게!",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003249.1.41",
          "form": "기대하삼",
          "original_form": "기대하삼",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003249.1.42",
          "form": "알겠어. 음료수는 너랑 똑같은 걸로 부탁해.",
          "original_form": "알겠어. 음료수는 너랑 똑같은 걸로 부탁해.",
          "speaker_id": "1",
          "time": "20210820 21:26"
        }
      ]
    }
