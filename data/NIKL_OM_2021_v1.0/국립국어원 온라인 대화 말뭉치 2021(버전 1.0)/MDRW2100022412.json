        {
          "id": "MDRW2100022412.1.1",
          "form": "요즘 날씨 많이 추워지지않았나요?",
          "original_form": "요즘 날씨 많이 추워지지않았나요?",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022412.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022412.1.3",
          "form": "쌀쌀하기는 한데음 여긴비가와서 습해서그런지 아직 좀 더워요",
          "original_form": "쌀쌀하기는 한데음 여긴비가와서 습해서그런지 아직 좀 더워요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.4",
          "form": "name1님 지역은 어떤가요",
          "original_form": "&name1&님 지역은 어떤가요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.5",
          "form": "아 맞아요 요즘 비가오더라고요",
          "original_form": "아 맞아요 요즘 비가오더라고요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.6",
          "form": "비오는날좋아하세요?",
          "original_form": "비오는날좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.7",
          "form": "비가 너무 자주와서",
          "original_form": "비가 너무 자주와서",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.8",
          "form": "약간 지겨운..ㅎ",
          "original_form": "약간 지겨운..ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.9",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.11",
          "form": "저는 우산이 너무 거추장스러워서 ㅠㅠ",
          "original_form": "저는 우산이 너무 거추장스러워서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022412.1.12",
          "form": "우산ㅋㅋ약간 우산써도 다 젖잖아요ㅋㅋ",
          "original_form": "우산ㅋㅋ약간 우산써도 다 젖잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022412.1.13",
          "form": "양말도 젖고 ㅠㅠ",
          "original_form": "양말도 젖고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022412.1.14",
          "form": "우산을 쓰는 의미가 없는ㅋㅋㅋ",
          "original_form": "우산을 쓰는 의미가 없는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022412.1.15",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022412.1.16",
          "form": "빨리 가을왔음 좋겠네요",
          "original_form": "빨리 가을왔음 좋겠네요",
