      "utterance": [
        {
          "id": "MDRW2100031112.1.1",
          "form": "너구리 들어간것도 아닌데",
          "original_form": "너구리 들어간것도 아닌데",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031112.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게ㅋㅋㅋ 왜 너구리일까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게ㅋㅋㅋ 왜 너구리일까",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031112.1.3",
          "form": "통통한 동물은 너구리 말고도 있는데",
          "original_form": "통통한 동물은 너구리 말고도 있는데",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.4",
          "form": "왜 너구리지",
          "original_form": "왜 너구리지",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.5",
          "form": "인생의 아이러니다",
          "original_form": "인생의 아이러니다",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.6",
          "form": "난 너구리 좋아하는데",
          "original_form": "난 너구리 좋아하는데",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.7",
          "form": "바나나로 만들수있는 디저트없으려나",
          "original_form": "바나나로 만들수있는 디저트없으려나",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.8",
          "form": "너구리 좋아하니",
          "original_form": "너구리 좋아하니",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.9",
          "form": "응 근데 맛이 기억이 안 나 웃기지",
          "original_form": "응 근데 맛이 기억이 안 나 웃기지",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.10",
          "form": "먹은지 오래됐어",
          "original_form": "먹은지 오래됐어",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031112.1.11",
          "form": "바나나 우유랑 갈아서 마시면 맛있던데",
          "original_form": "바나나 우유랑 갈아서 마시면 맛있던데",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.12",
          "form": "어이없네",
          "original_form": "어이없네",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.13",
          "form": "헤봤니",
          "original_form": "헤봤니",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.14",
          "form": "기억속에서 미화된거아냐",
          "original_form": "기억속에서 미화된거아냐",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.15",
          "form": "ㅋㅋㅋㅋ그럴수도 있어",
          "original_form": "ㅋㅋㅋㅋ그럴수도 있어",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.16",
          "form": "바나나우유라 클래식하구만",
          "original_form": "바나나우유라 클래식하구만",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031112.1.17",
          "form": "다른 사람은 초코바나나도 만들어먹더라",
          "original_form": "다른 사람은 초코바나나도 만들어먹더라",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.18",
          "form": "퐁듀 굳혀서",
          "original_form": "퐁듀 굳혀서",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.19",
          "form": "스프링클 뿌려먹던데",
          "original_form": "스프링클 뿌려먹던데",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.20",
          "form": "흠 바나나가 지금 집에서",
          "original_form": "흠 바나나가 지금 집에서",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.21",
          "form": "표범되거있거든",
          "original_form": "표범되거있거든",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.22",
          "form": "점 많이 생긴 바나나는 부스러져서 될 지 모르겠네",
          "original_form": "점 많이 생긴 바나나는 부스러져서 될 지 모르겠네",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.23",
          "form": "ㅋㅋㅋㅋ표범이래",
          "original_form": "ㅋㅋㅋㅋ표범이래",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.24",
          "form": "레오파드 바나나라고 불러",
          "original_form": "레오파드 바나나라고 불러",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031112.1.25",
          "form": "귀엽다",
          "original_form": "귀엽다",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031112.1.26",
          "form": "난 푹 익은 건 죄다 우유에 갈아먹어",
          "original_form": "난 푹 익은 건 죄다 우유에 갈아먹어",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031112.1.27",
          "form": "딸기 있으면 딸바도 괜춘",
          "original_form": "딸기 있으면 딸바도 괜춘",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100031112.1.28",
          "form": "그럴까 우유사와서 먹고 치워야겠다",
          "original_form": "그럴까 우유사와서 먹고 치워야겠다",
          "speaker_id": "2",
          "time": "20210904 16:59"
        }
      ]
    }
