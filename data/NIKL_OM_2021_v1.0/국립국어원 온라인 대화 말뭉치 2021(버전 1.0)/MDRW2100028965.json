      "utterance": [
        {
          "id": "MDRW2100028965.1.1",
          "form": "맛있는거면",
          "original_form": "맛있는거면",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028965.1.2",
          "form": "좋아하시구사",
          "original_form": "좋아하시구사",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028965.1.3",
          "form": "넹ㅋㅋ",
          "original_form": "넹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028965.1.4",
          "form": "저두 달달하면 좋아해요",
          "original_form": "저두 달달하면 좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028965.1.5",
          "form": "갑자기 미숭가루는왜",
          "original_form": "갑자기 미숭가루는왜",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028965.1.6",
          "form": "아 지금 마시고 있거든요ㅋㅋ",
          "original_form": "아 지금 마시고 있거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028965.1.7",
          "form": "아그러시구나",
          "original_form": "아그러시구나",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.8",
          "form": "집에서탔어요 밥먹기구찮아서",
          "original_form": "집에서탔어요 밥먹기구찮아서",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.9",
          "form": "전 연어 먹었어요",
          "original_form": "전 연어 먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.10",
          "form": "크 대박",
          "original_form": "크 대박",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.11",
          "form": "내일은 또 어떤걸 먹을지 설레요ㅋㅋ",
          "original_form": "내일은 또 어떤걸 먹을지 설레요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.12",
          "form": "먹기위해사는 인생이죠",
          "original_form": "먹기위해사는 인생이죠",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028965.1.13",
          "form": "어떤 메뉴 좋아하세요?",
          "original_form": "어떤 메뉴 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.14",
          "form": "연어도 좋아하고 스시도",
          "original_form": "연어도 좋아하고 스시도",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.15",
          "form": "돈가스류는요?",
          "original_form": "돈가스류는요?",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.16",
          "form": "치즈돈가스는좋아해요",
          "original_form": "치즈돈가스는좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.17",
          "form": "전 왕돈가스 얇은거요",
          "original_form": "전 왕돈가스 얇은거요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.18",
          "form": "소스맛으로 먹는 ?",
          "original_form": "소스맛으로 먹는 ?",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.19",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028965.1.20",
          "form": "내일 먹어야겠다",
          "original_form": "내일 먹어야겠다",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028965.1.21",
          "form": "메뉴 정해지셨네요ㅋ기",
          "original_form": "메뉴 정해지셨네요ㅋ기",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028965.1.22",
          "form": "연어좋아하는사람",
          "original_form": "연어좋아하는사람",
          "speaker_id": "1",
          "time": "20210902 23:20"
        }
      ]
    }
