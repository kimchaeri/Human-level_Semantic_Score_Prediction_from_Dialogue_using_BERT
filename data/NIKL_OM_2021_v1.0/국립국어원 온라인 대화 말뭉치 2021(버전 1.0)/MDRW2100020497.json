      "utterance": [
        {
          "id": "MDRW2100020497.1.1",
          "form": "저부터 말할까요",
          "original_form": "저부터 말할까요",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.2",
          "form": "네 먼저하세요",
          "original_form": "네 먼저하세요",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.3",
          "form": "전 사실 치킨브랜드는 가리지않는데 약간 후라이드의 매력이 조금씩 달라서",
          "original_form": "전 사실 치킨브랜드는 가리지않는데 약간 후라이드의 매력이 조금씩 달라서",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.4",
          "form": "그래도 요즘은 자담치킨 괜찮더라구요",
          "original_form": "그래도 요즘은 자담치킨 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.5",
          "form": "그쵸 노랑치킨과 네네는 천지차이죠",
          "original_form": "그쵸 노랑치킨과 네네는 천지차이죠",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.7",
          "form": "앗 자담치킨 최근에 추천을 들었어요",
          "original_form": "앗 자담치킨 최근에 추천을 들었어요",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.8",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210828 08:28"
        },
        {
          "id": "MDRW2100020497.1.9",
          "form": "후라이드가 깔끔한맛이에요",
          "original_form": "후라이드가 깔끔한맛이에요",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.10",
          "form": "먹고나면 아 잘먹었다 생각드는 맛",
          "original_form": "먹고나면 아 잘먹었다 생각드는 맛",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.11",
          "form": "조금 더 묘사해주세요",
          "original_form": "조금 더 묘사해주세요",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.12",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.13",
          "form": "비슷한 브랜드는 어디가 있죠",
          "original_form": "비슷한 브랜드는 어디가 있죠",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.14",
          "form": "이걸",
          "original_form": "이걸",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.15",
          "form": "이거 비슷한거..",
          "original_form": "이거 비슷한거..",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.16",
          "form": "못찾겠어요",
          "original_form": "못찾겠어요",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.17",
          "form": "자담은 자담이 유일해요",
          "original_form": "자담은 자담이 유일해요",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.18",
          "form": "저런 독보적 스타일이군요",
          "original_form": "저런 독보적 스타일이군요",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.19",
          "form": "한번 도전해봐야겠어요",
          "original_form": "한번 도전해봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020497.1.20",
          "form": "친구추천이 자담인지 고담인지 헷갈렸는데",
          "original_form": "친구추천이 자담인지 고담인지 헷갈렸는데",
          "speaker_id": "2",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020497.1.21",
          "form": "어디꺼 좋아하세요",
          "original_form": "어디꺼 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020497.1.22",
          "form": "자담이었가봐요",
          "original_form": "자담이었가봐요",
          "speaker_id": "2",
          "time": "20210828 08:30"
        }
      ]
