      "utterance": [
        {
          "id": "MDRW2100025603.1.1",
          "form": "뀨는 사실 매체로 넘어오고서ㅇ알았는더ㅗ",
          "original_form": "뀨는 사실 매체로 넘어오고서ㅇ알았는더ㅗ",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025603.1.2",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025603.1.3",
          "form": "완전내서타일",
          "original_form": "완전내서타일",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025603.1.4",
          "form": "러블리함",
          "original_form": "러블리함",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025603.1.5",
          "form": "미도링은 생각보다 늦게넘어왔고 ..",
          "original_form": "미도링은 생각보다 늦게넘어왔고 ..",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.6",
          "form": "러블리함하니깐",
          "original_form": "러블리함하니깐",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.7",
          "form": "소현언니 진짜 러블리하더라",
          "original_form": "소현언니 진짜 러블리하더라",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.8",
          "form": "아이돌이었으면",
          "original_form": "아이돌이었으면",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.9",
          "form": "아 김소현??",
          "original_form": "아 김소현??",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.10",
          "form": "본업존잘에 씹덕미까지",
          "original_form": "본업존잘에 씹덕미까지",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.11",
          "form": "주안마덜",
          "original_form": "주안마덜",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.12",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.13",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.14",
          "form": "마쟈",
          "original_form": "마쟈",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025603.1.15",
          "form": "놀토 나와서 봤는데 진짜 인간 러블리",
          "original_form": "놀토 나와서 봤는데 진짜 인간 러블리",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.16",
          "form": "아직도 그나이에 러블리한",
          "original_form": "아직도 그나이에 러블리한",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.17",
          "form": "우리엄마는 전미도가 뮤배라는걸 아직도 안믿어",
          "original_form": "우리엄마는 전미도가 뮤배라는걸 아직도 안믿어",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.18",
          "form": "ㅠㅠ 정말 내최애",
          "original_form": "ㅠㅠ 정말 내최애",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.19",
          "form": "슬의로 인한 음치ㅈ이미지",
          "original_form": "슬의로 인한 음치ㅈ이미지",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.20",
          "form": "ㅋㄱㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 송화캐릭터가",
          "original_form": "ㅋㄱㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 송화캐릭터가",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.21",
          "form": "뮤배라고????",
          "original_form": "뮤배라고????",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.22",
          "form": "싶긴하지",
          "original_form": "싶긴하지",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ음치 연기 너무 잘해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ음치 연기 너무 잘해",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.24",
          "form": "노래못하는것도잘해",
          "original_form": "노래못하는것도잘해",
          "speaker_id": "1",
          "time": "20210901 14:16"
