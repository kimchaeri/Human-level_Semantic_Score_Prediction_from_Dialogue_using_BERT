        {
          "id": "MDRW2100025603.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025603.1.26",
          "form": "소현배우 별명이 여신님",
          "original_form": "소현배우 별명이 여신님",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025603.1.27",
          "form": "제 지킬본진 류김김",
          "original_form": "제 지킬본진 류김김",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025603.1.28",
          "form": "그럴만해ㅜㅜㅜㅜ존예에 본업 존잘에 러블리",
          "original_form": "그럴만해ㅜㅜㅜㅜ존예에 본업 존잘에 러블리",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025603.1.29",
          "form": "류님여신여왕 트리플",
          "original_form": "류님여신여왕 트리플",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025603.1.30",
          "form": "한명의 김은 또 누구야",
          "original_form": "한명의 김은 또 누구야",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
