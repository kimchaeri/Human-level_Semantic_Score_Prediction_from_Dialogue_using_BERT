      "utterance": [
        {
          "id": "MDRW2100026447.1.1",
          "form": "아... 맞아요",
          "original_form": "아... 맞아요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026447.1.2",
          "form": "그러려니 합니다",
          "original_form": "그러려니 합니다",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026447.1.3",
          "form": "일하는 꿈꾸면 아침부터 기분이...ㅎㅎ",
          "original_form": "일하는 꿈꾸면 아침부터 기분이...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026447.1.4",
          "form": "저희회사는 최근에 상무님이 그만두셨는데",
          "original_form": "저희회사는 최근에 상무님이 그만두셨는데",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026447.1.5",
          "form": "그분이 상무인데도 일을 진짜 많이 하셨거든요",
          "original_form": "그분이 상무인데도 일을 진짜 많이 하셨거든요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026447.1.6",
          "form": "헉 사람 한 사람 빠지면 더 안바빠 지나요",
          "original_form": "헉 사람 한 사람 빠지면 더 안바빠 지나요",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026447.1.7",
          "form": "그분이 일을 더 안가져오는거에",
          "original_form": "그분이 일을 더 안가져오는거에",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026447.1.8",
          "form": "만족해야.....",
          "original_form": "만족해야.....",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026447.1.9",
          "form": "그만두셨으니...",
          "original_form": "그만두셨으니...",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026447.1.10",
          "form": "상무가 그만두고 대리가 한명 왔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "상무가 그만두고 대리가 한명 왔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026447.1.11",
          "form": "지시할사람 대신 부려먹을사람 추가",
          "original_form": "지시할사람 대신 부려먹을사람 추가",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026447.1.12",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026447.1.13",
          "form": "그래도 다행이네요ㅋㅋㅋㅋ",
          "original_form": "그래도 다행이네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026447.1.14",
          "form": "저희는 이번에 사람이 많이 빠져서",
          "original_form": "저희는 이번에 사람이 많이 빠져서",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026447.1.15",
          "form": "일이 많이 늘었는데..ㅠㅠ",
          "original_form": "일이 많이 늘었는데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026447.1.16",
          "form": "사람이 왜 많이 빠졌나요?",
          "original_form": "사람이 왜 많이 빠졌나요?",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026447.1.17",
          "form": "다들 지쳐서 그만둔?",
          "original_form": "다들 지쳐서 그만둔?",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026447.1.18",
          "form": "업무가 많다 보니 다들 약간 치를 떨고 나가더라구요...",
          "original_form": "업무가 많다 보니 다들 약간 치를 떨고 나가더라구요...",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026447.1.19",
          "form": "전 진짜 같이 일하는 분들이 별로 였음 저도 이미 나갔어요..",
          "original_form": "전 진짜 같이 일하는 분들이 별로 였음 저도 이미 나갔어요..",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.20",
          "form": "우리회사랑 똑같네....",
          "original_form": "우리회사랑 똑같네....",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.21",
          "form": "하..ㅎㅎ 슬프네요",
          "original_form": "하..ㅎㅎ 슬프네요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.22",
          "form": "님은 어떻게 1년이나 버티셨어요?",
          "original_form": "님은 어떻게 1년이나 버티셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.23",
          "form": "그냥 죽었다 생각하고 버티니까",
          "original_form": "그냥 죽었다 생각하고 버티니까",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.24",
          "form": "일년이 됐네요",
          "original_form": "일년이 됐네요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026447.1.25",
          "form": "원래 생각없이 해야 오래 한다고..",
          "original_form": "원래 생각없이 해야 오래 한다고..",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026447.1.26",
          "form": "밥은 드시고 야근하세요?",
          "original_form": "밥은 드시고 야근하세요?",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026447.1.27",
          "form": "밥은 집에 와서 먹어요",
          "original_form": "밥은 집에 와서 먹어요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026447.1.28",
          "form": "따로 회사에서 먹진 않아요",
          "original_form": "따로 회사에서 먹진 않아요",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026447.1.29",
          "form": "일단 일초라도 빨리 회사에서 벗어나야. ..",
          "original_form": "일단 일초라도 빨리 회사에서 벗어나야. ..",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026447.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026447.1.31",
          "form": "야근 3시간이상해야 야근수당준다고",
          "original_form": "야근 3시간이상해야 야근수당준다고",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026447.1.32",
          "form": "어설프게 야근할거면 퇴근하라는데",
          "original_form": "어설프게 야근할거면 퇴근하라는데",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026447.1.33",
          "form": "와... 그걸 어떻게 또 챠우나요",
          "original_form": "와... 그걸 어떻게 또 챠우나요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026447.1.34",
          "form": "이런말하는 사장 어떻게 생각하세요?",
          "original_form": "이런말하는 사장 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026447.1.35",
          "form": "최대한 안하는게 낫겠네요",
          "original_form": "최대한 안하는게 낫겠네요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026447.1.36",
          "form": "전 언제나 마음 속에 쟁반 하나를 품고 살아요",
          "original_form": "전 언제나 마음 속에 쟁반 하나를 품고 살아요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026447.1.37",
          "form": "헛소리에 쟁반 노래방 한대",
          "original_form": "헛소리에 쟁반 노래방 한대",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026447.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026447.1.39",
          "form": "저도 같이 일하는사람들이 다들 착하고 좋아서",
          "original_form": "저도 같이 일하는사람들이 다들 착하고 좋아서",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026447.1.40",
          "form": "버티는데",
          "original_form": "버티는데",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026447.1.41",
          "form": "슬슬 못버티고 나갈 각재는 사람들 생기고 있어요",
          "original_form": "슬슬 못버티고 나갈 각재는 사람들 생기고 있어요",
          "speaker_id": "1",
