      "utterance": [
        {
          "id": "MDRW2100032329.1.1",
          "form": "우리 언제 또 여행가냐 ㅠㅠㅠㅠ",
          "original_form": "우리 언제 또 여행가냐 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032329.1.2",
          "form": "그니까....ㅠㅠ",
          "original_form": "그니까....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032329.1.3",
          "form": "우리 마지막으로 간게 전주지?",
          "original_form": "우리 마지막으로 간게 전주지?",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032329.1.4",
          "form": "어 점례씨 피순대국밥 먹고싶다ㅠㅠ",
          "original_form": "어 점례씨 피순대국밥 먹고싶다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032329.1.5",
          "form": "아 진짜 그거 부산에서 분점내면 월천 가능이야",
          "original_form": "아 진짜 그거 부산에서 분점내면 월천 가능이야",
          "speaker_id": "1",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032329.1.6",
          "form": "안그래도 국밥의 도시애 이만한 국밥집 대박이지",
          "original_form": "안그래도 국밥의 도시애 이만한 국밥집 대박이지",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032329.1.7",
          "form": "콩나물 국밥도 대박이였지",
          "original_form": "콩나물 국밥도 대박이였지",
          "speaker_id": "1",
          "time": "20210905 15:44"
        },
        {
          "id": "MDRW2100032329.1.8",
          "form": "대박이지 나 오징어 안넣을라고 했는데",
          "original_form": "대박이지 나 오징어 안넣을라고 했는데",
          "speaker_id": "2",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032329.1.9",
          "form": "니가 넣자했잖아 진짜 신의한수 였다",
          "original_form": "니가 넣자했잖아 진짜 신의한수 였다",
          "speaker_id": "2",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032329.1.10",
          "form": "진짜...또가고싶다",
          "original_form": "진짜...또가고싶다",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032329.1.11",
          "form": "오징어 그득하게 수란하고 먹고싶다",
          "original_form": "오징어 그득하게 수란하고 먹고싶다",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032329.1.12",
          "form": "전주는 진짜 뭘먹어도 맛있었어",
          "original_form": "전주는 진짜 뭘먹어도 맛있었어",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032329.1.13",
          "form": "물짜장도 좋았어 묘한 중독성이 있어",
          "original_form": "물짜장도 좋았어 묘한 중독성이 있어",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032329.1.14",
          "form": "아맞다 물짜장 나는 근데 저번에 갔던곳이 더 좋았음",
          "original_form": "아맞다 물짜장 나는 근데 저번에 갔던곳이 더 좋았음",
          "speaker_id": "2",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032329.1.15",
          "form": "나도 거기한번 가보고싶다 안그래도 맛있었는데",
          "original_form": "나도 거기한번 가보고싶다 안그래도 맛있었는데",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032329.1.16",
          "form": "코로나끝나면 또 가자",
          "original_form": "코로나끝나면 또 가자",
          "speaker_id": "2",
          "time": "20210905 15:48"
        }
      ]
