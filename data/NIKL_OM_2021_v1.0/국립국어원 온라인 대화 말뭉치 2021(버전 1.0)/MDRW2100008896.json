      "utterance": [
        {
          "id": "MDRW2100008896.1.1",
          "form": "둘째들이 생존본능인지 다들 튼실하군요 ㅎ",
          "original_form": "둘째들이 생존본능인지 다들 튼실하군요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008896.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.3",
          "form": "둘째가 그래요",
          "original_form": "둘째가 그래요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.4",
          "form": "요새 둘째 다욧중요",
          "original_form": "요새 둘째 다욧중요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.5",
          "form": "다요트요? 몇살인데요?",
          "original_form": "다요트요? 몇살인데요?",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.6",
          "form": "키로 다가겠죠~~",
          "original_form": "키로 다가겠죠~~",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.7",
          "form": "초6요",
          "original_form": "초6요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.8",
          "form": "요새 한달동안 잘 안먹어요 ㅡㅡ",
          "original_form": "요새 한달동안 잘 안먹어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008896.1.9",
          "form": "독해요",
          "original_form": "독해요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008896.1.10",
          "form": "와 진짜 대단하네요",
          "original_form": "와 진짜 대단하네요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008896.1.11",
          "form": "그렇게 맘먹기쉽지않은데요",
          "original_form": "그렇게 맘먹기쉽지않은데요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008896.1.12",
          "form": "내 할땐 독해요",
          "original_form": "내 할땐 독해요",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008896.1.13",
          "form": "제가 막 먹으라해요",
          "original_form": "제가 막 먹으라해요",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008896.1.14",
          "form": "그럼 더먹기싫어서 안먹을듯요 ㅋ",
          "original_form": "그럼 더먹기싫어서 안먹을듯요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008896.1.15",
          "form": "그런가요 ㅜㅜ",
          "original_form": "그런가요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008896.1.16",
          "form": "엄마맘이 자꾸 먹이고 싶어요",
          "original_form": "엄마맘이 자꾸 먹이고 싶어요",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008896.1.17",
          "form": "안타깝죠 걱정되고",
          "original_form": "안타깝죠 걱정되고",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008896.1.18",
          "form": "다요트안해도 이쁠때인데 사회분위기가 말라야이쁘다는 인식들이 강해져서 안타까워요",
          "original_form": "다요트안해도 이쁠때인데 사회분위기가 말라야이쁘다는 인식들이 강해져서 안타까워요",
          "speaker_id": "1",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008896.1.19",
          "form": "맞아요 무조건 연예인 처럼 말라야 하고",
          "original_form": "맞아요 무조건 연예인 처럼 말라야 하고",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008896.1.20",
          "form": "안타깝고 속상해요",
          "original_form": "안타깝고 속상해요",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008896.1.21",
          "form": "첫째도 밥먹는데 한시간 걸려서 먹어도 다 먹게하거든요",
          "original_form": "첫째도 밥먹는데 한시간 걸려서 먹어도 다 먹게하거든요",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008896.1.22",
          "form": "조금이라도 더 먹이려고요",
          "original_form": "조금이라도 더 먹이려고요",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008896.1.23",
          "form": "아 ㅡㅡ정말 도를 쌓는 기분이죠",
          "original_form": "아 ㅡㅡ정말 도를 쌓는 기분이죠",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008896.1.24",
          "form": "근데 저희애들은 밥먹기전과 후가 너무달라요 ㅎ",
          "original_form": "근데 저희애들은 밥먹기전과 후가 너무달라요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008896.1.25",
          "form": "밥먹기전에는 배고픈지 막짜증내고 둘이 싸우고 분위기 안좋은데",
          "original_form": "밥먹기전에는 배고픈지 막짜증내고 둘이 싸우고 분위기 안좋은데",
          "speaker_id": "1",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008896.1.26",
          "form": "밥먹고나면 언제싸웠냐는듯이 둘이 노래부르고춤추고 뛰어다니고 엄청 사이좋게 놀아요 ㅎ",
          "original_form": "밥먹고나면 언제싸웠냐는듯이 둘이 노래부르고춤추고 뛰어다니고 엄청 사이좋게 놀아요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008896.1.27",
          "form": "오 그렇군요 세상에 밥이 문제네요 ㅋㅋ",
          "original_form": "오 그렇군요 세상에 밥이 문제네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008896.1.28",
          "form": "전 밥먹고 올께요 ㅎㅎ",
          "original_form": "전 밥먹고 올께요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008896.1.29",
          "form": "아들이 언제 밥먹냐고 ㅋㅋ",
          "original_form": "아들이 언제 밥먹냐고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008896.1.30",
          "form": "난편이제 왔어요 ㅡㅡ운동가서",
          "original_form": "난편이제 왔어요 ㅡㅡ운동가서",
