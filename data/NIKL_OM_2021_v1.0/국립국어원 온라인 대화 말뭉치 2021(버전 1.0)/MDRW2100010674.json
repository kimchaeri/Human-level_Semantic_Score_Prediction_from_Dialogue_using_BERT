      "utterance": [
        {
          "id": "MDRW2100010674.1.1",
          "form": "곰표 맥주 드셔보셨나요?!",
          "original_form": "곰표 맥주 드셔보셨나요?!",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010674.1.2",
          "form": "씨유 돌아댕겨도 없더라고요",
          "original_form": "씨유 돌아댕겨도 없더라고요",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010674.1.3",
          "form": "마셔봤어요!",
          "original_form": "마셔봤어요!",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010674.1.4",
          "form": "일반 맥주랑 좀 맛이 달랐는데 꽤 맛있었어요",
          "original_form": "일반 맥주랑 좀 맛이 달랐는데 꽤 맛있었어요",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010674.1.5",
          "form": "오오오오 승자시군요",
          "original_form": "오오오오 승자시군요",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010674.1.6",
          "form": "요즘 콜라보 많은데 젤 좋았던 콜라보 있으신가요?",
          "original_form": "요즘 콜라보 많은데 젤 좋았던 콜라보 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010674.1.7",
          "form": "저는 메로나랑 모나미가 제일 귀여웠고 그 최근에 플립이랑 톰브라운 콜라보한게 이쁘더라고요ㅠ",
          "original_form": "저는 메로나랑 모나미가 제일 귀여웠고 그 최근에 플립이랑 톰브라운 콜라보한게 이쁘더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010674.1.8",
          "form": "플립 진짜 예쁘더라고요",
          "original_form": "플립 진짜 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010674.1.9",
          "form": "교촌치킨이란 뚜레쥬르랑 콜라보 해서 나온 고로케가 있는데",
          "original_form": "교촌치킨이란 뚜레쥬르랑 콜라보 해서 나온 고로케가 있는데",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010674.1.10",
          "form": "진짜 맛있게 먹었어요",
          "original_form": "진짜 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010674.1.11",
          "form": "헉 맞아요 그거 극찬이었어요",
          "original_form": "헉 맞아요 그거 극찬이었어요",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.12",
          "form": "고로케!!!",
          "original_form": "고로케!!!",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.13",
          "form": "그래서 파바도 닭 고로케 냈는데 뚜쥬보다는 핫하지 않더라고요",
          "original_form": "그래서 파바도 닭 고로케 냈는데 뚜쥬보다는 핫하지 않더라고요",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.14",
          "form": "파바 고로케도 맛있어요",
          "original_form": "파바 고로케도 맛있어요",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.15",
          "form": "흑흑 플립 너무 갖고싶었어요",
          "original_form": "흑흑 플립 너무 갖고싶었어요",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.16",
          "form": "요즘 콜라보 제품이 진짜 많이 나오네요",
          "original_form": "요즘 콜라보 제품이 진짜 많이 나오네요",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.17",
          "form": "근데 그",
          "original_form": "근데 그",
          "speaker_id": "1",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100010674.1.18",
          "form": "문제로 삼는게 그거라면서요 그 먹을거랑 먹지 못하는 거 콜라보할 때",
          "original_form": "문제로 삼는게 그거라면서요 그 먹을거랑 먹지 못하는 거 콜라보할 때",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.19",
          "form": "디자인을 먹을 수 있는것처럼한다고",
          "original_form": "디자인을 먹을 수 있는것처럼한다고",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.20",
          "form": "그거 문제점으로 말씀하시는 분들이 많더라고요",
          "original_form": "그거 문제점으로 말씀하시는 분들이 많더라고요",
          "speaker_id": "1",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.21",
          "form": "아 맞아요 딱풀이나 유성매직 이런 거",
          "original_form": "아 맞아요 딱풀이나 유성매직 이런 거",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.22",
          "form": "근데 그건 잔짜 위험한 거 같아요",
          "original_form": "근데 그건 잔짜 위험한 거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.23",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210823 20:37"
        },
        {
          "id": "MDRW2100010674.1.24",
          "form": "맞아요 글고 유성매직은 그 음료 디자인을 왜 글케했는지..",
          "original_form": "맞아요 글고 유성매직은 그 음료 디자인을 왜 글케했는지..",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010674.1.25",
          "form": "제일 많이 지적 받았던게",
          "original_form": "제일 많이 지적 받았던게",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010674.1.26",
          "form": "그 서울우유였던거같고",
          "original_form": "그 서울우유였던거같고",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010674.1.27",
          "form": "저는 그 구두약 모양 통에 사탕든게 제일 이상했던 것 같아요",
          "original_form": "저는 그 구두약 모양 통에 사탕든게 제일 이상했던 것 같아요",
          "speaker_id": "1",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010674.1.28",
          "form": "근데 진로 두꺼비처럼 캐릭터 콜라보는 좋아해요",
          "original_form": "근데 진로 두꺼비처럼 캐릭터 콜라보는 좋아해요",
          "speaker_id": "1",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010674.1.29",
          "form": "캐릭터 콜라보는 좋은 거 같아요",
          "original_form": "캐릭터 콜라보는 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010674.1.30",
          "form": "진로 요즘에 굿즈팔던데 너무 사고싶더라고요",
          "original_form": "진로 요즘에 굿즈팔던데 너무 사고싶더라고요",
          "speaker_id": "1",
          "time": "20210823 20:40"
        }
      ]
