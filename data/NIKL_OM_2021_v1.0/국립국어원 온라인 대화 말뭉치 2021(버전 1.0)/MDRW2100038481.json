        {
          "id": "MDRW2100038481.1.1",
          "form": "콘서트 가보고 싶어요",
          "original_form": "콘서트 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038481.1.2",
          "form": "저도요 방탄",
          "original_form": "저도요 방탄",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100038481.1.3",
          "form": "헛 저도 방탄 콘서트 가보고 싶어요ㅋㅋ",
          "original_form": "헛 저도 방탄 콘서트 가보고 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100038481.1.4",
          "form": "표구하기가 힘들대요",
          "original_form": "표구하기가 힘들대요",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038481.1.5",
          "form": "티켓팅 1초컷 당했었어요",
          "original_form": "티켓팅 1초컷 당했었어요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038481.1.6",
          "form": "17년 윙파콘땤ㅋ",
          "original_form": "17년 윙파콘땤ㅋ",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038481.1.7",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038481.1.8",
          "form": "요즘은 온라인으로 공연봐요",
          "original_form": "요즘은 온라인으로 공연봐요",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038481.1.9",
          "form": "맞아요 티켓값만 내도 볼수 있죠",
          "original_form": "맞아요 티켓값만 내도 볼수 있죠",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038481.1.10",
          "form": "그게 좋은거 같아요",
          "original_form": "그게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038481.1.11",
          "form": "방에서 혼자 열광하기",
          "original_form": "방에서 혼자 열광하기",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038481.1.12",
          "form": "응원봉 흔들면서 응원법 외치고 울고",
          "original_form": "응원봉 흔들면서 응원법 외치고 울고",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038481.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038481.1.14",
          "form": "이 나이도 그러네요",
          "original_form": "이 나이도 그러네요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038481.1.15",
          "form": "좋아한다면 모두가 그럴 수 있죠",
          "original_form": "좋아한다면 모두가 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
