      "utterance": [
        {
          "id": "MDRW2100041638.1.1",
          "form": "좋아하는 빵 있으세요?",
          "original_form": "좋아하는 빵 있으세요?",
          "speaker_id": "1",
          "time": "20210906 13:40"
        },
        {
          "id": "MDRW2100041638.1.2",
          "form": "좋아하는 빵 있으세유?",
          "original_form": "좋아하는 빵 있으세유?",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100041638.1.3",
          "form": "저는 애플파이 좋아해요!",
          "original_form": "저는 애플파이 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100041638.1.4",
          "form": "우와 맥도날드 애플파이 좋아하시나요?",
          "original_form": "우와 맥도날드 애플파이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041638.1.5",
          "form": "아, 고것도 좋아하는데 빵집의 애플파이가 더 취향이에요",
          "original_form": "아, 고것도 좋아하는데 빵집의 애플파이가 더 취향이에요",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041638.1.6",
          "form": "저도 빵집거가 더 좋아요 ㅎㅎ 애플파이 부스러기 많이날리는거만 빼고 좋아요!",
          "original_form": "저도 빵집거가 더 좋아요 ㅎㅎ 애플파이 부스러기 많이날리는거만 빼고 좋아요!",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041638.1.7",
          "form": "맞아요, 부스러기가 좀 날리죠ㅋㅋㅋ",
          "original_form": "맞아요, 부스러기가 좀 날리죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041638.1.8",
          "form": "갓 만든 뜨거운 파이 먹어보고싶어요",
          "original_form": "갓 만든 뜨거운 파이 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041638.1.9",
          "form": "핳...따끈따끈하면 더 맛있겠죠?!",
          "original_form": "핳...따끈따끈하면 더 맛있겠죠?!",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041638.1.10",
          "form": "ㅎㅎㅎ 전 호두파이 좋아해요",
          "original_form": "ㅎㅎㅎ 전 호두파이 좋아해요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100041638.1.11",
          "form": "호두파이도 맛있죠ㅎㅎㅎ",
          "original_form": "호두파이도 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100041638.1.12",
          "form": "케이크 대신 호두파이로 파티하고 그랫어요",
          "original_form": "케이크 대신 호두파이로 파티하고 그랫어요",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100041638.1.13",
          "form": "진짜 좋아하시나봐요ㅋㅋㅋ",
          "original_form": "진짜 좋아하시나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041638.1.14",
          "form": "가격대비 ㅎㅎㅎ 맛잇는거같아요",
          "original_form": "가격대비 ㅎㅎㅎ 맛잇는거같아요",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041638.1.15",
          "form": "저는 파이도 좋지만 타르트를 더 좋아해요ㅎㅎ",
          "original_form": "저는 파이도 좋지만 타르트를 더 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041638.1.16",
          "form": "아 타르트 진짜 좋아요",
          "original_form": "아 타르트 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041638.1.17",
          "form": "생과일타르트 좋더라구요",
