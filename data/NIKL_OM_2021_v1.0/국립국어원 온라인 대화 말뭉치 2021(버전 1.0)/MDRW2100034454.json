      "utterance": [
        {
          "id": "MDRW2100034454.1.1",
          "form": "데뷔곡 모든 잠든후에도 자기가 참여했네",
          "original_form": "데뷔곡 모든 잠든후에도 자기가 참여했네",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100034454.1.2",
          "form": "아 맞나?  전혀 몰랐네",
          "original_form": "아 맞나?  전혀 몰랐네",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100034454.1.3",
          "form": "9위 양준일 가나다라마바사 ㅋㅋ제목 웃기다",
          "original_form": "9위 양준일 가나다라마바사 ㅋㅋ제목 웃기다",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100034454.1.4",
          "form": "아 양준일ㅋ",
          "original_form": "아 양준일ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100034454.1.5",
          "form": "나도 전혀 몰랐다 김원준이 작사작곡하는지",
          "original_form": "나도 전혀 몰랐다 김원준이 작사작곡하는지",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100034454.1.6",
          "form": "양준일 저때 되게 트랜드 하지 않나? 너무 앞서 나갔다",
          "original_form": "양준일 저때 되게 트랜드 하지 않나? 너무 앞서 나갔다",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100034454.1.7",
          "form": "작사작곡을 했네 양준일도",
          "original_form": "작사작곡을 했네 양준일도",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100034454.1.8",
          "form": "어 자기가 하드라",
          "original_form": "어 자기가 하드라",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100034454.1.9",
          "form": "그렇게 너무 많이 앞서 간거 같지는 않은데",
          "original_form": "그렇게 너무 많이 앞서 간거 같지는 않은데",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100034454.1.10",
          "form": "그런가?  난 저때 양준일을 전혀 몰랐다",
          "original_form": "그런가?  난 저때 양준일을 전혀 몰랐다",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100034454.1.11",
          "form": "나두 슈가 맞나 거기서 나와서 알았지",
          "original_form": "나두 슈가 맞나 거기서 나와서 알았지",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100034454.1.12",
          "form": "슈가맨이지",
          "original_form": "슈가맨이지",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100034454.1.13",
          "form": "응 이제 또 잘 안나오시더라",
          "original_form": "응 이제 또 잘 안나오시더라",
