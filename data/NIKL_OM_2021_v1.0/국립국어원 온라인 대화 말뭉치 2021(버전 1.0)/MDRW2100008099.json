      "utterance": [
        {
          "id": "MDRW2100008099.1.1",
          "form": "혹시 영화 좋아하세요?",
          "original_form": "혹시 영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008099.1.2",
          "form": "전 영화나 드라마는 잘 안봐요",
          "original_form": "전 영화나 드라마는 잘 안봐요",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008099.1.3",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008099.1.4",
          "form": "네 저도 드라마는 잘 안보는데 영화는 좋아해요",
          "original_form": "네 저도 드라마는 잘 안보는데 영화는 좋아해요",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008099.1.5",
          "form": "유튜브로는 뭐보세요?",
          "original_form": "유튜브로는 뭐보세요?",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008099.1.6",
          "form": "예능 클립이요",
          "original_form": "예능 클립이요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008099.1.7",
          "form": "편하게 볼 수 있는게 좋더라고요",
          "original_form": "편하게 볼 수 있는게 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008099.1.8",
          "form": "전 지금도 무한도전 다시보기보고있는데",
          "original_form": "전 지금도 무한도전 다시보기보고있는데",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008099.1.9",
          "form": "무한도전 좋아하세요?",
          "original_form": "무한도전 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008099.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008099.1.11",
          "form": "무한도전 뭔가 심심할 때보면 후딱 지나가는 것 같아요",
          "original_form": "무한도전 뭔가 심심할 때보면 후딱 지나가는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008099.1.12",
          "form": "어떤 멤버 제일 좋아하시나요?",
          "original_form": "어떤 멤버 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008099.1.13",
          "form": "저는 정준하가 좋더라구요",
          "original_form": "저는 정준하가 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008099.1.14",
          "form": "어떤분 좋아하세요?",
          "original_form": "어떤분 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008099.1.15",
          "form": "저는 하하나 정형돈이요",
          "original_form": "저는 하하나 정형돈이요",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008099.1.16",
          "form": "혹시 넷플릭스 같은걸로 보시는거 있으세요?",
          "original_form": "혹시 넷플릭스 같은걸로 보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008099.1.17",
          "form": "저는 주로 다큐멘터리봐요",
          "original_form": "저는 주로 다큐멘터리봐요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008099.1.18",
          "form": "지인님은요?",
          "original_form": "지인님은요?",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008099.1.19",
          "form": "저는 영화나 애니메이션 자주봐요",
          "original_form": "저는 영화나 애니메이션 자주봐요",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008099.1.20",
          "form": "다큐멘터리 어떤내용 좋아하세요?",
          "original_form": "다큐멘터리 어떤내용 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008099.1.21",
          "form": "저는 주로 범죄사건 추리하는거요",
          "original_form": "저는 주로 범죄사건 추리하는거요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008099.1.22",
          "form": "뭔가 추리하는 과정이 재밌어요",
          "original_form": "뭔가 추리하는 과정이 재밌어요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008099.1.23",
          "form": "재밌게 본 애니메이션 있으신가요?",
          "original_form": "재밌게 본 애니메이션 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008099.1.24",
          "form": "저는 너의이름은이 좋았어요",
          "original_form": "저는 너의이름은이 좋았어요",
          "speaker_id": "1",
          "time": "20210822 13:35"
