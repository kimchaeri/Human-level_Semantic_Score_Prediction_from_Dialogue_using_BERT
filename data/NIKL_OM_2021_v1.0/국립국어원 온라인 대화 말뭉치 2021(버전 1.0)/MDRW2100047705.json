      "utterance": [
        {
          "id": "MDRW2100047705.1.1",
          "form": "좋아하는 음식이 무언가요",
          "original_form": "좋아하는 음식이 무언가요",
          "speaker_id": "1",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100047705.1.2",
          "form": "가리지 않아요",
          "original_form": "가리지 않아요",
          "speaker_id": "2",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100047705.1.3",
          "form": "고기요 ㅎㅎ",
          "original_form": "고기요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100047705.1.4",
          "form": "뭐 좋아 하세요?",
          "original_form": "뭐 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100047705.1.5",
          "form": "전 비지장 청국장 된장 장류를 좋아해요",
          "original_form": "전 비지장 청국장 된장 장류를 좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100047705.1.6",
          "form": "아 저도 좋아하는데 요새 안해 먹었네요",
          "original_form": "아 저도 좋아하는데 요새 안해 먹었네요",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100047705.1.7",
          "form": "주말에 좀 먹어봐야겠어요",
          "original_form": "주말에 좀 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100047705.1.8",
          "form": "첫째가 청국장 엄청 좋아해요",
          "original_form": "첫째가 청국장 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100047705.1.9",
          "form": "저도 오램만에 비지장 끓여 먹어야 겠어요",
          "original_form": "저도 오램만에 비지장 끓여 먹어야 겠어요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100047705.1.10",
          "form": "네 맛나게 만들어 드세요 ㅎㅎ",
          "original_form": "네 맛나게 만들어 드세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100047705.1.11",
          "form": "전 사실 비지장은 안해봤어요",
          "original_form": "전 사실 비지장은 안해봤어요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100047705.1.12",
          "form": "사먹어는 봤어요 ㅎㅎ",
          "original_form": "사먹어는 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100047705.1.13",
          "form": "어떻게 하세요? 나만의 레시피 가 있나요?",
          "original_form": "어떻게 하세요? 나만의 레시피 가 있나요?",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100047705.1.14",
          "form": "채썬 소고기 기름에 볶다가 물넣고 깍두기썰어 놓고 끓여요",
          "original_form": "채썬 소고기 기름에 볶다가 물넣고 깍두기썰어 놓고 끓여요",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100047705.1.15",
          "form": "아 멎아요",
          "original_form": "아 멎아요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100047705.1.16",
          "form": "깍두기",
          "original_form": "깍두기",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100047705.1.17",
          "form": "소고기 기름이 비법 이군요",
          "original_form": "소고기 기름이 비법 이군요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100047705.1.18",
          "form": "뭔가 실력자의 기운이 느껴져요 ㅎㅎ",
          "original_form": "뭔가 실력자의 기운이 느껴져요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100047705.1.19",
          "form": "알려 주셔서 감사해요 ㅎㅎ",
          "original_form": "알려 주셔서 감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100047705.1.20",
          "form": "아니 김장때 담근 알타리를 썰어 넣으면 맛있어요",
          "original_form": "아니 김장때 담근 알타리를 썰어 넣으면 맛있어요",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100047705.1.21",
          "form": "오 있어요",
          "original_form": "오 있어요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100047705.1.22",
          "form": "저 까먹고 있었네요",
          "original_form": "저 까먹고 있었네요",
