      "utterance": [
        {
          "id": "MDRW2100028494.1.1",
          "form": "고양이 아직도 자니",
          "original_form": "고양이 아직도 자니",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028494.1.2",
          "form": "모르겠어 아까 밥 먹으러 간 뒤로 소식이 없는",
          "original_form": "모르겠어 아까 밥 먹으러 간 뒤로 소식이 없는",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028494.1.3",
          "form": "오늘 병원은 관리 받으려고 간 거?",
          "original_form": "오늘 병원은 관리 받으려고 간 거?",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028494.1.4",
          "form": "사상충 약 바르러",
          "original_form": "사상충 약 바르러",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028494.1.5",
          "form": "한달에 한번 발라야 하는 거라",
          "original_form": "한달에 한번 발라야 하는 거라",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028494.1.6",
          "form": "근데 간 김에 코가 너무 더러워용 했더니",
          "original_form": "근데 간 김에 코가 너무 더러워용 했더니",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.7",
          "form": "닦아주심",
          "original_form": "닦아주심",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.9",
          "form": "코가 너무 더러워용",
          "original_form": "코가 너무 더러워용",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.10",
          "form": "집에선 눈꼽 눈물도 걍 닦아주는데",
          "original_form": "집에선 눈꼽 눈물도 걍 닦아주는데",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.11",
          "form": "그것도 식염수로 닦아주시고",
          "original_form": "그것도 식염수로 닦아주시고",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.12",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028494.1.13",
          "form": "코딱지도 빼고 하여간 관리받고 온",
          "original_form": "코딱지도 빼고 하여간 관리받고 온",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028494.1.14",
          "form": "고앵... 건강해지고 있어서 다행이야",
          "original_form": "고앵... 건강해지고 있어서 다행이야",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028494.1.15",
          "form": "고앵 건강하고 행복해...",
          "original_form": "고앵 건강하고 행복해...",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.16",
          "form": "엄청 건강해서",
          "original_form": "엄청 건강해서",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.17",
          "form": "일주일에 100g씩 찜",
          "original_form": "일주일에 100g씩 찜",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.18",
          "form": "의사선생님이 갈 때마다 좀 놀라심",
          "original_form": "의사선생님이 갈 때마다 좀 놀라심",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.19",
          "form": "너무 잘 큰다고",
          "original_form": "너무 잘 큰다고",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.20",
          "form": "ㅠㅠㅠㅠ 건강고영 해피고영",
          "original_form": "ㅠㅠㅠㅠ 건강고영 해피고영",
          "speaker_id": "2",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028494.1.21",
          "form": "대견해",
          "original_form": "대견해",
          "speaker_id": "2",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028494.1.22",
          "form": "잘 커서 다행이야...",
          "original_form": "잘 커서 다행이야...",
          "speaker_id": "1",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028494.1.23",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028494.1.24",
          "form": "원래 그래야 잔병치레도 덜 하지...",
          "original_form": "원래 그래야 잔병치레도 덜 하지...",
          "speaker_id": "1",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028494.1.25",
          "form": "기특해죽겟서",
          "original_form": "기특해죽겟서",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028494.1.26",
          "form": "내가 키우는 것도 아님서",
          "original_form": "내가 키우는 것도 아님서",
          "speaker_id": "2",
          "time": "20210902 20:33"
        }
      ]
    }
