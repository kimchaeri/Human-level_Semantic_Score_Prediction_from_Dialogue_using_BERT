      "utterance": [
        {
          "id": "MDRW2100038140.1.1",
          "form": "전시자주보시나요?",
          "original_form": "전시자주보시나요?",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038140.1.2",
          "form": "요샌 잘못봐유ㅜㅜ",
          "original_form": "요샌 잘못봐유ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100038140.1.3",
          "form": "ㅜㅜ저도.",
          "original_form": "ㅜㅜ저도.",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038140.1.4",
          "form": "전시회도요?",
          "original_form": "전시회도요?",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038140.1.5",
          "form": "지금 예술의전당에서 하는 피카소전이 마지막이네요",
          "original_form": "지금 예술의전당에서 하는 피카소전이 마지막이네요",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038140.1.6",
          "form": "피카소전..",
          "original_form": "피카소전..",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038140.1.7",
          "form": "피카소는 너무 어려워요..'",
          "original_form": "피카소는 너무 어려워요..'",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038140.1.8",
          "form": "저도 그분의 머릿속을 모르겠슈",
          "original_form": "저도 그분의 머릿속을 모르겠슈",
          "speaker_id": "2",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100038140.1.9",
          "form": "돈많은 천재의 머리를 어찌알겠어요ㅋㅋ",
          "original_form": "돈많은 천재의 머리를 어찌알겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100038140.1.10",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100038140.1.11",
          "form": "2차세계대전때 전쟁안나가고 여행댕기며 어린여자 만나고 그림그리던 천재",
          "original_form": "2차세계대전때 전쟁안나가고 여행댕기며 어린여자 만나고 그림그리던 천재",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038140.1.12",
          "form": "돈도 많고..",
          "original_form": "돈도 많고..",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038140.1.13",
          "form": "엄청 많았고",
          "original_form": "엄청 많았고",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038140.1.14",
          "form": "ㅋㅋ  마정",
          "original_form": "ㅋㅋ  마정",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038140.1.15",
          "form": "욕심도 많았고",
          "original_form": "욕심도 많았고",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038140.1.16",
          "form": "주변에 괜찮은 작가 있으면",
          "original_form": "주변에 괜찮은 작가 있으면",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038140.1.17",
          "form": "모방하는데 교묘하게 본인의 생각을 넣어서",
          "original_form": "모방하는데 교묘하게 본인의 생각을 넣어서",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038140.1.18",
          "form": "여자만나는 사람에따라 작풍바뀌드라",
          "original_form": "여자만나는 사람에따라 작풍바뀌드라",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038140.1.19",
          "form": "대박치는 현존하면서 돈 많이번 화가..",
          "original_form": "대박치는 현존하면서 돈 많이번 화가..",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038140.1.20",
          "form": "나쁜 사람임 ㅎ",
          "original_form": "나쁜 사람임 ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038140.1.21",
          "form": "자기 그림 글케 고가인데 남도 좀 돕지",
          "original_form": "자기 그림 글케 고가인데 남도 좀 돕지",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100038140.1.22",
          "form": "알폰스무하는 민족위해 그림그리다 나치에게",
          "original_form": "알폰스무하는 민족위해 그림그리다 나치에게",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100038140.1.23",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100038140.1.24",
          "form": "고문받고 후유증으로 돌아가심",
          "original_form": "고문받고 후유증으로 돌아가심",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038140.1.25",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038140.1.26",
          "form": "너 돈많으니 남 도와!",
          "original_form": "너 돈많으니 남 도와!",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038140.1.27",
          "form": "이건 강요죠",
          "original_form": "이건 강요죠",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038140.1.28",
          "form": "그냥 인성별로인 화가로 합시다",
          "original_form": "그냥 인성별로인 화가로 합시다",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038140.1.29",
          "form": "그래 뭐",
          "original_form": "그래 뭐",
          "speaker_id": "2",
          "time": "20210902 09:33"
        }
      ]
    }
