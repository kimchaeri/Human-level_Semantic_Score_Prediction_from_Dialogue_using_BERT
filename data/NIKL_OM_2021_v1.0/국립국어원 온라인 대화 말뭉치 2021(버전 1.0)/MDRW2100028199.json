      "utterance": [
        {
          "id": "MDRW2100028199.1.1",
          "form": "저는 ASMR이랑 고양이, 요리레시피 많이 찾아봐요",
          "original_form": "저는 ASMR이랑 고양이, 요리레시피 많이 찾아봐요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028199.1.2",
          "form": "오오 저도 고양이 많이봐요",
          "original_form": "오오 저도 고양이 많이봐요",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028199.1.3",
          "form": "음식 브이로그 여행 게임 이렇게 봐요",
          "original_form": "음식 브이로그 여행 게임 이렇게 봐요",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028199.1.4",
          "form": "haha ha님 채널 제일 열심히 봅니다",
          "original_form": "haha ha님 채널 제일 열심히 봅니다",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028199.1.5",
          "form": "그분껀 연님이 부분만 봤어요 ㅋㅋㅋ",
          "original_form": "그분껀 연님이 부분만 봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028199.1.6",
          "form": "저는 매탈남님꺼",
          "original_form": "저는 매탈남님꺼",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028199.1.7",
          "form": "오늘  축구한데요",
          "original_form": "오늘  축구한데요",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028199.1.8",
          "form": "오 어디랑 어디요?",
          "original_form": "오 어디랑 어디요?",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028199.1.9",
          "form": "한국 이라크 인가봐요",
          "original_form": "한국 이라크 인가봐요",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028199.1.10",
          "form": "치킨먹고싶어져요",
          "original_form": "치킨먹고싶어져요",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028199.1.11",
          "form": "아 그건 못 참죠",
          "original_form": "아 그건 못 참죠",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028199.1.12",
          "form": "치킨 주문하셔야죠",
          "original_form": "치킨 주문하셔야죠",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028199.1.13",
          "form": "목요일 재밌는거 하는거 있을까요?",
          "original_form": "목요일 재밌는거 하는거 있을까요?",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028199.1.14",
          "form": "저는 TV를 잘 안봐서 잘 모르겠어요",
          "original_form": "저는 TV를 잘 안봐서 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028199.1.15",
          "form": "보통 웨이브나 넷플릭스같은데서 해외드라마 봐요",
          "original_form": "보통 웨이브나 넷플릭스같은데서 해외드라마 봐요",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028199.1.16",
          "form": "시간제약없이 원하는 때에 볼 수 있어서 좋더라고요",
          "original_form": "시간제약없이 원하는 때에 볼 수 있어서 좋더라고요",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028199.1.17",
          "form": "유료 결제 다 하셨나봐요?!",
          "original_form": "유료 결제 다 하셨나봐요?!",
          "speaker_id": "1",
