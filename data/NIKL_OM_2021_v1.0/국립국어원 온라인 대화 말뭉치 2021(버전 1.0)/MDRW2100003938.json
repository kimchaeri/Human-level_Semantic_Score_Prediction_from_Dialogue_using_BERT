      "utterance": [
        {
          "id": "MDRW2100003938.1.1",
          "form": "중고책을팔려고 올려놨는데 팔리지가않네요",
          "original_form": "중고책을팔려고 올려놨는데 팔리지가않네요",
          "speaker_id": "1",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003938.1.2",
          "form": "중고책이요?",
          "original_form": "중고책이요?",
          "speaker_id": "2",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003938.1.3",
          "form": "가격을 좀만 내려보세요",
          "original_form": "가격을 좀만 내려보세요",
          "speaker_id": "2",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003938.1.4",
          "form": "가격은 이미내릴만큼 내린거같은데",
          "original_form": "가격은 이미내릴만큼 내린거같은데",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003938.1.5",
          "form": "인기가없는거같아요",
          "original_form": "인기가없는거같아요",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003938.1.6",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003938.1.7",
          "form": "어떤 책인데요?",
          "original_form": "어떤 책인데요?",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003938.1.8",
          "form": "전공책같은거면 그래도",
          "original_form": "전공책같은거면 그래도",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003938.1.9",
          "form": "인기는 있을텐데",
          "original_form": "인기는 있을텐데",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003938.1.10",
          "form": "전공책은 아직 개강을안해서 안팔리는거같구",
          "original_form": "전공책은 아직 개강을안해서 안팔리는거같구",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003938.1.11",
          "form": "에세이책도올려놨는데",
          "original_form": "에세이책도올려놨는데",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003938.1.12",
          "form": "안팔리네요..",
          "original_form": "안팔리네요..",
          "speaker_id": "1",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003938.1.13",
          "form": "에세이 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "에세이 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003938.1.14",
          "form": "에세이는 인기 없을 것 같아요",
          "original_form": "에세이는 인기 없을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003938.1.15",
          "form": "그거... 재미가 별로 없어서 ㅠㅠ 소설이면 좀 나았으려나",
          "original_form": "그거... 재미가 별로 없어서 ㅠㅠ 소설이면 좀 나았으려나",
          "speaker_id": "2",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003938.1.16",
          "form": "그쵸ㅋㅋㅋ흑흑",
          "original_form": "그쵸ㅋㅋㅋ흑흑",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003938.1.17",
          "form": "게다가 이미 팔려고올려놓은 사람이 너무많아서",
          "original_form": "게다가 이미 팔려고올려놓은 사람이 너무많아서",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003938.1.18",
          "form": "걍 전공서적 팔리길 기다려봐야겠네요..",
          "original_form": "걍 전공서적 팔리길 기다려봐야겠네요..",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003938.1.19",
          "form": "여러개 올려놓으셨나봐요",
          "original_form": "여러개 올려놓으셨나봐요",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003938.1.20",
          "form": "네..돈이없어서",
          "original_form": "네..돈이없어서",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003938.1.21",
          "form": "집에있는책 다 뒤져서 올려놨어요",
          "original_form": "집에있는책 다 뒤져서 올려놨어요",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003938.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003938.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003938.1.24",
          "form": "좀 팔리나요?",
          "original_form": "좀 팔리나요?",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003938.1.25",
          "form": "저도 책은 많은데 몇몇개는 선물받은거고",
          "original_form": "저도 책은 많은데 몇몇개는 선물받은거고",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003938.1.26",
          "form": "종이밥같이 필독도서인데 재미없는 것들 천지라서",
          "original_form": "종이밥같이 필독도서인데 재미없는 것들 천지라서",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003938.1.27",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003938.1.28",
          "form": "아ㅠㅠㅋㅋ 글군요",
          "original_form": "아ㅠㅠㅋㅋ 글군요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003938.1.29",
          "form": "저는 책모임했어서 그때 산 책이 좀 있어요",
          "original_form": "저는 책모임했어서 그때 산 책이 좀 있어요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003938.1.30",
          "form": "근데지금까지 한권팔았네요",
          "original_form": "근데지금까지 한권팔았네요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003938.1.31",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003938.1.32",
          "form": "휴..",
          "original_form": "휴..",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003938.1.33",
          "form": "알라딘에는 올려보셨어요?",
          "original_form": "알라딘에는 올려보셨어요?",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003938.1.34",
          "form": "거기도 중고로 많이 팔던데",
          "original_form": "거기도 중고로 많이 팔던데",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003938.1.35",
          "form": "네네ㅠ 알라딘에 다 올려뒀어요",
          "original_form": "네네ㅠ 알라딘에 다 올려뒀어요",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003938.1.36",
          "form": "전공서적은 에브리타임에 올려놓고",
          "original_form": "전공서적은 에브리타임에 올려놓고",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003938.1.37",
          "form": "인기많은책은 하루만에 팔리더라고요",
          "original_form": "인기많은책은 하루만에 팔리더라고요",
          "speaker_id": "1",
          "time": "20210821 14:19"
        }
      ]
