      "utterance": [
        {
          "id": "MDRW2100037692.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100037692.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100037692.1.3",
          "form": "점심 맛있게 드셨나요?",
          "original_form": "점심 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037692.1.4",
          "form": "네~집에서 참치찌개 끓여먹었네요!",
          "original_form": "네~집에서 참치찌개 끓여먹었네요!",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037692.1.5",
          "form": "오! 밥 말아 먹으면 맛있죠!",
          "original_form": "오! 밥 말아 먹으면 맛있죠!",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100037692.1.6",
          "form": "맞아여!!! 김이랑 같이 맛잇게먹엇어요 ㅎㅎ",
          "original_form": "맞아여!!! 김이랑 같이 맛잇게먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037692.1.7",
          "form": "앜ㅋㅋㅋ 입에 침 고여요 ㅎㅎ",
          "original_form": "앜ㅋㅋㅋ 입에 침 고여요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100037692.1.8",
          "form": "전 근데 ㅋㅋ양조절은 못해서..ㅋㅋㅋ저녁에두 먹어야해요",
          "original_form": "전 근데 ㅋㅋ양조절은 못해서..ㅋㅋㅋ저녁에두 먹어야해요",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100037692.1.9",
          "form": "참치찌개에 라면사리 넣어 먹으면 이상할까요?",
          "original_form": "참치찌개에 라면사리 넣어 먹으면 이상할까요?",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100037692.1.10",
          "form": "갑자기 그렇게 먹어보고싶네요 ㅎㅎ",
          "original_form": "갑자기 그렇게 먹어보고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037692.1.11",
          "form": "맛잇죠!!",
          "original_form": "맛잇죠!!",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037692.1.12",
          "form": "당면사리도좋고요!!",
          "original_form": "당면사리도좋고요!!",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037692.1.13",
          "form": "전 당면이 더좋긴해요 ㅎㅎ",
          "original_form": "전 당면이 더좋긴해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037692.1.14",
          "form": "당면도 맛있겠어요!",
          "original_form": "당면도 맛있겠어요!",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100037692.1.15",
          "form": "집에서 해먹는 음식은 다양하게 조리해볼 수 있어서 괜찮은 것 같아요 ㅎㅎ",
          "original_form": "집에서 해먹는 음식은 다양하게 조리해볼 수 있어서 괜찮은 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100037692.1.16",
          "form": "맞아요~~ 그냥 다 떼려넣기 ㅎㅎ",
          "original_form": "맞아요~~ 그냥 다 떼려넣기 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037692.1.17",
          "form": "소면도 넣어보고싶네요 ㅋㅋㅋㅋ",
          "original_form": "소면도 넣어보고싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037692.1.18",
          "form": "소면 말씀하시니 낙지소면먹고싶네요ㅠㅠ ㅋㅋ",
          "original_form": "소면 말씀하시니 낙지소면먹고싶네요ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037692.1.19",
          "form": "가끔 그렇게 때려 넣었는데 성공적이면 너무 좋죠 ㅋㅋㅋㅋ",
          "original_form": "가끔 그렇게 때려 넣었는데 성공적이면 너무 좋죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037692.1.20",
          "form": "어제 비빔국수먹엇지만",
          "original_form": "어제 비빔국수먹엇지만",
