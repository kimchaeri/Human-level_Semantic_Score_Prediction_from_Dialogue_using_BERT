      "utterance": [
        {
          "id": "MDRW2100035268.1.1",
          "form": "식사 하셨나요~?",
          "original_form": "식사 하셨나요~?",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100035268.1.2",
          "form": "네 방금 라면먹었어요",
          "original_form": "네 방금 라면먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100035268.1.3",
          "form": "만사귀찮네요 ㅋㅋ",
          "original_form": "만사귀찮네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100035268.1.4",
          "form": "ㅋㅋㅋ주말이라 더 그런거 같아요",
          "original_form": "ㅋㅋㅋ주말이라 더 그런거 같아요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100035268.1.5",
          "form": "주말 점심은 거의 면이 국룰인거같아요",
          "original_form": "주말 점심은 거의 면이 국룰인거같아요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100035268.1.6",
          "form": "맞아요 일요일은 짜파게티",
          "original_form": "맞아요 일요일은 짜파게티",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100035268.1.7",
          "form": "맞아요~  그거 진짜 잘지었어요ㅋㅋㅋ",
          "original_form": "맞아요~  그거 진짜 잘지었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100035268.1.8",
          "form": "ㅋㅋㅋ맞아요 저녁은 또 뭘 먹나 귀찮아요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 저녁은 또 뭘 먹나 귀찮아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100035268.1.9",
          "form": "으윽 맞아요ㅋㅋㅋ",
          "original_form": "으윽 맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035268.1.10",
          "form": "엄마의 고충은 이것이었나 싶어요",
          "original_form": "엄마의 고충은 이것이었나 싶어요",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035268.1.11",
          "form": "맞아요 새삼 감사한 마음 ㅠㅠ",
          "original_form": "맞아요 새삼 감사한 마음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035268.1.12",
          "form": "이렇게 감사함도 알아가네요",
          "original_form": "이렇게 감사함도 알아가네요",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035268.1.13",
          "form": "아 6시가 다가오네요",
          "original_form": "아 6시가 다가오네요",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035268.1.14",
          "form": "그러네요 오늘 저녁은 제가 고민해서 해드려야겠어요",
          "original_form": "그러네요 오늘 저녁은 제가 고민해서 해드려야겠어요",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100035268.1.15",
          "form": "오오 제 고민을 덜어주시다니 천사십니닷!",
          "original_form": "오오 제 고민을 덜어주시다니 천사십니닷!",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100035268.1.16",
          "form": "name1님저녁은 제가 고민해드려도 되나요?",
          "original_form": "&name1&님저녁은 제가 고민해드려도 되나요?",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100035268.1.17",
          "form": "네네 메뉴 추천해주세요",
          "original_form": "네네 메뉴 추천해주세요",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100035268.1.18",
          "form": "카레어떠세요!",
          "original_form": "카레어떠세요!",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100035268.1.19",
          "form": "미역국!",
          "original_form": "미역국!",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100035268.1.20",
          "form": "김치볶음밥",
          "original_form": "김치볶음밥",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100035268.1.21",
          "form": "카레! 저도 생각한거에요!",
          "original_form": "카레! 저도 생각한거에요!",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035268.1.22",
          "form": "돈까스!",
          "original_form": "돈까스!",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035268.1.23",
          "form": "김치찌개!",
          "original_form": "김치찌개!",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035268.1.24",
          "form": "오 !! 저는 이미 소고기뭇국을 끓였어요",
          "original_form": "오 !! 저는 이미 소고기뭇국을 끓였어요",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100035268.1.25",
          "form": "뭐로 결정하셨어요",
          "original_form": "뭐로 결정하셨어요",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100035268.1.26",
          "form": "아 늦었군욥ㅜ",
          "original_form": "아 늦었군욥ㅜ",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035268.1.27",
          "form": "볶음밥이요!",
          "original_form": "볶음밥이요!",
          "speaker_id": "2",
          "time": "20210829 19:10"
        }
      ]
    }
