      "utterance": [
        {
          "id": "MDRW2100017856.1.1",
          "form": "무슨 계절 제일 좋아하세요?",
          "original_form": "무슨 계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017856.1.2",
          "form": "저 겨울을 제일 좋아해요",
          "original_form": "저 겨울을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017856.1.3",
          "form": "겨울을 좋아하시는 이유가 있나요?",
          "original_form": "겨울을 좋아하시는 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017856.1.4",
          "form": "왜냐면 외투 껴입으니까 패션 신경 안써도 돼서..",
          "original_form": "왜냐면 외투 껴입으니까 패션 신경 안써도 돼서..",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017856.1.5",
          "form": "굉장히 실용적인 이유네요 ㅋㅋㅋㅋ",
          "original_form": "굉장히 실용적인 이유네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017856.1.6",
          "form": "ㅎㅎ 님은 어떤 계절 좋아하시는데요?",
          "original_form": "ㅎㅎ 님은 어떤 계절 좋아하시는데요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017856.1.7",
          "form": "전 봄 좋아해요",
          "original_form": "전 봄 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017856.1.8",
          "form": "황사빼고...ㅋㅋㅋ",
          "original_form": "황사빼고...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017856.1.9",
          "form": "봄 갈수록 짧아지는 느낌 들지 않아요?ㅠㅠ",
          "original_form": "봄 갈수록 짧아지는 느낌 들지 않아요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017856.1.10",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017856.1.11",
          "form": "겨울패딩 벗으면 반팔입어야할 판 ㅠㅠ",
          "original_form": "겨울패딩 벗으면 반팔입어야할 판 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017856.1.12",
          "form": "봄이 짧아져서 슬퍼요 ㅠㅠ",
          "original_form": "봄이 짧아져서 슬퍼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017856.1.13",
          "form": "그니까요ㅜㅜ 그래도 여행다니기 좋은 계절~",
          "original_form": "그니까요ㅜㅜ 그래도 여행다니기 좋은 계절~",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017856.1.14",
          "form": "사진 찍기 좋은 계절!!",
          "original_form": "사진 찍기 좋은 계절!!",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017856.1.15",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017856.1.16",
          "form": "그리고 생기넘치는 느낌이라 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그리고 생기넘치는 느낌이라 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017856.1.17",
          "form": "곧 가을인데 가을도 괜찮더라구요",
          "original_form": "곧 가을인데 가을도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017856.1.18",
          "form": "특히 핑크뮬리 예뻐요!!",
          "original_form": "특히 핑크뮬리 예뻐요!!",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017856.1.19",
          "form": "아 맞아요 핑크뮬리 진짜 예쁘더라구요",
          "original_form": "아 맞아요 핑크뮬리 진짜 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017856.1.20",
          "form": "가을엔 코스모스도 너무 예뻐요",
          "original_form": "가을엔 코스모스도 너무 예뻐요",
