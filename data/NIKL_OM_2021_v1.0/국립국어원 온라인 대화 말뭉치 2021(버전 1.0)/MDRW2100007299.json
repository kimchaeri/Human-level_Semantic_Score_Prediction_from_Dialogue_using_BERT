        },
        {
          "id": "MDRW2100007299.1.2",
          "form": "내가백수인거",
          "original_form": "내가백수인거",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007299.1.3",
          "form": "얘기하고싶었니",
          "original_form": "얘기하고싶었니",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007299.1.4",
          "form": "아죄송해요!!!친군줄알았어요진짜죄송해요",
          "original_form": "아죄송해요!!!친군줄알았어요진짜죄송해요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.5",
          "form": "ㅋㅋ편하게 하세요",
          "original_form": "ㅋㅋ편하게 하세요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.6",
          "form": "드립인줄ㅋㅋㅋㄲ",
          "original_form": "드립인줄ㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.7",
          "form": "요새 같은 시기에 쉴 수도 있죠",
          "original_form": "요새 같은 시기에 쉴 수도 있죠",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.8",
          "form": "ㅋㅋㅋㅋㅋ제가 최근에졸업을해서 ^^ 이해감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋ제가 최근에졸업을해서 ^^ 이해감사합니다",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.9",
          "form": "2월에졸업햇어요",
          "original_form": "2월에졸업햇어요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007299.1.10",
          "form": "ㅠ아부끄러 님은 무슨일하시나요",
          "original_form": "ㅠ아부끄러 님은 무슨일하시나요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007299.1.11",
          "form": "에이 2월이면 아직 ㅎㅎ",
          "original_form": "에이 2월이면 아직 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007299.1.12",
          "form": "저는 지하철 역에서 일해요ㅎㅎ",
          "original_form": "저는 지하철 역에서 일해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007299.1.13",
          "form": "저도 취준 한 1년 했어요~~",
          "original_form": "저도 취준 한 1년 했어요~~",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007299.1.14",
          "form": "우왕ㅋㅋ디트로 그런건가요",
          "original_form": "우왕ㅋㅋ디트로 그런건가요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007299.1.15",
          "form": "저도 빨리 취직하구싶네요",
          "original_form": "저도 빨리 취직하구싶네요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007299.1.16",
          "form": "금공 가고싶었는데ㅠ",
          "original_form": "금공 가고싶었는데ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007299.1.17",
          "form": "디트로 친구입니다 ㅎㅎ",
          "original_form": "디트로 친구입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007299.1.18",
          "form": "금공은 좋죠!",
          "original_form": "금공은 좋죠!",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007299.1.19",
          "form": "부럽네요ㅎㅎ능력자!!",
          "original_form": "부럽네요ㅎㅎ능력자!!",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007299.1.20",
          "form": "일이 힘들진않나요?",
          "original_form": "일이 힘들진않나요?",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007299.1.21",
          "form": "name2님은 더 좋으신데 가실거에요",
          "original_form": "&name2&님은 더 좋으신데 가실거에요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007299.1.22",
          "form": "음 힘든거 보다는",
          "original_form": "음 힘든거 보다는",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.23",
          "form": "더럽죠ㅎㅎ",
          "original_form": "더럽죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.24",
          "form": "질이 낮은 사람이 너무 많아요",
          "original_form": "질이 낮은 사람이 너무 많아요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.25",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.26",
          "form": "디트로는 되게 대우가 좋다고 들었는데",
          "original_form": "디트로는 되게 대우가 좋다고 들었는데",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.27",
          "form": "아아 승객들이",
          "original_form": "아아 승객들이",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.28",
          "form": "진상이 많겠네요",
          "original_form": "진상이 많겠네요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.29",
          "form": "네 그쵸",
          "original_form": "네 그쵸",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007299.1.30",
          "form": "진상이 많아요 생각보다",
          "original_form": "진상이 많아요 생각보다",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007299.1.31",
          "form": "1호선광인처럼ㅠ",
          "original_form": "1호선광인처럼ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007299.1.32",
          "form": "이직하실건가용",
          "original_form": "이직하실건가용",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007299.1.33",
          "form": "음 이직은 안하죠ㅎㅎ 단점보다는 장점이 많은듯해요",
          "original_form": "음 이직은 안하죠ㅎㅎ 단점보다는 장점이 많은듯해요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007299.1.34",
          "form": "ㅋㅋㅋ부러워요 정시퇴근도",
          "original_form": "ㅋㅋㅋ부러워요 정시퇴근도",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007299.1.35",
          "form": "가능이겠죠",
          "original_form": "가능이겠죠",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007299.1.36",
          "form": "요즘드는건데 하반기에 무기계약직도 넣을까봐요",
          "original_form": "요즘드는건데 하반기에 무기계약직도 넣을까봐요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007299.1.37",
          "form": "음 일단 지금같은 시기에는",
          "original_form": "음 일단 지금같은 시기에는",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007299.1.38",
          "form": "어디든 안정적인 곳",
          "original_form": "어디든 안정적인 곳",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007299.1.39",
          "form": "가는게 중요한듯해요",
          "original_form": "가는게 중요한듯해요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007299.1.40",
          "form": "무기계약직 대우가 많이열악하진않나요?",
          "original_form": "무기계약직 대우가 많이열악하진않나요?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007299.1.41",
          "form": "현직자가아니다보니ㅠ",
          "original_form": "현직자가아니다보니ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007299.1.42",
          "form": "막 무시를한다거나",
          "original_form": "막 무시를한다거나",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007299.1.43",
          "form": "음 상황마다 다르긴한데",
          "original_form": "음 상황마다 다르긴한데",
          "speaker_id": "1",
