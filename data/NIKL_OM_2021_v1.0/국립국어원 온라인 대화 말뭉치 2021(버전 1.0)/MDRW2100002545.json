      "utterance": [
        {
          "id": "MDRW2100002545.1.1",
          "form": "오오 이상형은??",
          "original_form": "오오 이상형은??",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002545.1.2",
          "form": "조금 통통하고 애교 있고 간섭 거의 없는사람?",
          "original_form": "조금 통통하고 애교 있고 간섭 거의 없는사람?",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002545.1.3",
          "form": "프라이버시는 지켜주는 사람?",
          "original_form": "프라이버시는 지켜주는 사람?",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002545.1.4",
          "form": "프라이버시도 그렇고",
          "original_form": "프라이버시도 그렇고",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.5",
          "form": "친구랑 놀때나 그럴때",
          "original_form": "친구랑 놀때나 그럴때",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.6",
          "form": "카톡으로 몇십통씩 걱정 안부 이런 문자보내는 사람 싫어함",
          "original_form": "카톡으로 몇십통씩 걱정 안부 이런 문자보내는 사람 싫어함",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.7",
          "form": "좀 편한 연애를 하고싶은데",
          "original_form": "좀 편한 연애를 하고싶은데",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.8",
          "form": "하나하나 간섭하는거 너무너무 싫음",
          "original_form": "하나하나 간섭하는거 너무너무 싫음",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.9",
          "form": "내가 클럽이나 이런곳도 안가는 사람이라서",
          "original_form": "내가 클럽이나 이런곳도 안가는 사람이라서",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.10",
          "form": "그럼 집착같은거 엄청 싫어하겠네",
          "original_form": "그럼 집착같은거 엄청 싫어하겠네",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002545.1.11",
          "form": "응응 엄청 싫어하지",
          "original_form": "응응 엄청 싫어하지",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002545.1.12",
          "form": "카톡으로 몇십통 안부는 나도 좀 싫더라고",
          "original_form": "카톡으로 몇십통 안부는 나도 좀 싫더라고",
          "speaker_id": "1",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002545.1.13",
          "form": "뭐든 적당히가 최고인것 같아",
          "original_form": "뭐든 적당히가 최고인것 같아",
          "speaker_id": "1",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002545.1.14",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002545.1.15",
          "form": "마음은 이해를 하겠는데",
          "original_form": "마음은 이해를 하겠는데",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002545.1.16",
          "form": "그거를 지적하기도 뭐하고 나만 끙끙앓다가 갈라질거같음",
          "original_form": "그거를 지적하기도 뭐하고 나만 끙끙앓다가 갈라질거같음",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002545.1.17",
          "form": "너무 혼자만 앓는건 안좋아",
          "original_form": "너무 혼자만 앓는건 안좋아",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002545.1.18",
          "form": "직설적으로 말하는것도 나쁘지 않은듯",
          "original_form": "직설적으로 말하는것도 나쁘지 않은듯",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002545.1.19",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002545.1.20",
          "form": "성격 말고 외모는 보는편이야?",
          "original_form": "성격 말고 외모는 보는편이야?",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002545.1.21",
          "form": "외모는 옛날에 안봤는데",
          "original_form": "외모는 옛날에 안봤는데",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.22",
          "form": "요즘은 보는 편이긴하지",
          "original_form": "요즘은 보는 편이긴하지",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.23",
          "form": "그렇다고 막 엄청 보는건 아니고",
          "original_form": "그렇다고 막 엄청 보는건 아니고",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.24",
          "form": "주로 보는건 성격",
          "original_form": "주로 보는건 성격",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.25",
          "form": "성격도 좋고 외모도 좋으면 최고긴하지",
          "original_form": "성격도 좋고 외모도 좋으면 최고긴하지",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.26",
          "form": "거기다가 몸매까지? 크으~",
          "original_form": "거기다가 몸매까지? 크으~",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002545.1.27",
          "form": "인생살면서 느낀건데",
          "original_form": "인생살면서 느낀건데",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002545.1.28",
          "form": "그런건 없더라 허허",
          "original_form": "그런건 없더라 허허",
          "speaker_id": "2",
          "time": "20210820 13:39"
        }
      ]
    }
  ]
}
