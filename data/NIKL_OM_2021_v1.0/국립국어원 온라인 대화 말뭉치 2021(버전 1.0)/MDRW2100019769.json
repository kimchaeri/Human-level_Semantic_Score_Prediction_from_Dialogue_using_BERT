      "utterance": [
        {
          "id": "MDRW2100019769.1.1",
          "form": "외동이신가요?",
          "original_form": "외동이신가요?",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100019769.1.2",
          "form": "아뇨! 저는 오빠가 있어요 name1님은요?",
          "original_form": "아뇨! 저는 오빠가 있어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100019769.1.3",
          "form": "저는 여동생있어요",
          "original_form": "저는 여동생있어요",
          "speaker_id": "1",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100019769.1.4",
          "form": "좋은점이뭔가요?",
          "original_form": "좋은점이뭔가요?",
          "speaker_id": "1",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100019769.1.5",
          "form": "음....음......음.....",
          "original_form": "음....음......음.....",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100019769.1.6",
          "form": "으음....",
          "original_form": "으음....",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100019769.1.7",
          "form": "절...챙겨준다는...것....?",
          "original_form": "절...챙겨준다는...것....?",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100019769.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ여동생의 장점은 많을 것 같은데 어떠신가요?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ여동생의 장점은 많을 것 같은데 어떠신가요?!",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100019769.1.9",
          "form": "저는 동생이동먹을걸 잘 알아요",
          "original_form": "저는 동생이동먹을걸 잘 알아요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100019769.1.10",
          "form": "맛잘알이어서 따리다니면서 먹을수있다는거",
          "original_form": "맛잘알이어서 따리다니면서 먹을수있다는거",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100019769.1.11",
          "form": "글고 저 먹을거잘사줘요",
          "original_form": "글고 저 먹을거잘사줘요",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100019769.1.12",
          "form": "끝입니다..",
          "original_form": "끝입니다..",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100019769.1.13",
          "form": "ㅋㅌㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100019769.1.14",
          "form": "우와 맛잘알동생 있으면 너무 좋겠어요ㅜㅜ",
          "original_form": "우와 맛잘알동생 있으면 너무 좋겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100019769.1.15",
          "form": "저희오빠는 음식에 관심이 없거든요ㅜㅜ...",
          "original_form": "저희오빠는 음식에 관심이 없거든요ㅜㅜ...",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100019769.1.16",
          "form": "되게마른편이신가요?",
          "original_form": "되게마른편이신가요?",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100019769.1.17",
          "form": "오빠분이?",
          "original_form": "오빠분이?",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100019769.1.18",
          "form": "네ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100019769.1.19",
          "form": "신기해요 진짜 음식에 관심이없어서;",
          "original_form": "신기해요 진짜 음식에 관심이없어서;",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100019769.1.20",
          "form": "우와~",
          "original_form": "우와~",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100019769.1.21",
          "form": "근데 제가 그래요",
          "original_form": "근데 제가 그래요",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100019769.1.22",
          "form": "오빠분처럼",
          "original_form": "오빠분처럼",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100019769.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100019769.1.24",
          "form": "그래서 잘 따라다녀요^^..",
          "original_form": "그래서 잘 따라다녀요^^..",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100019769.1.25",
          "form": "와 신기해요 진짜...어떻게 음식에 관심이없냐고요!!!!!!",
          "original_form": "와 신기해요 진짜...어떻게 음식에 관심이없냐고요!!!!!!",
          "speaker_id": "2",
          "time": "20210829 23:39"
        },
        {
          "id": "MDRW2100019769.1.26",
          "form": "신기하게도 지금 저도...여동생..입장에...있네요?!?!?!?!",
          "original_form": "신기하게도 지금 저도...여동생..입장에...있네요?!?!?!?!",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100019769.1.27",
          "form": "오매..",
          "original_form": "오매..",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100019769.1.28",
          "form": "신기해욬ㅋㅋㅋ",
          "original_form": "신기해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100019769.1.29",
          "form": "제동생이 절 그렇게보는군요 ㅋㅋㅋㅋ",
          "original_form": "제동생이 절 그렇게보는군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100019769.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ아놔 그렇게 되는건가요?!?!?!?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ아놔 그렇게 되는건가요?!?!?!?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100019769.1.31",
          "form": "근데 진짜 신기해요 음식을 버ㅏ도 그냥 음식이구나..본다는 점이",
          "original_form": "근데 진짜 신기해요 음식을 버ㅏ도 그냥 음식이구나..본다는 점이",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100019769.1.32",
          "form": "저는 음 개인적으로",
          "original_form": "저는 음 개인적으로",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100019769.1.33",
          "form": "살기위해먹는달까",
          "original_form": "살기위해먹는달까",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100019769.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100019769.1.35",
