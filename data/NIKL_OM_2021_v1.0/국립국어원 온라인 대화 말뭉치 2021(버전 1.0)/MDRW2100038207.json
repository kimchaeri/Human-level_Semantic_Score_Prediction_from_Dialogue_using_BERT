      "utterance": [
        {
          "id": "MDRW2100038207.1.1",
          "form": "할인받아서 사는거 재미있어서 사는거 같기도 해요",
          "original_form": "할인받아서 사는거 재미있어서 사는거 같기도 해요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038207.1.2",
          "form": "ㅋㅋ꼭 안사도 될거 같은데 좀 사는것도 있어요",
          "original_form": "ㅋㅋ꼭 안사도 될거 같은데 좀 사는것도 있어요",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038207.1.3",
          "form": "맞죠?",
          "original_form": "맞죠?",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038207.1.4",
          "form": "저 지금까지 하프갤런 사본적없거든요",
          "original_form": "저 지금까지 하프갤런 사본적없거든요",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038207.1.5",
          "form": "깁콘 털려고 지른듯한 재미 ㅋ",
          "original_form": "깁콘 털려고 지른듯한 재미 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038207.1.6",
          "form": "싱글 4장 파인트 1장 해피포인트 천포로",
          "original_form": "싱글 4장 파인트 1장 해피포인트 천포로",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038207.1.7",
          "form": "실결제 없이 ㅎㅎ 왠지 뿌듯 ㅎ",
          "original_form": "실결제 없이 ㅎㅎ 왠지 뿌듯 ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038207.1.8",
          "form": "ㅋㅋ근데 진짜 잘사셨는데요??",
          "original_form": "ㅋㅋ근데 진짜 잘사셨는데요??",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038207.1.9",
          "form": "저였어도 뿌듯할거 같음ㅋㅋㅋ",
          "original_form": "저였어도 뿌듯할거 같음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038207.1.10",
          "form": "무슨 맛 좋아하세요?",
          "original_form": "무슨 맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038207.1.11",
          "form": "저는 쫀떡궁합 맛있다고 생각했는데 이제 없더라고요",
          "original_form": "저는 쫀떡궁합 맛있다고 생각했는데 이제 없더라고요",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038207.1.12",
          "form": "저는 엄마는 외계인이요",
          "original_form": "저는 엄마는 외계인이요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038207.1.13",
          "form": "쫀떡궁합 맛이 있어요?",
          "original_form": "쫀떡궁합 맛이 있어요?",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038207.1.14",
          "form": "네 인절미랑 흑임자맛 나서 맛있어요",
          "original_form": "네 인절미랑 흑임자맛 나서 맛있어요",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038207.1.15",
          "form": "엄마는 외계인도 맛있죠",
          "original_form": "엄마는 외계인도 맛있죠",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038207.1.16",
          "form": "오 흑임자 인절미 좋아하는데",
          "original_form": "오 흑임자 인절미 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038207.1.17",
          "form": "아니면 치즈 들어간거 좋아해요 바람과함께사라지다",
          "original_form": "아니면 치즈 들어간거 좋아해요 바람과함께사라지다",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038207.1.18",
          "form": "왜 이런걸 몰랐을까요?",
          "original_form": "왜 이런걸 몰랐을까요?",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.19",
          "form": "근데 안팔더라고요 ㅜㅜ 작년에만 팔았나",
          "original_form": "근데 안팔더라고요 ㅜㅜ 작년에만 팔았나",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.20",
          "form": "아., ,아숩네요",
          "original_form": "아., ,아숩네요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.21",
          "form": "요즘은 무슨 콜라보 많이하는거같아요",
          "original_form": "요즘은 무슨 콜라보 많이하는거같아요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.22",
          "form": "레몬맛 나는 아이스크림도 맛있었어요",
          "original_form": "레몬맛 나는 아이스크림도 맛있었어요",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.23",
          "form": "색깔 까만거",
          "original_form": "색깔 까만거",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.24",
          "form": "맞아요 맥심 하던데",
          "original_form": "맞아요 맥심 하던데",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038207.1.25",
          "form": "죠리퐁 코북칩",
          "original_form": "죠리퐁 코북칩",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038207.1.26",
          "form": "드셔보셨어요??",
          "original_form": "드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038207.1.27",
          "form": "맥심 저도 궁금해요 맛이",
          "original_form": "맥심 저도 궁금해요 맛이",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038207.1.28",
          "form": "맥심은 그냥 맥심맛인데 맛은 있을거 같긴 해요",
          "original_form": "맥심은 그냥 맥심맛인데 맛은 있을거 같긴 해요",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038207.1.29",
          "form": "달달하게 커피맛이날것같아요",
          "original_form": "달달하게 커피맛이날것같아요",
          "speaker_id": "1",
          "time": "20210902 00:51"
        }
      ]
