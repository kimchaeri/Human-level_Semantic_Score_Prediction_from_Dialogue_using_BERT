      "utterance": [
        {
          "id": "MDRW2100030264.1.1",
          "form": "싸이버거랑 몇백원차이안났어묘",
          "original_form": "싸이버거랑 몇백원차이안났어묘",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030264.1.2",
          "form": "100원?",
          "original_form": "100원?",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030264.1.3",
          "form": "200원정도",
          "original_form": "200원정도",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030264.1.4",
          "form": "치킨버거중 맘스터치가 최고에요",
          "original_form": "치킨버거중 맘스터치가 최고에요",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030264.1.5",
          "form": "남포점아ㅚ는 못먹어봤네요",
          "original_form": "남포점아ㅚ는 못먹어봤네요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.6",
          "form": "바삭하고요",
          "original_form": "바삭하고요",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.7",
          "form": "맞아요 존맛이죠",
          "original_form": "맞아요 존맛이죠",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.8",
          "form": "개인체인이라 맛 편차가 있죠",
          "original_form": "개인체인이라 맛 편차가 있죠",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.9",
          "form": "그런데 배고파서",
          "original_form": "그런데 배고파서",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.10",
          "form": "허겁지겁먹다가",
          "original_form": "허겁지겁먹다가",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.11",
          "form": "시장이 반찬이죠",
          "original_form": "시장이 반찬이죠",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.12",
          "form": "탈이 났어요",
          "original_form": "탈이 났어요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.13",
          "form": "장염걸려서 며칠 고생해서요",
          "original_form": "장염걸려서 며칠 고생해서요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.14",
          "form": "그뒤로 못먹고밌어묘",
          "original_form": "그뒤로 못먹고밌어묘",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030264.1.15",
          "form": "너무 슬픈데 생각만해도",
          "original_form": "너무 슬픈데 생각만해도",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.16",
          "form": "고생하셨네요",
          "original_form": "고생하셨네요",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.17",
          "form": "속이 받치더라구요",
          "original_form": "속이 받치더라구요",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.18",
          "form": "먹고 체하거나",
          "original_form": "먹고 체하거나",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.19",
          "form": "한동안 못먹죠",
          "original_form": "한동안 못먹죠",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.20",
          "form": "속수무책이더라구요",
          "original_form": "속수무책이더라구요",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030264.1.21",
          "form": "당분간  못드시겠네요",
          "original_form": "당분간  못드시겠네요",
          "speaker_id": "1",
          "time": "20210903 23:22"
        }
      ]
