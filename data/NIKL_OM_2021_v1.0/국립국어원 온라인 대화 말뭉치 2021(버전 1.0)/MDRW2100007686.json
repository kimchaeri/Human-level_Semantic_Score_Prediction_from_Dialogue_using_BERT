      "utterance": [
        {
          "id": "MDRW2100007686.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007686.1.2",
          "form": "음 자주 하는 게임이 있으세요?",
          "original_form": "음 자주 하는 게임이 있으세요?",
          "speaker_id": "1",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007686.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007686.1.4",
          "form": "저 게임은 잘 안 하는데 돈스타브랑 오버워치 좋아했어요!",
          "original_form": "저 게임은 잘 안 하는데 돈스타브랑 오버워치 좋아했어요!",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007686.1.5",
          "form": "아시나요 혹시 ㅎㅎ",
          "original_form": "아시나요 혹시 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007686.1.6",
          "form": "오버와치는 들어봤어요",
          "original_form": "오버와치는 들어봤어요",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007686.1.7",
          "form": "해본적은 없어요ㅜ",
          "original_form": "해본적은 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007686.1.8",
          "form": "거기 무슨 캐릭터랑 닮았다고 한동안 별명이 그거였어요 ㅎㅎ",
          "original_form": "거기 무슨 캐릭터랑 닮았다고 한동안 별명이 그거였어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007686.1.9",
          "form": "엌ㅋㅋㅋㅋ 거기 캐릭터 다 간지나게 생겼어요!",
          "original_form": "엌ㅋㅋㅋㅋ 거기 캐릭터 다 간지나게 생겼어요!",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007686.1.10",
          "form": "어릴땐 크레이지아케이드랑 메이플 되게많이했어요!",
          "original_form": "어릴땐 크레이지아케이드랑 메이플 되게많이했어요!",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007686.1.11",
          "form": "앗 저도 그 카트라이더는 했었어요",
          "original_form": "앗 저도 그 카트라이더는 했었어요",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007686.1.12",
          "form": "메이플은 요즘도 있는것 같더라고요",
          "original_form": "메이플은 요즘도 있는것 같더라고요",
          "speaker_id": "1",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007686.1.13",
          "form": "카트라이더 저 진짜 못해서 운전면허도 못 딸줄 알았는데 면허는 땄어요 ㅋㅋㅋㅋㅋ 되게 어렵더라구요 잘 하셨나요",
          "original_form": "카트라이더 저 진짜 못해서 운전면허도 못 딸줄 알았는데 면허는 땄어요 ㅋㅋㅋㅋㅋ 되게 어렵더라구요 잘 하셨나요",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007686.1.14",
          "form": "메이플 진짜 장수게임이네요",
          "original_form": "메이플 진짜 장수게임이네요",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007686.1.15",
          "form": "아뇨ㅋㅋ 저는 게임도 어려울수있단걸 깨달았어요",
          "original_form": "아뇨ㅋㅋ 저는 게임도 어려울수있단걸 깨달았어요",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007686.1.16",
          "form": "게임도 잘해야 중독이 된다는 사실을 깨달았다는 ㅋㅋ",
          "original_form": "게임도 잘해야 중독이 된다는 사실을 깨달았다는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007686.1.17",
          "form": "ㅋㅋㅋ아 진짜 맞아요 잘해야 중독되는데 잘 못해서 중독조차 되지못했던..",
          "original_form": "ㅋㅋㅋ아 진짜 맞아요 잘해야 중독되는데 잘 못해서 중독조차 되지못했던..",
          "speaker_id": "2",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007686.1.18",
          "form": "테일즈런너 카트라이더 이런 건 진짜 어렵더라구요",
          "original_form": "테일즈런너 카트라이더 이런 건 진짜 어렵더라구요",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007686.1.19",
          "form": "저는 학생때도 좀 웃겼던거같아요",
          "original_form": "저는 학생때도 좀 웃겼던거같아요",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007686.1.20",
          "form": "이게 재미있나 싶기도하고",
          "original_form": "이게 재미있나 싶기도하고",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007686.1.21",
          "form": "매번 지니까 ㅜㅜ",
          "original_form": "매번 지니까 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007686.1.22",
          "form": "맞아요 맨날 지고 ㅋㅋㅋ 저는 인공지능하고 대결에서도 졌어요",
          "original_form": "맞아요 맨날 지고 ㅋㅋㅋ 저는 인공지능하고 대결에서도 졌어요",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007686.1.23",
          "form": "못해서 점점 안하게 되더라구요",
          "original_form": "못해서 점점 안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007686.1.24",
          "form": "아 게임이면그래도 그건 좀했지않나요",
          "original_form": "아 게임이면그래도 그건 좀했지않나요",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007686.1.25",
          "form": "프린세스메이커!",
          "original_form": "프린세스메이커!",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007686.1.26",
          "form": "아 전 그건 안해봤는데! 그거 무슨 엔딩 되게 여러가지인 게임 맞죠?",
          "original_form": "아 전 그건 안해봤는데! 그거 무슨 엔딩 되게 여러가지인 게임 맞죠?",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007686.1.27",
          "form": "잘 못키우면 마녀같은거 되고 잘키우면 왕되나요",
          "original_form": "잘 못키우면 마녀같은거 되고 잘키우면 왕되나요",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007686.1.28",
          "form": "결혼도 시켜요",
          "original_form": "결혼도 시켜요",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007686.1.29",
          "form": "왕자랑  마족이랑 등등",
          "original_form": "왕자랑  마족이랑 등등",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007686.1.30",
          "form": "아 결혼도 시키곸ㅋㅋㅋ 진짜 딸키우는거같이 하네요",
          "original_form": "아 결혼도 시키곸ㅋㅋㅋ 진짜 딸키우는거같이 하네요",
          "speaker_id": "2",
          "time": "20210822 10:21"
        }
      ]
