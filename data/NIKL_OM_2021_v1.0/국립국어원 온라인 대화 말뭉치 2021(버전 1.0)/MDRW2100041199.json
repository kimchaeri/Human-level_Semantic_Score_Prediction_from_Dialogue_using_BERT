      "utterance": [
        {
          "id": "MDRW2100041199.1.1",
          "form": "네 창문 열어놨는데 발이 시리네요ㅎㅎ",
          "original_form": "네 창문 열어놨는데 발이 시리네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041199.1.2",
          "form": "저도 방금 창문 닫았어요",
          "original_form": "저도 방금 창문 닫았어요",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100041199.1.3",
          "form": "요즘은 잘때 창문 꼭 닫고 자야하더라고요",
          "original_form": "요즘은 잘때 창문 꼭 닫고 자야하더라고요",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100041199.1.4",
          "form": "이불도 곧 두꺼운 걸로 교체해줘야 할 것 같아요",
          "original_form": "이불도 곧 두꺼운 걸로 교체해줘야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100041199.1.5",
          "form": "저는 엊그제부터 두꺼운이불로 바꿨어요",
          "original_form": "저는 엊그제부터 두꺼운이불로 바꿨어요",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100041199.1.6",
          "form": "저도 미리 바꿀 걸 그랬나 싶네요ㅋㅋ",
          "original_form": "저도 미리 바꿀 걸 그랬나 싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041199.1.7",
          "form": "오늘밤부터 두꺼운 이불 덮으셔요",
          "original_form": "오늘밤부터 두꺼운 이불 덮으셔요",
          "speaker_id": "1",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100041199.1.8",
          "form": "지금이라도 꺼내야겠네요ㅋㅋ",
          "original_form": "지금이라도 꺼내야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100041199.1.9",
          "form": "네네 추천합니다",
          "original_form": "네네 추천합니다",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100041199.1.10",
          "form": "새벽엔 서늘하잖아요",
          "original_form": "새벽엔 서늘하잖아요",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041199.1.11",
          "form": "화요일엔 다시 비 예보 있더라구요",
          "original_form": "화요일엔 다시 비 예보 있더라구요",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041199.1.12",
          "form": "에고 또 비오는구나",
          "original_form": "에고 또 비오는구나",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041199.1.13",
          "form": "잘 봐서 빨래 해야겠어요ㅎㅎ",
          "original_form": "잘 봐서 빨래 해야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041199.1.14",
          "form": "네네 맑을 때 빨래하고 청소하고",
          "original_form": "네네 맑을 때 빨래하고 청소하고",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041199.1.15",
          "form": "그래도 일단 내일은 날 좋다니까 좋네요ㅋㅋ",
          "original_form": "그래도 일단 내일은 날 좋다니까 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041199.1.16",
          "form": "네 내일 날씨를 누려요",
          "original_form": "네 내일 날씨를 누려요",
          "speaker_id": "1",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041199.1.17",
          "form": "오늘은 비가 많이 오네요",
          "original_form": "오늘은 비가 많이 오네요",
