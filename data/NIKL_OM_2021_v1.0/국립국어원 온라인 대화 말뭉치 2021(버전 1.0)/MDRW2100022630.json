        {
          "id": "MDRW2100022630.1.1",
          "form": "싸이월드 세대이신가요 ㅎㅎ",
          "original_form": "싸이월드 세대이신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022630.1.2",
          "form": "예전에 많이 했었네요ㅋ",
          "original_form": "예전에 많이 했었네요ㅋ",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100022630.1.3",
          "form": "ㅎㅎ저도요 대학교때 도토리질 좀 한 것같아요",
          "original_form": "ㅎㅎ저도요 대학교때 도토리질 좀 한 것같아요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022630.1.4",
          "form": "도토리ㅎㅎ 음악도 샀었는데",
          "original_form": "도토리ㅎㅎ 음악도 샀었는데",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100022630.1.5",
          "form": "그쵸 음악 사고 커플미니미 맺고",
          "original_form": "그쵸 음악 사고 커플미니미 맺고",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100022630.1.6",
          "form": "일촌평도 남기고ㅋ",
          "original_form": "일촌평도 남기고ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100022630.1.7",
          "form": "악 일촌평 ㅋㅋㅋ 다시 부활하면 하실 것 같으신가요",
          "original_form": "악 일촌평 ㅋㅋㅋ 다시 부활하면 하실 것 같으신가요",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100022630.1.8",
          "form": "절대로요ㅋ",
          "original_form": "절대로요ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100022630.1.9",
          "form": "저는 기존꺼 열어보지도 않을듯요",
          "original_form": "저는 기존꺼 열어보지도 않을듯요",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100022630.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100022630.1.11",
          "form": "두려워요",
          "original_form": "두려워요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100022630.1.12",
          "form": "오글오글 아직 옛 남친과도 일촌일 것같아요",
