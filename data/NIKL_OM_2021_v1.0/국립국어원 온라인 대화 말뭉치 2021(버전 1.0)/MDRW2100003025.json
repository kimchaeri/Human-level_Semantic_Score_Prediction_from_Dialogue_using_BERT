      "utterance": [
        {
          "id": "MDRW2100003025.1.1",
          "form": "나 공연티켓당첨됐어",
          "original_form": "나 공연티켓당첨됐어",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003025.1.2",
          "form": "옥상달빛",
          "original_form": "옥상달빛",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003025.1.3",
          "form": "전혀 모르겠다 나는 공연 뮤지컬 연극 이런거 내평생 단한번도 본적이 없움",
          "original_form": "전혀 모르겠다 나는 공연 뮤지컬 연극 이런거 내평생 단한번도 본적이 없움",
          "speaker_id": "2",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003025.1.4",
          "form": "무료라 기분이가 좋아",
          "original_form": "무료라 기분이가 좋아",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003025.1.5",
          "form": "대학축제때 공연안봤어?",
          "original_form": "대학축제때 공연안봤어?",
          "speaker_id": "1",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003025.1.6",
          "form": "그딴걸 공연이라고 해야할까 싶다",
          "original_form": "그딴걸 공연이라고 해야할까 싶다",
          "speaker_id": "2",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003025.1.7",
          "form": "브아걸 와서 노래하는거 구경하다가 더워서 기숙사 들아온거 같은데",
          "original_form": "브아걸 와서 노래하는거 구경하다가 더워서 기숙사 들아온거 같은데",
          "speaker_id": "2",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003025.1.8",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003025.1.9",
          "form": "그래도 그것도 공연이지 뭐",
          "original_form": "그래도 그것도 공연이지 뭐",
          "speaker_id": "1",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003025.1.10",
          "form": "그럼 나도 공연 가봤던거네 ㅋㅋ",
          "original_form": "그럼 나도 공연 가봤던거네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003025.1.11",
          "form": "그럼 운동 끝났다 덥다",
          "original_form": "그럼 운동 끝났다 덥다",
          "speaker_id": "1",
          "time": "20210820 19:31"
        },
        {
          "id": "MDRW2100003025.1.12",
          "form": "운동 열심히하는구만 공연",
          "original_form": "운동 열심히하는구만 공연",
          "speaker_id": "2",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003025.1.13",
          "form": "그럼 그럼에도 살은 빠지지 않는 마법",
          "original_form": "그럼 그럼에도 살은 빠지지 않는 마법",
          "speaker_id": "1",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003025.1.14",
          "form": "유지하기 위함으로 하는기지 뮤지컬",
          "original_form": "유지하기 위함으로 하는기지 뮤지컬",
          "speaker_id": "2",
