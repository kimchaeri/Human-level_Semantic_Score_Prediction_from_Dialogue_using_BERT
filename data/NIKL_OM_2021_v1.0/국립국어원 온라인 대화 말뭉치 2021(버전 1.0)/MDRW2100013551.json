        },
        {
          "id": "MDRW2100013551.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013551.1.3",
          "form": "오늘도 name1님 지역에 비 오고 있나요?",
          "original_form": "오늘도 &name1&님 지역에 비 오고 있나요?",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013551.1.4",
          "form": "여기는 왔다 안왔다 하네요",
          "original_form": "여기는 왔다 안왔다 하네요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013551.1.5",
          "form": "네 비가 많이 오네요",
          "original_form": "네 비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013551.1.6",
          "form": "우산 요즘 잘 챙겨 다니시나요",
          "original_form": "우산 요즘 잘 챙겨 다니시나요",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.7",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.8",
          "form": "양산챙기고 다니다가",
          "original_form": "양산챙기고 다니다가",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.9",
          "form": "우산으로 바꼈네욯ㅎㅎㅎ",
          "original_form": "우산으로 바꼈네욯ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.10",
          "form": "저는 우의 입고 다닙니다",
          "original_form": "저는 우의 입고 다닙니다",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.11",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.12",
          "form": "엄청 편하네요",
          "original_form": "엄청 편하네요",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.13",
          "form": "우의!! 넘 신기하네요",
          "original_form": "우의!! 넘 신기하네요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.14",
          "form": "일회용은 아니실것같고",
          "original_form": "일회용은 아니실것같고",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.15",
          "form": "약간 패션우의로 따로 사신건가요?",
          "original_form": "약간 패션우의로 따로 사신건가요?",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.16",
          "form": "다이소에서 삼천원짜리 샀어요",
          "original_form": "다이소에서 삼천원짜리 샀어요",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.17",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013551.1.18",
          "form": "양손 편하니 좋울 것 같아요",
          "original_form": "양손 편하니 좋울 것 같아요",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.19",
          "form": "우의를 이용하시게 된 계기가 있으신가요?!",
          "original_form": "우의를 이용하시게 된 계기가 있으신가요?!",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.20",
          "form": "자전거를 타고 다니는 편이라서",
          "original_form": "자전거를 타고 다니는 편이라서",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.21",
          "form": "비올땐 자전거 못타는게 화나서 우의를 입었더니",
          "original_form": "비올땐 자전거 못타는게 화나서 우의를 입었더니",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.22",
          "form": "너무 편하네요",
          "original_form": "너무 편하네요",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.23",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013551.1.24",
          "form": "자전거 타시는구나",
          "original_form": "자전거 타시는구나",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013551.1.25",
          "form": "출퇴근용입니다",
          "original_form": "출퇴근용입니다",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013551.1.26",
          "form": "헉 글네요 넘사로 편하겠어요",
          "original_form": "헉 글네요 넘사로 편하겠어요",
