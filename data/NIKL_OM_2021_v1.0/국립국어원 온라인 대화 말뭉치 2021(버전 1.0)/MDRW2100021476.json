      "utterance": [
        {
          "id": "MDRW2100021476.1.1",
          "form": "아 진짜? 잘 안 맞았니",
          "original_form": "아 진짜? 잘 안 맞았니",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021476.1.2",
          "form": "응 계속 미끄러질것같이 불안해서",
          "original_form": "응 계속 미끄러질것같이 불안해서",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021476.1.3",
          "form": "중고로 다시 팔았걷는",
          "original_form": "중고로 다시 팔았걷는",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021476.1.4",
          "form": "그 돈으로 다시 신발 중고로 사게",
          "original_form": "그 돈으로 다시 신발 중고로 사게",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021476.1.5",
          "form": "ㅋㅋㅋㅋ어느 정도 비용 회수할 수 있어서 다행이었다",
          "original_form": "ㅋㅋㅋㅋ어느 정도 비용 회수할 수 있어서 다행이었다",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.6",
          "form": "이번에는 어떤 신발 사려구?",
          "original_form": "이번에는 어떤 신발 사려구?",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.7",
          "form": "응 2만원 손해봤는데 이정도면 괜찮다 싶어",
          "original_form": "응 2만원 손해봤는데 이정도면 괜찮다 싶어",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.8",
          "form": "나이키에 윈드브레이커라는",
          "original_form": "나이키에 윈드브레이커라는",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.9",
          "form": "운동화 사려고",
          "original_form": "운동화 사려고",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.10",
          "form": "어떻게 생겼는지 궁금하다",
          "original_form": "어떻게 생겼는지 궁금하다",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021476.1.11",
          "form": "검색해봐야지",
          "original_form": "검색해봐야지",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.12",
          "form": "잠시만",
          "original_form": "잠시만",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.13",
          "form": "써밋화이트를 살건데",
          "original_form": "써밋화이트를 살건데",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.14",
          "form": "원가가 14만원",
          "original_form": "원가가 14만원",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.15",
          "form": "중고가 7만원",
          "original_form": "중고가 7만원",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.16",
          "form": "괜찮지않냐?",
          "original_form": "괜찮지않냐?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021476.1.17",
          "form": "데이브레이크 모델 말하는 거지?",
          "original_form": "데이브레이크 모델 말하는 거지?",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.18",
          "form": "어 맞어",
          "original_form": "어 맞어",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.19",
          "form": "윈드브레이크라니",
          "original_form": "윈드브레이크라니",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.20",
          "form": "나 뭐라니",
          "original_form": "나 뭐라니",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.21",
          "form": "중고가 완전 반값이네",
          "original_form": "중고가 완전 반값이네",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아냐 알아들었잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아냐 알아들었잖아",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021476.1.23",
          "form": "근데 중고라해도 다들",
          "original_form": "근데 중고라해도 다들",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.24",
          "form": "깨끗하게 신었더라고",
          "original_form": "깨끗하게 신었더라고",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.25",
          "form": "내가 새거 살필요없이",
          "original_form": "내가 새거 살필요없이",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.26",
          "form": "중고로 사면",
          "original_form": "중고로 사면",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.27",
          "form": "개이득아니냐",
          "original_form": "개이득아니냐",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.28",
          "form": "그러게 큰 하자 없다면",
          "original_form": "그러게 큰 하자 없다면",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.29",
          "form": "돈 많이 아끼겠다",
          "original_form": "돈 많이 아끼겠다",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.30",
          "form": "나이키는 또 인지도 있으니까",
          "original_form": "나이키는 또 인지도 있으니까",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021476.1.31",
          "form": "안 맞으면 다시 되팔기도 좋고",
          "original_form": "안 맞으면 다시 되팔기도 좋고",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.32",
          "form": "이번에 사는 건 잘 맞았으면 좋겠다",
          "original_form": "이번에 사는 건 잘 맞았으면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.33",
          "form": "근데 요즘 고민인게",
          "original_form": "근데 요즘 고민인게",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.34",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.35",
          "form": "다이어트로 치수가 거의다 바꼈거든",
          "original_form": "다이어트로 치수가 거의다 바꼈거든",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.36",
          "form": "옛날 치수 신발로 사야될지",
          "original_form": "옛날 치수 신발로 사야될지",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.37",
          "form": "반치수작은거살지",
          "original_form": "반치수작은거살지",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.38",
          "form": "그게 고민이여",
          "original_form": "그게 고민이여",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021476.1.39",
          "form": "음 기존 신발들이 커졌어?",
          "original_form": "음 기존 신발들이 커졌어?",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021476.1.40",
          "form": "음 이번에 원래 치수로 샀더니 너무 크더라고",
          "original_form": "음 이번에 원래 치수로 샀더니 너무 크더라고",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021476.1.41",
          "form": "그래서 반치수 작은거 살려니",
          "original_form": "그래서 반치수 작은거 살려니",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021476.1.42",
          "form": "신어본적이 있어야지",
          "original_form": "신어본적이 있어야지",
          "speaker_id": "1",
          "time": "20210828 19:33"
        }
      ]
    }
  ]
