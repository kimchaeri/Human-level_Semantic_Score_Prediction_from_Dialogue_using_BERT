      "utterance": [
        {
          "id": "MDRW2100023337.1.1",
          "form": "맞아여 ㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023337.1.2",
          "form": "여의도공원 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여의도공원 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100023337.1.3",
          "form": "재방송도 엄청하잖아요 ㅋㅋㅋ",
          "original_form": "재방송도 엄청하잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023337.1.4",
          "form": "어제 놀면뭐하니 완전 무도재질이었어요!!",
          "original_form": "어제 놀면뭐하니 완전 무도재질이었어요!!",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023337.1.5",
          "form": "신나더라구요~",
          "original_form": "신나더라구요~",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023337.1.6",
          "form": "어제 놀뭐 못봤어요..\n 낼 아침에 봐야지...",
          "original_form": "어제 놀뭐 못봤어요..\n 낼 아침에 봐야지...",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023337.1.7",
          "form": "그 노홍철 똘끼 있을때가.. ㅋㅋㅋ",
          "original_form": "그 노홍철 똘끼 있을때가.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023337.1.8",
          "form": "대박이었죠 ㅋㅋㅋㅋㅋ",
          "original_form": "대박이었죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023337.1.9",
          "form": "진짜 아무도 못이기는ㅋㅋㅋ",
          "original_form": "진짜 아무도 못이기는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023337.1.10",
          "form": "첨엔 웃겼는데 나중엔 진짜 그 컨셉에 먹히는 느낌이 들긴했어요",
          "original_form": "첨엔 웃겼는데 나중엔 진짜 그 컨셉에 먹히는 느낌이 들긴했어요",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023337.1.11",
          "form": "진짜.... 그때가 다시돌아올수없다는게 슬프지만 추억할수있어서 좋아요 ㅎㅎ",
          "original_form": "진짜.... 그때가 다시돌아올수없다는게 슬프지만 추억할수있어서 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023337.1.12",
          "form": "지금 20대-30대는 무도랑 함께 자란거죠...",
          "original_form": "지금 20대-30대는 무도랑 함께 자란거죠...",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.13",
          "form": "그쳐 완전요ㅜㅜ",
          "original_form": "그쳐 완전요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.14",
          "form": "요즘 런닝맨 액기스도 유튭에서 보는데..",
          "original_form": "요즘 런닝맨 액기스도 유튭에서 보는데..",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.15",
          "form": "무도에 10대 20대 30대 다 있어요 ㅠㅠ",
          "original_form": "무도에 10대 20대 30대 다 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.16",
          "form": "유재석 깐족이는게 데뷔 초창기 같아서",
          "original_form": "유재석 깐족이는게 데뷔 초창기 같아서",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.17",
          "form": "엄청 캐이하던데요..",
          "original_form": "엄청 캐이하던데요..",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023337.1.19",
          "form": "케리",
          "original_form": "케리",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023337.1.20",
          "form": "런닝맨도 오래됐어요~",
          "original_form": "런닝맨도 오래됐어요~",
          "speaker_id": "2",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023337.1.21",
          "form": "참.. 무도하면서 런닝맨하기 힘들겠다 하면서 약간 2순위 취급했는데 최장기로 하면서 메인이 된느낌..",
          "original_form": "참.. 무도하면서 런닝맨하기 힘들겠다 하면서 약간 2순위 취급했는데 최장기로 하면서 메인이 된느낌..",
          "speaker_id": "2",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023337.1.22",
          "form": "놀뭐를 혼자하기 힘들단말이 런닝맨보면 이해가 가는것 같아요",
          "original_form": "놀뭐를 혼자하기 힘들단말이 런닝맨보면 이해가 가는것 같아요",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023337.1.23",
          "form": "전 런닝맨을 젆혀 안봤엇거든요..",
          "original_form": "전 런닝맨을 젆혀 안봤엇거든요..",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023337.1.24",
          "form": "근데.최근 유튭 보니까.. \n이거도 엄청 재밋더라구요..",
          "original_form": "근데.최근 유튭 보니까.. \n이거도 엄청 재밋더라구요..",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023337.1.25",
          "form": "저도 챙겨보지는 않아요 ㅎㅎ 가끔 게스트보면서 챙겨보고 ㅎㅎㅎ",
          "original_form": "저도 챙겨보지는 않아요 ㅎㅎ 가끔 게스트보면서 챙겨보고 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023337.1.26",
          "form": "여기서도 관계성이 나오고 케미가 보이더라구요",
          "original_form": "여기서도 관계성이 나오고 케미가 보이더라구요",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023337.1.27",
          "form": "유느 여기서 편해보여요",
          "original_form": "유느 여기서 편해보여요",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023337.1.28",
          "form": "그툐...",
          "original_form": "그툐...",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023337.1.29",
          "form": "뭔가 다른 캐릭터들이 많이 받쳐두니까..",
          "original_form": "뭔가 다른 캐릭터들이 많이 받쳐두니까..",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023337.1.30",
          "form": "편안해보이는게 좋아보이면서도 무도팬으로는 씁쓸 ㅠㅠㅠㅠㅠ",
          "original_form": "편안해보이는게 좋아보이면서도 무도팬으로는 씁쓸 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023337.1.31",
          "form": "그래도 놀뭐가 나름 2년 잘 해왓으니까여..",
          "original_form": "그래도 놀뭐가 나름 2년 잘 해왓으니까여..",
          "speaker_id": "1",
          "time": "20210830 00:34"
        }
      ]
    }
