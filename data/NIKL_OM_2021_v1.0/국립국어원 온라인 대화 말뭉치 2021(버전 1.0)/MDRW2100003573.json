      "utterance": [
        {
          "id": "MDRW2100003573.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003573.1.2",
          "form": "네 햄스터 키우고 있어요",
          "original_form": "네 햄스터 키우고 있어요",
          "speaker_id": "2",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003573.1.3",
          "form": "우와 몇살이에요?",
          "original_form": "우와 몇살이에요?",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003573.1.4",
          "form": "골든햄스터에요?",
          "original_form": "골든햄스터에요?",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003573.1.5",
          "form": "네 골든햄스터예요",
          "original_form": "네 골든햄스터예요",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003573.1.6",
          "form": "2개월밖에 안됐어요",
          "original_form": "2개월밖에 안됐어요",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003573.1.7",
          "form": "데려온지 일주일도 안돼서 ㅋㅋ",
          "original_form": "데려온지 일주일도 안돼서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003573.1.8",
          "form": "헉 대박 ! 진짜 골든햄스터 !!",
          "original_form": "헉 대박 ! 진짜 골든햄스터 !!",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003573.1.9",
          "form": "저는 강아지 키워요",
          "original_form": "저는 강아지 키워요",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003573.1.10",
          "form": "16살 입니다 ㅎㅎㅎㅎ",
          "original_form": "16살 입니다 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003573.1.11",
          "form": "와 강아지 나이가 많네요",
          "original_form": "와 강아지 나이가 많네요",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003573.1.12",
          "form": "오래 건강하게 키우셨나봐요",
          "original_form": "오래 건강하게 키우셨나봐요",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003573.1.13",
          "form": "애기가 시고르자브종이라",
          "original_form": "애기가 시고르자브종이라",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003573.1.14",
          "form": "튼튼해요 ㅋㅋ!!",
          "original_form": "튼튼해요 ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003573.1.15",
          "form": "햄스터는 사료로 뭘 먹나요?",
          "original_form": "햄스터는 사료로 뭘 먹나요?",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003573.1.16",
          "form": "햄스터 전용 사료가 있는데",
          "original_form": "햄스터 전용 사료가 있는데",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003573.1.17",
          "form": "거기 알곡들이랑 해바라기씨 같은게 들어있어요",
          "original_form": "거기 알곡들이랑 해바라기씨 같은게 들어있어요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003573.1.18",
          "form": "밀웜도 좋아해요",
          "original_form": "밀웜도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003573.1.19",
          "form": "우와 밀웜 !!!",
          "original_form": "우와 밀웜 !!!",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003573.1.20",
          "form": "햄스터는 야행성이라",
          "original_form": "햄스터는 야행성이라",
          "speaker_id": "1",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003573.1.21",
          "form": "밤에 시끄럽진않나요?",
          "original_form": "밤에 시끄럽진않나요?",
          "speaker_id": "1",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003573.1.22",
          "form": "다른 방에 두고 키우는 편이라 괜찮더라구요",
          "original_form": "다른 방에 두고 키우는 편이라 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003573.1.23",
          "form": "딱히 우는것도 아니고 쳇바퀴 소리정도?",
          "original_form": "딱히 우는것도 아니고 쳇바퀴 소리정도?",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.24",
          "form": "그것도 심한게 아니라서",
          "original_form": "그것도 심한게 아니라서",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.25",
          "form": "아 쳇바퀴소리만 나는군요",
          "original_form": "아 쳇바퀴소리만 나는군요",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.26",
          "form": "무슨색이에요?",
          "original_form": "무슨색이에요?",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.27",
          "form": "햄스터도 색이 엄청 다양하던데!",
          "original_form": "햄스터도 색이 엄청 다양하던데!",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.28",
          "form": "약간 회색흰색 섞인 색이에요!",
          "original_form": "약간 회색흰색 섞인 색이에요!",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.29",
          "form": "헤테르그레이라고",
          "original_form": "헤테르그레이라고",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003573.1.30",
          "form": "우와 검색해볼게요",
          "original_form": "우와 검색해볼게요",
          "speaker_id": "1",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003573.1.31",
          "form": "귀엽겠다 ㅠㅠ",
          "original_form": "귀엽겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003573.1.32",
          "form": "완전 귀여워요!!",
          "original_form": "완전 귀여워요!!",
          "speaker_id": "2",
          "time": "20210821 02:30"
        }
      ]
    }
