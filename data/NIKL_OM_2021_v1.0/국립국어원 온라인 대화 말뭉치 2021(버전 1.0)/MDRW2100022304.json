      "utterance": [
        {
          "id": "MDRW2100022304.1.1",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022304.1.2",
          "form": "받으셨나요",
          "original_form": "받으셨나요",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022304.1.3",
          "form": "저는 내년 예정이에요!",
          "original_form": "저는 내년 예정이에요!",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022304.1.4",
          "form": "아 올해는 안받으시구요",
          "original_form": "아 올해는 안받으시구요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022304.1.5",
          "form": "저는 받으라고 문자오긴했는데",
          "original_form": "저는 받으라고 문자오긴했는데",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022304.1.6",
          "form": "계속 일정이 틀어지네요",
          "original_form": "계속 일정이 틀어지네요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022304.1.7",
          "form": "혹시 어디 아픈곳 있으신가요",
          "original_form": "혹시 어디 아픈곳 있으신가요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022304.1.8",
          "form": "아직은 없는데 곧 스케일링을 받으러 가는 건 걱정이네요...ㅎ",
          "original_form": "아직은 없는데 곧 스케일링을 받으러 가는 건 걱정이네요...ㅎ",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022304.1.9",
          "form": "스케일링 어때요",
          "original_form": "스케일링 어때요",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022304.1.10",
          "form": "저 첨받을 때 너무아파서",
          "original_form": "저 첨받을 때 너무아파서",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022304.1.11",
          "form": "울었어요",
          "original_form": "울었어요",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022304.1.12",
          "form": "ㅜㅜㅜ 사람마다 아픈 정도가 차이가 있어서ㅠㅠ 저는 참을만 했어요!",
          "original_form": "ㅜㅜㅜ 사람마다 아픈 정도가 차이가 있어서ㅠㅠ 저는 참을만 했어요!",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022304.1.13",
          "form": "약간 받고나면 개운하긴한데",
          "original_form": "약간 받고나면 개운하긴한데",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022304.1.14",
          "form": "치아사이사이가 어색하지않나요",
          "original_form": "치아사이사이가 어색하지않나요",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022304.1.15",
          "form": "맞아요! 음식을 씹을 때도 불편하구요...",
          "original_form": "맞아요! 음식을 씹을 때도 불편하구요...",
          "speaker_id": "1",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022304.1.16",
          "form": "그걸 매년반복할 때 마다 적응이 안되더라구오ㅋㅋ",
          "original_form": "그걸 매년반복할 때 마다 적응이 안되더라구오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022304.1.17",
          "form": "치과 소리 기구소리 치과냄새",
          "original_form": "치과 소리 기구소리 치과냄새",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022304.1.18",
          "form": "언제가도.. 적응이 안되는거같아요",
          "original_form": "언제가도.. 적응이 안되는거같아요",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022304.1.19",
          "form": "그 의자에 누우면 약간 너무 무서운 기분이...ㅠㅠㅠ",
          "original_form": "그 의자에 누우면 약간 너무 무서운 기분이...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022304.1.20",
          "form": "그 머리위로 쏘는 빛도",
          "original_form": "그 머리위로 쏘는 빛도",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022304.1.21",
          "form": "약간 무섭.. ㅎ",
          "original_form": "약간 무섭.. ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022304.1.22",
          "form": "딱 누우면 별생각 다 들잖아요ㅋㅋ",
          "original_form": "딱 누우면 별생각 다 들잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022304.1.23",
          "form": "눈 가리시잖아요....입만 뚫린 천으로ㅠㅠㅠㅠ",
          "original_form": "눈 가리시잖아요....입만 뚫린 천으로ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022304.1.24",
          "form": "맞아요....ㅋㅋㅋㅋ 안보여서 더 무섭",
          "original_form": "맞아요....ㅋㅋㅋㅋ 안보여서 더 무섭",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022304.1.25",
          "form": "근데 생각해보면 눈가리는게 나은거같기도하고",
          "original_form": "근데 생각해보면 눈가리는게 나은거같기도하고",
          "speaker_id": "2",
          "time": "20210829 14:11"
        }
      ]
    }
  ]
