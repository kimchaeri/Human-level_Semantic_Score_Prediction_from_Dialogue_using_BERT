      "utterance": [
        {
          "id": "MDRW2100035745.1.1",
          "form": "친구들이랑은 보통맛 먹어요!",
          "original_form": "친구들이랑은 보통맛 먹어요!",
          "speaker_id": "1",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100035745.1.2",
          "form": "오 매운거 잘 드시네요 전 보통 덜매운맛으로 먹는데",
          "original_form": "오 매운거 잘 드시네요 전 보통 덜매운맛으로 먹는데",
          "speaker_id": "2",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100035745.1.3",
          "form": "저는 착한맛도 먹어봤는데 ㅋㅋ 어느정돈 매워야 맛있더라구요",
          "original_form": "저는 착한맛도 먹어봤는데 ㅋㅋ 어느정돈 매워야 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100035745.1.4",
          "form": "맞아요 너무 매운 맛이면 오히려 엽떡 맛이 안나더라고요",
          "original_form": "맞아요 너무 매운 맛이면 오히려 엽떡 맛이 안나더라고요",
          "speaker_id": "2",
          "time": "20210830 00:15"
        },
        {
          "id": "MDRW2100035745.1.5",
          "form": "맞아요 !!",
          "original_form": "맞아요 !!",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100035745.1.6",
          "form": "응떡도 드셔보셨어요?",
          "original_form": "응떡도 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035745.1.7",
          "form": "응떡도 드셔보셨나요?",
          "original_form": "응떡도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100035745.1.8",
          "form": "네 응떡도 좋아해요",
          "original_form": "네 응떡도 좋아해요",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035745.1.9",
          "form": "치즈가 맛있지않나요ㅎㅎ",
          "original_form": "치즈가 맛있지않나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100035745.1.10",
          "form": "맞아요 치즈가 기본제공이 아닌게 아쉬울 정도로 치즈맛집이죠!!",
          "original_form": "맞아요 치즈가 기본제공이 아닌게 아쉬울 정도로 치즈맛집이죠!!",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100035745.1.11",
          "form": "맞아요! 치즈가 3000원이죠 ?",
          "original_form": "맞아요! 치즈가 3000원이죠 ?",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100035745.1.12",
          "form": "네 약간 비싸긴하죠ㅠㅠ",
          "original_form": "네 약간 비싸긴하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100035745.1.13",
          "form": "응떡 로제는 맛없다던데 ㅜㅜ",
          "original_form": "응떡 로제는 맛없다던데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100035745.1.14",
          "form": "그래요?제가 아직 응떡 로제는 못먹어봤어요",
          "original_form": "그래요?제가 아직 응떡 로제는 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100035745.1.15",
          "form": "안먹었어요 ㅎㅎ",
          "original_form": "안먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100035745.1.16",
          "form": "배떡도 좋아하세요?",
          "original_form": "배떡도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100035745.1.17",
          "form": "네 배떡도 좋아해요",
          "original_form": "네 배떡도 좋아해요",
          "speaker_id": "2",
          "time": "20210830 00:43"
        }
      ]
