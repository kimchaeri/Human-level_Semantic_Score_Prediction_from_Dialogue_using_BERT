      "utterance": [
        {
          "id": "MDRW2100024433.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100024433.1.2",
          "form": "게임 좋아하세요'?",
          "original_form": "게임 좋아하세요'?",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100024433.1.3",
          "form": "아니요 별로요",
          "original_form": "아니요 별로요",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100024433.1.4",
          "form": "게임 하는거 있나요?",
          "original_form": "게임 하는거 있나요?",
          "speaker_id": "2",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024433.1.5",
          "form": "pc게임은 거의 안 하고",
          "original_form": "pc게임은 거의 안 하고",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024433.1.6",
          "form": "모바일게임은 해요",
          "original_form": "모바일게임은 해요",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024433.1.7",
          "form": "꿈의집이나 프렌즈타운 같은 거요",
          "original_form": "꿈의집이나 프렌즈타운 같은 거요",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024433.1.8",
          "form": "어떤거요",
          "original_form": "어떤거요",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024433.1.9",
          "form": "아ㅡㅡ 들어 본거 같긴 해요^^",
          "original_form": "아ㅡㅡ 들어 본거 같긴 해요^^",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024433.1.10",
          "form": "그럼 어몽어스 아세요?",
          "original_form": "그럼 어몽어스 아세요?",
          "speaker_id": "1",
          "time": "20210831 09:30"
        },
        {
          "id": "MDRW2100024433.1.11",
          "form": "작년에 좀 핫했는데",
          "original_form": "작년에 좀 핫했는데",
          "speaker_id": "1",
          "time": "20210831 09:30"
        },
        {
          "id": "MDRW2100024433.1.12",
          "form": "어몽어스 들어봤는데.. 뭔진 몰라요 ㅋ",
          "original_form": "어몽어스 들어봤는데.. 뭔진 몰라요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100024433.1.13",
          "form": "무슨 캐릭터에요?",
          "original_form": "무슨 캐릭터에요?",
          "speaker_id": "2",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100024433.1.14",
          "form": "오! 캐릭터는 아니고 게임이름이 어몽어스예요",
          "original_form": "오! 캐릭터는 아니고 게임이름이 어몽어스예요",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100024433.1.15",
          "form": "마피아게임이요 ㅋㅋ",
          "original_form": "마피아게임이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:31"
        },
        {
          "id": "MDRW2100024433.1.16",
          "form": "아 게임 이름이었군요ㅡㅡ\r\n게임 케릭터 이름인 줄 알았어요 ㅋ",
          "original_form": "아 게임 이름이었군요ㅡㅡ\r\n게임 케릭터 이름인 줄 알았어요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024433.1.17",
          "form": "이거 되게 핫했던 게임인데 모르시는구나..",
          "original_form": "이거 되게 핫했던 게임인데 모르시는구나..",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100024433.1.18",
          "form": "게임엔 관심이 없어요 ㅎ",
          "original_form": "게임엔 관심이 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100024433.1.19",
          "form": "채팅도 하고 마피아게임처럼 서로 추리하고 그런ㅎㅎ",
          "original_form": "채팅도 하고 마피아게임처럼 서로 추리하고 그런ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100024433.1.20",
          "form": "아아 그러면 예전에도 게임 안 하셨어요?",
          "original_form": "아아 그러면 예전에도 게임 안 하셨어요?",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100024433.1.21",
          "form": "예전에는 PC 게임은 했는데\r\n안하게 되더라구요",
          "original_form": "예전에는 PC 게임은 했는데\r\n안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100024433.1.22",
          "form": "우리 애들이 하도 베그를 해서 뜯어 말리는 중이라서요 ㅋㅋ",
          "original_form": "우리 애들이 하도 베그를 해서 뜯어 말리는 중이라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024433.1.23",
          "form": "베그는 안하세요?",
          "original_form": "베그는 안하세요?",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024433.1.24",
          "form": "뭔지는 알아요!",
          "original_form": "뭔지는 알아요!",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100024433.1.25",
          "form": "화면이 확확 바뀌니까 어지럽더라고요ㅠ",
          "original_form": "화면이 확확 바뀌니까 어지럽더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100024433.1.26",
          "form": "총 들고 다니면서 서로 쏘고 그러는데 넘 잔인해서 하지 말라는데도 애들은 재밌다고 하네요",
          "original_form": "총 들고 다니면서 서로 쏘고 그러는데 넘 잔인해서 하지 말라는데도 애들은 재밌다고 하네요",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100024433.1.27",
          "form": "딸이도 그거 하는데 정신없고 왜 하는지 모르겠어요",
          "original_form": "딸이도 그거 하는데 정신없고 왜 하는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100024433.1.28",
          "form": "애들은 몇 살이에요?",
          "original_form": "애들은 몇 살이에요?",
          "speaker_id": "1",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100024433.1.29",
          "form": "제 동생도 좋아하더라고요....",
          "original_form": "제 동생도 좋아하더라고요....",
          "speaker_id": "1",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100024433.1.30",
          "form": "중고딩이요",
          "original_form": "중고딩이요",
          "speaker_id": "2",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100024433.1.31",
          "form": "아 게임 좋아할나이죠ㅠ",
          "original_form": "아 게임 좋아할나이죠ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        }
      ]
    }
  ]
