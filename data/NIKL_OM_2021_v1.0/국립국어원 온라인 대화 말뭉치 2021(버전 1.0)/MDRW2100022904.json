      "utterance": [
        {
          "id": "MDRW2100022904.1.1",
          "form": "요즘 춥지",
          "original_form": "요즘 춥지",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022904.1.2",
          "form": "거긴 추워?",
          "original_form": "거긴 추워?",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022904.1.3",
          "form": "여기 해지면 추워",
          "original_form": "여기 해지면 추워",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022904.1.4",
          "form": "근데 에어컨도 22도야 ㅡㅡ",
          "original_form": "근데 에어컨도 22도야 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.5",
          "form": "여긴 아직 더운데?",
          "original_form": "여긴 아직 더운데?",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.6",
          "form": "에어컨 꺼",
          "original_form": "에어컨 꺼",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.7",
          "form": "달수라 못꺼",
          "original_form": "달수라 못꺼",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.8",
          "form": "달수가 뭐야?",
          "original_form": "달수가 뭐야?",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.9",
          "form": "딸수",
          "original_form": "딸수",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.10",
          "form": "우린 기수가있고든",
          "original_form": "우린 기수가있고든",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.11",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.12",
          "form": "거기서 긴팔입어 반팔입어?",
          "original_form": "거기서 긴팔입어 반팔입어?",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022904.1.13",
          "form": "반팔",
          "original_form": "반팔",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.14",
          "form": "근데 곧 긴팔 입게해주지않을까",
          "original_form": "근데 곧 긴팔 입게해주지않을까",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.15",
          "form": "섬이라 추운가보다",
          "original_form": "섬이라 추운가보다",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.16",
          "form": "사면이 바다야 바다",
          "original_form": "사면이 바다야 바다",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.17",
          "form": "여기는 아직 더워서 벌레가 많아",
          "original_form": "여기는 아직 더워서 벌레가 많아",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.18",
          "form": "벌레 많어? 여기도 벌레많어",
          "original_form": "벌레 많어? 여기도 벌레많어",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022904.1.19",
          "form": "추운데도 벌레가 많아?",
          "original_form": "추운데도 벌레가 많아?",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.20",
          "form": "나방같은게많어 모기도아직있고",
          "original_form": "나방같은게많어 모기도아직있고",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.21",
          "form": "생활반이 춥나 에어컨땜에",
          "original_form": "생활반이 춥나 에어컨땜에",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.22",
          "form": "에어컨을 틀면 춥고 끄면 덥더리",
          "original_form": "에어컨을 틀면 춥고 끄면 덥더리",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.23",
          "form": "라",
          "original_form": "라",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.24",
          "form": "너도그래?",
          "original_form": "너도그래?",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022904.1.25",
          "form": "거기는 그럼 에어컨 못꺼?",
          "original_form": "거기는 그럼 에어컨 못꺼?",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.26",
          "form": "끌수있어 근데 허락받어야돼 ㅋ ㅋ 선임핮테 ㅋ ㅋ",
          "original_form": "끌수있어 근데 허락받어야돼 ㅋ ㅋ 선임핮테 ㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.27",
          "form": "허락 받고 꺼",
          "original_form": "허락 받고 꺼",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.28",
          "form": "추우면 감기걸려",
          "original_form": "추우면 감기걸려",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.29",
          "form": "싫어싫어",
          "original_form": "싫어싫어",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.30",
          "form": "왜 춥다며",
          "original_form": "왜 춥다며",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022904.1.31",
          "form": "감기는 이미 걸린거같애",
          "original_form": "감기는 이미 걸린거같애",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022904.1.32",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022904.1.33",
          "form": "어쩔수없지 군대라는게",
          "original_form": "어쩔수없지 군대라는게",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022904.1.34",
          "form": "선임들 따라야지 에효",
          "original_form": "선임들 따라야지 에효",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022904.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:04"
        }
      ]
