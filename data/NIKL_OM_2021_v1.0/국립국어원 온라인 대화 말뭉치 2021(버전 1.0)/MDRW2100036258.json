      "utterance": [
        {
          "id": "MDRW2100036258.1.1",
          "form": "오 소화에도 좋아요?",
          "original_form": "오 소화에도 좋아요?",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036258.1.2",
          "form": "저도 마셔야겠어요 !",
          "original_form": "저도 마셔야겠어요 !",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036258.1.3",
          "form": "네 소화에도 좋고 불면증에도 효과있대요ㅎㅎ",
          "original_form": "네 소화에도 좋고 불면증에도 효과있대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036258.1.4",
          "form": "오 ㅎㅎ 제가 더 정보를 얻어가네요!",
          "original_form": "오 ㅎㅎ 제가 더 정보를 얻어가네요!",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036258.1.5",
          "form": "감사합니다 ㅎㅎㅎ",
          "original_form": "감사합니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036258.1.6",
          "form": "낼 마트가서 하나 사야겠어요!",
          "original_form": "낼 마트가서 하나 사야겠어요!",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036258.1.7",
          "form": "ㅎㅎ 네 저도 내일 마트가서 국화차랑 과일 홍차 좀 사 와야겠어요ㅎㅎ",
          "original_form": "ㅎㅎ 네 저도 내일 마트가서 국화차랑 과일 홍차 좀 사 와야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036258.1.8",
          "form": "과일은 어떤거 사실껀가요 ㅎㅎ ?",
          "original_form": "과일은 어떤거 사실껀가요 ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036258.1.9",
          "form": "청사과랑 자두 사고싶은데 일단 마트가보고 포도 있음 포도도 살까해요ㅎㅎ",
          "original_form": "청사과랑 자두 사고싶은데 일단 마트가보고 포도 있음 포도도 살까해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100036258.1.10",
          "form": "아 요즘 포도 맛있던데 ! 청사과도요 !",
          "original_form": "아 요즘 포도 맛있던데 ! 청사과도요 !",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100036258.1.11",
          "form": "자두도 좋아하는데 요즘 있는지 모르겠네요 ㅎㅎ",
          "original_form": "자두도 좋아하는데 요즘 있는지 모르겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100036258.1.12",
          "form": "저희집 근처에 자두 농장이 있어서 가을자두 자주 사먹요ㅎㅎ",
          "original_form": "저희집 근처에 자두 농장이 있어서 가을자두 자주 사먹요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100036258.1.13",
          "form": "헐 자두 농장 대박이네요!",
          "original_form": "헐 자두 농장 대박이네요!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036258.1.14",
          "form": "가을에도 자두가 재배되나요??",
          "original_form": "가을에도 자두가 재배되나요??",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036258.1.15",
          "form": "가을에 출하되는 자두가 있더라구요 여름자두에 비해 씨가 작고 과육이 좀 더 단단해요ㅎㅎ 전 여름 자두보다 가을 자두를 더 좋아해요ㅎㅎ",
          "original_form": "가을에 출하되는 자두가 있더라구요 여름자두에 비해 씨가 작고 과육이 좀 더 단단해요ㅎㅎ 전 여름 자두보다 가을 자두를 더 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100036258.1.16",
          "form": "오오 막 인터넷으로도 살 수 있겠죠 ??!",
          "original_form": "오오 막 인터넷으로도 살 수 있겠죠 ??!",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100036258.1.17",
          "form": "인터넷에도 있지 않을까요? 인터넷엔 없는게 없으니ㅎㅎ",
          "original_form": "인터넷에도 있지 않을까요? 인터넷엔 없는게 없으니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036258.1.18",
          "form": "오 검색하니까",
          "original_form": "오 검색하니까",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036258.1.19",
          "form": "이만원정도하네요 !",
          "original_form": "이만원정도하네요 !",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036258.1.20",
          "form": "드셔보세요 2-3일 정도 숙성시켜 먹으면 새콤달콤 맛있어요ㅎㅎ",
          "original_form": "드셔보세요 2-3일 정도 숙성시켜 먹으면 새콤달콤 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036258.1.21",
          "form": "넵 ! 제가 또 과일을 엄청 좋아해서 ㅎㅎ",
          "original_form": "넵 ! 제가 또 과일을 엄청 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:42"
        }
      ]
    }
  ]
