      "utterance": [
        {
          "id": "MDRW2100027874.1.1",
          "form": "학교어떰",
          "original_form": "학교어떰",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100027874.1.2",
          "form": "난 이틀밖에 안됐는데 괴롭다ㅋㅋㅋㄱㄱㄲ",
          "original_form": "난 이틀밖에 안됐는데 괴롭다ㅋㅋㅋㄱㄱㄲ",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100027874.1.3",
          "form": "난 안나가서 아직 잘 모르겠음.",
          "original_form": "난 안나가서 아직 잘 모르겠음.",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027874.1.4",
          "form": "언제 감",
          "original_form": "언제 감",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027874.1.5",
          "form": "내일 대면수업인데 오티라 30분만 하고 들어올듯.",
          "original_form": "내일 대면수업인데 오티라 30분만 하고 들어올듯.",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027874.1.6",
          "form": "난 오늘 오티여서 학교 걍 안 갔음ㅋㅋㄱㄲ",
          "original_form": "난 오늘 오티여서 학교 걍 안 갔음ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027874.1.7",
          "form": "오버워치나 한판 때리고 있었다",
          "original_form": "오버워치나 한판 때리고 있었다",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027874.1.8",
          "form": "나도 안가고싶은데 오티도 출첵함",
          "original_form": "나도 안가고싶은데 오티도 출첵함",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027874.1.9",
          "form": "와 겁나 너무하다",
          "original_form": "와 겁나 너무하다",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027874.1.10",
          "form": "심지어 1교시",
          "original_form": "심지어 1교시",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100027874.1.11",
          "form": "이야... 나였다면 걍 잔다고 못갔다...",
          "original_form": "이야... 나였다면 걍 잔다고 못갔다...",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100027874.1.12",
          "form": "실험복 안가져와서",
          "original_form": "실험복 안가져와서",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027874.1.13",
          "form": "빌려야한다",
          "original_form": "빌려야한다",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027874.1.14",
          "form": "실험복도 따로 줘??",
          "original_form": "실험복도 따로 줘??",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100027874.1.15",
          "form": "아니 내가 사야해",
          "original_form": "아니 내가 사야해",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100027874.1.16",
          "form": "그래서 집에 있는데",
          "original_form": "그래서 집에 있는데",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100027874.1.17",
          "form": "안가져왔어",
          "original_form": "안가져왔어",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
