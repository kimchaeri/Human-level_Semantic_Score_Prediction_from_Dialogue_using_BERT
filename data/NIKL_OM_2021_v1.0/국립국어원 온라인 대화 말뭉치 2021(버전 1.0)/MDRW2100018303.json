        },
        {
          "id": "MDRW2100018303.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018303.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018303.1.4",
          "form": "저는 직장다녀요ㅎㅎ",
          "original_form": "저는 직장다녀요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018303.1.5",
          "form": "졸업한지 2년째예요",
          "original_form": "졸업한지 2년째예요",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018303.1.6",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018303.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.8",
          "form": "동아리 해보셨나요?",
          "original_form": "동아리 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.9",
          "form": "동아리 하루 나가고 안나갔어요ㅋㅋㅋ",
          "original_form": "동아리 하루 나가고 안나갔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.10",
          "form": "어떤 동아리였나요?",
          "original_form": "어떤 동아리였나요?",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.11",
          "form": "다들 아는사람들끼리 모여있더라고요ㅠㅠ",
          "original_form": "다들 아는사람들끼리 모여있더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.12",
          "form": "사진동아리였어요",
          "original_form": "사진동아리였어요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018303.1.13",
          "form": "혼자 들어가셨던건가요?",
          "original_form": "혼자 들어가셨던건가요?",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018303.1.14",
          "form": "네네 궁금해서 들어갔다가",
          "original_form": "네네 궁금해서 들어갔다가",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018303.1.15",
          "form": "너무 아는사람들끼리만 뭉쳐서",
          "original_form": "너무 아는사람들끼리만 뭉쳐서",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018303.1.16",
          "form": "탈주했어요..",
          "original_form": "탈주했어요..",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018303.1.17",
          "form": "아...그럼 그 후로 동아리는 안 들어가신건가요?",
          "original_form": "아...그럼 그 후로 동아리는 안 들어가신건가요?",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018303.1.18",
          "form": "ㅋㅋㅋ네네",
          "original_form": "ㅋㅋㅋ네네",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018303.1.19",
          "form": "통학하기 바빠서 힘들도라구요",
          "original_form": "통학하기 바빠서 힘들도라구요",
          "speaker_id": "2",
          "time": "20210827 10:54"
        },
