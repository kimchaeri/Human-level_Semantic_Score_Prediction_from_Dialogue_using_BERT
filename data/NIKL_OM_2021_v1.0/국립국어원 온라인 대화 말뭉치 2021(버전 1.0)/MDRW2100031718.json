        {
          "id": "MDRW2100031718.1.1",
          "form": "타이레놀 드셨어요?",
          "original_form": "타이레놀 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031718.1.2",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031718.1.3",
          "form": "약 잘 들던가요??",
          "original_form": "약 잘 들던가요??",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031718.1.4",
          "form": "네 약 먹으니까 팔 괜찮더라고요",
          "original_form": "네 약 먹으니까 팔 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031718.1.5",
          "form": "오 저는 먹어도 뻐근해서 한",
          "original_form": "오 저는 먹어도 뻐근해서 한",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.6",
          "form": "3일고생했어요",
          "original_form": "3일고생했어요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.7",
          "form": "아이고 고생하셨네요",
          "original_form": "아이고 고생하셨네요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.8",
          "form": "원래 몸이 좀 약하세요 ?",
          "original_form": "원래 몸이 좀 약하세요 ?",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.9",
          "form": "어 약간 그렇기도해요!",
          "original_form": "어 약간 그렇기도해요!",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.10",
          "form": "2차가 조금",
          "original_form": "2차가 조금",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.11",
          "form": "걱정되네요ㅠ",
          "original_form": "걱정되네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031718.1.12",
          "form": "2차 전까지 꾸준히 운동한번 해봐요",
          "original_form": "2차 전까지 꾸준히 운동한번 해봐요",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031718.1.13",
          "form": "오 name2님은 운동하세요?",
          "original_form": "오 &name2&님은 운동하세요?",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031718.1.14",
          "form": "네 많이 걷습니다",
          "original_form": "네 많이 걷습니다",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031718.1.15",
          "form": "걷기가 건강에 참 좋죠!!",
          "original_form": "걷기가 건강에 참 좋죠!!",
