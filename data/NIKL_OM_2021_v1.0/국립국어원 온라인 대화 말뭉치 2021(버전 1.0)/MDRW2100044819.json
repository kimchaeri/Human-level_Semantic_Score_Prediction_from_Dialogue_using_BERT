      "utterance": [
        {
          "id": "MDRW2100044819.1.1",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044819.1.2",
          "form": "설마 인구가 많다고 분포까지 많아서",
          "original_form": "설마 인구가 많다고 분포까지 많아서",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044819.1.3",
          "form": "계속 하는건 아니시죠?",
          "original_form": "계속 하는건 아니시죠?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044819.1.4",
          "form": "하셔야죠 은퇴..그래야 취업이..되죠..",
          "original_form": "하셔야죠 은퇴..그래야 취업이..되죠..",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044819.1.5",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044819.1.6",
          "form": "저도 좀 널널하게 일해봅시다...",
          "original_form": "저도 좀 널널하게 일해봅시다...",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044819.1.7",
          "form": "ㅠㅠㅠ 박터집니다.. 위가 안비키니까 갈길이 없습니다...",
          "original_form": "ㅠㅠㅠ 박터집니다.. 위가 안비키니까 갈길이 없습니다...",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044819.1.8",
          "form": "머 은퇴 안하셔도 취업 어려울것 같지만 핑계라도 대자",
          "original_form": "머 은퇴 안하셔도 취업 어려울것 같지만 핑계라도 대자",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044819.1.9",
          "form": "아냐 부모님 세대는 졸업장만 잇음 됐다고 했는걸 ㅜ",
          "original_form": "아냐 부모님 세대는 졸업장만 잇음 됐다고 했는걸 ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044819.1.10",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044819.1.11",
          "form": "나도 졸업장만 있으면 모셔가는 인재이고 싶다",
          "original_form": "나도 졸업장만 있으면 모셔가는 인재이고 싶다",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044819.1.12",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044819.1.13",
          "form": "나도 기업 골라가고싶다...",
          "original_form": "나도 기업 골라가고싶다...",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044819.1.14",
          "form": "옆동네 일본이 그렇다고 그랬는데(요즘)",
          "original_form": "옆동네 일본이 그렇다고 그랬는데(요즘)",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044819.1.15",
          "form": "우린 앞으로 40년은 더 있어야 하나요",
          "original_form": "우린 앞으로 40년은 더 있어야 하나요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044819.1.16",
          "form": "ㅇㅇ 나도 얘기 들음 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 나도 얘기 들음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044819.1.17",
          "form": "ㅋ큐ㅠ",
          "original_form": "ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044819.1.18",
          "form": "아냐 일본은 우리나라 10년 뒤래",
          "original_form": "아냐 일본은 우리나라 10년 뒤래",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044819.1.19",
          "form": "일본가서 취업할수도 없고",
          "original_form": "일본가서 취업할수도 없고",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.20",
          "form": "앞으로 10년 뒤에도 구직하고 있으면",
          "original_form": "앞으로 10년 뒤에도 구직하고 있으면",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.21",
          "form": "일단 엄빠가 날 내쫓을듯",
          "original_form": "일단 엄빠가 날 내쫓을듯",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.22",
          "form": "실제로도 우리나라에서 일본가서 취직하는 사람 꽤 잇댓음",
          "original_form": "실제로도 우리나라에서 일본가서 취직하는 사람 꽤 잇댓음",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.24",
          "form": "ㅇㅇ 일본에서 외국인까지 모셔가더라",
          "original_form": "ㅇㅇ 일본에서 외국인까지 모셔가더라",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044819.1.25",
          "form": "내국인도 모자라서",
          "original_form": "내국인도 모자라서",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044819.1.26",
          "form": "그치만 일본까지 가서 취업을..해야하는가..?",
          "original_form": "그치만 일본까지 가서 취업을..해야하는가..?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044819.1.27",
          "form": "거긴... 인형회사가 하나뿐이라구..!(이게 아님",
          "original_form": "거긴... 인형회사가 하나뿐이라구..!(이게 아님",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044819.1.28",
          "form": "아닠 그게 핵심이냐구 ㅋㅋㅋㅋ",
          "original_form": "아닠 그게 핵심이냐구 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044819.1.29",
          "form": "일본이 인형회사 더 많을거같은데",
          "original_form": "일본이 인형회사 더 많을거같은데",
