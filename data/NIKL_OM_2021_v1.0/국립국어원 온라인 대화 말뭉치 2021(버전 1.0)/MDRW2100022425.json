      "utterance": [
        {
          "id": "MDRW2100022425.1.1",
          "form": "자주가는 카페 있으세요?",
          "original_form": "자주가는 카페 있으세요?",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022425.1.2",
          "form": "저는 학교 앞에 스타벅스를 가는편인데",
          "original_form": "저는 학교 앞에 스타벅스를 가는편인데",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022425.1.3",
          "form": "꽉 차면 옆에 있는",
          "original_form": "꽉 차면 옆에 있는",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022425.1.4",
          "form": "파세쿠치인가?",
          "original_form": "파세쿠치인가?",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022425.1.5",
          "form": "거기를 가는 편이에요",
          "original_form": "거기를 가는 편이에요",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022425.1.6",
          "form": "오 저도 스벅자주가요",
          "original_form": "오 저도 스벅자주가요",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022425.1.7",
          "form": "뭐 드세요?",
          "original_form": "뭐 드세요?",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022425.1.8",
          "form": "저는 음료류는 별로 안 좋아해서",
          "original_form": "저는 음료류는 별로 안 좋아해서",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022425.1.9",
          "form": "아아만 보통 마십니다",
          "original_form": "아아만 보통 마십니다",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022425.1.10",
          "form": "하나 시키고 공부하고 책 읽어요",
          "original_form": "하나 시키고 공부하고 책 읽어요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022425.1.11",
          "form": "학교 앞이라서 사실상 스카에요 ㅋㅋㅋ",
          "original_form": "학교 앞이라서 사실상 스카에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022425.1.12",
          "form": "헉 저도 아아 많이 먹어요 ㅋㅋㅋ",
          "original_form": "헉 저도 아아 많이 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022425.1.13",
          "form": "스벅이 무난하더라고요 맛이",
          "original_form": "스벅이 무난하더라고요 맛이",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100022425.1.14",
          "form": "분위기도 제일 깔끔하고",
          "original_form": "분위기도 제일 깔끔하고",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100022425.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022425.1.16",
          "form": "가격이야 다른 브랜드도 다 똑같아서",
          "original_form": "가격이야 다른 브랜드도 다 똑같아서",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022425.1.17",
          "form": "공부하기도 좋고 눈치안주고",
          "original_form": "공부하기도 좋고 눈치안주고",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022425.1.18",
          "form": "저도 스벅 위주로 가는 편이에요 ㅋㅋㅋ",
          "original_form": "저도 스벅 위주로 가는 편이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022425.1.19",
          "form": "쿠폰도 자주 뿌리더라고요 스벅이 ㅋㅋ",
          "original_form": "쿠폰도 자주 뿌리더라고요 스벅이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022425.1.20",
          "form": "맞아요 ㅋㅋㅋ 별 같은 것도 적립할 수 있고",
          "original_form": "맞아요 ㅋㅋㅋ 별 같은 것도 적립할 수 있고",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022425.1.21",
          "form": "텀블러도  이쁘고요",
          "original_form": "텀블러도  이쁘고요",
          "speaker_id": "1",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022425.1.22",
          "form": "친구들끼리 제일 많이 주고 받는 것도 스벅깊콘이고",
          "original_form": "친구들끼리 제일 많이 주고 받는 것도 스벅깊콘이고",
          "speaker_id": "2",
          "time": "20210829 13:56"
        }
      ]
