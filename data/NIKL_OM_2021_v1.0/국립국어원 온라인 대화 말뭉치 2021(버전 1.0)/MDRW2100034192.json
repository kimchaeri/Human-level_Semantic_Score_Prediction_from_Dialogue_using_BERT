      "utterance": [
        {
          "id": "MDRW2100034192.1.1",
          "form": "떡볶이 좋아하시나봐용",
          "original_form": "떡볶이 좋아하시나봐용",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034192.1.2",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034192.1.3",
          "form": "좋아하세요??",
          "original_form": "좋아하세요??",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034192.1.4",
          "form": "네 저도 넘 즇아하죠",
          "original_form": "네 저도 넘 즇아하죠",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034192.1.5",
          "form": "로제는 안먹어봤어요",
          "original_form": "로제는 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034192.1.6",
          "form": "크림이나 로제 좋아하시면 드셔보세요 ㅎㅎ",
          "original_form": "크림이나 로제 좋아하시면 드셔보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034192.1.7",
          "form": "도전해봐야겠어요",
          "original_form": "도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034192.1.8",
          "form": "어디 로제가 맛나나요",
          "original_form": "어디 로제가 맛나나요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034192.1.9",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034192.1.10",
          "form": "신전이 좋더라구여!",
          "original_form": "신전이 좋더라구여!",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034192.1.11",
          "form": "오 신전 로제 적어둬야지",
          "original_form": "오 신전 로제 적어둬야지",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034192.1.12",
          "form": "배떡도 드셔보숐나요?",
          "original_form": "배떡도 드셔보숐나요?",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034192.1.13",
          "form": "당연하져 그게 젤 인기 많더라구요 ㅋㅋㅋ",
          "original_form": "당연하져 그게 젤 인기 많더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100034192.1.14",
          "form": "오오 그래요 ?",
          "original_form": "오오 그래요 ?",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034192.1.15",
          "form": "저는 시장 떡볶이 좋아해요ㅠ",
          "original_form": "저는 시장 떡볶이 좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034192.1.16",
          "form": "아니면 백종원 레시피",
          "original_form": "아니면 백종원 레시피",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034192.1.17",
          "form": "시장떡볶이 맛없없이죠ㅠㅠㅠ",
          "original_form": "시장떡볶이 맛없없이죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034192.1.18",
          "form": "백종원도 믿고 먹는ㅋㅋㅋ",
          "original_form": "백종원도 믿고 먹는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034192.1.19",
          "form": "낼 먹어야겠어요",
          "original_form": "낼 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100034192.1.20",
          "form": "얘기하니 급 땡겨요 저도 ㅠㅠ",
          "original_form": "얘기하니 급 땡겨요 저도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034192.1.21",
          "form": "참았다가 낼 먹어요 저희",
          "original_form": "참았다가 낼 먹어요 저희",
          "speaker_id": "1",
          "time": "20210827 21:24"
        }
      ]
