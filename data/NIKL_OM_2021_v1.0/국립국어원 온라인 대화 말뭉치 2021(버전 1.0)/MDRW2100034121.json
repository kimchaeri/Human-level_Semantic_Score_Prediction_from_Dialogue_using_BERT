      "utterance": [
        {
          "id": "MDRW2100034121.1.1",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100034121.1.2",
          "form": "지금은 안하고 있고 가끔 몸이 무겁다느껴지면 해요",
          "original_form": "지금은 안하고 있고 가끔 몸이 무겁다느껴지면 해요",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100034121.1.3",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100034121.1.4",
          "form": "저는 귀차니즘이 강해서 항상 다이어트 작심일일로 끝내요ㅋㅋㅋㅋ",
          "original_form": "저는 귀차니즘이 강해서 항상 다이어트 작심일일로 끝내요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100034121.1.5",
          "form": "앜ㅋㅋㅋㅋ다이어트 힘들죠 귀찮기도하구요",
          "original_form": "앜ㅋㅋㅋㅋ다이어트 힘들죠 귀찮기도하구요",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100034121.1.6",
          "form": "운동은 하세요?",
          "original_form": "운동은 하세요?",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100034121.1.7",
          "form": "산책이랑 플랭크는 꾸준히 하려고 하는 편이에요!",
          "original_form": "산책이랑 플랭크는 꾸준히 하려고 하는 편이에요!",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100034121.1.8",
          "form": "요즘 비와서 산책하기 힘들지만요ㅠ",
          "original_form": "요즘 비와서 산책하기 힘들지만요ㅠ",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100034121.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100034121.1.10",
          "form": "저는 링피트로 조금씩 운동하고있어요",
          "original_form": "저는 링피트로 조금씩 운동하고있어요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034121.1.11",
          "form": "요즘 산책힘들죠ㅠ 시원은 한데...",
          "original_form": "요즘 산책힘들죠ㅠ 시원은 한데...",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034121.1.12",
          "form": "그쵸ㅠ 링피트하면 조금만해도 힘들다던데 정말인가요?",
          "original_form": "그쵸ㅠ 링피트하면 조금만해도 힘들다던데 정말인가요?",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034121.1.13",
          "form": "처음 에는 이십분만에 녹다운이였는데",
          "original_form": "처음 에는 이십분만에 녹다운이였는데",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034121.1.14",
          "form": "요즘은 삼십분하면 딱적당해요",
          "original_form": "요즘은 삼십분하면 딱적당해요",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034121.1.15",
          "form": "초반부가 힘들어요 적응하느라",
          "original_form": "초반부가 힘들어요 적응하느라",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034121.1.16",
          "form": "우와 완전 운동되겠어요",
          "original_form": "우와 완전 운동되겠어요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100034121.1.17",
          "form": "적당히 되는거 같아요",
          "original_form": "적당히 되는거 같아요",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034121.1.18",
          "form": "백신은 맞으셨나요?",
          "original_form": "백신은 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034121.1.19",
          "form": "네 저 일주일 전에 1차 맞았어요!",
          "original_form": "네 저 일주일 전에 1차 맞았어요!",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100034121.1.20",
          "form": "괜찮으셨어요?",
          "original_form": "괜찮으셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034121.1.21",
          "form": "맞고나면 어때요?",
          "original_form": "맞고나면 어때요?",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034121.1.22",
          "form": "전 아무렇지 않았어요",
          "original_form": "전 아무렇지 않았어요",
          "speaker_id": "1",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100034121.1.23",
          "form": "맞은 곳 주변까지 근육통 조금 있다가 없어졌구 다른건 딱히 못느꼈어요",
          "original_form": "맞은 곳 주변까지 근육통 조금 있다가 없어졌구 다른건 딱히 못느꼈어요",
          "speaker_id": "1",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100034121.1.24",
          "form": "정말 사바사인가봐요",
          "original_form": "정말 사바사인가봐요",
          "speaker_id": "1",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100034121.1.25",
          "form": "다행이네요! 사바사인가봐요 저는 다음주에 맞거든요",
          "original_form": "다행이네요! 사바사인가봐요 저는 다음주에 맞거든요",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034121.1.26",
          "form": "긴장돼요",
          "original_form": "긴장돼요",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034121.1.27",
          "form": "우왕 별거 아니에요",
          "original_form": "우왕 별거 아니에요",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034121.1.28",
          "form": "놔주는 사람마다 다르겠지만",
          "original_form": "놔주는 사람마다 다르겠지만",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034121.1.29",
          "form": "친구들보면 맞고나서 쉰친구들은 괜찮은데",
          "original_form": "친구들보면 맞고나서 쉰친구들은 괜찮은데",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034121.1.30",
          "form": "일한친구들은 골골댔어가지구요",
          "original_form": "일한친구들은 골골댔어가지구요",
          "speaker_id": "2",
          "time": "20210827 16:59"
        }
      ]
    }
