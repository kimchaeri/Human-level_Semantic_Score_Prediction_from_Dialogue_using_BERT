      "utterance": [
        {
          "id": "MDRW2100038434.1.1",
          "form": "날씨가 참... 그렇죠",
          "original_form": "날씨가 참... 그렇죠",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038434.1.2",
          "form": "많이 추워지기도 했구요",
          "original_form": "많이 추워지기도 했구요",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038434.1.3",
          "form": "추우시구나ㅜ 전 시원하게 딱좋은데요",
          "original_form": "추우시구나ㅜ 전 시원하게 딱좋은데요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038434.1.4",
          "form": "날이 흐려 그렇긴하네요",
          "original_form": "날이 흐려 그렇긴하네요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038434.1.5",
          "form": "아직 긴옷을 준비 못했어요",
          "original_form": "아직 긴옷을 준비 못했어요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038434.1.6",
          "form": "장마 끝나고 나면 좀 괜찮을거 같은데",
          "original_form": "장마 끝나고 나면 좀 괜찮을거 같은데",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038434.1.7",
          "form": "그렇죠 슬슬ㅈ가을옷도 준비해야되네요",
          "original_form": "그렇죠 슬슬ㅈ가을옷도 준비해야되네요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100038434.1.8",
          "form": "겉옷은 많이 꺼내놨어요",
          "original_form": "겉옷은 많이 꺼내놨어요",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100038434.1.9",
          "form": "아ㅎㅎ 내일부터는 비가 안올것같긴한데 날이좀 좋아지면 좋겠네요",
          "original_form": "아ㅎㅎ 내일부터는 비가 안올것같긴한데 날이좀 좋아지면 좋겠네요",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038434.1.10",
          "form": "맞아요 너무 한번에 추워졌죠",
          "original_form": "맞아요 너무 한번에 추워졌죠",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038434.1.11",
          "form": "그래도 미세먼지없으니 좋네요",
          "original_form": "그래도 미세먼지없으니 좋네요",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100038434.1.12",
          "form": "하지만 마스크는 계속 써야한다는게ㅠ",
          "original_form": "하지만 마스크는 계속 써야한다는게ㅠ",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038434.1.13",
          "form": "그건그렇죠 코로나 종식되어야되니ㅜ",
          "original_form": "그건그렇죠 코로나 종식되어야되니ㅜ",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038434.1.14",
          "form": "빨리 종식 됐으면 좋겠네요",
          "original_form": "빨리 종식 됐으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038434.1.15",
          "form": "백신도 다 맞아야하고",
          "original_form": "백신도 다 맞아야하고",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100038434.1.16",
          "form": "안전수칙도잘 지켜야겠죠ㅜ",
