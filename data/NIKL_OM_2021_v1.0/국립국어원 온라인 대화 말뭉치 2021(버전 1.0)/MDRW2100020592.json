      "utterance": [
        {
          "id": "MDRW2100020592.1.1",
          "form": "아니 경락",
          "original_form": "아니 경락",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020592.1.2",
          "form": "아 놀이동산 이번주 가야된다 그러지 않았나",
          "original_form": "아 놀이동산 이번주 가야된다 그러지 않았나",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020592.1.3",
          "form": "아 이번달까지ㅎ",
          "original_form": "아 이번달까지ㅎ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020592.1.4",
          "form": "아 내일 가야겠네 그럼",
          "original_form": "아 내일 가야겠네 그럼",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020592.1.5",
          "form": "재밌겠다 놀이동산!!",
          "original_form": "재밌겠다 놀이동산!!",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020592.1.6",
          "form": "응 내키는대로 할거라 내일이라 정하지않았어ㅎㅎ",
          "original_form": "응 내키는대로 할거라 내일이라 정하지않았어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020592.1.7",
          "form": "놀이동산 안간지 꽤된듯",
          "original_form": "놀이동산 안간지 꽤된듯",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020592.1.8",
          "form": "놀이동산 나도 안간지 오래됐지ㅜㅜ",
          "original_form": "놀이동산 나도 안간지 오래됐지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020592.1.9",
          "form": "오랜만에 가면 느낌 다를거같아",
          "original_form": "오랜만에 가면 느낌 다를거같아",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020592.1.10",
          "form": "ㅇㅇ더 신나고 재밌겠다!!",
          "original_form": "ㅇㅇ더 신나고 재밌겠다!!",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020592.1.11",
          "form": "까서 뽕 뽑고 와 ㅋㅋ",
          "original_form": "까서 뽕 뽑고 와 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020592.1.12",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020592.1.13",
          "form": "대구 가면 대구 맛집도 갔다옴 좋을텐데 시간상 안되겠구만",
          "original_form": "대구 가면 대구 맛집도 갔다옴 좋을텐데 시간상 안되겠구만",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020592.1.14",
          "form": "아직도 고민이긴해\n가고싶은데 가도될까ㅋ",
          "original_form": "아직도 고민이긴해\n가고싶은데 가도될까ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020592.1.15",
          "form": "대구에 맛난거많지?",
          "original_form": "대구에 맛난거많지?",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020592.1.16",
          "form": "회사서 시간대별 문자오는중",
          "original_form": "회사서 시간대별 문자오는중",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.17",
          "form": "확진이 크게 늘어서ㅋㅋ",
          "original_form": "확진이 크게 늘어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.18",
          "form": "난 놀이기구는 괜찮을듯 야외라서",
          "original_form": "난 놀이기구는 괜찮을듯 야외라서",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.19",
          "form": "손소독 열심히",
          "original_form": "손소독 열심히",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.20",
          "form": "대구야 맞난거많지ㅋ",
          "original_form": "대구야 맞난거많지ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.21",
          "form": "근데 가는 버스가 위험할거같아",
          "original_form": "근데 가는 버스가 위험할거같아",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.22",
          "form": "구미도 쩌~기 다른지역 지금 뜨는곳 맛집많다더라",
          "original_form": "구미도 쩌~기 다른지역 지금 뜨는곳 맛집많다더라",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020592.1.23",
          "form": "버스 이동만 기본 2번이라ㅎ",
          "original_form": "버스 이동만 기본 2번이라ㅎ",
