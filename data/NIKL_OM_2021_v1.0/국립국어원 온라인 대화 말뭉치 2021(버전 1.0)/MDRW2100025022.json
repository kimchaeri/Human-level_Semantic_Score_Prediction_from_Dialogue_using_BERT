      "utterance": [
        {
          "id": "MDRW2100025022.1.1",
          "form": "저는 한식을 주로하고",
          "original_form": "저는 한식을 주로하고",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.2",
          "form": "전 김치찜을 잘합니다",
          "original_form": "전 김치찜을 잘합니다",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.3",
          "form": "동생이 이거저거잘해서",
          "original_form": "동생이 이거저거잘해서",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.4",
          "form": "양식도하고",
          "original_form": "양식도하고",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.5",
          "form": "오오..",
          "original_form": "오오..",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.6",
          "form": "별거별거다해요",
          "original_form": "별거별거다해요",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.7",
          "form": "근데 저",
          "original_form": "근데 저",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.8",
          "form": "김치찜은 진짜 잘하는데",
          "original_form": "김치찜은 진짜 잘하는데",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.9",
          "form": "김치찌개는 진짜 못하거든요",
          "original_form": "김치찌개는 진짜 못하거든요",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.10",
          "form": "김치찜에 물 부으면 김치찌개 아닌가요?",
          "original_form": "김치찜에 물 부으면 김치찌개 아닌가요?",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.11",
          "form": "오 방법이있어요",
          "original_form": "오 방법이있어요",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.12",
          "form": "대체 이유가 뭘까요",
          "original_form": "대체 이유가 뭘까요",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.14",
          "form": "ㅋㅋㄱㅋㅋㅋ 김치를 처음에",
          "original_form": "ㅋㅋㄱㅋㅋㅋ 김치를 처음에",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.15",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100025022.1.16",
          "form": "참기름 반스푼 식용유반스푼",
          "original_form": "참기름 반스푼 식용유반스푼",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.17",
          "form": "넣고 볶아주신담에",
          "original_form": "넣고 볶아주신담에",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.18",
          "form": "볶는거요?ㅠㅠ",
          "original_form": "볶는거요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.19",
          "form": "그렇게 하는데두 이상해요ㅠ..",
          "original_form": "그렇게 하는데두 이상해요ㅠ..",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.20",
          "form": "파를썰어서 살짝볶아주세요",
          "original_form": "파를썰어서 살짝볶아주세요",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.21",
          "form": "파....",
          "original_form": "파....",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.22",
          "form": "그리고 고기를 넣으시면",
          "original_form": "그리고 고기를 넣으시면",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.23",
          "form": "고기넣고 볶으시고",
          "original_form": "고기넣고 볶으시고",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.24",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.25",
          "form": "처음부터 고기랑 김치를 같이 볶는 것이 패인일까요",
          "original_form": "처음부터 고기랑 김치를 같이 볶는 것이 패인일까요",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100025022.1.26",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210831 22:23"
        }
      ]
