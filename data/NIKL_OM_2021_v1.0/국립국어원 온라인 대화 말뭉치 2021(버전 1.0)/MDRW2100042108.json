      "utterance": [
        {
          "id": "MDRW2100042108.1.1",
          "form": "네 저녁 내내 비네요",
          "original_form": "네 저녁 내내 비네요",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.2",
          "form": "퇴근하셨어요?",
          "original_form": "퇴근하셨어요?",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.3",
          "form": "네 비가 많이와서 신발 다 젖었어요",
          "original_form": "네 비가 많이와서 신발 다 젖었어요",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.4",
          "form": "저는 퇴근길엔 가랑비였는데 지금은 장대비가",
          "original_form": "저는 퇴근길엔 가랑비였는데 지금은 장대비가",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.5",
          "form": "그쵸 갈수록 더 내리는거같아요",
          "original_form": "그쵸 갈수록 더 내리는거같아요",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.6",
          "form": "아 퇴근길에 비가 많이 내리셨구나ㅠ",
          "original_form": "아 퇴근길에 비가 많이 내리셨구나ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042108.1.7",
          "form": "지금은 집이신거죠?",
          "original_form": "지금은 집이신거죠?",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042108.1.8",
          "form": "이제 도착했어요",
          "original_form": "이제 도착했어요",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042108.1.9",
          "form": "거의 비가 그쳤네요",
          "original_form": "거의 비가 그쳤네요",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042108.1.10",
          "form": "빗길에 오시려면",
          "original_form": "빗길에 오시려면",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042108.1.11",
          "form": "고생하셨겠어요ㅜ",
          "original_form": "고생하셨겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042108.1.12",
          "form": "비오면 버스가 막히는게 젤 힘든거같아요",
          "original_form": "비오면 버스가 막히는게 젤 힘든거같아요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042108.1.13",
          "form": "에고 비 오는 날 버스는 축축하기도 하고 힘들죠ㅜ",
          "original_form": "에고 비 오는 날 버스는 축축하기도 하고 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042108.1.14",
          "form": "길도 막히고",
          "original_form": "길도 막히고",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042108.1.15",
          "form": "네네 안그래도 축축한데 오래걸리기까지 하니까 피곤하네요",
          "original_form": "네네 안그래도 축축한데 오래걸리기까지 하니까 피곤하네요",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042108.1.16",
          "form": "에고 비 오는 날 고생하셨어요",
          "original_form": "에고 비 오는 날 고생하셨어요",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042108.1.17",
          "form": "화요일까지 비온다는데",
          "original_form": "화요일까지 비온다는데",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042108.1.18",
          "form": "남은 날짜는 좀 맑았으면 좋겠네요",
          "original_form": "남은 날짜는 좀 맑았으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 21:13"
        }
      ]
