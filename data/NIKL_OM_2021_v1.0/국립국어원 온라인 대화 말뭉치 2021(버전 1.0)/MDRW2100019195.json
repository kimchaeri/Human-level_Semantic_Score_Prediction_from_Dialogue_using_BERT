      "utterance": [
        {
          "id": "MDRW2100019195.1.1",
          "form": "오늘은 아직 안 마셨어",
          "original_form": "오늘은 아직 안 마셨어",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019195.1.2",
          "form": "사러 갈 타이밍을 놓쳐버렸네",
          "original_form": "사러 갈 타이밍을 놓쳐버렸네",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019195.1.3",
          "form": "나도 오늘은 커피 못마심 ㅋㅋㅋ",
          "original_form": "나도 오늘은 커피 못마심 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019195.1.4",
          "form": "아침에 맥심을 마시긴 했지만 아아 먹고싶다",
          "original_form": "아침에 맥심을 마시긴 했지만 아아 먹고싶다",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019195.1.5",
          "form": "불금이니 아아대신 맥주ㅎㅎ",
          "original_form": "불금이니 아아대신 맥주ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019195.1.6",
          "form": "맥주.. 요즘 다시 길맥이 어려워져서 못마심 ㅋㅋㅋㅋ",
          "original_form": "맥주.. 요즘 다시 길맥이 어려워져서 못마심 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019195.1.7",
          "form": "길에사 맥주마시기가 힘들어서",
          "original_form": "길에사 맥주마시기가 힘들어서",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019195.1.8",
          "form": "안주만 모아놨다 ㅋㅋㅋㅋㄱ",
          "original_form": "안주만 모아놨다 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019195.1.9",
          "form": "오늘 날씨에는 길맥하기 좀..",
          "original_form": "오늘 날씨에는 길맥하기 좀..",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019195.1.10",
          "form": "요즘 페퍼로니 피자에 꽂혔어",
          "original_form": "요즘 페퍼로니 피자에 꽂혔어",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019195.1.11",
          "form": "페퍼로니는 잘 먹지도 않던건데",
          "original_form": "페퍼로니는 잘 먹지도 않던건데",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019195.1.12",
          "form": "갑자기?",
          "original_form": "갑자기?",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019195.1.13",
          "form": "누가 맛있게 먹는걸 보기라도 했어?",
          "original_form": "누가 맛있게 먹는걸 보기라도 했어?",
          "speaker_id": "1",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019195.1.14",
          "form": "그것도 있고 동생이 꽂혀서 온걸 나한테도 꽂았어ㅋㅋ 씬도우 페퍼로니가 맛있더라고",
          "original_form": "그것도 있고 동생이 꽂혀서 온걸 나한테도 꽂았어ㅋㅋ 씬도우 페퍼로니가 맛있더라고",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019195.1.15",
          "form": "그거먹고 유행한다는 피자 먹으려니 너무 무거운맛..",
          "original_form": "그거먹고 유행한다는 피자 먹으려니 너무 무거운맛..",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019195.1.16",
          "form": "씬도우는 뭐든 맛있을거야",
          "original_form": "씬도우는 뭐든 맛있을거야",
          "speaker_id": "1",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019195.1.17",
          "form": "유행한다는 피자는 뭐야?",
          "original_form": "유행한다는 피자는 뭐야?",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019195.1.18",
          "form": "동생한테 낚인거같은데ㅡㅡ청년피자에 비프..무슨 소고기 올라간거..",
          "original_form": "동생한테 낚인거같은데ㅡㅡ청년피자에 비프..무슨 소고기 올라간거..",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019195.1.19",
          "form": "청년피자릉 태연이 광고하더라고",
          "original_form": "청년피자릉 태연이 광고하더라고",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019195.1.20",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019195.1.21",
          "form": "청년피자는 들어봤는데 낚인게 맞는거같다",
          "original_form": "청년피자는 들어봤는데 낚인게 맞는거같다",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019195.1.22",
          "form": "맛있어서 그냥 넘어갔어ㅋㅋ",
          "original_form": "맛있어서 그냥 넘어갔어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:04"
        },
        {
          "id": "MDRW2100019195.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019195.1.24",
          "form": "이번주 티데이 이벤트가 도미노피자 방문포장 할인이길래 물어봤더니 다들 싫다고 해서 패스",
          "original_form": "이번주 티데이 이벤트가 도미노피자 방문포장 할인이길래 물어봤더니 다들 싫다고 해서 패스",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019195.1.25",
          "form": "요즘은 주말에 치킨달려",
          "original_form": "요즘은 주말에 치킨달려",
          "speaker_id": "1",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100019195.1.26",
          "form": "치킨은 수시로 먹어서ㅋㅋ우리집 닭띠가..",
          "original_form": "치킨은 수시로 먹어서ㅋㅋ우리집 닭띠가..",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019195.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019195.1.28",
          "form": "피자는 엄마는 좋아하는데 아빠는 많이 안드시고 치킨이 무난 ㅋㅋㅋㅋㅋ",
          "original_form": "피자는 엄마는 좋아하는데 아빠는 많이 안드시고 치킨이 무난 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019195.1.29",
          "form": "식빵이랑 피자에 꽂혔어. 역시 밀가루 짱",
          "original_form": "식빵이랑 피자에 꽂혔어. 역시 밀가루 짱",
          "speaker_id": "2",
          "time": "20210827 18:18"
        }
      ]
