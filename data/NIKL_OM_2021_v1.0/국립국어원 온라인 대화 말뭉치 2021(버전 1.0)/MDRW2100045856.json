        {
          "id": "MDRW2100045856.1.1",
          "form": "빵 엄청 좋아하시나봐요",
          "original_form": "빵 엄청 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045856.1.2",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045856.1.3",
          "form": "네 빵 맛있잖아요~",
          "original_form": "네 빵 맛있잖아요~",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045856.1.4",
          "form": "빵도 치킨도 다 좋아해요~",
          "original_form": "빵도 치킨도 다 좋아해요~",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045856.1.5",
          "form": "치킨 어떤 거 제일 좋아하세요?",
          "original_form": "치킨 어떤 거 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100045856.1.6",
          "form": "깐풍 스타일이나 양념 좋아해요",
          "original_form": "깐풍 스타일이나 양념 좋아해요",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045856.1.7",
          "form": "지코바도 좋아하실 것 같아요",
          "original_form": "지코바도 좋아하실 것 같아요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045856.1.8",
          "form": "앗 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋ어떻게 아셨지",
          "original_form": "앗 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋ어떻게 아셨지",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045856.1.9",
          "form": "저도 그거 좋아하거든요ㅋㅋㄲㄱㄲㅋ",
          "original_form": "저도 그거 좋아하거든요ㅋㅋㄲㄱㄲㅋ",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045856.1.10",
          "form": "맛잘알이시네요ㅎㅎ",
          "original_form": "맛잘알이시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045856.1.11",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ지코바 잘 모르는 분도 계ㅅ셔서 깜짝 놀랐어요ㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ지코바 잘 모르는 분도 계ㅅ셔서 깜짝 놀랐어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045856.1.12",
          "form": "본가가 경상도라서요ㅋㅋㅋㅋ",
          "original_form": "본가가 경상도라서요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045856.1.13",
          "form": "근데 요즘은 숯불로 안해서 슬퍼요ㅠㅠ",
          "original_form": "근데 요즘은 숯불로 안해서 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045856.1.14",
          "form": "그래도 맛있지만요ㅜㅜ",
          "original_form": "그래도 맛있지만요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045856.1.15",
          "form": "ㅇ0ㅇ 지코바가 경상도 쪽 프랜차이즈에요?",
          "original_form": "ㅇ0ㅇ 지코바가 경상도 쪽 프랜차이즈에요?",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045856.1.16",
