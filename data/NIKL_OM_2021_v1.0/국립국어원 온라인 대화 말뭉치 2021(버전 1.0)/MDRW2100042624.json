      "utterance": [
        {
          "id": "MDRW2100042624.1.1",
          "form": "손이 건조한 편이세요?",
          "original_form": "손이 건조한 편이세요?",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042624.1.2",
          "form": "네 좀 그런편이에요",
          "original_form": "네 좀 그런편이에요",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042624.1.3",
          "form": "아 그래서 더 많이 바르시는 건가요?",
          "original_form": "아 그래서 더 많이 바르시는 건가요?",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042624.1.4",
          "form": "그런것도 있어요 ㅎㅎ",
          "original_form": "그런것도 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042624.1.5",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042624.1.6",
          "form": "지성인가요 ?",
          "original_form": "지성인가요 ?",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042624.1.7",
          "form": "네네 님은요 ? 지성인가요 ?",
          "original_form": "네네 님은요 ? 지성인가요 ?",
          "speaker_id": "2",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100042624.1.8",
          "form": "저는 지성은 아니에요!",
          "original_form": "저는 지성은 아니에요!",
          "speaker_id": "1",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100042624.1.9",
          "form": "아 그럼 그냥 잘 안바르는 건가요 ?",
          "original_form": "아 그럼 그냥 잘 안바르는 건가요 ?",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042624.1.10",
          "form": "ㅋㅋㄱㄱ맞아요ㅠ",
          "original_form": "ㅋㅋㄱㄱ맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042624.1.11",
          "form": "로션은 잘 바르나요 ?",
          "original_form": "로션은 잘 바르나요 ?",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042624.1.12",
          "form": "네 로션은 그래도 꼬박꼬박 발라요",
          "original_form": "네 로션은 그래도 꼬박꼬박 발라요",
          "speaker_id": "1",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042624.1.13",
          "form": "손을 많이써요 ?",
          "original_form": "손을 많이써요 ?",
          "speaker_id": "2",
          "time": "20210906 23:56"
        },
        {
          "id": "MDRW2100042624.1.14",
          "form": "어맞아요",
          "original_form": "어맞아요",
          "speaker_id": "1",
          "time": "20210906 23:56"
        },
        {
          "id": "MDRW2100042624.1.15",
          "form": "손을 좀 많이쓰는것같아요",
          "original_form": "손을 좀 많이쓰는것같아요",
          "speaker_id": "1",
          "time": "20210906 23:56"
        },
        {
          "id": "MDRW2100042624.1.16",
          "form": "그래서 로션에 손이 잘 안가나 보다 ㅎㅎ",
          "original_form": "그래서 로션에 손이 잘 안가나 보다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042624.1.17",
          "form": "어 그게맞는것같아요ㅠ",
          "original_form": "어 그게맞는것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042624.1.18",
          "form": "오히려 손에 로션을 안바르게되는",
          "original_form": "오히려 손에 로션을 안바르게되는",
          "speaker_id": "1",
          "time": "20210906 23:57"
        }
