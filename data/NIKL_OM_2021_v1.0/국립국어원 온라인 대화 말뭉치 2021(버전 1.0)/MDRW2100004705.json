      "utterance": [
        {
          "id": "MDRW2100004705.1.1",
          "form": "게임 하시는거 있으세요?",
          "original_form": "게임 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004705.1.2",
          "form": "스팀 게임으로 캐주얼 게임 몇 개 하고",
          "original_form": "스팀 게임으로 캐주얼 게임 몇 개 하고",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004705.1.3",
          "form": "요즘에 핸드폰으로는 미해결사건부 하고 있어요",
          "original_form": "요즘에 핸드폰으로는 미해결사건부 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004705.1.4",
          "form": "헉 뭔가 재밌어보이는 게임이네요",
          "original_form": "헉 뭔가 재밌어보이는 게임이네요",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004705.1.5",
          "form": "어떻게 하는 게임인가요",
          "original_form": "어떻게 하는 게임인가요",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004705.1.6",
          "form": "역전재판 아세요?",
          "original_form": "역전재판 아세요?",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004705.1.7",
          "form": "약간 그런 느낌이었어요",
          "original_form": "약간 그런 느낌이었어요",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004705.1.8",
          "form": "챕터 별로 사건이 있고 주인공이 변호사인데 재판 진행 전에 진술 조사, 현장 조사 같은 거 진행해요",
          "original_form": "챕터 별로 사건이 있고 주인공이 변호사인데 재판 진행 전에 진술 조사, 현장 조사 같은 거 진행해요",
          "speaker_id": "2",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004705.1.9",
          "form": "약간의 추리 콘텐츠 있고 추리를 못해도 기회가 여러번 주어져서 어려움은 없어요",
          "original_form": "약간의 추리 콘텐츠 있고 추리를 못해도 기회가 여러번 주어져서 어려움은 없어요",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004705.1.10",
          "form": "서브 컨텐츠들도 여러개 있어서 나름 재밌는 거 같아요",
          "original_form": "서브 컨텐츠들도 여러개 있어서 나름 재밌는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004705.1.11",
          "form": "헉 엄청 재밌겠어요!! 한번 해봐야겠어요!!",
          "original_form": "헉 엄청 재밌겠어요!! 한번 해봐야겠어요!!",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004705.1.12",
          "form": "저는 최근에 리얼월드에 있는",
          "original_form": "저는 최근에 리얼월드에 있는",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004705.1.13",
          "form": "리얼 크라임씬2 무료 이벤트해서",
          "original_form": "리얼 크라임씬2 무료 이벤트해서",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004705.1.14",
          "form": "친구들이랑 했는데 엄청 재밌었어요",
          "original_form": "친구들이랑 했는데 엄청 재밌었어요",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004705.1.15",
          "form": "우와 그 예능 크라임씬 같은 게임인가요?",
          "original_form": "우와 그 예능 크라임씬 같은 게임인가요?",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004705.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004705.1.17",
          "form": "각자 역할을 맞고",
          "original_form": "각자 역할을 맞고",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004705.1.18",
          "form": "서로 대화로 연기하면서",
          "original_form": "서로 대화로 연기하면서",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004705.1.19",
          "form": "범인 찾는 겁니다!!",
          "original_form": "범인 찾는 겁니다!!",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004705.1.20",
          "form": "꼭 해보세요",
          "original_form": "꼭 해보세요",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004705.1.21",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004705.1.22",
          "form": "진짜 재밌을 거 같아요",
          "original_form": "진짜 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004705.1.23",
          "form": "제가 추천드린 것도 현질 유도 별로 없고 꽤 할만 해요!",
          "original_form": "제가 추천드린 것도 현질 유도 별로 없고 꽤 할만 해요!",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004705.1.24",
          "form": "네네 꼭 해볼게요!!",
          "original_form": "네네 꼭 해볼게요!!",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004705.1.25",
          "form": "또 재밌게 한 게임 있으세요?",
          "original_form": "또 재밌게 한 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004705.1.26",
          "form": "아뇨 저 게임 잘 안해요 초등학교때 닌텐도? 외에는 잘 하는건 없는거 같아요",
          "original_form": "아뇨 저 게임 잘 안해요 초등학교때 닌텐도? 외에는 잘 하는건 없는거 같아요",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004705.1.27",
          "form": "저도 옛날에는 많이 했는데 요즘에는 가끔 시간 때우는 용으로만 하는 거 같아요",
          "original_form": "저도 옛날에는 많이 했는데 요즘에는 가끔 시간 때우는 용으로만 하는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004705.1.28",
          "form": "아 옛날에 방탈출 게임은 좋아했어요",
          "original_form": "아 옛날에 방탈출 게임은 좋아했어요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004705.1.29",
          "form": "그런 추리 콘텐츠가 좀 재밌더라고요",
          "original_form": "그런 추리 콘텐츠가 좀 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004705.1.30",
          "form": "창의력 필요한 문제들 푸는 것도 재밌고",
          "original_form": "창의력 필요한 문제들 푸는 것도 재밌고",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004705.1.31",
          "form": "헉 저는 방탈출 정말하고 싶었는데  못해봤어요ㅠ",
          "original_form": "헉 저는 방탈출 정말하고 싶었는데  못해봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004705.1.32",
          "form": "크라임씬 보는거 좋아해서 몇번씩 보고",
          "original_form": "크라임씬 보는거 좋아해서 몇번씩 보고",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004705.1.33",
          "form": "math라고 플레이스토어에 수수께끼와 퍼즐 수학 게임? 이라는 게임 있는데",
          "original_form": "math라고 플레이스토어에 수수께끼와 퍼즐 수학 게임? 이라는 게임 있는데",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004705.1.34",
          "form": "창의 문제들 모아져 있는 단순한 게임이에요",
          "original_form": "창의 문제들 모아져 있는 단순한 게임이에요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004705.1.35",
          "form": "콘탠츠가 많지는 않은데",
          "original_form": "콘탠츠가 많지는 않은데",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004705.1.36",
          "form": "킬탐용으로 하면 재밌게 하실 거 같아요",
          "original_form": "킬탐용으로 하면 재밌게 하실 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004705.1.37",
          "form": "헉 엄청 재밌을거 같아요!! 저 그런거 좋아하거든요",
          "original_form": "헉 엄청 재밌을거 같아요!! 저 그런거 좋아하거든요",
          "speaker_id": "1",
