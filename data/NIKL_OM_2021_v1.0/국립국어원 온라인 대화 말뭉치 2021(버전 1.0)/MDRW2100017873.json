      "utterance": [
        {
          "id": "MDRW2100017873.1.1",
          "form": "지금 방이지?",
          "original_form": "지금 방이지?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017873.1.2",
          "form": "ㅇㅇ 방이지",
          "original_form": "ㅇㅇ 방이지",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017873.1.3",
          "form": "저녁먹고 배부르니까 피곤이 몰려온다",
          "original_form": "저녁먹고 배부르니까 피곤이 몰려온다",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017873.1.4",
          "form": "ㅋㅋㅋ시원하게 하고있니",
          "original_form": "ㅋㅋㅋ시원하게 하고있니",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017873.1.5",
          "form": "요즘은 날이 크게 안더워서",
          "original_form": "요즘은 날이 크게 안더워서",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017873.1.6",
          "form": "그냥 선풍기 틀어놓고 있음 ㅋㅋㅋㅋ",
          "original_form": "그냥 선풍기 틀어놓고 있음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017873.1.7",
          "form": "ㅋㅋㅋ진짜 어제까진",
          "original_form": "ㅋㅋㅋ진짜 어제까진",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017873.1.8",
          "form": "선풍기랑에어컨",
          "original_form": "선풍기랑에어컨",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017873.1.9",
          "form": "덮개씌워야될줄",
          "original_form": "덮개씌워야될줄",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017873.1.10",
          "form": "ㅋㅋㅋㅋ 그러나 짜잔 오늘 더워졌습니다",
          "original_form": "ㅋㅋㅋㅋ 그러나 짜잔 오늘 더워졌습니다",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017873.1.11",
          "form": "그래도 한 10월까진 선풍기 놔둬야 할걸",
          "original_form": "그래도 한 10월까진 선풍기 놔둬야 할걸",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017873.1.12",
          "form": "호에에",
          "original_form": "호에에",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017873.1.13",
          "form": "빨래도 못해서",
          "original_form": "빨래도 못해서",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017873.1.14",
          "form": "쌓여있는데",
          "original_form": "쌓여있는데",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017873.1.15",
          "form": "오늘은 그냥빨래하기싫어서",
          "original_form": "오늘은 그냥빨래하기싫어서",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017873.1.16",
          "form": "또안했는데",
          "original_form": "또안했는데",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017873.1.17",
          "form": "비오려나",
          "original_form": "비오려나",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017873.1.18",
          "form": "토요일 비고 일요일 안오니까",
          "original_form": "토요일 비고 일요일 안오니까",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017873.1.19",
          "form": "일요일 하심 될듯?",
          "original_form": "일요일 하심 될듯?",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017873.1.20",
          "form": "오감사",
          "original_form": "오감사",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.21",
          "form": "너희집은 건조기돌림?",
          "original_form": "너희집은 건조기돌림?",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.22",
          "form": "근데 요새 날씨가 지맘대로라 어플을 신뢰할 수 없어",
          "original_form": "근데 요새 날씨가 지맘대로라 어플을 신뢰할 수 없어",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.23",
          "form": "ㄴㄴ 우리는 그냥 베란다 널음",
          "original_form": "ㄴㄴ 우리는 그냥 베란다 널음",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.24",
          "form": "세탁기가 겸용이라",
          "original_form": "세탁기가 겸용이라",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.25",
          "form": "한번해봤는데",
          "original_form": "한번해봤는데",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017873.1.26",
          "form": "다섯시간걸리던데",
          "original_form": "다섯시간걸리던데",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017873.1.27",
          "form": "헐...전기세 ㄱㅊ?",
          "original_form": "헐...전기세 ㄱㅊ?",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017873.1.28",
          "form": "그건모르겠고 그렇게할바에야",
          "original_form": "그건모르겠고 그렇게할바에야",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017873.1.29",
          "form": "드라이어로말리는게",
          "original_form": "드라이어로말리는게",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017873.1.30",
          "form": "더낫지않겠니ㅋㅋㅋ",
          "original_form": "더낫지않겠니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        }
      ]
    }
