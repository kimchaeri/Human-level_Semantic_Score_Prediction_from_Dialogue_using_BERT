      "utterance": [
        {
          "id": "MDRW2100039284.1.1",
          "form": "나 어제",
          "original_form": "나 어제",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039284.1.2",
          "form": "골때녀",
          "original_form": "골때녀",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039284.1.3",
          "form": "봤다",
          "original_form": "봤다",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039284.1.4",
          "form": "엄마 봤어?",
          "original_form": "엄마 봤어?",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039284.1.5",
          "form": "어 봤어",
          "original_form": "어 봤어",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039284.1.6",
          "form": "걔네 불쌍하더라",
          "original_form": "걔네 불쌍하더라",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.7",
          "form": "어디 구척장신?",
          "original_form": "어디 구척장신?",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.8",
          "form": "ㅇㅇ열심히 하던데",
          "original_form": "ㅇㅇ열심히 하던데",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.9",
          "form": "졌잖아",
          "original_form": "졌잖아",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.10",
          "form": "엄마 원래 걔네 별로 응원 안 하더니",
          "original_form": "엄마 원래 걔네 별로 응원 안 하더니",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.11",
          "form": "애들이 열심히 하더라",
          "original_form": "애들이 열심히 하더라",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039284.1.12",
          "form": "맞아 특히 차수민이랑 이현이",
          "original_form": "맞아 특히 차수민이랑 이현이",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039284.1.13",
          "form": "진짜 이악물고 뛰어다니던데",
          "original_form": "진짜 이악물고 뛰어다니던데",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039284.1.14",
          "form": "진짜 열심히해서 응원하게 되더라",
          "original_form": "진짜 열심히해서 응원하게 되더라",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039284.1.15",
          "form": "맞아ㅠㅜ한혜진이 못 뛰어서",
          "original_form": "맞아ㅠㅜ한혜진이 못 뛰어서",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039284.1.16",
          "form": "다른 사람들이 엄청 열심히 하더라",
          "original_form": "다른 사람들이 엄청 열심히 하더라",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039284.1.17",
          "form": "근데 코로나 걸리면 그렇게 호흡이 힘들대?",
          "original_form": "근데 코로나 걸리면 그렇게 호흡이 힘들대?",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039284.1.18",
          "form": "몰라 폐망가진다더니",
          "original_form": "몰라 폐망가진다더니",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039284.1.19",
          "form": "진짜 폐활량 딸리나봐ㅠㅜ",
          "original_form": "진짜 폐활량 딸리나봐ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039284.1.20",
          "form": "무섭네",
          "original_form": "무섭네",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.21",
          "form": "너도 코로나 안걸리게 조심해라",
          "original_form": "너도 코로나 안걸리게 조심해라",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.22",
          "form": "조심하지 당연",
          "original_form": "조심하지 당연",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.23",
          "form": "아 한혜진 팬인데",
          "original_form": "아 한혜진 팬인데",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.24",
          "form": "어제 넘 속상했음",
          "original_form": "어제 넘 속상했음",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.25",
          "form": "다음 경기 잘하면되지",
          "original_form": "다음 경기 잘하면되지",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.26",
          "form": "그때까지 괜찮아 져야되는데",
          "original_form": "그때까지 괜찮아 져야되는데",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039284.1.27",
          "form": "팀원들한테 고맙고 미안하겠더라",
          "original_form": "팀원들한테 고맙고 미안하겠더라",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039284.1.28",
          "form": "그래 한혜진이 진짜 열심히 했었는데",
          "original_form": "그래 한혜진이 진짜 열심히 했었는데",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039284.1.29",
          "form": "맞아 근성 있고 멋있음",
          "original_form": "맞아 근성 있고 멋있음",
          "speaker_id": "2",
          "time": "20210903 15:46"
        }
      ]
    }
