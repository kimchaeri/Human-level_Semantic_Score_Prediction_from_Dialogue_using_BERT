      "utterance": [
        {
          "id": "MDRW2100017501.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017501.1.2",
          "form": "여행 좋아해요 ㅠㅠㅠ 코로나땜에 전혀...못가고...마음만 먹고 있지만요 ㅠㅠㅠ",
          "original_form": "여행 좋아해요 ㅠㅠㅠ 코로나땜에 전혀...못가고...마음만 먹고 있지만요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017501.1.3",
          "form": "오 그럼 코로나전에 다녀오신곳 있나요?",
          "original_form": "오 그럼 코로나전에 다녀오신곳 있나요?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017501.1.4",
          "form": "음... 전주 갔었던거 같아요",
          "original_form": "음... 전주 갔었던거 같아요",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017501.1.5",
          "form": "오 전주 전주에서 뭐하셨나요?",
          "original_form": "오 전주 전주에서 뭐하셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017501.1.6",
          "form": "맛있는거 먹으러 다녔어요 ㅋㅋㅋㅋ",
          "original_form": "맛있는거 먹으러 다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017501.1.7",
          "form": "콩나물국밥이랑",
          "original_form": "콩나물국밥이랑",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017501.1.8",
          "form": "새우만두랑",
          "original_form": "새우만두랑",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017501.1.9",
          "form": "이것저것...",
          "original_form": "이것저것...",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017501.1.10",
          "form": "새우만두 맛있었겠어요",
          "original_form": "새우만두 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017501.1.11",
          "form": "네네 맛있었어요 ㅋㅋㅋㅋ",
          "original_form": "네네 맛있었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017501.1.12",
          "form": "야시장에서 파는 케밥도 맛있었는데",
          "original_form": "야시장에서 파는 케밥도 맛있었는데",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017501.1.13",
          "form": "그 집은 사라졌다더라구요 ㅠㅠ",
          "original_form": "그 집은 사라졌다더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017501.1.14",
          "form": "헉 ㅠㅠ 야시장이라 그런가봐요",
          "original_form": "헉 ㅠㅠ 야시장이라 그런가봐요",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017501.1.15",
          "form": "ㅠㅠ 그러니까요...",
          "original_form": "ㅠㅠ 그러니까요...",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017501.1.16",
          "form": "name1님은 인상깊은 여행장소 있으신가요??",
          "original_form": "&name1&님은 인상깊은 여행장소 있으신가요??",
          "speaker_id": "2",
          "time": "20210826 20:01"
        },
        {
          "id": "MDRW2100017501.1.17",
          "form": "저는 오사카 갔던게 인상 깊어요",
          "original_form": "저는 오사카 갔던게 인상 깊어요",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017501.1.18",
          "form": "첫 해외여행이라",
          "original_form": "첫 해외여행이라",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017501.1.19",
          "form": "와 오사카",
          "original_form": "와 오사카",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017501.1.20",
          "form": "어떤것들 하셨어요??",
          "original_form": "어떤것들 하셨어요??",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017501.1.21",
          "form": "그냥 구경 다녔어요 수족관 커서 고래상어 봤어요",
          "original_form": "그냥 구경 다녔어요 수족관 커서 고래상어 봤어요",
          "speaker_id": "1",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017501.1.22",
          "form": "와 수족관",
          "original_form": "와 수족관",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017501.1.23",
          "form": "수족관 가보고 싶네요 ㅠㅠ",
          "original_form": "수족관 가보고 싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017501.1.24",
          "form": "전 일본은 가본적없어요",
          "original_form": "전 일본은 가본적없어요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017501.1.25",
          "form": "디즈니랜드 때문에 가보고 싶긴해요 ㅠㅠ",
          "original_form": "디즈니랜드 때문에 가보고 싶긴해요 ㅠㅠ",
