      "utterance": [
        {
          "id": "MDRW2100017240.1.1",
          "form": "오늘도 야근이신가요?",
          "original_form": "오늘도 야근이신가요?",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017240.1.2",
          "form": "응 오늘도 열심히 코딩을하면서 야근 식사를 기다리고 있어 히힛",
          "original_form": "응 오늘도 열심히 코딩을하면서 야근 식사를 기다리고 있어 히힛",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017240.1.3",
          "form": "오늘 야근 식사 메뉴는 피자라네",
          "original_form": "오늘 야근 식사 메뉴는 피자라네",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017240.1.4",
          "form": "피자요? 맛있겠네요ㅎㅎ",
          "original_form": "피자요? 맛있겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017240.1.5",
          "form": "넌? 퇴근 준비중?",
          "original_form": "넌? 퇴근 준비중?",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017240.1.6",
          "form": "이틀 연속 야근이라니 고생이네요",
          "original_form": "이틀 연속 야근이라니 고생이네요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017240.1.7",
          "form": "오늘 어땠어? 스트레스는 없었남?",
          "original_form": "오늘 어땠어? 스트레스는 없었남?",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017240.1.8",
          "form": "노노... 2일 연속이 아니라.. 4일째 야근이야",
          "original_form": "노노... 2일 연속이 아니라.. 4일째 야근이야",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017240.1.9",
          "form": "헐 4일이요? 많이 피곤하시겠다",
          "original_form": "헐 4일이요? 많이 피곤하시겠다",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017240.1.10",
          "form": "월요일 부터 쭉~ 일과 스트레스~ 스트레스와 일~",
          "original_form": "월요일 부터 쭉~ 일과 스트레스~ 스트레스와 일~",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017240.1.11",
          "form": "일 엄청 많나봐요 분당으로 옮기면 일 더 많은거같아요",
          "original_form": "일 엄청 많나봐요 분당으로 옮기면 일 더 많은거같아요",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017240.1.12",
          "form": "앞으로 10월 말싸지는 스팀팩 맞아가면서 해야할지도 몰라",
          "original_form": "앞으로 10월 말싸지는 스팀팩 맞아가면서 해야할지도 몰라",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017240.1.13",
          "form": "스팀팩?",
          "original_form": "스팀팩?",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017240.1.14",
          "form": "응 우선은 인천은 퇴근 버스를 타야해서 무조간 시간 되면 나왔는데",
          "original_form": "응 우선은 인천은 퇴근 버스를 타야해서 무조간 시간 되면 나왔는데",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017240.1.15",
          "form": "여기 분당은 집까지 가는 차가 너무 많아",
          "original_form": "여기 분당은 집까지 가는 차가 너무 많아",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017240.1.16",
          "form": "인천이 좋았는데",
          "original_form": "인천이 좋았는데",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017240.1.17",
          "form": "헙...약 먹고 일한다로 순화 해야하는건가",
          "original_form": "헙...약 먹고 일한다로 순화 해야하는건가",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017240.1.18",
          "form": "밥도 잘나오고",
          "original_form": "밥도 잘나오고",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017240.1.19",
          "form": "응 맞아 흑... 그리워",
          "original_form": "응 맞아 흑... 그리워",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017240.1.20",
          "form": "아ㅋㅋ 링겔맞고 일한다는거라 비슷한건가요",
          "original_form": "아ㅋㅋ 링겔맞고 일한다는거라 비슷한건가요",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017240.1.21",
          "form": "히힛 응",
          "original_form": "히힛 응",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017240.1.22",
          "form": "여튼 너무 무리하지마세요",
          "original_form": "여튼 너무 무리하지마세요",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017240.1.23",
          "form": "난 괜찮은데 돈 버는 일이니..",
          "original_form": "난 괜찮은데 돈 버는 일이니..",
          "speaker_id": "2",
          "time": "20210826 18:27"
        }
      ]
    }
