      "utterance": [
        {
          "id": "MDRW2100006658.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100006658.1.2",
          "form": "오버워치 조금 했었습니다",
          "original_form": "오버워치 조금 했었습니다",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100006658.1.3",
          "form": "그러시군요 혹시 보드게임은 하셨을까요",
          "original_form": "그러시군요 혹시 보드게임은 하셨을까요",
          "speaker_id": "1",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100006658.1.4",
          "form": "모두의 마블 옛날에 했어요",
          "original_form": "모두의 마블 옛날에 했어요",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100006658.1.5",
          "form": "아 그게 휴대폰 게임이 아니었나봐요",
          "original_form": "아 그게 휴대폰 게임이 아니었나봐요",
          "speaker_id": "1",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100006658.1.6",
          "form": "아아 현물 보드게임은 잘 몰라요ㅠㅠ",
          "original_form": "아아 현물 보드게임은 잘 몰라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100006658.1.7",
          "form": "아 그러시군요 \n프로게이머 직업은 어떻게 보세요,",
          "original_form": "아 그러시군요 \n프로게이머 직업은 어떻게 보세요,",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100006658.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100006658.1.9",
          "form": "신기하죠. 전 똥손이라 겜을 잘 못해서요ㅋㅋ",
          "original_form": "신기하죠. 전 똥손이라 겜을 잘 못해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100006658.1.10",
          "form": "좋아하는 프로게이머 있으신가요?",
          "original_form": "좋아하는 프로게이머 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100006658.1.11",
          "form": "따로 있진 않아요 아이들이 흔히 프로게이머라는 직업을 장래희망으로 하는데 사실 쉽지 않을 것 같아 여쭤봤어요",
          "original_form": "따로 있진 않아요 아이들이 흔히 프로게이머라는 직업을 장래희망으로 하는데 사실 쉽지 않을 것 같아 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210908 05:43"
        },
        {
          "id": "MDRW2100006658.1.12",
          "form": "맞아요 프로로 데뷔하는 거 보면 극소수더라고요",
          "original_form": "맞아요 프로로 데뷔하는 거 보면 극소수더라고요",
          "speaker_id": "2",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100006658.1.13",
          "form": "저희 동생도 꿈꿨었는데 현실적으론 쉽지 않죠",
          "original_form": "저희 동생도 꿈꿨었는데 현실적으론 쉽지 않죠",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100006658.1.14",
          "form": "그렇죠 동생도 게임을 잘 했나요,",
          "original_form": "그렇죠 동생도 게임을 잘 했나요,",
          "speaker_id": "1",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100006658.1.15",
          "form": "자기 말로는 잘했다는데 허세인 거 같아요",
          "original_form": "자기 말로는 잘했다는데 허세인 거 같아요",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100006658.1.16",
          "form": "게임도 두뇌회전이 잘되야 잘하지 싶어요 전략같은것도 필요할거 같고",
