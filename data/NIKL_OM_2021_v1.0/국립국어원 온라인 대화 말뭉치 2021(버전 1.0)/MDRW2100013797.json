      "utterance": [
        {
          "id": "MDRW2100013797.1.1",
          "form": "쇼핑좋아해?",
          "original_form": "쇼핑좋아해?",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013797.1.2",
          "form": "휴일마다 쇼핑하지",
          "original_form": "휴일마다 쇼핑하지",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013797.1.3",
          "form": "주로 뭘 사?",
          "original_form": "주로 뭘 사?",
          "speaker_id": "1",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100013797.1.4",
          "form": "ㅋㅋ주로 먹을거 사 너는?",
          "original_form": "ㅋㅋ주로 먹을거 사 너는?",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013797.1.5",
          "form": "나도 먹을거 사는거 좋아해  최근에 탄산수를 샀어",
          "original_form": "나도 먹을거 사는거 좋아해  최근에 탄산수를 샀어",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013797.1.6",
          "form": "요즘 탄산수가 맛이 다양하더라",
          "original_form": "요즘 탄산수가 맛이 다양하더라",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013797.1.7",
          "form": "키위맛 탄산수추천할게",
          "original_form": "키위맛 탄산수추천할게",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013797.1.8",
          "form": "난 청포도 맛도 좋아해",
          "original_form": "난 청포도 맛도 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013797.1.9",
          "form": "청포도 듣기만해도 맛있을것같네 ㅎㅎㅎ 아 난 최근에 옷도 샀어",
          "original_form": "청포도 듣기만해도 맛있을것같네 ㅎㅎㅎ 아 난 최근에 옷도 샀어",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013797.1.10",
          "form": "오 가을옷 산거야?",
          "original_form": "오 가을옷 산거야?",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013797.1.11",
          "form": "응! 이제 곧 날씨가 쌀쌀해지잖아",
          "original_form": "응! 이제 곧 날씨가 쌀쌀해지잖아",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013797.1.12",
          "form": "나도 이제 가을옷 쇼핑해야겠다",
          "original_form": "나도 이제 가을옷 쇼핑해야겠다",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013797.1.13",
          "form": "맨투맨 종류도 사는거 좋아해?",
          "original_form": "맨투맨 종류도 사는거 좋아해?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013797.1.14",
          "form": "나는 후드를 좋아매",
          "original_form": "나는 후드를 좋아매",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013797.1.15",
          "form": "후드티는 모자가있어서 편하지",
          "original_form": "후드티는 모자가있어서 편하지",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013797.1.16",
          "form": "맞아 후드가 짱이야",
          "original_form": "맞아 후드가 짱이야",
          "speaker_id": "2",
          "time": "20210825 16:08"
        }
      ]
