        {
          "id": "MDRW2100040498.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100040498.1.9",
          "form": "그러고는 카톡으로 마음만 받을게",
          "original_form": "그러고는 카톡으로 마음만 받을게",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100040498.1.10",
          "form": "하고 우리사이에 불편함이 생겼네",
          "original_form": "하고 우리사이에 불편함이 생겼네",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
