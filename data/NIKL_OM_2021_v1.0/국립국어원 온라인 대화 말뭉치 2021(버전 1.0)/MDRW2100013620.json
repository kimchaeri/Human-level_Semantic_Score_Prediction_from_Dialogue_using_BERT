      "utterance": [
        {
          "id": "MDRW2100013620.1.1",
          "form": "요즘 날씨 너무 더웠죠",
          "original_form": "요즘 날씨 너무 더웠죠",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013620.1.2",
          "form": "네 진짜 더워요",
          "original_form": "네 진짜 더워요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013620.1.3",
          "form": "오늘은 비가 내리더라구요 ㅋㅋ",
          "original_form": "오늘은 비가 내리더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013620.1.4",
          "form": "그래서 더 습하고",
          "original_form": "그래서 더 습하고",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013620.1.5",
          "form": "맞아요 ㅋㅋ 비와서 좀 시원해지나 했더니 오히려 습해졌어요",
          "original_form": "맞아요 ㅋㅋ 비와서 좀 시원해지나 했더니 오히려 습해졌어요",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013620.1.6",
          "form": "금요일까지 내린다고 하더라구요",
          "original_form": "금요일까지 내린다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013620.1.7",
          "form": "장마 다 끝나면 진짜 가을일거같아요",
          "original_form": "장마 다 끝나면 진짜 가을일거같아요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013620.1.8",
          "form": "그러게요 그래도 많이 시원해졌어요",
          "original_form": "그러게요 그래도 많이 시원해졌어요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013620.1.9",
          "form": "이번 가을에는 진짜 가을날씨 볼 수 있을 거 같아요",
          "original_form": "이번 가을에는 진짜 가을날씨 볼 수 있을 거 같아요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013620.1.10",
          "form": "맞아요 ㅋㅋ 가을이 좀 길었으면 좋겠어요",
          "original_form": "맞아요 ㅋㅋ 가을이 좀 길었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013620.1.11",
          "form": "겨울 너무 추워서 ㅎㅎ",
          "original_form": "겨울 너무 추워서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013620.1.12",
          "form": "진짜 트렌치는 꺼내서 몇번 입지도 못하고 바로 패딩입어야해서 ㅋㅋㅋ",
          "original_form": "진짜 트렌치는 꺼내서 몇번 입지도 못하고 바로 패딩입어야해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013620.1.13",
          "form": "매년 아쉽죠",
          "original_form": "매년 아쉽죠",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013620.1.14",
          "form": "맞아요 ㅠㅠ이제 슬슬 반팔은 정리해야겠어요",
          "original_form": "맞아요 ㅠㅠ이제 슬슬 반팔은 정리해야겠어요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013620.1.15",
          "form": "사실 가을옷이 제일 이쁜거같아요 트렌치",
          "original_form": "사실 가을옷이 제일 이쁜거같아요 트렌치",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013620.1.16",
          "form": "그쵸 다양한 착장가능해서 좋은 거 같아요",
          "original_form": "그쵸 다양한 착장가능해서 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.17",
          "form": "한국 날씨가 점점 동남아 같아지는 거 같아요",
          "original_form": "한국 날씨가 점점 동남아 같아지는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.18",
          "form": "너무 습하고 찌고ㅠㅠ",
          "original_form": "너무 습하고 찌고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.19",
          "form": "코디하기도 쉽고 색상도 이뻐요 가을차림이 ㅋㅋ",
          "original_form": "코디하기도 쉽고 색상도 이뻐요 가을차림이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.21",
          "form": "아열대 같아요 진짜 ㅠㅠ",
          "original_form": "아열대 같아요 진짜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013620.1.22",
          "form": "숨이 막히는",
          "original_form": "숨이 막히는",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013620.1.23",
          "form": "정말 말그대로 숨이 턱턱 ㅠㅠ",
          "original_form": "정말 말그대로 숨이 턱턱 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013620.1.24",
          "form": "이제 사계절은 사라져가나봐요 ㅠㅠ",
          "original_form": "이제 사계절은 사라져가나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013620.1.25",
          "form": "그러게요 너무 아쉬워요ㅜㅜ",
          "original_form": "그러게요 너무 아쉬워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013620.1.26",
          "form": "봄가을이 참 좋은데ㅠㅠ",
          "original_form": "봄가을이 참 좋은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013620.1.27",
          "form": "맞아요 봄 가을이 진짜 최고ㅠㅠ",
          "original_form": "맞아요 봄 가을이 진짜 최고ㅠㅠ",
