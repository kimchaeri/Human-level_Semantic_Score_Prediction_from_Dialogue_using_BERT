      "utterance": [
        {
          "id": "MDRW2100031886.1.1",
          "form": "거의 스카이인데",
          "original_form": "거의 스카이인데",
          "speaker_id": "2",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031886.1.2",
          "form": "그냥 제가 너무 보는게 많은가 싶기도 하고",
          "original_form": "그냥 제가 너무 보는게 많은가 싶기도 하고",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031886.1.3",
          "form": "안그런사람이 대부분입니다",
          "original_form": "안그런사람이 대부분입니다",
          "speaker_id": "2",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031886.1.4",
          "form": "근데 여자 입장에서 보면 저남자랑 결혼하면",
          "original_form": "근데 여자 입장에서 보면 저남자랑 결혼하면",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031886.1.5",
          "form": "내 고생길이 열리겠다 이런게 빤하게 보이는 애들있거든요",
          "original_form": "내 고생길이 열리겠다 이런게 빤하게 보이는 애들있거든요",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031886.1.6",
          "form": "너무 수준 낮은 남자들만 보신것 같습니다",
          "original_form": "너무 수준 낮은 남자들만 보신것 같습니다",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031886.1.7",
          "form": "하긴 좀 그때 이상한 남자들이었어요 그래서 안 사귀긴 했는데",
          "original_form": "하긴 좀 그때 이상한 남자들이었어요 그래서 안 사귀긴 했는데",
          "speaker_id": "1",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031886.1.8",
          "form": "약간 트라우마처럼 남은게 있어요",
          "original_form": "약간 트라우마처럼 남은게 있어요",
          "speaker_id": "1",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031886.1.9",
          "form": "음 그런가보네요",
          "original_form": "음 그런가보네요",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031886.1.10",
          "form": "그냥 못생기면 안 돼요",
          "original_form": "그냥 못생기면 안 돼요",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.11",
          "form": "그때 내가 제가 너무 착해 가지고",
          "original_form": "그때 내가 제가 너무 착해 가지고",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.13",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.14",
          "form": "아니 근데 남자들은 자기가 못생겼는데 무슨 자신감으로",
          "original_form": "아니 근데 남자들은 자기가 못생겼는데 무슨 자신감으로",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.15",
          "form": "예쁜 여자 계속 추구하는지 모르겠어요",
          "original_form": "예쁜 여자 계속 추구하는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031886.1.16",
          "form": "거울좀봤으면...",
          "original_form": "거울좀봤으면...",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031886.1.17",
          "form": "본능 아니겠습니까 ㅎㅎ",
          "original_form": "본능 아니겠습니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031886.1.18",
          "form": "동물적 본능",
          "original_form": "동물적 본능",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031886.1.19",
          "form": "그거야 이해하는데",
          "original_form": "그거야 이해하는데",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031886.1.20",
          "form": "저는 나중에 미래에 저남자 닮아서 나오면",
          "original_form": "저는 나중에 미래에 저남자 닮아서 나오면",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031886.1.21",
          "form": "모성애가 안 생기겠다 이런 생각에 거부감 들더라고요",
          "original_form": "모성애가 안 생기겠다 이런 생각에 거부감 들더라고요",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.22",
          "form": "아오고",
          "original_form": "아오고",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.23",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.24",
          "form": "이것도 본능이라면 본능이죠 ㅋㅋㅋ",
          "original_form": "이것도 본능이라면 본능이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.25",
          "form": "결혼 안 하시는게 맞는것 같아요",
          "original_form": "결혼 안 하시는게 맞는것 같아요",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.26",
          "form": "선배 오빠가 못생겼는데 결혼해서 딸이 나왔어요",
          "original_form": "선배 오빠가 못생겼는데 결혼해서 딸이 나왔어요",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.27",
          "form": "그 딸이 오빠 닮아서 나왔어요",
          "original_form": "그 딸이 오빠 닮아서 나왔어요",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.28",
          "form": "그래도 너무 이쁩니다 자기 자식은",
          "original_form": "그래도 너무 이쁩니다 자기 자식은",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031886.1.29",
          "form": "전 그게안될거같은데요",
          "original_form": "전 그게안될거같은데요",
          "speaker_id": "1",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100031886.1.30",
          "form": "낳으면 바뀔라나 ㅋㅋㅋ",
          "original_form": "낳으면 바뀔라나 ㅋㅋㅋ",
          "speaker_id": "1",
