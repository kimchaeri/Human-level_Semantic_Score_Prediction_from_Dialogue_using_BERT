      "utterance": [
        {
          "id": "MDRW2100028808.1.1",
          "form": "전형적인 내로남불ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전형적인 내로남불ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028808.1.2",
          "form": "선바는 여초여서 좀 치명타였던듯",
          "original_form": "선바는 여초여서 좀 치명타였던듯",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028808.1.3",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028808.1.4",
          "form": "90프로가 여자라고하더라고",
          "original_form": "90프로가 여자라고하더라고",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028808.1.5",
          "form": "풍월량이랑?\r\n이렇게 둘이",
          "original_form": "풍월량이랑?\r\n이렇게 둘이",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028808.1.6",
          "form": "저도 잘안보지만 동숲때매 유명해져가지고",
          "original_form": "저도 잘안보지만 동숲때매 유명해져가지고",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028808.1.7",
          "form": "뽀얌님으로",
          "original_form": "뽀얌님으로",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028808.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나는 그 숭실대 철학과였나 그걸로 알게됨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나는 그 숭실대 철학과였나 그걸로 알게됨",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028808.1.9",
          "form": "핫게에도 종종 글 올라오더라구",
          "original_form": "핫게에도 종종 글 올라오더라구",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028808.1.10",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028808.1.11",
          "form": "말도웃기고해서",
          "original_form": "말도웃기고해서",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028808.1.12",
          "form": "유명한어록도 많을텐데",
          "original_form": "유명한어록도 많을텐데",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028808.1.13",
          "form": "우린 이걸 얼음이라고 하기로 했어요",
          "original_form": "우린 이걸 얼음이라고 하기로 했어요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028808.1.14",
          "form": "우리는 이걸 ㅇㅇ라고 부르기로 헀어요도 걔고",
          "original_form": "우리는 이걸 ㅇㅇ라고 부르기로 헀어요도 걔고",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028808.1.15",
          "form": "이것도 선바잖아",
          "original_form": "이것도 선바잖아",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
