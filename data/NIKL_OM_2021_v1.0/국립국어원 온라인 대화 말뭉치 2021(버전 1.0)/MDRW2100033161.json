      "utterance": [
        {
          "id": "MDRW2100033161.1.1",
          "form": "저녁은 머 드실 생각이에요?",
          "original_form": "저녁은 머 드실 생각이에요?",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033161.1.2",
          "form": "저녁 고민중이에요 김치찌개 남은거 먹을지",
          "original_form": "저녁 고민중이에요 김치찌개 남은거 먹을지",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033161.1.3",
          "form": "마라샹궈 시킬지 ^^",
          "original_form": "마라샹궈 시킬지 ^^",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033161.1.4",
          "form": "오  ㅎㅎ 여기는 비와서 저는 부침개 해먹으려고요 ㅎ",
          "original_form": "오  ㅎㅎ 여기는 비와서 저는 부침개 해먹으려고요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033161.1.5",
          "form": "우아우아 부침개ㅠㅠ 직접 하세요?",
          "original_form": "우아우아 부침개ㅠㅠ 직접 하세요?",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033161.1.6",
          "form": "네  부침개 파는데는 잘 없어서요 주변에 ㅋㅋㅋ",
          "original_form": "네  부침개 파는데는 잘 없어서요 주변에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033161.1.7",
          "form": "레시피 있나요?",
          "original_form": "레시피 있나요?",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033161.1.8",
          "form": "그냥하는거죠 뭐 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥하는거죠 뭐 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033161.1.9",
          "form": "글쿤요 ㅋㅋ 저는 부침개 하면 잘 안되더라고요",
          "original_form": "글쿤요 ㅋㅋ 저는 부침개 하면 잘 안되더라고요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033161.1.10",
          "form": "반죽할때 농도가 중요한거같더라고요 ㅋㅋ",
          "original_form": "반죽할때 농도가 중요한거같더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100033161.1.11",
          "form": "와 농도 맞아요",
          "original_form": "와 농도 맞아요",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100033161.1.12",
          "form": "김치전? 재료는 뭐할거에요",
          "original_form": "김치전? 재료는 뭐할거에요",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100033161.1.13",
          "form": "호박전이요 ㅎㅎ",
          "original_form": "호박전이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100033161.1.14",
          "form": "아하 호박전 드시는구나",
          "original_form": "아하 호박전 드시는구나",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100033161.1.15",
          "form": "호박이랑 부침가루만 있음 되나요?",
          "original_form": "호박이랑 부침가루만 있음 되나요?",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100033161.1.16",
          "form": "계란도넣고 부침가루랑 튀김가루 조금?",
          "original_form": "계란도넣고 부침가루랑 튀김가루 조금?",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033161.1.17",
          "form": "아 계란도 들어가는군요",
          "original_form": "아 계란도 들어가는군요",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033161.1.18",
          "form": "감자전분있으면 그것도 좀 넣고요 ㅎ",
