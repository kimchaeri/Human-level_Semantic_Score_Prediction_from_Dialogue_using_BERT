      "utterance": [
        {
          "id": "MDRW2100035302.1.1",
          "form": "배달어플 어떤거 쓰세요?",
          "original_form": "배달어플 어떤거 쓰세요?",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100035302.1.2",
          "form": "초기에는 배달의 민족 사용했어요!",
          "original_form": "초기에는 배달의 민족 사용했어요!",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100035302.1.3",
          "form": "요즘은 쿠팡이츠 쓰고 있어요 ㅎㅎ",
          "original_form": "요즘은 쿠팡이츠 쓰고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100035302.1.4",
          "form": "어떤 거 쓰세요?",
          "original_form": "어떤 거 쓰세요?",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100035302.1.5",
          "form": "저는 배민이요!",
          "original_form": "저는 배민이요!",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035302.1.6",
          "form": "쿠팡이츠 괜찮나요?",
          "original_form": "쿠팡이츠 괜찮나요?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035302.1.7",
          "form": "초반에 쿠폰을 많이 뿌려서 쿠폰 쓰는 맛이 있었어요 ㅎㅎ",
          "original_form": "초반에 쿠폰을 많이 뿌려서 쿠폰 쓰는 맛이 있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035302.1.8",
          "form": "그거 말고는 배민이랑 비슷한 것 같아요",
          "original_form": "그거 말고는 배민이랑 비슷한 것 같아요",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035302.1.9",
          "form": "아항ㅋㅋ 역시 처음에 생기는 곳들은 체험을 해봐야 하네요",
          "original_form": "아항ㅋㅋ 역시 처음에 생기는 곳들은 체험을 해봐야 하네요",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100035302.1.10",
          "form": "다 놓치고 있었어요",
          "original_form": "다 놓치고 있었어요",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100035302.1.11",
          "form": "그래도 아직 첫주문 쿠폰이 있잖아요 ㅎㅎ",
          "original_form": "그래도 아직 첫주문 쿠폰이 있잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100035302.1.12",
          "form": "앗 첫주문 거기도 있군요!",
          "original_form": "앗 첫주문 거기도 있군요!",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100035302.1.13",
          "form": "또 뭐먹을지 고민을 할때네요ㅋㅋ",
          "original_form": "또 뭐먹을지 고민을 할때네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100035302.1.14",
          "form": "벌써 돌아왔나요.. 뭐먹지타임...",
          "original_form": "벌써 돌아왔나요.. 뭐먹지타임...",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100035302.1.15",
          "form": "누가 급식표 좀 만들어주면 좋겠어요 ㅋㅋㅋ",
          "original_form": "누가 급식표 좀 만들어주면 좋겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100035302.1.16",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100035302.1.17",
          "form": "만들어주면 더 좋구요",
          "original_form": "만들어주면 더 좋구요",
          "speaker_id": "1",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100035302.1.18",
          "form": "아파트도 룸서비스 해주는 아파트 생기면 좋겠어요 ㅋㅋㅋㅋ",
          "original_form": "아파트도 룸서비스 해주는 아파트 생기면 좋겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035302.1.19",
          "form": "아ㅋㅋ 진짜 좋을거 같네요",
          "original_form": "아ㅋㅋ 진짜 좋을거 같네요",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035302.1.20",
          "form": "그런 서비스 생기면 대박 터질거에요",
          "original_form": "그런 서비스 생기면 대박 터질거에요",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035302.1.21",
          "form": "저 진짜 바로 이사갈거에요 그런 아파트 생기면 ㅋㅋㅋㅋ",
          "original_form": "저 진짜 바로 이사갈거에요 그런 아파트 생기면 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100035302.1.22",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035302.1.23",
          "form": "그날을 위해 돈 모아야겠어요ㅋㅋ",
          "original_form": "그날을 위해 돈 모아야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        }
      ]
    }
