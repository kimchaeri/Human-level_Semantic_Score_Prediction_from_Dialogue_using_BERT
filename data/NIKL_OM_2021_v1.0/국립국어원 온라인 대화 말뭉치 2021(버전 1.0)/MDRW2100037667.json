        {
          "id": "MDRW2100037667.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100037667.1.2",
          "form": "오리고기 어때요?",
          "original_form": "오리고기 어때요?",
          "speaker_id": "1",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100037667.1.3",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100037667.1.4",
          "form": "오리 주물럭은?",
          "original_form": "오리 주물럭은?",
          "speaker_id": "1",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100037667.1.5",
          "form": "안먹어봐서 모르겟네요.",
          "original_form": "안먹어봐서 모르겟네요.",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100037667.1.6",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100037667.1.7",
          "form": "그럼 먹어보죠",
          "original_form": "그럼 먹어보죠",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100037667.1.8",
          "form": "어떻게여?",
          "original_form": "어떻게여?",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100037667.1.9",
          "form": "마트가서 사서 해먹죠",
          "original_form": "마트가서 사서 해먹죠",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100037667.1.10",
          "form": "만들줄아세여?",
          "original_form": "만들줄아세여?",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100037667.1.11",
          "form": "양념만 따라하면 되는데",
          "original_form": "양념만 따라하면 되는데",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100037667.1.12",
          "form": "말은 쉽죠",
          "original_form": "말은 쉽죠",
          "speaker_id": "1",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100037667.1.13",
          "form": "계량스푼.이라는 위대한것이있죠",
