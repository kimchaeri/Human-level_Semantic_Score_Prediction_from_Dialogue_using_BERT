      "utterance": [
        {
          "id": "MDRW2100044329.1.1",
          "form": "아이들은 밥 잘 먹나요??",
          "original_form": "아이들은 밥 잘 먹나요??",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044329.1.2",
          "form": "저희 큰아이는 밥을 잘 안 먹어요ㅠ",
          "original_form": "저희 큰아이는 밥을 잘 안 먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044329.1.3",
          "form": "ㅠ.ㅠ 저희 첫째도 잘 안먹어요",
          "original_form": "ㅠ.ㅠ 저희 첫째도 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044329.1.4",
          "form": "원래 잘먹던 아이였는데",
          "original_form": "원래 잘먹던 아이였는데",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044329.1.5",
          "form": "편식도 심해지고",
          "original_form": "편식도 심해지고",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044329.1.6",
          "form": "군것질만 찾아요",
          "original_form": "군것질만 찾아요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044329.1.7",
          "form": "아하 ㅠ 저희도 군것질 대마왕",
          "original_form": "아하 ㅠ 저희도 군것질 대마왕",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044329.1.8",
          "form": "편식에 ㅠ",
          "original_form": "편식에 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044329.1.9",
          "form": "쥐꼬리만큼 먹어요",
          "original_form": "쥐꼬리만큼 먹어요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044329.1.10",
          "form": "늘 산만한 아이라 밥먹을때도 가만히 있지를 않는답니다 ...으휴...",
          "original_form": "늘 산만한 아이라 밥먹을때도 가만히 있지를 않는답니다 ...으휴...",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044329.1.11",
          "form": "ㅠㅠ 저희 아이같군요",
          "original_form": "ㅠㅠ 저희 아이같군요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044329.1.12",
          "form": "딱 저희 아이가 그래요",
          "original_form": "딱 저희 아이가 그래요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044329.1.13",
          "form": "밥먹는데 항상 조건이 따라요 ㅋ",
          "original_form": "밥먹는데 항상 조건이 따라요 ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044329.1.14",
          "form": "네 그렇죠 ㅠ",
          "original_form": "네 그렇죠 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044329.1.15",
          "form": "저는 이제 큰 애가 여덟 살이니까",
          "original_form": "저는 이제 큰 애가 여덟 살이니까",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044329.1.16",
          "form": "조건은 없는데",
          "original_form": "조건은 없는데",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044329.1.17",
          "form": "화가 납니다ㅋㅋㅋㅋ",
          "original_form": "화가 납니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044329.1.18",
          "form": "ㅋㅋㅋㅋ 학교에서는 어떻데요?",
          "original_form": "ㅋㅋㅋㅋ 학교에서는 어떻데요?",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044329.1.19",
          "form": "학교에서도 잘 안 먹는 거 같더라구요",
          "original_form": "학교에서도 잘 안 먹는 거 같더라구요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044329.1.20",
          "form": "오늘 2학기 상담 시간이라서",
          "original_form": "오늘 2학기 상담 시간이라서",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044329.1.21",
          "form": "선생님과 통화를 했었는데",
          "original_form": "선생님과 통화를 했었는데",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044329.1.22",
          "form": "안 그래도 밥을 잘 안 먹는다며",
          "original_form": "안 그래도 밥을 잘 안 먹는다며",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044329.1.23",
          "form": "얘기하시더라구요",
          "original_form": "얘기하시더라구요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044329.1.24",
          "form": "아ㅠㅠ 집에서는 안먹어도\r\n나가서는 잘 먹는다는건\r\n남의집 아이들 얘기였나봐요",
          "original_form": "아ㅠㅠ 집에서는 안먹어도\r\n나가서는 잘 먹는다는건\r\n남의집 아이들 얘기였나봐요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044329.1.25",
          "form": "저희 아이도 안먹는데요ㅠ",
          "original_form": "저희 아이도 안먹는데요ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044329.1.26",
          "form": "ㅎㅎㅎ 네 맞아요 유치원에서도 늘 늦게 먹어서",
          "original_form": "ㅎㅎㅎ 네 맞아요 유치원에서도 늘 늦게 먹어서",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044329.1.27",
          "form": "원감선생님이랑 따로 밥먹은적도 있어요",
          "original_form": "원감선생님이랑 따로 밥먹은적도 있어요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044329.1.28",
          "form": "처음에는 원감선생님이랑 먹었다고\r\n 자기만 늦게 먹어서 그렇다고 하더니",
          "original_form": "처음에는 원감선생님이랑 먹었다고\r\n 자기만 늦게 먹어서 그렇다고 하더니",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044329.1.29",
          "form": "나중에는 자주 먹으니까 \r\n나 오늘도 원감선생님이랑 밥 먹었어 라며 쿨하게 얘기하더라구요 ㅋㅋㅋㅋ 어후 참 ㅋㅋㅋㅋㄱ",
          "original_form": "나중에는 자주 먹으니까 \r\n나 오늘도 원감선생님이랑 밥 먹었어 라며 쿨하게 얘기하더라구요 ㅋㅋㅋㅋ 어후 참 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044329.1.30",
          "form": "ㅋㅋㅋㅋ맙소사ㅠ",
          "original_form": "ㅋㅋㅋㅋ맙소사ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044329.1.31",
          "form": "잘들 먹으면 좋으련만 ㅜㅜ",
          "original_form": "잘들 먹으면 좋으련만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044329.1.32",
          "form": "좋아하는 반찬은 뭐에요?",
          "original_form": "좋아하는 반찬은 뭐에요?",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044329.1.33",
          "form": "다행히  단백질 류는 다 좋아해서",
          "original_form": "다행히  단백질 류는 다 좋아해서",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044329.1.34",
          "form": "두부 고기 우유 치즈 생선",
          "original_form": "두부 고기 우유 치즈 생선",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044329.1.35",
          "form": "이런 건 다 좋아해서 키는 커요",
          "original_form": "이런 건 다 좋아해서 키는 커요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044329.1.36",
          "form": "갈비뼈는 보이지만ㅠ",
          "original_form": "갈비뼈는 보이지만ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044329.1.37",
          "form": "오 키는 크다니 괜찮네요:)",
          "original_form": "오 키는 크다니 괜찮네요:)",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044329.1.38",
          "form": "네 좋아하는건 그나마 좀 먹는데 \r\n싫어하는 음식 새로운음식에 도전하는걸 싫어해요",
          "original_form": "네 좋아하는건 그나마 좀 먹는데 \r\n싫어하는 음식 새로운음식에 도전하는걸 싫어해요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044329.1.39",
          "form": "똑같네요 ㅠㅠ",
          "original_form": "똑같네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        }
      ]
    }
