      "utterance": [
        {
          "id": "MDRW2100041972.1.1",
          "form": "저는 전공 살려서 취직하면",
          "original_form": "저는 전공 살려서 취직하면",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041972.1.2",
          "form": "공장...연구실.... 같은 데 갈거같은데",
          "original_form": "공장...연구실.... 같은 데 갈거같은데",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041972.1.3",
          "form": "그러면 몸에 안좋아서 그냥 사무직 하고싶어요ㅠㅠㅠㅠ",
          "original_form": "그러면 몸에 안좋아서 그냥 사무직 하고싶어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041972.1.4",
          "form": "연구실!! 본새절정",
          "original_form": "연구실!! 본새절정",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041972.1.5",
          "form": "교직원 한번 노려보세요",
          "original_form": "교직원 한번 노려보세요",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.6",
          "form": "방학때 단축근무도하고",
          "original_form": "방학때 단축근무도하고",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.7",
          "form": "와압",
          "original_form": "와압",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.8",
          "form": "네네 그러니까요",
          "original_form": "네네 그러니까요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.9",
          "form": "모든 자리를 다 써볼래요,,,",
          "original_form": "모든 자리를 다 써볼래요,,,",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.10",
          "form": "그기간에 월급이 주는것도아니고",
          "original_form": "그기간에 월급이 주는것도아니고",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.11",
          "form": "오 그렇군요!!!",
          "original_form": "오 그렇군요!!!",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.12",
          "form": "그리고 학교다보니까... 그 젊은 에너지....",
          "original_form": "그리고 학교다보니까... 그 젊은 에너지....",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041972.1.13",
          "form": "어맞아요!!!!!",
          "original_form": "어맞아요!!!!!",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.14",
          "form": "봄에 벚꽃을 즐길수있고",
          "original_form": "봄에 벚꽃을 즐길수있고",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.15",
          "form": "교정에 애옹들이 있고ㅠ",
          "original_form": "교정에 애옹들이 있고ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.16",
          "form": "그니까요 ㅠㅠ",
          "original_form": "그니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.17",
          "form": "학교에서 축제도 하고",
          "original_form": "학교에서 축제도 하고",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.18",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.19",
          "form": "역시 고양이들이",
          "original_form": "역시 고양이들이",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.20",
          "form": "학교마다 있군요ㅋㅋㅋㅋ",
          "original_form": "학교마다 있군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.21",
          "form": "ㅋㅋㅋㄱ애옹이!!세상을 !!!구하니까요!!!",
          "original_form": "ㅋㅋㅋㄱ애옹이!!세상을 !!!구하니까요!!!",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041972.1.22",
          "form": "정말 공감합니다",
          "original_form": "정말 공감합니다",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.23",
          "form": "그리고 맛집도 주변에 많고,,,",
          "original_form": "그리고 맛집도 주변에 많고,,,",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.24",
          "form": "물론 직장이긴 하니까",
          "original_form": "물론 직장이긴 하니까",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.25",
          "form": "근데 정직원도 낮은연차는 월급이 박봉이에요!",
          "original_form": "근데 정직원도 낮은연차는 월급이 박봉이에요!",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.26",
          "form": "어딜 가나 스트레스는 있지만 그래도...",
          "original_form": "어딜 가나 스트레스는 있지만 그래도...",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.27",
          "form": "간신히200이래요",
          "original_form": "간신히200이래요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.28",
          "form": "허걱 그렇군요 뭔가",
          "original_form": "허걱 그렇군요 뭔가",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.29",
          "form": "계약직은 200도 안되지만~!",
          "original_form": "계약직은 200도 안되지만~!",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.30",
          "form": "공무원처럼 다니는거죠?",
          "original_form": "공무원처럼 다니는거죠?",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.31",
          "form": "어흐흑... 어흐흑..... 진짜....",
          "original_form": "어흐흑... 어흐흑..... 진짜....",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.32",
          "form": "네네 정직들은 퇴사하면 연금나온대요",
          "original_form": "네네 정직들은 퇴사하면 연금나온대요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.33",
          "form": "대신 잘 안잘리지 않나요....? (제발",
          "original_form": "대신 잘 안잘리지 않나요....? (제발",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.34",
          "form": "와ㅠ",
          "original_form": "와ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041972.1.35",
          "form": "그 맛에 다니겠네요 진짜....",
          "original_form": "그 맛에 다니겠네요 진짜....",
          "speaker_id": "2",
          "time": "20210906 18:08"
        }
      ]
