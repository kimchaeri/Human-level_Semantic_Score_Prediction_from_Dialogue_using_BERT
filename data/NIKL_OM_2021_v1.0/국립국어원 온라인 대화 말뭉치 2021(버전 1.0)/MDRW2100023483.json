      "utterance": [
        {
          "id": "MDRW2100023483.1.1",
          "form": "무한 제출이구만",
          "original_form": "무한 제출이구만",
          "speaker_id": "2",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100023483.1.2",
          "form": "그럼 나 경품받음",
          "original_form": "그럼 나 경품받음",
          "speaker_id": "1",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023483.1.3",
          "form": "cu4000원, 스벅 아메리카노",
          "original_form": "cu4000원, 스벅 아메리카노",
          "speaker_id": "1",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023483.1.4",
          "form": "ㅇㅇ 정말 열심히 살어",
          "original_form": "ㅇㅇ 정말 열심히 살어",
          "speaker_id": "2",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023483.1.5",
          "form": "그럼 그래야지",
          "original_form": "그럼 그래야지",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023483.1.6",
          "form": "경제적 자유를 이뤄야지",
          "original_form": "경제적 자유를 이뤄야지",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023483.1.7",
          "form": "요즘 생각드는게 욕심 부리지말고 박스권단타나 할걸",
          "original_form": "요즘 생각드는게 욕심 부리지말고 박스권단타나 할걸",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023483.1.8",
          "form": "1-3퍼 먹는게 너무 감질나서",
          "original_form": "1-3퍼 먹는게 너무 감질나서",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023483.1.9",
          "form": "지금이라도해",
          "original_form": "지금이라도해",
          "speaker_id": "1",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100023483.1.10",
          "form": "그거안해 0900 매매",
          "original_form": "그거안해 0900 매매",
          "speaker_id": "1",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100023483.1.11",
          "form": "넌 익절 잘 안하지 않냐?",
          "original_form": "넌 익절 잘 안하지 않냐?",
          "speaker_id": "2",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100023483.1.12",
          "form": "이번에도 5~15퍼 먹던것들 다 익절안해서 손절하고",
          "original_form": "이번에도 5~15퍼 먹던것들 다 익절안해서 손절하고",
          "speaker_id": "2",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.13",
          "form": "난 분할로 매수 매도해",
          "original_form": "난 분할로 매수 매도해",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.14",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.15",
          "form": "30프로씩",
          "original_form": "30프로씩",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.16",
          "form": "나도 어제 비골",
          "original_form": "나도 어제 비골",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.17",
          "form": "최근에 메디에 집착하다가 많이 날렷네",
          "original_form": "최근에 메디에 집착하다가 많이 날렷네",
          "speaker_id": "2",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100023483.1.18",
          "form": "익절안하고 냅뒀다가 지금 -10프로다",
          "original_form": "익절안하고 냅뒀다가 지금 -10프로다",
          "speaker_id": "1",
          "time": "20210830 08:51"
        },
