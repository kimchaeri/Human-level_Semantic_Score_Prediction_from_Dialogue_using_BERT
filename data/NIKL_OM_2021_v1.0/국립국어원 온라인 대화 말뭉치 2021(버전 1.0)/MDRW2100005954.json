      "utterance": [
        {
          "id": "MDRW2100005954.1.1",
          "form": "그래 타고 운동하려고",
          "original_form": "그래 타고 운동하려고",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.2",
          "form": "웅웅 ㅋㅋ 뭔 부품이 많네",
          "original_form": "웅웅 ㅋㅋ 뭔 부품이 많네",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.3",
          "form": "이것저것 같이 사놔야지",
          "original_form": "이것저것 같이 사놔야지",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.4",
          "form": "ㅋㅋ 뒤에 짐 실을수 있겠네",
          "original_form": "ㅋㅋ 뒤에 짐 실을수 있겠네",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.5",
          "form": "그래 짐 실을 수 있는 거 다 사 놨다",
          "original_form": "그래 짐 실을 수 있는 거 다 사 놨다",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.6",
          "form": "좋넴 장볼때도 좋겠다",
          "original_form": "좋넴 장볼때도 좋겠다",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005954.1.7",
          "form": "그렇지 걷는 것보다 편하다",
          "original_form": "그렇지 걷는 것보다 편하다",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005954.1.8",
          "form": "웅웅 나도 타야겠다",
          "original_form": "웅웅 나도 타야겠다",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005954.1.9",
          "form": "너는 높아서 타기 힘들 듯",
          "original_form": "너는 높아서 타기 힘들 듯",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005954.1.10",
          "form": "헐..!",
          "original_form": "헐..!",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005954.1.11",
          "form": "안장을 좀 낮추면 될 듯",
          "original_form": "안장을 좀 낮추면 될 듯",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005954.1.12",
          "form": "구래 ㅋㅋ 나도 키큰뎁!",
          "original_form": "구래 ㅋㅋ 나도 키큰뎁!",
