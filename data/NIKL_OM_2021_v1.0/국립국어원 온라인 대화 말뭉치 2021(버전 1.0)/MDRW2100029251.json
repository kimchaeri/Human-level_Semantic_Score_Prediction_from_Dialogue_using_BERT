      "utterance": [
        {
          "id": "MDRW2100029251.1.1",
          "form": "약은 좀 챙겨 드시나요",
          "original_form": "약은 좀 챙겨 드시나요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.3",
          "form": "운동은 하고 있나요",
          "original_form": "운동은 하고 있나요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.4",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.5",
          "form": "시도는하는데",
          "original_form": "시도는하는데",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.6",
          "form": "매번불발",
          "original_form": "매번불발",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.7",
          "form": "지도 하는데",
          "original_form": "지도 하는데",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.8",
          "form": "혹시 단명하는 게 꿈이신가요",
          "original_form": "혹시 단명하는 게 꿈이신가요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.9",
          "form": "운동은 꾸준히하기",
          "original_form": "운동은 꾸준히하기",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.10",
          "form": "어랍",
          "original_form": "어랍",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.11",
          "form": "약이라도 챙겨 드세요",
          "original_form": "약이라도 챙겨 드세요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.12",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.13",
          "form": "영양제",
          "original_form": "영양제",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029251.1.14",
          "form": "먹고싶긴한데",
          "original_form": "먹고싶긴한데",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.15",
          "form": "비타민",
          "original_form": "비타민",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.16",
          "form": "두가지점에서",
          "original_form": "두가지점에서",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.17",
          "form": "마그네슘",
          "original_form": "마그네슘",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.18",
          "form": "걸려서 잘 못먹는데",
          "original_form": "걸려서 잘 못먹는데",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.19",
          "form": "젓째는",
          "original_form": "젓째는",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.20",
          "form": "알약을 잘 못삼켜서",
          "original_form": "알약을 잘 못삼켜서",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.21",
          "form": "한번먹으면 굉장히 속이 불편하다는 사실이고",
          "original_form": "한번먹으면 굉장히 속이 불편하다는 사실이고",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.22",
          "form": "알약 조그마한 것도 있는데",
          "original_form": "알약 조그마한 것도 있는데",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.23",
          "form": "가루약은 별로 에요",
          "original_form": "가루약은 별로 에요",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.24",
          "form": "ㅇㅇ가루약은못먹지",
          "original_form": "ㅇㅇ가루약은못먹지",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.25",
          "form": "둘째는",
          "original_form": "둘째는",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029251.1.26",
          "form": "영양이 필수기는하지만",
          "original_form": "영양이 필수기는하지만",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029251.1.27",
          "form": "영양제 못 먹으면",
          "original_form": "영양제 못 먹으면",
          "speaker_id": "1",
          "time": "20210903 00:41"
        }
