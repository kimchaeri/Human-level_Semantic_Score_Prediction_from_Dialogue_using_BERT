      "utterance": [
        {
          "id": "MDRW2100013909.1.1",
          "form": "저 옛날에 게임 엄청 했어요 ㅎㅎ",
          "original_form": "저 옛날에 게임 엄청 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013909.1.2",
          "form": "요즘 게임 하시나요??",
          "original_form": "요즘 게임 하시나요??",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013909.1.3",
          "form": "아뇨ㅜㅜ게임하시는거 있으세요?",
          "original_form": "아뇨ㅜㅜ게임하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013909.1.4",
          "form": "저도 요즘은 안해요 ㅠㅠ",
          "original_form": "저도 요즘은 안해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013909.1.5",
          "form": "예전에 막 크아 카트 이런거에 머물러있어요 .. ㅎㅎ",
          "original_form": "예전에 막 크아 카트 이런거에 머물러있어요 .. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013909.1.6",
          "form": "아 저도ㅎㅎ 그때 참 재밌게했는데ㅋㅋ",
          "original_form": "아 저도ㅎㅎ 그때 참 재밌게했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013909.1.7",
          "form": "맞죠 ㅋㅋㅋ 저도 예전에 친구들이랑 엄청 했던거같아요",
          "original_form": "맞죠 ㅋㅋㅋ 저도 예전에 친구들이랑 엄청 했던거같아요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013909.1.8",
          "form": "요즘은 그때처럼 끌리는 게임이 별로 없는거같아요ㅠㅠ",
          "original_form": "요즘은 그때처럼 끌리는 게임이 별로 없는거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013909.1.9",
          "form": "진짜요 ㅠㅠ 그리고 요즘 게임은 거의",
          "original_form": "진짜요 ㅠㅠ 그리고 요즘 게임은 거의",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013909.1.10",
          "form": "총게임이라던지",
          "original_form": "총게임이라던지",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013909.1.11",
          "form": "아니면 롤처럼 그런 게임밖에 없는거같아요",
          "original_form": "아니면 롤처럼 그런 게임밖에 없는거같아요",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013909.1.12",
          "form": "맞아요 잔인하고 스킬이 필요한...ㅎㅎ",
          "original_form": "맞아요 잔인하고 스킬이 필요한...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013909.1.13",
          "form": "네 그런거에 흥미없으면 하기 힘들더라구요",
          "original_form": "네 그런거에 흥미없으면 하기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013909.1.14",
          "form": "약간 스마트폰 게임도 비슷한것만 나오는거같아요",
          "original_form": "약간 스마트폰 게임도 비슷한것만 나오는거같아요",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013909.1.15",
          "form": "예전에는 막 바운스볼처럼 쉬운것도 많았는데 ㅎㅎ",
          "original_form": "예전에는 막 바운스볼처럼 쉬운것도 많았는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013909.1.16",
          "form": "맞아요ㅠㅠㅜ다양하지않구ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅜ다양하지않구ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013909.1.17",
          "form": "그나마 최근에 나온거중에는",
          "original_form": "그나마 최근에 나온거중에는",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013909.1.18",
          "form": "카트라이더 모바일? 버전으로 한게 제일 최근인거같아요",
          "original_form": "카트라이더 모바일? 버전으로 한게 제일 최근인거같아요",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013909.1.19",
          "form": "해보셨어요??",
          "original_form": "해보셨어요??",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013909.1.20",
          "form": "아 네 근데 그 예전에 하던 그 카트가 아니더라구요ㅠㅠ",
          "original_form": "아 네 근데 그 예전에 하던 그 카트가 아니더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013909.1.21",
          "form": "아 맞아요 ㅠㅠ 약간 옛날 감성이 안나와요 ..",
          "original_form": "아 맞아요 ㅠㅠ 약간 옛날 감성이 안나와요 ..",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013909.1.22",
          "form": "뭔가 이상한게 많이 생겼더라구요",
          "original_form": "뭔가 이상한게 많이 생겼더라구요",
