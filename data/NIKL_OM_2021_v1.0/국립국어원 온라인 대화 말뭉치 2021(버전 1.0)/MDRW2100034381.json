      "utterance": [
        {
          "id": "MDRW2100034381.1.1",
          "form": "안녕하세요, 여행 좋아하시나요?",
          "original_form": "안녕하세요, 여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100034381.1.2",
          "form": "네 좋아하는데 요즘은 못갔네요ㅜㅜ",
          "original_form": "네 좋아하는데 요즘은 못갔네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.3",
          "form": "여행 자주 다니세요?",
          "original_form": "여행 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.4",
          "form": "종종 갔는데 코로나 이후로는 못 갔네요ㅠ",
          "original_form": "종종 갔는데 코로나 이후로는 못 갔네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.5",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.6",
          "form": "마지막으로 어디가셨어요,",
          "original_form": "마지막으로 어디가셨어요,",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100034381.1.8",
          "form": "강원도 속초요!",
          "original_form": "강원도 속초요!",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100034381.1.9",
          "form": "와 좋았겠다",
          "original_form": "와 좋았겠다",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100034381.1.10",
          "form": "뭐가 제일 좋았나용",
          "original_form": "뭐가 제일 좋았나용",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100034381.1.11",
          "form": "바다에서 스노쿨링을 한 게 가장 좋았어요",
          "original_form": "바다에서 스노쿨링을 한 게 가장 좋았어요",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100034381.1.12",
          "form": "헉 저도 해보고싶네용ㅎㅎ속초는 진짜 옛날에 가보고 안갔는데 가고싶어요~~",
          "original_form": "헉 저도 해보고싶네용ㅎㅎ속초는 진짜 옛날에 가보고 안갔는데 가고싶어요~~",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034381.1.13",
          "form": "대게도 먹구,,",
          "original_form": "대게도 먹구,,",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100034381.1.14",
          "form": "대게 생각하니 먹고싶네요ㅠㅠ 새우튀김도요ㅠ",
          "original_form": "대게 생각하니 먹고싶네요ㅠㅠ 새우튀김도요ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034381.1.15",
          "form": "와 새우튀김ㅜㅠ",
          "original_form": "와 새우튀김ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034381.1.16",
          "form": "아 그리고 닭강정도요!!!ㅠㅠ",
          "original_form": "아 그리고 닭강정도요!!!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034381.1.17",
          "form": "닭강정....속초 중앙시장에서 파는 감자전도 별미죠ㅠㅠ",
          "original_form": "닭강정....속초 중앙시장에서 파는 감자전도 별미죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100034381.1.18",
          "form": "앗 감자전은 몰랐는데 강원도 감자 유명하니까 맛있을 수밖에 없겠네요!!!",
          "original_form": "앗 감자전은 몰랐는데 강원도 감자 유명하니까 맛있을 수밖에 없겠네요!!!",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100034381.1.19",
