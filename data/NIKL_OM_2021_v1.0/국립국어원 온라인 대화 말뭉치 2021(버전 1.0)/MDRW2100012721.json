      "utterance": [
        {
          "id": "MDRW2100012721.1.1",
          "form": "사고싶은건있나요?",
          "original_form": "사고싶은건있나요?",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012721.1.2",
          "form": "아 사진 않을 건데 컴퓨터 갖고싶어요 ㅋㅋㅋ",
          "original_form": "아 사진 않을 건데 컴퓨터 갖고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012721.1.3",
          "form": "컴퓨터요?",
          "original_form": "컴퓨터요?",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012721.1.4",
          "form": "저는 노트북이요ㅋㅋㅋ",
          "original_form": "저는 노트북이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012721.1.5",
          "form": "그램",
          "original_form": "그램",
          "speaker_id": "1",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012721.1.6",
          "form": "아 그램!저도요 ㅋㅋㅋ 가벼운 노트북이 짱이죠",
          "original_form": "아 그램!저도요 ㅋㅋㅋ 가벼운 노트북이 짱이죠",
          "speaker_id": "2",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012721.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012721.1.8",
          "form": "근데 엄청",
          "original_form": "근데 엄청",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012721.1.9",
          "form": "비싸더라고요?ㅎ",
          "original_form": "비싸더라고요?ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012721.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012721.1.11",
          "form": "전자기기... 어쩔 수 없는...",
          "original_form": "전자기기... 어쩔 수 없는...",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012721.1.12",
          "form": "성능대비",
          "original_form": "성능대비",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012721.1.13",
          "form": "오바더라고요ㅠ",
          "original_form": "오바더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012721.1.14",
          "form": "가벼운 걸로 성능을 다한... ㅋㅋㅋㅋㅋ",
          "original_form": "가벼운 걸로 성능을 다한... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012721.1.15",
          "form": "ㅋㅋㅋ 신제품가격이",
          "original_form": "ㅋㅋㅋ 신제품가격이",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012721.1.16",
          "form": "갈수록 비싸져요",
          "original_form": "갈수록 비싸져요",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012721.1.17",
          "form": "물가가 미쳤어요",
          "original_form": "물가가 미쳤어요",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012721.1.18",
          "form": "진짜요ㅋㅋ",
          "original_form": "진짜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012721.1.19",
          "form": "플립도 사고싶어요",
          "original_form": "플립도 사고싶어요",
          "speaker_id": "1",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012721.1.20",
          "form": "이번에 워치도 예쁘던데 삼성 열일 ㅋㅋㅋㅋ",
          "original_form": "이번에 워치도 예쁘던데 삼성 열일 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012721.1.21",
          "form": "엄청싸게나왔더라고요ㅋㅋㅋ",
          "original_form": "엄청싸게나왔더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012721.1.22",
          "form": "제 친구들도 많이 샀어요 ㅋㅋㅋ",
          "original_form": "제 친구들도 많이 샀어요 ㅋㅋㅋ",
