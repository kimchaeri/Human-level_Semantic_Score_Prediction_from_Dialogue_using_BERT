      "utterance": [
        {
          "id": "MDRW2100044741.1.1",
          "form": "기념품중에 마음에 드는게 있으신가요?",
          "original_form": "기념품중에 마음에 드는게 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044741.1.2",
          "form": "음 전 엽서랑 음식이요 ㅋㅋㅋㅋㅋ",
          "original_form": "음 전 엽서랑 음식이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044741.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044741.1.4",
          "form": "저는 친구가 준 팔찌요ㅎㅎ",
          "original_form": "저는 친구가 준 팔찌요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044741.1.5",
          "form": "여행지에서 만난친구였어요? 오오 팔찌라니",
          "original_form": "여행지에서 만난친구였어요? 오오 팔찌라니",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044741.1.6",
          "form": "엽서는 여햏지마다 사시나요?",
          "original_form": "엽서는 여햏지마다 사시나요?",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044741.1.7",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044741.1.8",
          "form": "현장에서 나무에다 이름을 새겨준다고 해왔더라구요",
          "original_form": "현장에서 나무에다 이름을 새겨준다고 해왔더라구요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044741.1.9",
          "form": "바다를 배겨으로 ㅎ으",
          "original_form": "바다를 배겨으로 ㅎ으",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044741.1.10",
          "form": "저는 마음에드는배경있으면 사요 ㅎㅎ",
          "original_form": "저는 마음에드는배경있으면 사요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044741.1.11",
          "form": "오 이쁠거같아요",
          "original_form": "오 이쁠거같아요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044741.1.12",
          "form": "네 그래서 한동안 하구 다녔어요",
          "original_form": "네 그래서 한동안 하구 다녔어요",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044741.1.13",
          "form": "친구가뿌듯했을거같아여 ㅎㅎ",
          "original_form": "친구가뿌듯했을거같아여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044741.1.14",
          "form": "여행가시면 관광지꼭가시능편이세요?",
          "original_form": "여행가시면 관광지꼭가시능편이세요?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044741.1.15",
          "form": "그럼 엽서 모으시는거세요?",
          "original_form": "그럼 엽서 모으시는거세요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044741.1.16",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044741.1.17",
          "form": "모으는정도는아니구여 ㅎㅎ걍이쁜거 몇개사고 자석도 이쁜거보이면 사고그래요",
          "original_form": "모으는정도는아니구여 ㅎㅎ걍이쁜거 몇개사고 자석도 이쁜거보이면 사고그래요",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044741.1.18",
          "form": "호오 자석은 저도 그냥 몆개 모아봤는데 색다르더라구요",
          "original_form": "호오 자석은 저도 그냥 몆개 모아봤는데 색다르더라구요",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044741.1.19",
          "form": "네 냉장고에붙여놓고 ㅋㅋㅋㅋㅋ알록달록 맛이있더라구여",
          "original_form": "네 냉장고에붙여놓고 ㅋㅋㅋㅋㅋ알록달록 맛이있더라구여",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044741.1.20",
          "form": "어디갔다왔는지 추억도 떠올리게 되구요 좋은거 같아요",
          "original_form": "어디갔다왔는지 추억도 떠올리게 되구요 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:24"
        }
      ]
