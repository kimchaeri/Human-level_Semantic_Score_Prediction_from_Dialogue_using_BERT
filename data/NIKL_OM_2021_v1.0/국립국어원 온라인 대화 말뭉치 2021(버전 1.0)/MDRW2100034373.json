      "utterance": [
        {
          "id": "MDRW2100034373.1.1",
          "form": "헛다리 짚었네요 ㅌㅋㅋㅋㅋ",
          "original_form": "헛다리 짚었네요 ㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100034373.1.2",
          "form": "앗 ㅋㅋㅋ 맛있었음 좋겠어용",
          "original_form": "앗 ㅋㅋㅋ 맛있었음 좋겠어용",
          "speaker_id": "2",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100034373.1.3",
          "form": "무화과랑 복숭아면 맛없없일거같아요 ㅠㅠ",
          "original_form": "무화과랑 복숭아면 맛없없일거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100034373.1.4",
          "form": "그쳐?!?! ㅋㅋㅋㅋㅋ",
          "original_form": "그쳐?!?! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034373.1.5",
          "form": "초코맛 vs 인절미 흑임자",
          "original_form": "초코맛 vs 인절미 흑임자",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100034373.1.6",
          "form": "네 ㅋㅋㅋ 과일들어간 케익이면..",
          "original_form": "네 ㅋㅋㅋ 과일들어간 케익이면..",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034373.1.7",
          "form": "뭐를 고르시겠어여?",
          "original_form": "뭐를 고르시겠어여?",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034373.1.8",
          "form": "헉 저는..",
          "original_form": "헉 저는..",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034373.1.9",
          "form": "초코요!!! 제가 초코친냔이거든요",
          "original_form": "초코요!!! 제가 초코친냔이거든요",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034373.1.10",
          "form": "최근에 흑임자도 빠졌는데 초코는 못 이겨요",
          "original_form": "최근에 흑임자도 빠졌는데 초코는 못 이겨요",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100034373.1.11",
          "form": "아니잇!! 하긴 초코는 클래식이죠",
          "original_form": "아니잇!! 하긴 초코는 클래식이죠",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.12",
          "form": "인정합니당",
          "original_form": "인정합니당",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.14",
          "form": "뭐가 좋으세요!",
          "original_form": "뭐가 좋으세요!",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.15",
          "form": "저는 할매입맛이라 ㅋㅋㅋㅋ설빙가도 인절미빙수만먹어요",
          "original_form": "저는 할매입맛이라 ㅋㅋㅋㅋ설빙가도 인절미빙수만먹어요",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.16",
          "form": "흑임자 라떼있음 무조건 그걸로마시구 ㅋㅋㅋㅋ흑임자케이크도 좋아해요!!",
          "original_form": "흑임자 라떼있음 무조건 그걸로마시구 ㅋㅋㅋㅋ흑임자케이크도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100034373.1.17",
          "form": "흑임자케이크도 맛있죠 ㅠㅠ",
          "original_form": "흑임자케이크도 맛있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100034373.1.18",
          "form": "씨유에 흑임자찰떡꼬치 드셔보셨어요?",
          "original_form": "씨유에 흑임자찰떡꼬치 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100034373.1.19",
          "form": "헉 아뇨 ?!?! 그것도 맛나보이네요 ㅋㅋㅋㅋㅋ아까 티라미슈떡꼬치도 추천해주셨죠?!?!?! ㅋㅋㅋㅋㅋ",
          "original_form": "헉 아뇨 ?!?! 그것도 맛나보이네요 ㅋㅋㅋㅋㅋ아까 티라미슈떡꼬치도 추천해주셨죠?!?!?! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100034373.1.20",
          "form": "씨유 함 가야겠어요",
          "original_form": "씨유 함 가야겠어요",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100034373.1.21",
          "form": "넼ㅋㅋㅋㅋㅋ 찰떡꼬치 엄청 좋아해요",
          "original_form": "넼ㅋㅋㅋㅋㅋ 찰떡꼬치 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
