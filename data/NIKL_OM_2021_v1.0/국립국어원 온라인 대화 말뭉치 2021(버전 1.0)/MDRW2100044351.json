      "utterance": [
        {
          "id": "MDRW2100044351.1.1",
          "form": "조기은퇴하려는 사람들 이해가면서도",
          "original_form": "조기은퇴하려는 사람들 이해가면서도",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044351.1.2",
          "form": "그럼 심심할거 같기도 하도",
          "original_form": "그럼 심심할거 같기도 하도",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.4",
          "form": "근데 일 없는 삶",
          "original_form": "근데 일 없는 삶",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.5",
          "form": "일 자체는 흥미롭단말이지",
          "original_form": "일 자체는 흥미롭단말이지",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.6",
          "form": "행복한가",
          "original_form": "행복한가",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.7",
          "form": "난 사람들 가르치는 것도 좋아하니까",
          "original_form": "난 사람들 가르치는 것도 좋아하니까",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.8",
          "form": "일은 하되 쉬고 싶을 때 쉴 수 있는 삶",
          "original_form": "일은 하되 쉬고 싶을 때 쉴 수 있는 삶",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.9",
          "form": "그게 좋지",
          "original_form": "그게 좋지",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.10",
          "form": "근데 돈에 연연하지 않고 가족들과 함께하면서 자기가 하고싶은 일 한다는 거 자체는 좋아보여",
          "original_form": "근데 돈에 연연하지 않고 가족들과 함께하면서 자기가 하고싶은 일 한다는 거 자체는 좋아보여",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.11",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.12",
          "form": "일하는 걸 선택하는 삶",
          "original_form": "일하는 걸 선택하는 삶",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044351.1.13",
          "form": "어쩔수없이 하는 게 아니라",
          "original_form": "어쩔수없이 하는 게 아니라",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044351.1.14",
          "form": "그치 누구한테 연연할것듀 없고",
          "original_form": "그치 누구한테 연연할것듀 없고",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044351.1.15",
          "form": "월급에 매달릴 것도 없고",
          "original_form": "월급에 매달릴 것도 없고",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044351.1.16",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044351.1.17",
          "form": "일하다가 쉬다가~",
          "original_form": "일하다가 쉬다가~",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044351.1.18",
          "form": "글고 먹고싶은거 먹고",
          "original_form": "글고 먹고싶은거 먹고",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044351.1.19",
          "form": "사고싶은 거 사고",
          "original_form": "사고싶은 거 사고",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044351.1.20",
          "form": "그래서 보통 자기 평소생활비 기준으로 은퇴가능 비용 계산하더라",
          "original_form": "그래서 보통 자기 평소생활비 기준으로 은퇴가능 비용 계산하더라",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044351.1.21",
          "form": "아 그렇구만",
          "original_form": "아 그렇구만",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044351.1.22",
          "form": "우린 아직 아니야",
          "original_form": "우린 아직 아니야",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044351.1.23",
          "form": "그걸 역산해서 투자도 하고 부업도 하고",
          "original_form": "그걸 역산해서 투자도 하고 부업도 하고",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044351.1.24",
          "form": "일 더 열심히 해야혀",
          "original_form": "일 더 열심히 해야혀",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044351.1.25",
          "form": "근데 생각해보면 난 은퇴해도 병원비 정도 모으면 될거같긴 해",
          "original_form": "근데 생각해보면 난 은퇴해도 병원비 정도 모으면 될거같긴 해",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044351.1.26",
          "form": "딱히 여행가고 싶지도 않아서",
          "original_form": "딱히 여행가고 싶지도 않아서",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044351.1.27",
          "form": "기본생활비+병원비",
          "original_form": "기본생활비+병원비",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044351.1.28",
          "form": "딱히 은퇴준비는 아니고 그냥 생활 규모 점검",
          "original_form": "딱히 은퇴준비는 아니고 그냥 생활 규모 점검",
          "speaker_id": "1",
          "time": "20210907 22:28"
        }
      ]
    }
