      "utterance": [
        {
          "id": "MDRW2100010715.1.1",
          "form": "혹시 싸이월드 아세요?",
          "original_form": "혹시 싸이월드 아세요?",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010715.1.2",
          "form": "아 네ㅎㅎ 흑역사 많이 있죠ㅎㅎ",
          "original_form": "아 네ㅎㅎ 흑역사 많이 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010715.1.3",
          "form": "아시는군요 ㅎㅎ 들어가 보셨나요?",
          "original_form": "아시는군요 ㅎㅎ 들어가 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100010715.1.4",
          "form": "이번에요? 아니요ㅜㅜ저번에 다밀고 탈퇴했어요ㅋㅋ",
          "original_form": "이번에요? 아니요ㅜㅜ저번에 다밀고 탈퇴했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010715.1.5",
          "form": "앗!! 흑역사를 청산하셨군요.. ㅎㅎ",
          "original_form": "앗!! 흑역사를 청산하셨군요.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010715.1.6",
          "form": "네ㅋㅋ도저히 남겨둘수가 없더라구요ㅋㅋ",
          "original_form": "네ㅋㅋ도저히 남겨둘수가 없더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010715.1.7",
          "form": "들어가보셨어요?",
          "original_form": "들어가보셨어요?",
          "speaker_id": "2",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010715.1.8",
          "form": "아뇨.. ㅋ 들어갔으면 저도 필요한것만 다운받고 밀었겠죠? ㅋ",
          "original_form": "아뇨.. ㅋ 들어갔으면 저도 필요한것만 다운받고 밀었겠죠? ㅋ",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010715.1.9",
          "form": "ㅎㅎ부활한다는 기사만 자꾸 보이네요ㅠㅠ",
          "original_form": "ㅎㅎ부활한다는 기사만 자꾸 보이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010715.1.10",
          "form": "아직 부활한건 아닌가봐요?",
          "original_form": "아직 부활한건 아닌가봐요?",
          "speaker_id": "1",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010715.1.11",
          "form": "저도 잘 모르겠네요ㅎㅎ 아주 예전에 백업하라고 할때 지웠었어요ㅎㅎ",
          "original_form": "저도 잘 모르겠네요ㅎㅎ 아주 예전에 백업하라고 할때 지웠었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010715.1.12",
          "form": "전 백업하라고 할때 해야지~ 하다 잊었어요. 다 날아갔을까요?",
          "original_form": "전 백업하라고 할때 해야지~ 하다 잊었어요. 다 날아갔을까요?",
          "speaker_id": "1",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010715.1.13",
          "form": "남아있을거같기도 하고ㅎㅎ저도 확인해봐야겠네요",
          "original_form": "남아있을거같기도 하고ㅎㅎ저도 확인해봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010715.1.14",
