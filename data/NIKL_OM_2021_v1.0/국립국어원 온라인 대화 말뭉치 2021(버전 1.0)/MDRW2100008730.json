      "utterance": [
        {
          "id": "MDRW2100008730.1.1",
          "form": "오늘 날씨 후덥지근하지 않아?",
          "original_form": "오늘 날씨 후덥지근하지 않아?",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008730.1.2",
          "form": "하루종일 집에만 있어서 그런지 괜찮은데",
          "original_form": "하루종일 집에만 있어서 그런지 괜찮은데",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008730.1.3",
          "form": "요즘 시원해졌잖아",
          "original_form": "요즘 시원해졌잖아",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008730.1.4",
          "form": "늦여름이라 풀리긴 했지",
          "original_form": "늦여름이라 풀리긴 했지",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008730.1.5",
          "form": "일하고 와서 덥게 느껴지나봐ㅋㅋ",
          "original_form": "일하고 와서 덥게 느껴지나봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008730.1.6",
          "form": "일요일인데 일했어? 수고했다...!",
          "original_form": "일요일인데 일했어? 수고했다...!",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008730.1.7",
          "form": "아직까진 밖에 나가면 덥긴하지 ㅋㅋ",
          "original_form": "아직까진 밖에 나가면 덥긴하지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008730.1.8",
          "form": "고마워어ㅋㅋ 일기예보 자주 보는 편이야?",
          "original_form": "고마워어ㅋㅋ 일기예보 자주 보는 편이야?",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008730.1.9",
          "form": "잘 안봐 집순이거든 ㅋㅋ",
          "original_form": "잘 안봐 집순이거든 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008730.1.10",
          "form": "근데 담주 내내 비온다더라",
          "original_form": "근데 담주 내내 비온다더라",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008730.1.11",
          "form": "나도ㅋㅋ 근데 네가 더 잘 안다",
          "original_form": "나도ㅋㅋ 근데 네가 더 잘 안다",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008730.1.12",
          "form": "담주 내내 비오는 거 지금 알았네",
          "original_form": "담주 내내 비오는 거 지금 알았네",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008730.1.13",
          "form": "비 오면 보통 옷 어떻게 입어? 나는 어차피 물 닿을 거 짧은 반바지 입는데",
          "original_form": "비 오면 보통 옷 어떻게 입어? 나는 어차피 물 닿을 거 짧은 반바지 입는데",
          "speaker_id": "1",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008730.1.14",
          "form": "나도 반바지 ㅋㅋㅋ 젖으면 너무 찝찝하잖아",
          "original_form": "나도 반바지 ㅋㅋㅋ 젖으면 너무 찝찝하잖아",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008730.1.15",
          "form": "반바지 반팔",
          "original_form": "반바지 반팔",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008730.1.16",
          "form": "맞아 나도 찝찝해서 그렇게 입어ㅋㅋㅋ",
          "original_form": "맞아 나도 찝찝해서 그렇게 입어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008730.1.17",
          "form": "좋아하는 계절은 뭐야?",
          "original_form": "좋아하는 계절은 뭐야?",
          "speaker_id": "1",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008730.1.18",
          "form": "덥지않으면 다 좋아",
          "original_form": "덥지않으면 다 좋아",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008730.1.19",
          "form": "여름빼고 다 좋아 ㅋㅋㅋㅋㅋ",
          "original_form": "여름빼고 다 좋아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008730.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008730.1.21",
          "form": "나도 지금은 겨울이 빨리 왔음 좋겠어",
          "original_form": "나도 지금은 겨울이 빨리 왔음 좋겠어",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008730.1.22",
          "form": "진짜... 빨리 여름을 벗어나고 싶어",
          "original_form": "진짜... 빨리 여름을 벗어나고 싶어",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008730.1.23",
          "form": "요즘엔 그러는지 모르겠는데",
          "original_form": "요즘엔 그러는지 모르겠는데",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008730.1.24",
          "form": "매미들 새벽에도 운다?",
          "original_form": "매미들 새벽에도 운다?",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008730.1.25",
          "form": "아 ㅋㅋㅋ 맞아",
          "original_form": "아 ㅋㅋㅋ 맞아",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008730.1.26",
          "form": "응??? 벌레로 착각했는데 매미??? 매미가 새벽에 울어?",
          "original_form": "응??? 벌레로 착각했는데 매미??? 매미가 새벽에 울어?",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008730.1.27",
          "form": "어ㅋㅋㅋㅋㅋ 새벽 5시쯤에 울더라ㅋㅋㅋㅋ",
          "original_form": "어ㅋㅋㅋㅋㅋ 새벽 5시쯤에 울더라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008730.1.28",
          "form": "뭐야 여름끝나가니까 필사적으로 우는건가 ㅋㅋㅋ",
          "original_form": "뭐야 여름끝나가니까 필사적으로 우는건가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:54"
        }
      ]
