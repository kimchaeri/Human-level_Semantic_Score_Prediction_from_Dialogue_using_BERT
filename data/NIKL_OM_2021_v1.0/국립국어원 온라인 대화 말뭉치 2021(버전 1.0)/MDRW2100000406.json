      "utterance": [
        {
          "id": "MDRW2100000406.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000406.1.2",
          "form": "ㅎㅇㅎㅇ",
          "original_form": "ㅎㅇㅎㅇ",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.3",
          "form": "이번주 교회오라는데 가기싫어",
          "original_form": "이번주 교회오라는데 가기싫어",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.4",
          "form": "저번주에도 안갔자나",
          "original_form": "저번주에도 안갔자나",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.5",
          "form": "베이스 굳이 해야되는걸까",
          "original_form": "베이스 굳이 해야되는걸까",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 응... 이번주도가기싫어...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 응... 이번주도가기싫어...",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.7",
          "form": "ㅋㅋㅋ 베이스 다른사람도 할수있어?",
          "original_form": "ㅋㅋㅋ 베이스 다른사람도 할수있어?",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.8",
          "form": "찬양팀... 아직 서기엔 오바아닌가..",
          "original_form": "찬양팀... 아직 서기엔 오바아닌가..",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.9",
          "form": "너밖에 할 사람이 없는거 아니니",
          "original_form": "너밖에 할 사람이 없는거 아니니",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.10",
          "form": "그건그래",
          "original_form": "그건그래",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.11",
          "form": "기타 하는애가 베이스도할수있는데",
          "original_form": "기타 하는애가 베이스도할수있는데",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000406.1.12",
          "form": "걔한테 넘겨",
          "original_form": "걔한테 넘겨",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.13",
          "form": "맞아 그래버리고싶어",
          "original_form": "맞아 그래버리고싶어",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.14",
          "form": "예배고뭐고....",
          "original_form": "예배고뭐고....",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.15",
          "form": "ㅋ진지하게 얘기해봐봐",
          "original_form": "ㅋ진지하게 얘기해봐봐",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.16",
          "form": "근데또 알지",
          "original_form": "근데또 알지",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.17",
          "form": "당장은 하기싫은데 나중엔",
          "original_form": "당장은 하기싫은데 나중엔",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.18",
          "form": "아쉬운거",
          "original_form": "아쉬운거",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000406.1.20",
          "form": "알짘ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "알짘ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000406.1.21",
          "form": "그래서일단 그만두지는못하고",
          "original_form": "그래서일단 그만두지는못하고",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000406.1.22",
          "form": "잠깐쉬어그럼ㅋ",
          "original_form": "잠깐쉬어그럼ㅋ",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000406.1.23",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000406.1.24",
          "form": "싱어도아니고 인도자도아니고",
          "original_form": "싱어도아니고 인도자도아니고",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000406.1.25",
          "form": "베이스는 물론 있음 좋긴하지만ㅋㅋㅋ",
          "original_form": "베이스는 물론 있음 좋긴하지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.26",
          "form": "아주아주 필수는아니라서",
          "original_form": "아주아주 필수는아니라서",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.27",
          "form": "맞아!!! 예배할때 그냥",
          "original_form": "맞아!!! 예배할때 그냥",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.28",
          "form": "피아노랑 인도자랑",
          "original_form": "피아노랑 인도자랑",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.29",
          "form": "전도사님만있우면뭐",
          "original_form": "전도사님만있우면뭐",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.30",
          "form": "그칰ㅋㅋㅋ 잠깐쉬어도 큰무리없을듯",
          "original_form": "그칰ㅋㅋㅋ 잠깐쉬어도 큰무리없을듯",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000406.1.31",
          "form": "요즘도 영상예배?",
          "original_form": "요즘도 영상예배?",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.32",
          "form": "응응ㅋㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.33",
          "form": "20명인가모여도되긴한다는데",
          "original_form": "20명인가모여도되긴한다는데",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.34",
          "form": "그래서ㅜ찬양팀 시키는듯",
          "original_form": "그래서ㅜ찬양팀 시키는듯",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.35",
          "form": "오 20명된대?",
          "original_form": "오 20명된대?",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.36",
          "form": "4단계라 아예 안될줄",
          "original_form": "4단계라 아예 안될줄",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.37",
          "form": "응웅 근데오바야",
          "original_form": "응웅 근데오바야",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.38",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.39",
          "form": "왜교회만 봐주는거야",
          "original_form": "왜교회만 봐주는거야",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.40",
          "form": "의외네",
          "original_form": "의외네",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000406.1.41",
          "form": "이러니까 교회도욕먹고 ㅠ",
          "original_form": "이러니까 교회도욕먹고 ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.42",
          "form": "맞으....",
          "original_form": "맞으....",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.43",
          "form": "솔직히 뭐요즘은 교회믄 그런건아니지만",
          "original_form": "솔직히 뭐요즘은 교회믄 그런건아니지만",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.44",
          "form": "제일 타겟이",
          "original_form": "제일 타겟이",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.45",
          "form": "된다고",
          "original_form": "된다고",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.46",
          "form": "마자 타겟ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자 타겟ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.47",
          "form": "힘내렴",
          "original_form": "힘내렴",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.48",
          "form": "ㅇㅇㅇ이번주도 일단 찬양팀은 안가야지",
          "original_form": "ㅇㅇㅇ이번주도 일단 찬양팀은 안가야지",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.49",
          "form": "중고등부는 하",
          "original_form": "중고등부는 하",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000406.1.50",
          "form": "그것도너무가기싫어 그낭 다가기싫엌ㅋㅋㅋㅋㅋ",
          "original_form": "그것도너무가기싫어 그낭 다가기싫엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
