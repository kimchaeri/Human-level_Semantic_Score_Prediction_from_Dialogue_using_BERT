      "utterance": [
        {
          "id": "MDRW2100025054.1.1",
          "form": "까.라면 까는게 안되신다니",
          "original_form": "까.라면 까는게 안되신다니",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025054.1.2",
          "form": "ㅎㅎㅎ하는게 이해가 되어야 잘하는 스타이르",
          "original_form": "ㅎㅎㅎ하는게 이해가 되어야 잘하는 스타이르",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025054.1.3",
          "form": "일?",
          "original_form": "일?",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025054.1.4",
          "form": "무슨 일을 하고 계신지 궁금하네요 ㅋㅋ",
          "original_form": "무슨 일을 하고 계신지 궁금하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025054.1.5",
          "form": "까.라면 까는게 안되면\n일반 회사생활도 어렵잖아여ㅜㅜ",
          "original_form": "까.라면 까는게 안되면\n일반 회사생활도 어렵잖아여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025054.1.6",
          "form": "그래서 어릴 때 공부도 막무가내 암기 이런거 잘 못했어요",
          "original_form": "그래서 어릴 때 공부도 막무가내 암기 이런거 잘 못했어요",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025054.1.7",
          "form": "지금은 개발입니다ㅎ 캐드 주로 써요",
          "original_form": "지금은 개발입니다ㅎ 캐드 주로 써요",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025054.1.8",
          "form": "회사는 군대만큼은 아니에요",
          "original_form": "회사는 군대만큼은 아니에요",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025054.1.9",
          "form": "어느 정도 알려주고 해 이런다면",
          "original_form": "어느 정도 알려주고 해 이런다면",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025054.1.10",
          "form": "군대는 백지상태로 너 이거해",
          "original_form": "군대는 백지상태로 너 이거해",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025054.1.11",
          "form": "이런 느낌인지라",
          "original_form": "이런 느낌인지라",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025054.1.12",
          "form": "ㅋㅋ 무에서 유를 창조하는 곳이고",
          "original_form": "ㅋㅋ 무에서 유를 창조하는 곳이고",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025054.1.13",
          "form": "회사는 베이스는 있는 곳이라",
          "original_form": "회사는 베이스는 있는 곳이라",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025054.1.14",
          "form": "조금 다르다는거죵? ㅋㅋ",
          "original_form": "조금 다르다는거죵? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025054.1.15",
          "form": "지금 일을 하신지는\n얼마나 되셨어요?",
          "original_form": "지금 일을 하신지는\n얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025054.1.16",
          "form": "네네 완전 달라요 저한테는ㅋㅋ",
          "original_form": "네네 완전 달라요 저한테는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025054.1.17",
          "form": "일은 8년 넘었습니다ㅎ",
          "original_form": "일은 8년 넘었습니다ㅎ",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025054.1.18",
          "form": "무슨 일 하시나요?",
          "original_form": "무슨 일 하시나요?",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025054.1.19",
          "form": "저는 지금은 주부에요:)",
          "original_form": "저는 지금은 주부에요:)",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025054.1.20",
          "form": "전에 뭐 하시던건 있으세요?",
          "original_form": "전에 뭐 하시던건 있으세요?",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025054.1.21",
          "form": "cs강사요 :)",
          "original_form": "cs강사요 :)",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025054.1.22",
          "form": "오우 대단하시네요",
          "original_form": "오우 대단하시네요",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025054.1.23",
          "form": "캐드로 작업하시는\n제드님이 저 대단하세요 bb",
          "original_form": "캐드로 작업하시는\n제드님이 저 대단하세요 bb",
          "speaker_id": "1",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100025054.1.24",
          "form": "일은 아이 때문에 쉬시는건가요?",
          "original_form": "일은 아이 때문에 쉬시는건가요?",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100025054.1.25",
          "form": "네:) 첫째 낳고 복직했다가\n아이 봐 줄 사람이 없어서\n퇴사했어요ㅠ  휴직이 아니라 퇴사",
          "original_form": "네:) 첫째 낳고 복직했다가\n아이 봐 줄 사람이 없어서\n퇴사했어요ㅠ  휴직이 아니라 퇴사",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100025054.1.26",
          "form": "ㅠ.. 다 그러더라구요",
          "original_form": "ㅠ.. 다 그러더라구요",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100025054.1.27",
          "form": "그나마 집에서 일 되시는 분들은 다행이던데",
          "original_form": "그나마 집에서 일 되시는 분들은 다행이던데",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025054.1.28",
          "form": "그래도 육아 때문에 스트레스 많이 받고ㅎ",
          "original_form": "그래도 육아 때문에 스트레스 많이 받고ㅎ",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025054.1.29",
          "form": "이걸로 부부끼리 또 싸우고...",
          "original_form": "이걸로 부부끼리 또 싸우고...",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
