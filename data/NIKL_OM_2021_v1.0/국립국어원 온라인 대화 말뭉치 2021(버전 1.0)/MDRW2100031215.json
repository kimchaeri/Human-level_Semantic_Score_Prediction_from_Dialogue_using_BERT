      "utterance": [
        {
          "id": "MDRW2100031215.1.1",
          "form": "아하 아까도 말했지만 꼭 가봐",
          "original_form": "아하 아까도 말했지만 꼭 가봐",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031215.1.2",
          "form": "재밌어 직관",
          "original_form": "재밌어 직관",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031215.1.3",
          "form": "가봤어 나도 어릴땐",
          "original_form": "가봤어 나도 어릴땐",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.5",
          "form": "근데 경기 보는기분보다 솔직히그냥",
          "original_form": "근데 경기 보는기분보다 솔직히그냥",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.6",
          "form": "분위기 즐기는 느낌이지",
          "original_form": "분위기 즐기는 느낌이지",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.7",
          "form": "이열 어릴때부터 야구랑 인연이 있구만 ㅋㅋㅋㅋㅋ",
          "original_form": "이열 어릴때부터 야구랑 인연이 있구만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031215.1.9",
          "form": "대박이야",
          "original_form": "대박이야",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031215.1.10",
          "form": "8개로 또 이닝마무리",
          "original_form": "8개로 또 이닝마무리",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031215.1.11",
          "form": "크크 삼자범퇴가 뭔진모르지만",
          "original_form": "크크 삼자범퇴가 뭔진모르지만",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031215.1.12",
          "form": "삼자범퇴래",
          "original_form": "삼자범퇴래",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031215.1.13",
          "form": "출루없이 3아웃!",
          "original_form": "출루없이 3아웃!",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.14",
          "form": "아 천재넹",
          "original_form": "아 천재넹",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.15",
          "form": "그거구나",
          "original_form": "그거구나",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.16",
          "form": "왜지?",
          "original_form": "왜지?",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.17",
          "form": "한자인가!!?",
          "original_form": "한자인가!!?",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.18",
          "form": "감사합니다 나도 이유까진 모르겠아!!",
          "original_form": "감사합니다 나도 이유까진 모르겠아!!",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜 오키",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜 오키",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.20",
          "form": "삼자범퇴 알았다",
          "original_form": "삼자범퇴 알았다",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031215.1.21",
          "form": "찾아봤어??",
          "original_form": "찾아봤어??",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.22",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.23",
          "form": "그건아니고",
          "original_form": "그건아니고",
          "speaker_id": "1",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.24",
          "form": "그냥 알앗다구",
          "original_form": "그냥 알앗다구",
          "speaker_id": "1",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.25",
          "form": "귀차나,,",
          "original_form": "귀차나,,",
          "speaker_id": "1",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.26",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난 또 알아온줄 알았어",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난 또 알아온줄 알았어",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031215.1.27",
          "form": "이번 이닝에 점수내길 바라",
          "original_form": "이번 이닝에 점수내길 바라",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100031215.1.28",
          "form": "상대팀 투수도 꽤하넵",
          "original_form": "상대팀 투수도 꽤하넵",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100031215.1.29",
          "form": "ㅋㅋㅋㅋㅋ기아 투수 누구야?",
          "original_form": "ㅋㅋㅋㅋㅋ기아 투수 누구야?",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031215.1.30",
          "form": "말해도 이젠 내가 모르려나",
          "original_form": "말해도 이젠 내가 모르려나",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031215.1.31",
          "form": "임기영 이라는데?",
          "original_form": "임기영 이라는데?",
          "speaker_id": "1",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031215.1.32",
          "form": "그렇댕",
          "original_form": "그렇댕",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031215.1.33",
          "form": "1대 6으로 우리 이기는중 야구!",
          "original_form": "1대 6으로 우리 이기는중 야구!",
