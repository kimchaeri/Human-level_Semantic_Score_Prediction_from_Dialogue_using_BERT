      "utterance": [
        {
          "id": "MDRW2100025246.1.1",
          "form": "요즘 게임 뭐해?",
          "original_form": "요즘 게임 뭐해?",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025246.1.2",
          "form": "게임 엄청 하다가 요즘 쉬는중.. 넌?",
          "original_form": "게임 엄청 하다가 요즘 쉬는중.. 넌?",
          "speaker_id": "2",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025246.1.3",
          "form": "나는 게임은 필받으면 하는 편인데, 가장 최근에 한 게임은  모바일카트!",
          "original_form": "나는 게임은 필받으면 하는 편인데, 가장 최근에 한 게임은  모바일카트!",
          "speaker_id": "1",
          "time": "20210901 02:42"
        },
        {
          "id": "MDRW2100025246.1.4",
          "form": "카트라이더랑 같은거야?",
          "original_form": "카트라이더랑 같은거야?",
          "speaker_id": "2",
          "time": "20210901 02:42"
        },
        {
          "id": "MDRW2100025246.1.5",
          "form": "응응! 컴퓨터 카트라이더랑 같은거야~",
          "original_form": "응응! 컴퓨터 카트라이더랑 같은거야~",
          "speaker_id": "1",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025246.1.6",
          "form": "재밌겠다 예전에 카트 했었는데 ..",
          "original_form": "재밌겠다 예전에 카트 했었는데 ..",
          "speaker_id": "2",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025246.1.7",
          "form": "그치~~ 갑자기 지난 달에는 새로운 게임이 하고싶은거야!",
          "original_form": "그치~~ 갑자기 지난 달에는 새로운 게임이 하고싶은거야!",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025246.1.8",
          "form": "그래서 추억 속 게임 앵그리버드도 했었어ㅋㅋㅋ",
          "original_form": "그래서 추억 속 게임 앵그리버드도 했었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025246.1.9",
          "form": "모바일 게임중에 해본 거 있어?",
          "original_form": "모바일 게임중에 해본 거 있어?",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025246.1.10",
          "form": "나도 앵그리버드 진짜 열심히 했었어ㅋㅋ",
          "original_form": "나도 앵그리버드 진짜 열심히 했었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025246.1.11",
          "form": "역시!! 예전에는 버드도 몇마리 없었던 것 같은데",
          "original_form": "역시!! 예전에는 버드도 몇마리 없었던 것 같은데",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025246.1.12",
          "form": "이제는 몇게임 안해도 다양한 버드와 함께 게임할 수 있더라구!",
          "original_form": "이제는 몇게임 안해도 다양한 버드와 함께 게임할 수 있더라구!",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025246.1.13",
          "form": "오 또 해볼까?ㅎ",
          "original_form": "오 또 해볼까?ㅎ",
          "speaker_id": "2",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025246.1.14",
          "form": "퍼즐게임은 해?",
          "original_form": "퍼즐게임은 해?",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025246.1.15",
          "form": "퍼즐게임? 진짜 그 퍼즐맞추기같은 게임?",
          "original_form": "퍼즐게임? 진짜 그 퍼즐맞추기같은 게임?",
          "speaker_id": "1",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025246.1.16",
          "form": "웹에서 하는거는 해봤어!!! 연예인 사진 넣어서 하는거!",
          "original_form": "웹에서 하는거는 해봤어!!! 연예인 사진 넣어서 하는거!",
          "speaker_id": "1",
          "time": "20210901 03:05"
        },
        {
          "id": "MDRW2100025246.1.17",
          "form": "나그거 중독이었어ㅋㅋ",
          "original_form": "나그거 중독이었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:05"
        },
        {
          "id": "MDRW2100025246.1.18",
          "form": "너는 했어?",
          "original_form": "너는 했어?",
          "speaker_id": "2",
          "time": "20210901 03:07"
        },
        {
          "id": "MDRW2100025246.1.19",
          "form": "나도 중독이다싶을정도로 했었지!!",
          "original_form": "나도 중독이다싶을정도로 했었지!!",
          "speaker_id": "1",
          "time": "20210901 03:08"
        },
        {
          "id": "MDRW2100025246.1.20",
          "form": "나 혼자 하기도 하고, 친구한테 공유해서 같이하기도하고!",
          "original_form": "나 혼자 하기도 하고, 친구한테 공유해서 같이하기도하고!",
          "speaker_id": "1",
          "time": "20210901 03:08"
        },
        {
          "id": "MDRW2100025246.1.21",
          "form": "나도 공유도 하고 혼자도 했다",
          "original_form": "나도 공유도 하고 혼자도 했다",
          "speaker_id": "2",
          "time": "20210901 03:09"
        }
      ]
    }
